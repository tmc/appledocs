{
  "abstract": [
    {
      "text": "Implement the Live Caller ID Lookup app extension to provide call-blocking and identity services.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
