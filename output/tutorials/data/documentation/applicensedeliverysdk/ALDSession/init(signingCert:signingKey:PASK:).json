    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ALD provider signing cert",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signingCert"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ALD provider signing key in DER encoded format. When provided, one can use finalizeLicenseResponse() to automarically sign the payload.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If this key is not provided, then one must sign each license payload out of band and add it to the license response.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signingKey"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The server side secret provided to the provider",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "PASK"
        }
      ]
    }
