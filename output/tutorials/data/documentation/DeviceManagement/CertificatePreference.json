          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An email address (in RFC 822 format) or other name for which a preferred certificate is requested.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.12.0",
          "name": "Name",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UUID of the certificate payload within the same profile to use for the identity credential.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.12.0",
          "name": "PayloadCertificateUUID",
