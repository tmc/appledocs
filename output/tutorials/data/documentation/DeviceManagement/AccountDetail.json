  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: Device Assignment Services"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "AccountDetail"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Apple ID of the person who generated the currently in-use tokens.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "admin_id",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The legacy equivalent to the ",
                  "type": "text"
                },
                {
                  "code": "admin_id",
                  "type": "codeVoice"
                },
                {
                  "text": " key. This key is deprecated and may not be returned in future responses.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "facilitator_id",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The organization address.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "org_address",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The organization email address.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "org_email",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The customer ID. This key is available only in protocol version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "org_id",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The SHA hash of an org identifier. This helps Mobile Device Management (MDM) server match the hash with the ",
                  "type": "text"
                },
                {
                  "code": "organizationIdHash",
                  "type": "codeVoice"
                },
                {
                  "text": " key in the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Client-Configuration",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " API. This key is available only in protocol version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "org_id_hash",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The organization name.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "org_name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The organization phone.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "org_phone",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "edu",
                "org",
                "tvprovider"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of organization. Possible values are ",
                  "type": "text"
                },
                {
                  "code": "edu",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "org",
                  "type": "codeVoice"
                },
                {
                  "text": ". This key is available only in protocol version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "org_type",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Possible values: ",
                  "type": "text"
                },
                {
                  "code": "v1",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "v2",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                },
                {
                  "code": "v1",
                  "type": "codeVoice"
                },
                {
                  "text": " is for Apple Deployment Programs (like Device Enrollment Program or Volume Purchase Program) organizations and ",
                  "type": "text"
                },
                {
                  "code": "v2",
                  "type": "codeVoice"
                },
                {
                  "text": " is for Apple School Manager (ASM) organizations. Currently ",
                  "type": "text"
                },
                {
                  "code": "v2",
                  "type": "codeVoice"
                },
                {
                  "text": " is applicable only to educational organizations. This key is available only in protocol version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "org_version",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the MDM server.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "server_name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The system-generated server identifier.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "server_uuid",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of URLs available in the MDM service. This key is valid in X-Server-Protocol-Version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "urls",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Url",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-services:dep:Url",
              "text": "Url"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
