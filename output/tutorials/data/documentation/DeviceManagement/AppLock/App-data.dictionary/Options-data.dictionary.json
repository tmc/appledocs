              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system disables the ringer switch. When disabled, the ringer behavior depends on what position the switch was in when it was first disabled.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "6.0.0",
          "name": "DisableRingerSwitch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
