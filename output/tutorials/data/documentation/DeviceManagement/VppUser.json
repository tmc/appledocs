  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: VPP License Management"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "VppUser"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier supplied by the client when registering a user. An identifier must be unique within the organization.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientUserIdStr",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user’s email address.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "email",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The hash of the user’s iTunes Store ID. The ",
                  "type": "text"
                },
                {
                  "code": "itsIdHash",
                  "type": "codeVoice"
                },
                {
                  "text": " field is included only when the user is associated with an iTunes Store account.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "itsIdHash",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of licenses which have been assigned to this user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "licenses",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VppLicense",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-apps:vpp:VppLicense",
              "text": "VppLicense"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The status of the user’s account in VPP. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Registered",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Indicates the user has been created, but is not yet associated with an iTunes account.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Associated",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Indicates that the user has been associated with an iTunes account. When a user is associated with an iTunes account, an ",
                          "type": "text"
                        },
                        {
                          "code": "itsIdHash",
                          "type": "codeVoice"
                        },
                        {
                          "text": " is generated for the user.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Retired",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Indicates the user has been retired via  ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Retire-a-User",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Deleted",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Indicates that a VPP user is retired and its associated iTunes user has since been invited and associated with a new VPP user that shares the same ",
                          "type": "text"
                        },
                        {
                          "code": "clientUserIdStr",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Because there are two VPP users with distinct ",
                          "type": "text"
                        },
                        {
                          "code": "userId",
                          "type": "codeVoice"
                        },
                        {
                          "text": " values but the same ",
                          "type": "text"
                        },
                        {
                          "code": "clientUserIdStr",
                          "type": "codeVoice"
                        },
                        {
                          "text": " value, the Deleted status is used to ensure database consistency.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "A user with a ",
                  "type": "text"
                },
                {
                  "code": "Deleted",
                  "type": "codeVoice"
                },
                {
                  "text": " status will never change status; its sole purpose is to ensure that your software can recognize that the userId is no longer associated with the ",
                  "type": "text"
                },
                {
                  "code": "clientUserIdStr",
                  "type": "codeVoice"
                },
                {
                  "text": " record, and update any internal references appropriately.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "status",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier assigned by the VPP when registering the user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "userId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
