    {
      "items": [
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the user is prompted for a password when the screen saver is unlocked or stopped. When you use this prompt, you must also provide ",
                  "type": "text"
                },
                {
                  "code": "askForPasswordDelay",
                  "type": "codeVoice"
                },
                {
                  "text": ". Available in macOS 10.13 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.11.0",
          "name": "askForPassword",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of seconds to delay before the password will be required to unlock or stop the screen saver (the grace period). A value of ",
                  "type": "text"
                },
                {
                  "code": "2147483647",
                  "type": "codeVoice"
                },
                {
                  "text": " (for example, ",
                  "type": "text"
                },
                {
                  "code": "0x7FFFFFFF",
                  "type": "codeVoice"
                },
                {
                  "text": ") disables this requirement. To use this option, you must set ",
                  "type": "text"
                },
                {
                  "code": "askForPassword",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ". Available in macOS 10.13 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.11.0",
          "name": "askForPasswordDelay",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of seconds of inactivity before the screen saver activates (0 = Never activate).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.11.0",
          "name": "loginWindowIdleTime",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The full path to the screen-saver module to use.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.11.0",
          "name": "loginWindowModulePath",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the screen saver module.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.11.0",
          "name": "moduleName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
