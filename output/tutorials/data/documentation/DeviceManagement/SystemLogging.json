    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary enabling the logging level for subsystems. See ",
                  "type": "text"
                },
                {
                  "code": "Customizing Logging Behavior While Debugging",
                  "type": "codeVoice"
                },
                {
                  "text": " for more details about the format of the dictionary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.12.0",
          "name": "Subsystems",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SystemLogging/Subsystems-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:SystemLogging.Subsystems",
              "text": "SystemLogging.Subsystems"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This dictionary has one key, ",
                  "type": "text"
                },
                {
                  "code": "Enable-Private-Data",
                  "type": "codeVoice"
                },
                {
                  "text": ". Setting that value to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " enables private data logging for the entire system.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.12.0",
          "name": "System",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SystemLogging/System-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:SystemLogging.System",
              "text": "SystemLogging.System"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Not to be used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.12.0",
          "name": "Processes",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SystemLogging/Processes-data.dictionary",
