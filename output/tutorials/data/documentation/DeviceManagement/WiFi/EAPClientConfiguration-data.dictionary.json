    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "LEAP",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "17",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "EAP-SIM",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "18",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "TTLS",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "21",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "EAP-AKA",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "23",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "PEAP",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "25",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "EAP-FAST",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  },
                  {
                    "inlineContent": [
                      {
                        "text": "For EAP-TLS authentication without a network payload, install the necessary identity certificates and have your users select EAP-TLS mode in the 802.1X credentials dialog that appears when they connect to the network. For other EAP types, a network payload is necessary and must specify the correct settings for the network.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  },
                  {
                    "items": [
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "EAPFASTProvisionPAC: If ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": ", allows PAC provisioning.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "This value is only applicable if ",
                                "type": "text"
                              },
                              {
                                "code": "EAPFASTUsePAC",
                                "type": "codeVoice"
                              },
                              {
                                "text": " is ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": ". This value must be ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": " for EAP-FAST PAC usage to succeed because there’s no other way to provision a PAC.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "EAPFASTProvisionPACAnonymously: If ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": ", provisions the device anonymously. Note that there are known machine-in-the-middle attacks for anonymous provisioning.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "EAPFASTUsePAC: If ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": ", the device uses an existing PAC if it’s present. Otherwise, the server must present its identity using a certificate.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "EAPSIMNumberOfRANDs: The minimum number of RAND values to accept from the server.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "For use with EAP-SIM only.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "OneTimeUserPassword: If ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": ", the user receives a prompt for a password each time they connect to the network.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "OuterIdentity: A name that hides the user’s true name. The user’s actual name appears only inside the encrypted tunnel. For example, you might set this to ",
                                "type": "text"
                              },
                              {
                                "inlineContent": [
                                  {
                                    "text": "anonymous",
                                    "type": "text"
                                  }
                                ],
                                "type": "emphasis"
                              },
                              {
                                "text": " or ",
                                "type": "text"
                              },
                              {
                                "inlineContent": [
                                  {
                                    "text": "anon",
                                    "type": "text"
                                  }
                                ],
                                "type": "emphasis"
                              },
                              {
                                "text": ", or ",
                                "type": "text"
                              },
                              {
                                "inlineContent": [
                                  {
                                    "text": "anon@mycompany.net",
                                    "type": "text"
                                  }
                                ],
                                "type": "emphasis"
                              },
                              {
                                "text": ". It can increase security because an attacker can’t see the authenticating user’s name in the clear.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "This key is only relevant to TTLS, PEAP, and EAP-FAST.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "This field is required if ",
                                "type": "text"
                              },
                              {
                                "code": "TLSMinimumVersion",
                                "type": "codeVoice"
                              },
                              {
                                "text": " is ",
                                "type": "text"
                              },
                              {
                                "code": "1.3",
                                "type": "codeVoice"
                              },
                              {
                                "text": ".",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "PayloadCertificateAnchorUUID: An array of the UUID of a certificate payloads in the same profile to trust for authentication. Use this key to prevent the device from asking the user whether to trust the listed certificates. Dynamic trust (the certificate dialogue) is in a disabled state if you specify this property without also enabling ",
                                "type": "text"
                              },
                              {
                                "code": "TLSAllowTrustExceptions",
                                "type": "codeVoice"
                              },
                              {
                                "text": ".",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "SystemModeCredentialsSource: Set this string to ",
                                "type": "text"
                              },
                              {
                                "code": "ActiveDirectory",
                                "type": "codeVoice"
                              },
                              {
                                "text": " to use the AD computer name and password credentials.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "If using this property, you can’t use ",
                                "type": "text"
                              },
                              {
                                "code": "SystemModeUseOpenDirectoryCredentials",
                                "type": "codeVoice"
                              },
                              {
                                "text": ".",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "SystemModeUseOpenDirectoryCredentials: If ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": ", the system mode connection tries to use the Open Directory credentials.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "If using this property, you can’t use ",
                                "type": "text"
                              },
                              {
                                "code": "SystemModeCredentialsSource",
                                "type": "codeVoice"
                              },
                              {
                                "text": ".",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "TLSAllowTrustExceptions: If ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": ", allows a dynamic trust decision by the user. The dynamic trust is the certificate dialogue that appears when the system doesn’t trust a certificate.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "If ",
                                "type": "text"
                              },
                              {
                                "code": "false",
                                "type": "codeVoice"
                              },
                              {
                                "text": ", the authentication fails if the system doesn’t already trust the certificate.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "As of iOS 8, Apple no longer supports this key.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "TLSCertificateIsRequired: If ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": ", allows for two-factor authentication for EAP-TTLS, PEAP, or EAP-FAST. If ",
                                "type": "text"
                              },
                              {
                                "code": "false",
                                "type": "codeVoice"
                              },
                              {
                                "text": ", allows for zero-factor authentication for EAP-TLS.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "If you don’t specify a value, the default is ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": " for EAP-TLS, and ",
                                "type": "text"
                              },
                              {
                                "code": "false",
                                "type": "codeVoice"
                              },
                              {
                                "text": " for other EAP types.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "TLSMaximumVersion: The maximum TLS version for EAP authentication.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "TLSMinimumVersion: The minimum TLS version for EAP authentication.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "TLSTrustedCertificates: An array of trusted certificates. Each entry in the array must contain certificate data that represents an anchor certificate used for verifying the server certificate.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "TLSTrustedServerNames: The list of accepted server certificate common names. If a server presents a certificate that isn’t in this list, the system doesn’t trust it.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "If you specify this property, the system disables dynamic trust (the certificate dialog) unless you also specify ",
                                "type": "text"
                              },
                              {
                                "code": "TLSAllowTrustExceptions",
                                "type": "codeVoice"
                              },
                              {
                                "text": " with the value ",
                                "type": "text"
                              },
                              {
                                "code": "true",
                                "type": "codeVoice"
                              },
                              {
                                "text": ".",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          },
                          {
                            "inlineContent": [
                              {
                                "text": "If necessary, use wildcards to specify the name, such as ",
                                "type": "text"
                              },
                              {
                                "code": "wpa.*.example.com",
                                "type": "codeVoice"
                              },
                              {
                                "text": ".",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "TTLSInnerAuthentication: The inner authentication that the TTLS module uses.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "UserName: The user name for the account. If you don’t specify a value, the system prompts the user during login.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      },
                      {
                        "content": [
                          {
                            "inlineContent": [
                              {
                                "text": "UserPassword: The user’s password. If you don’t specify a value, the system prompts the user during login.",
                                "type": "text"
                              }
                            ],
                            "type": "paragraph"
                          }
                        ]
                      }
                    ],
                    "type": "unorderedList"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "43",
                    "type": "text"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        }
      ],
