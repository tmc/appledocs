    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The human-readable description of this payload. This description appears on the Detail screen.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PayloadDescription",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The human-readable name for the profile payload. The name appears on the Detail screen and doesn’t need to be unique.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PayloadDisplayName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The reverse-DNS-style identifier for the payload. This identifier is usually the same as the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/TopLevel",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " value, with an additional appended component. This string must be unique within the profile.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "During a profile replacement, the system updates payloads with the same ",
                  "type": "text"
                },
                {
                  "code": "PayloadIdentifier",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "PayloadUUID",
                  "type": "codeVoice"
                },
                {
                  "text": " in the old and new profiles.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PayloadIdentifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The human-readable string containing the name of the organization that provides the profile. This value doesn’t need to match the organization payload value in the enclosing dictionary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PayloadOrganization",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The payload type, which each payload domain’s reference page specifies.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PayloadType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The globally unique identifier for the payload. The actual content is unimportant, but must be globally unique. In macOS, use ",
                  "type": "text"
                },
                {
                  "code": "uuidgen",
                  "type": "codeVoice"
                },
                {
                  "text": " to generate UUIDs.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "During a profile replacement, the system updates payloads with the same ",
                  "type": "text"
                },
                {
                  "code": "PayloadIdentifier",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "PayloadUUID",
                  "type": "codeVoice"
                },
                {
                  "text": " in the old and new profiles.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PayloadUUID",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The version of this specific payload.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PayloadVersion",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        }
      ],
