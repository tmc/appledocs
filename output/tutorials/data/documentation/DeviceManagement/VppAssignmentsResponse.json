      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of dictionaries representing the current assignments.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "assignments",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VppAssignment",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-apps:vpp:VppAssignment",
              "text": "VppAssignment"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of assignments in the current page.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "assignmentsInCurrentPage",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value currently associated with the provided ",
                  "type": "text"
                },
                {
                  "code": "sToken",
                  "type": "codeVoice"
                },
                {
                  "text": ". This field is only included in the response when a value has been set via the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Client-Configuration",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " endpoint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/protecting-your-vpp-account",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientContext",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The index of the page being returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "currentPageIndex",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UNIX epoch timestamp, in milliseconds, when the account’s ",
                  "type": "text"
                },
                {
                  "code": "sToken",
                  "type": "codeVoice"
                },
                {
                  "text": " or password expires (whichever is earlier).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "expirationMillis",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The location associated with the provided ",
                  "type": "text"
                },
                {
                  "code": "sToken",
                  "type": "codeVoice"
                },
                {
                  "text": ". This field is only returned when a location token is used with an Apple School Manager account.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "location",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VppLocation",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-apps:vpp:VppLocation",
              "text": "VppLocation"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The index of the next assignments page. This field will only be returned if there are additional assignments pages to read.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nextPageIndex",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ID to be used for subsequent assignments page lookups. This field will only be returned if there are greater than 300 assignments.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "requestId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The status code for the response. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " = Success.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "-1",
                  "type": "codeVoice"
                },
                {
                  "text": " = Failure.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "status",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of assignments for the provided criteria.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalAssignments",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of pages of assignments. There will be 300 assignments per page.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalPages",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
