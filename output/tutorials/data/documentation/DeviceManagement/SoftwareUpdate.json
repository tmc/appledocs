    {
      "items": [
        {
          "attributes": [
            {
              "kind": "default",
              "value": "true"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", prerelease software can be installed on this computer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.7.0",
          "name": "AllowPreReleaseInstallation",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "true"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", deselects the “Install app updates from the App Store” option and prevents the user from changing the option.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.7.0",
          "name": "AutomaticallyInstallAppUpdates",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "true"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", restricts the “Install macOS Updates” option and prevents the user from changing the option.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.7.0",
          "name": "AutomaticallyInstallMacOSUpdates",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "true"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", deselects the “Check for updates” option and prevents the user from changing the option.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.7.0",
          "name": "AutomaticCheckEnabled",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "true"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", deselects the “Download new updates when available from the App Store” option and prevents the user from changing the option.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.7.0",
          "name": "AutomaticDownload",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL of the software update catalog. This property is not supported in macOS 11 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "introducedVersion": "10.7.0",
          "name": "CatalogURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "true"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", restricts the automatic installation of configuration data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.7.0",
          "name": "ConfigDataInstall",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "true"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", disables the automatic installation of critical updates and prevents the user from changing the “Install system data files and security updates” option.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.7.0",
          "name": "CriticalUpdateInstall",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", restrict app installations to admin users. This key has the same function as the  ",
                  "type": "text"
                },
                {
                  "code": "restrict-store-require-admin-to-install",
                  "type": "codeVoice"
                },
                {
                  "text": " key in the ",
                  "type": "text"
                },
                {
                  "code": "com.apple.appstore",
                  "type": "codeVoice"
                },
                {
                  "text": " payload.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.7.0",
          "name": "restrict-software-update-require-admin-to-install",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        }
