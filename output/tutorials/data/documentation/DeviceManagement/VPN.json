          "content": [
            {
              "inlineContent": [
                {
                  "text": "The vendor-specific configuration dictionary, which the system reads only when ",
                  "type": "text"
                },
                {
                  "code": "VPNSubType",
                  "type": "codeVoice"
                },
                {
                  "text": " has a value. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "VendorConfig",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VPN/VendorConfig-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:VPN.VendorConfig",
              "text": "VPN.VendorConfig"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dictionary to use to specify a VPN when ",
                  "type": "text"
                },
                {
                  "code": "VPNType",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "VPN",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "IPSec",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "IKEv2",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "VPN",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VPN/VPN-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:VPN.VPN",
              "text": "VPN.VPN"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An identifier for a vendor-specified configuration dictionary when the value for ",
                  "type": "text"
                },
                {
                  "code": "VPNType",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "VPN",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "VPNType",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "VPN",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system requires this field. If the configuration targets a VPN solution that uses a VPN plugin, then this field contains the bundle identifier of the plugin. Here are some examples:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Cisco AnyConnect: ",
                          "type": "text"
                        },
                        {
                          "code": "com.cisco.anyconnect.applevpn.plugin",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Juniper SSL: ",
                          "type": "text"
                        },
                        {
                          "code": "net.juniper.sslvpn",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "F5 SSL: ",
                          "type": "text"
                        },
                        {
                          "code": "com.f5.F5-Edge-Client.vpnplugin",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "SonicWALL Mobile Connect: ",
                          "type": "text"
                        },
                        {
                          "code": "com.sonicwall.SonicWALL-SSLVPN.vpnplugin",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "``Aruba VIA: ",
                          "type": "text"
                        },
                        {
                          "code": "com.arubanetworks.aruba-via.vpnplugin",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "If the configuration targets a VPN solution that uses a network extension provider, then this field contains the bundle identifier of the app that contains the provider. Contact the VPN solution vendor for the value of the identifier.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "VPNType",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "IKEv2",
                  "type": "codeVoice"
                },
                {
                  "text": ", then the ",
                  "type": "text"
                },
                {
                  "code": "VPNSubType",
                  "type": "codeVoice"
                },
                {
                  "text": " field is optional and reserved for future use. If it’s specified, it needs to contain an empty string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "VPNSubType",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "VPN",
                "L2TP",
                "IPSec",
                "IKEv2",
                "AlwaysOn",
                "TransparentProxy"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of the VPN, which defines which settings are appropriate for this VPN payload.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If the type is ",
                  "type": "text"
                },
                {
                  "code": "VPN",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "TransparentProxy",
                  "type": "codeVoice"
                },
                {
                  "text": ", then the system requires a value for ",
                  "type": "text"
                },
                {
                  "code": "VPNSubType",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "TransparentProxy",
                  "type": "codeVoice"
                },
                {
                  "text": " is only available in macOS. ",
                  "type": "text"
                },
                {
                  "code": "L2TP",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "IPSec",
                  "type": "codeVoice"
                },
                {
                  "text": " aren’t available in tvOS. ",
                  "type": "text"
                },
                {
                  "code": "AlwaysOn",
                  "type": "codeVoice"
                },
                {
                  "text": " is only available on iOS and Apple Watch pairing isn’t supported with ",
                  "type": "text"
                },
                {
                  "code": "AlwaysOn",
                  "type": "codeVoice"
                },
                {
                  "text": ". For a previously paired Apple Watch, all phone-watch communications cease when ",
                  "type": "text"
                },
                {
                  "code": "AlwaysOn",
                  "type": "codeVoice"
                },
                {
                  "text": " is enabled. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "VPNType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
