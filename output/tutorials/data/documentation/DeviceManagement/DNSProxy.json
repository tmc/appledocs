      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle identifier of the app containing the DNS proxy network extension.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "AppBundleIdentifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A globally-unique identifier for this DNS proxy configuration. Managed apps with the same ",
                  "type": "text"
                },
                {
                  "code": "DNSProxyUUID",
                  "type": "codeVoice"
                },
                {
                  "text": " in their app attributes have their DNS lookups traffic processed by the proxy.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "DNSProxyUUID",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle identifier of the DNS proxy network extension to use. Declaring the bundle identifier is useful for apps that contain more than one DNS proxy extension.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "ProviderBundleIdentifier",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dictionary of vendor-specific configuration items.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "ProviderConfiguration",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/DNSProxy/ProviderConfiguration-data.dictionary",
