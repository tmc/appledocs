    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Allowed",
                "ReadOnly",
                "Disallowed"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies the mount policy for external storage:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Allowed",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": the system can mount external storage that is read-write or read-only.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "ReadOnly",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": the system can only mount read-only external storage. Note that external storage that is read-write will not be mounted read-only.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Disallowed",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": The system can’t mount any external storage.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "introducedVersion": "15.0.0",
          "name": "ExternalStorage",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Allowed",
                "ReadOnly",
                "Disallowed"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies the mount policy for network storage:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Allowed",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": the system can mount network storage that is read-write or read-only.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "ReadOnly",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": the system can only mount read-only network storage. Note that network storage that is read-write will not be mounted read-only.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Disallowed",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": The system can’t mount any network storage.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
