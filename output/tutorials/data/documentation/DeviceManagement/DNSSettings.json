    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary that defines a configuration for an encrypted DNS server.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "DNSSettings",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/DNSSettings/DNSSettings-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:DNSSettings.DNSSettings",
              "text": "DNSSettings.DNSSettings"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of rules that define the DNS settings. If not set, the system always applies the DNS settings. These rules are identical to the ",
                  "type": "text"
                },
                {
                  "code": "OnDemandRules",
                  "type": "codeVoice"
                },
                {
                  "text": " array in VPN payloads.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "OnDemandRules",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/DNSSettings/OnDemandRulesElement",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:DNSSettings.OnDemandRulesElement",
              "text": "DNSSettings.OnDemandRulesElement"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UUID that points to an identity certificate payload. The system uses this identity to authenticate the user to the DNS resolver.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "PayloadCertificateUUID",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system prohibits users from disabling DNS settings. This key is only available on supervised devices.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "ProhibitDisablement",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Specify ",
              "type": "text"
            },
            {
              "code": "com.apple.dnsSettings.managed",
              "type": "codeVoice"
            },
            {
              "text": " as the payload type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "When installed from an MDM, the setting only applies to managed wifi networks",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "When installed manually, this setting also applies to cellular.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Profile-Availability",
          "level": 3,
