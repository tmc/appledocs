  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: iOS",
            "Unsupported OS: macOS",
            "Unsupported OS: tvOS"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "SoftwareUpdateEnforcementSpecific"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL of a web page that shows details that the organization provides about the enforced update.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "DetailsURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The target build version to update the device to by the appropriate time, for example, ",
                  "type": "text"
                },
                {
                  "code": "20A242",
                  "type": "codeVoice"
                },
                {
                  "text": ". The system uses the build version for testing during seeding periods. The build version can include a supplemental version identifier, for example, ",
                  "type": "text"
                },
                {
                  "code": "20A242a",
                  "type": "codeVoice"
                },
                {
                  "text": ". If the build version isn’t consistent with the target OS version specified in the ",
                  "type": "text"
                },
                {
                  "code": "TargetOSVersion",
                  "type": "codeVoice"
                },
                {
                  "text": " key, the target OS version takes precedence.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "TargetBuildVersion",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The local date time value that specifies when to force install the software update. Use the format ",
                  "type": "text"
                },
                {
                  "code": "yyyy-mm-ddThh:mm:ss",
                  "type": "codeVoice"
                },
                {
                  "text": ", which is derived from RFC3339 but doesn’t include a time zone offset. If the user doesn’t trigger the software update before this time, the device force installs it.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "TargetLocalDateTime",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The target OS version to update the device to by the appropriate time. This is the OS version number, for example, ",
                  "type": "text"
                },
                {
                  "code": "16.1",
                  "type": "codeVoice"
                },
                {
                  "text": ". It may also include a supplemental version identifier, for example, ",
                  "type": "text"
                },
                {
                  "code": "16.1.1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "TargetOSVersion",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Specify ",
              "type": "text"
            },
            {
              "code": "com.apple.configuration.softwareupdate.enforcement.specific",
              "type": "codeVoice"
            },
            {
              "text": " as the declaration type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If the ",
              "type": "text"
            },
            {
              "code": "TargetVersion",
              "type": "codeVoice"
            },
            {
              "text": " is an OS version that includes both a minor and patch version, then the system installs that specific version, for example, ",
              "type": "text"
            },
            {
              "code": "16.1.1",
              "type": "codeVoice"
            },
            {
              "text": ". If the minor version doesn’t include a patch version, the system installs the latest available patch version. For example, if the ",
              "type": "text"
            },
            {
              "code": "TargetVersion",
              "type": "codeVoice"
            },
            {
              "text": " is ",
              "type": "text"
            },
            {
              "code": "16.1",
              "type": "codeVoice"
            },
            {
              "text": " and a ",
              "type": "text"
            },
            {
              "code": ".1",
              "type": "codeVoice"
            },
            {
              "text": " patch is available, the system installs ",
              "type": "text"
            },
            {
              "code": "16.1.1",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The system can only install a supplemental software update on a device that already has the base OS version installed. For example, the system can only install a ",
              "type": "text"
            },
            {
              "code": "16.1",
              "type": "codeVoice"
            },
            {
              "text": "(a) update on a device that currently has ",
              "type": "text"
            },
            {
              "code": "16.1",
              "type": "codeVoice"
            },
            {
              "text": " installed, but it can’t install that update on a device that only has ",
              "type": "text"
            },
            {
              "code": "16.0",
              "type": "codeVoice"
            },
            {
              "text": " installed. To update to a supplemental version from an older base version, use two configurations. Use the first configuration to update to the new base version, and the second configuration to update the new base version to its supplemental version.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If the device isn’t running at the target date-time, the system enforces the update one hour after restarting, or when the device meets all required conditions, such as minimum battery level.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Configuration-Availability",
          "level": 3,
          "text": "Configuration Availability",
          "type": "heading"
        },
        {
          "header": "row",
          "rows": [
            [
              [
                {
                  "inlineContent": [
                    {
                      "text": "Allowed in Device Enrollment",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "iOS, macOS",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "text": "Allowed in User Enrollment",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "-",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "text": "Allowed in Local Enrollment",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "-",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "text": "Allowed in System Scope",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "iOS, macOS, Shared iPad",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "text": "Allowed in User Scope",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "-",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ]
          ],
          "type": "table"
        }
      ],
      "kind": "content"
    }
  ],
