    "doc://com.apple.devicemanagement/documentation/DeviceManagement/SoftwareUpdateEnforcementSpecific": {
      "abstract": [
        {
          "text": "A software update enforcement policy for a specific OS release.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "text",
          "text": "object "
