  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: Device Assignment Services"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "RosterPerson"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The email address of the person that Apple School Manager (ASM) displays. Available in X-Protocol version 5 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "email_address",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The person’s first (given) name. Maximum length iw 1024 UTF-8 characters. Available in protocol version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "first_name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The grade information for the student. Maximum length is 256 UTF-8 characters. Value can be null. Not used for instructors.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "grade",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system enables federated authentication for the person from an external identity provider. Available in X-Protocol version 6 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "is_federated_auth",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The person’s last name. Maximum length is 1024 UTF-8 characters. Available in protocol version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "last_name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The managed Apple ID for the person. Maximum length is 1024 UTF-8 characters.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "managed_apple_id",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The person’s middle name. Maximum length is 1024 UTF-8 characters. Available in protocol version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "middle_name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The person’s name. Maximum length is 1024 UTF-8 characters.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time stamp, in iSO 8601 format, when the system added, updated, or deleted the person.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "op_date",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The password policy of the person. Possible values are ",
                  "type": "text"
                },
                {
                  "code": "complex",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "four",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "six",
                  "type": "codeVoice"
                },
                {
                  "text": ". Available in protocol version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "passcode_type",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "personid",
                  "type": "codeVoice"
                },
                {
                  "text": " of the person that ASM displays. Available in X-Protocol version 4 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "person_id",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The SIS ",
                  "type": "text"
                },
                {
                  "code": "username",
                  "type": "codeVoice"
                },
                {
                  "text": " of the person that ASM displays. Available in X-Protocol version 5 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sis_username",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "LDAP",
                "SIS",
                "CSV",
                "MANUAL",
                "SYSTEM",
                "ENROLLMENT",
                "DEP",
                "FORMULA_GENERATED",
                "SFTP"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The creation data source of the class.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier that the organization configures for the person. Maximum length is 256 UTF-8 characters.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source_system_identifier",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The status of the person. Possible values are ",
                  "type": "text"
                },
                {
                  "code": "Active,",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "InActive,",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "FailedPasswordLocked",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "AtoLocked",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                },
                {
                  "code": "AtoLocked",
                  "type": "codeVoice"
                },
                {
                  "text": " indicates that the system detects account take over and temporarily locks the person until an authentic user claims it. Available in X-Protocol version 3 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "status",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier for the person. Maximum length 256 UTF-8 characters.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "unique_identifier",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
