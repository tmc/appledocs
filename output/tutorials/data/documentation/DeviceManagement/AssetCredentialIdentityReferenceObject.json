          "content": [
            {
              "inlineContent": [
                {
                  "text": "The media type that describes the data, which needs to use a URL that starts with ",
                  "type": "text"
                },
                {
                  "code": "https://",
                  "type": "codeVoice"
                },
                {
                  "text": ". If present, the system checks the actual media type of the downloaded data, and an error occurs if the values don’t match.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "ContentType",
