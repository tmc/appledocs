    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The device ID of the AirPlay destination in the format ",
                  "type": "text"
                },
                {
                  "code": "xx:xx:xx:xx:xx:xx",
                  "type": "codeVoice"
                },
                {
                  "text": ". This field isn’t case-sensitive.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The system limits the list of visible AirPlay destinations to devices that are present in the ",
                  "type": "text"
                },
                {
                  "code": "AllowList",
                  "type": "codeVoice"
                },
                {
                  "text": " field of all installed AirPlay payloads.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Specifying the same MACAddress more than once, whether in the same payload across different payloads, results in undefined behavior.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "As of tvOS 18, ",
                  "type": "text"
                },
                {
                  "code": "DeviceID",
                  "type": "codeVoice"
                },
                {
                  "text": " isn’t supported.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "introducedVersion": "7.0.0",
          "name": "DeviceID",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the AirPlay device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The system limits the list of visible AirPlay destinations to devices that are present in the ",
                  "type": "text"
                },
                {
                  "code": "AllowList",
                  "type": "codeVoice"
                },
                {
                  "text": " field of all installed AirPlay payloads.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "7.0.0",
          "name": "DeviceName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
