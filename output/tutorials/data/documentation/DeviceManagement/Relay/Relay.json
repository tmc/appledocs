    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary that contains custom HTTP header keys and values to add to each request. The dictionary key name represents the HTTP header field name to use, and the dictionary value is the string to use as the HTTP header field value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "AdditionalHTTPHeaderFields",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Relay/Relay/AdditionalHTTPHeaderFields-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:Relay.Relay.AdditionalHTTPHeaderFields",
              "text": "Relay.Relay.AdditionalHTTPHeaderFields"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL or URI template, as defined in RFC 9298, of a relay server that’s reachable using HTTP/2 and supports proxying TCP and UDP using the CONNECT method.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Each relay needs to include either ",
                  "type": "text"
                },
                {
                  "code": "HTTP2RelayURL",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "HTTP3RelayURL",
                  "type": "codeVoice"
                },
                {
                  "text": ", or it can include both.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "HTTP2RelayURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL or URI template, as defined in RFC 9298, of a relay server that’s reachable using HTTP/3 and supports proxying TCP and UDP using the CONNECT method.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Each relay needs to include either ",
                  "type": "text"
                },
                {
                  "code": "HTTP2RelayURL",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "HTTP3RelayURL",
                  "type": "codeVoice"
                },
                {
                  "text": ", or it can include both.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "HTTP3RelayURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UUID that points to an identity certificate payload, which the system uses to authenticate the user to the relay server.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "PayloadCertificateUUID",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of raw public keys that the system uses to authenticate the server during a TLS handshake. The server needs to use one of the keys in the handshake to authenticate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If this array is empty, the system uses the default TLS trust evaluation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "RawPublicKeys",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[data]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
