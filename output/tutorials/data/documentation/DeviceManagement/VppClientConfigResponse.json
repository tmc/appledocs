  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: VPP License Management"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "VppClientConfigResponse"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Apple Push Notification token to use for notifications.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "apnToken",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The AppleID associated with the provided sToken.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "appleId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value currently associated with the provided sToken. This field is only included in the response when a value has been set via the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Client-Configuration",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " endpoint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/protecting-your-vpp-account",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientContext",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The two-letter ISO 3166-1 code that designates the country where the VPP account is located. For example, ",
                  "type": "text"
                },
                {
                  "code": "US",
                  "type": "codeVoice"
                },
                {
                  "text": " stands for United States, ",
                  "type": "text"
                },
                {
                  "code": "CA",
                  "type": "codeVoice"
                },
                {
                  "text": " for Canada, ",
                  "type": "text"
                },
                {
                  "code": "JP",
                  "type": "codeVoice"
                },
                {
                  "text": " for Japan, and so on.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "countryCode",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value to be passed for the platform parameter in the ",
                  "type": "text"
                },
                {
                  "code": "contentMetadataLookupUrl",
                  "type": "codeVoice"
                },
                {
                  "text": " request. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "volumestore",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": For apps in the educational store.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "enterprisestore",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": For apps in the enterprise store.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/getting-app-and-book-information-legacy",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "defaultPlatform",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The email address associated with the provided sToken.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "email",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The human-readable explanation of the error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "errorMessage",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The numeric code of the error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "errorNumber",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UNIX epoch timestamp, in milliseconds, when the account’s sToken or password expires (whichever is earlier).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "expirationMillis",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The location associated with the provided sToken. This field is only returned when a location token is used with an Apple School Manager account.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "location",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VppLocation",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-apps:vpp:VppLocation",
              "text": "VppLocation"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier assigned to an organization by the VPP.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "organizationId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The hash of ",
                  "type": "text"
                },
                {
                  "code": "organizationId",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "organizationIdHash",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The status code for the response. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "0",
                          "type": "codeVoice"
                        },
                        {
                          "text": " = Success.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "-1",
                          "type": "codeVoice"
                        },
                        {
                          "text": " = Failure.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "status",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique library identifier. When querying records using multiple tokens that may share libraries, use the ",
                  "type": "text"
                },
                {
                  "code": "uId",
                  "type": "codeVoice"
                },
                {
                  "text": " field to filter duplicates. In this way, you can avoid double-counting records when duplicate tokens are uploaded by different content managers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "uId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
