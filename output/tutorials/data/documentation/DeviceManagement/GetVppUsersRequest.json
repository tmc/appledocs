    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A token that signifies which batch is being returned. A ",
                  "type": "text"
                },
                {
                  "code": "batchToken",
                  "type": "codeVoice"
                },
                {
                  "text": " value is generated by the server and can be several kilobytes in size.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "For an initial request that doesn’t include ",
                  "type": "text"
                },
                {
                  "code": "batchToken",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "sinceModifiedToken",
                  "type": "codeVoice"
                },
                {
                  "text": ", a ",
                  "type": "text"
                },
                {
                  "code": "batchToken",
                  "type": "codeVoice"
                },
                {
                  "text": " value is returned if the number of results exceeds a server-controlled limit. Subsequent requests must include batchToken. As long as additional batches remain, the server returns a new ",
                  "type": "text"
                },
                {
                  "code": "batchToken",
                  "type": "codeVoice"
                },
                {
                  "text": " value in its response.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/retrieving-a-large-record-set#Batched-Responses",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "batchToken",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", returns retired users in the results.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "includeRetired",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", returns only retired users in the results.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "includeRetiredOnly",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value from 1 to the ",
                  "type": "text"
                },
                {
                  "code": "totalBatchCount",
                  "type": "codeVoice"
                },
                {
                  "text": " value that specifies the batch of results to return. For requests with a value equal to the ",
                  "type": "text"
                },
                {
                  "code": "totalBatchCount",
                  "type": "codeVoice"
                },
                {
                  "text": " value, the ",
                  "type": "text"
                },
                {
                  "code": "sinceModifiedToken",
                  "type": "codeVoice"
                },
                {
                  "text": " value is returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "overrideIndex",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A token that marks a place in time. After all records have been returned for a request, the server includes a ",
                  "type": "text"
                },
                {
                  "code": "sinceModifiedToken",
                  "type": "codeVoice"
                },
                {
                  "text": " value in the response. Use it in subsequent requests to get licenses that have been modified since the token was generated.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/retrieving-a-large-record-set#About-SinceModifiedToken",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sinceModifiedToken",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The authentication token. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/managing-apps-and-books-through-web-services-legacy#Authentication",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sToken",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "batchToken",
                  "type": "codeVoice"
                },
                {
                  "text": " encodes the original value of ",
                  "type": "text"
                },
                {
                  "code": "includeRetired",
                  "type": "codeVoice"
                },
                {
                  "text": "; therefore, if a ",
                  "type": "text"
                },
                {
                  "code": "batchToken",
                  "type": "codeVoice"
                },
                {
                  "text": " is present on the request, the ",
                  "type": "text"
                },
                {
                  "code": "includeRetired",
                  "type": "codeVoice"
                },
                {
                  "text": " field (if passed) is ignored.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
