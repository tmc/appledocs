    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier of an asset declaration that contains a reference to the files to use for system service configuration. Ensure that the corresponding asset:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Is of type ",
                          "type": "text"
                        },
                        {
                          "code": "com.apple.asset.data",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Is a zip archive of an entire directory",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Has a ",
                          "type": "text"
                        },
                        {
                          "code": "Reference",
                          "type": "codeVoice"
                        },
                        {
                          "text": " key that includes the ",
                          "type": "text"
                        },
                        {
                          "code": "ContentType",
                          "type": "codeVoice"
                        },
                        {
                          "text": " and ",
                          "type": "text"
                        },
                        {
                          "code": "Hash-SHA-256",
                          "type": "codeVoice"
                        },
                        {
                          "text": " keys, which the system requires",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "The system expands the zip archive and stores the data in a well-known location for the service.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "DataAssetReference",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier of the system service with managed configuration files. Use a reverse DNS style for this identifier. However, the system reserves ",
                  "type": "text"
                },
                {
                  "code": "com.apple.",
                  "type": "codeVoice"
                },
                {
                  "text": " prefix for built-in services. The available built-in services are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "com.apple.sshd",
                          "type": "codeVoice"
                        },
                        {
                          "text": " configures sshd",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "com.apple.sudo",
                          "type": "codeVoice"
                        },
                        {
                          "text": " configures sudo",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "com.apple.pam",
                          "type": "codeVoice"
                        },
                        {
                          "text": " configures PAM",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "com.apple.cups",
                          "type": "codeVoice"
                        },
                        {
                          "text": " configures CUPS",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "com.apple.apache.httpd",
                          "type": "codeVoice"
                        },
                        {
                          "text": " configures Apache httpd",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "com.apple.bash",
                          "type": "codeVoice"
                        },
                        {
                          "text": " configures bash",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "com.apple.zsh",
                          "type": "codeVoice"
                        },
                        {
                          "text": " configures zsh",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "introducedVersion": "14.0.0",
