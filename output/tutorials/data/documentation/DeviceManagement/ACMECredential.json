  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: iOS",
            "Unsupported OS: macOS",
            "Unsupported OS: tvOS",
            "Unsupported OS: visionOS",
            "Unsupported OS: watchOS"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "ACMECredential"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the device provides attestations that describe the device and the generated key to the ACME server. The server can use the attestations as strong evidence that the key is bound to the device, and that the device has properties listed in the attestation. The server can use that as part of a trust score to decide whether to issue the requested certificate. When ",
                  "type": "text"
                },
                {
                  "code": "Attest",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", set ",
                  "type": "text"
                },
                {
                  "code": "HardwareBound",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ". On macOS, set this key, if present, to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Attest",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The server can use this as a nonce to prevent issuing multiple certificates. It also indicates to the ACME server that the device has access to a valid client identifier that the enterprise infrastructure issued. This can help the ACME server determine whether to trust the device, however this is a relatively weak indication because of the risk that an attacker may intercept and duplicate the client identifier.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "ClientIdentifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies the directory URL of the ACME server. Use the ",
                  "type": "text"
                },
                {
                  "code": "https",
                  "type": "codeVoice"
                },
                {
                  "text": " scheme for the URL.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "DirectoryURL",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The device requests this extended key usage for the certificate that the ACME server issues. The ACME server may override or ignore this field in the certificate it issues.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The value is an array of strings. Each string is an OID in dotted notation. For example, ",
                  "type": "text"
                },
                {
                  "code": "[\"1.3.6.1.5.5.7.3.2\", \"1.3.6.1.5.5.7.3.4\"]",
                  "type": "codeVoice"
                },
                {
                  "text": " indicates client authentication and email protection.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "ExtendedKeyUsage",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", the private key isn’t bound to the device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the private key is bound to the device. The Secure Enclave generates the key pair, and the private key is cryptographically entangled with a system key. This protects the private key from being exported.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "KeyType",
                  "type": "codeVoice"
                },
                {
                  "text": " needs to be ",
                  "type": "text"
                },
                {
                  "code": "ECSECPrimeRandom",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "KeySize",
                  "type": "codeVoice"
                },
                {
                  "text": " needs to be ",
                  "type": "text"
                },
                {
                  "code": "256",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "384",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "On macOS, this is a required key. Set the value to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "HardwareBound",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The valid values for ",
                  "type": "text"
                },
                {
                  "code": "KeySize",
                  "type": "codeVoice"
                },
                {
                  "text": " depend on the values of ",
                  "type": "text"
                },
                {
                  "code": "KeyType",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "HardwareBound",
                  "type": "codeVoice"
                },
                {
                  "text": ". See those keys for specific requirements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "KeySize",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "RSA",
                "ECSECPrimeRandom"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies the type of key pair to generate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "RSA",
                  "type": "codeVoice"
                },
                {
                  "text": " specifies an RSA key pair. If you set this value to ",
                  "type": "text"
                },
                {
                  "code": "RSA",
                  "type": "codeVoice"
                },
                {
                  "text": ", set ",
                  "type": "text"
                },
                {
                  "code": "KeySize",
                  "type": "codeVoice"
                },
                {
                  "text": " in the range ",
                  "type": "text"
                },
                {
                  "code": "[1024..4096]",
                  "type": "codeVoice"
                },
                {
                  "text": " inclusive and a multiple of ",
                  "type": "text"
                },
                {
                  "code": "8",
                  "type": "codeVoice"
                },
                {
                  "text": ", and set ",
                  "type": "text"
                },
                {
                  "code": "HardwareBound",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "ECSECPrimeRandom",
                  "type": "codeVoice"
                },
                {
                  "text": " specifies a key pair on the P-256, P-384 or P-521 curves as defined in FIPS Pub 186-4, and ",
                  "type": "text"
                },
                {
                  "code": "KeySize",
                  "type": "codeVoice"
                },
                {
                  "text": " determines the specific curve. If you set this value to ",
                  "type": "text"
                },
                {
                  "code": "ECSECPrimeRandom",
                  "type": "codeVoice"
                },
                {
                  "text": ", set ",
                  "type": "text"
                },
                {
                  "code": "KeySize",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "256",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "384",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "521",
                  "type": "codeVoice"
                },
                {
                  "text": ". The system only supports ",
                  "type": "text"
                },
                {
                  "code": "256",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "384",
                  "type": "codeVoice"
                },
                {
                  "text": " for hardware bound keys.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "The key size is ",
                      "type": "text"
                    },
                    {
                      "code": "521",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", not ",
                      "type": "text"
                    },
                    {
                      "code": "512",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", even though the other key sizes are multiples of ",
                      "type": "text"
                    },
                    {
                      "code": "64",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "name": "Note",
              "style": "note",
              "type": "aside"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "KeyType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The device requests this subject for the certificate that the ACME server issues. The ACME server may override or ignore this field in the certificate it issues.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The representation of an X.500 name is an array of OID and value. For example, ",
                  "type": "text"
                },
                {
                  "code": "/C=US/O=Apple Inc./CN=foo/1.2.5.3=bar",
                  "type": "codeVoice"
                },
                {
                  "text": " corresponds to:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "[ [ [\"C\", \"US\"] ], [ [\"O\", \"Apple Inc.\"] ], [ [ \"CN\", \"foo\"] ], [ [ \"1.2.5.3\", \"bar\" ] ] ]",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You can represent OIDs as dotted numbers or use shortcuts for country (",
                  "type": "text"
                },
                {
                  "code": "C",
                  "type": "codeVoice"
                },
                {
                  "text": "), locality (",
                  "type": "text"
                },
                {
                  "code": "L",
                  "type": "codeVoice"
                },
                {
                  "text": "), state (",
                  "type": "text"
                },
                {
                  "code": "ST",
                  "type": "codeVoice"
                },
                {
                  "text": "), organization (",
                  "type": "text"
                },
                {
                  "code": "O",
                  "type": "codeVoice"
                },
                {
                  "text": "), organizational unit (",
                  "type": "text"
                },
                {
                  "code": "OU",
                  "type": "codeVoice"
                },
                {
                  "text": "), and common name (",
                  "type": "text"
                },
                {
                  "code": "CN",
                  "type": "codeVoice"
                },
                {
                  "text": ").",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Subject",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[[[string]]]"
            }
          ],
          "typeDetails": [
            {
              "arrayMode": true,
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies the subject’s alternative name that the device requests for the certificate that the ACME server issues. The ACME server may override or ignore this field in the certificate it issues.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "SubjectAltName",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/ACMECredentialSubjectAltNameObject",
              "kind": "typeIdentifier",
              "preciseIdentifier": "rmdm-declarations:ACMECredentialSubjectAltNameObject",
              "text": "ACMECredentialSubjectAltNameObject"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The device requests this key usage for the certificate that the ACME server issues. The ACME server may override or ignore this field in the certificate it issues.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The value is a bit field. Bit ",
                  "type": "text"
                },
                {
                  "code": "0x01",
                  "type": "codeVoice"
                },
                {
                  "text": " indicates digital signature, and bit ",
                  "type": "text"
                },
                {
                  "code": "0x04",
                  "type": "codeVoice"
                },
                {
                  "text": " indicates key encipherment.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "UsageFlags",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
