        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The status code for the response. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " = Success.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "-1",
                  "type": "codeVoice"
                },
                {
                  "text": " = Failure.",
