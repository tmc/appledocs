      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ISO alpha-2 country code that designates the organization’s location.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "countryISO2ACode",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "volumestore",
                "enterprisestore"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value that the MDM client passes for the platform parameter in the ",
                  "type": "text"
                },
                {
                  "code": "contentMetadataLookup",
                  "type": "codeVoice"
                },
                {
                  "text": " request. For more information about how the MDM client queries metadata by using ",
                  "type": "text"
                },
                {
                  "code": "contentMetadataLookup",
                  "type": "codeVoice"
                },
                {
                  "text": ", see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/getting-app-and-book-information-legacy",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "defaultPlatform",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The current URL to post notifications to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "notificationUrl",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "ASSET_MANAGEMENT",
                "USER_MANAGEMENT",
                "USER_ASSOCIATED",
                "ASSET_COUNT"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The set of currently subscribed notification types.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "subscribedNotificationTypes",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "https://school.apple.com",
                "https://business.apple.com"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The current website URL for the specified platform.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "websiteURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The current information for the provided token.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The response only includes this field when the MDM client sets a value using the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Client-Config-4szk1",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " endpoint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mdmInfo",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/MdmInfo",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-apps:vpp:MdmInfo",
              "text": "MdmInfo"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The current shared secret that the server returns in the Authorization header of notifications.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "notificationAuthToken",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The token’s expiration date in an ISO-8601 format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Note: The server shows all dates and times in UTC.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tokenExpirationDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique library identifier. When querying records using multiple tokens that may share libraries, use the ",
                  "type": "text"
                },
                {
                  "code": "uId",
                  "type": "codeVoice"
                },
                {
                  "text": " field to filter duplicates and avoid double-counting records when different content managers upload duplicate tokens.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "uId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The current name of the library.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "locationName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Client config should be checked regularly in order to verify expected values for the various fields.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
