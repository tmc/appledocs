    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of App IDs that specify apps that Enrollment SSO developer mode can use. In Enrollment SSO documents delivered through the developer endpoint, this key must be present and contain at least one value. In Enrollment SSO documents delivered by the standard Enrollment SSO endpoint, this key must not be present.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "16.0.0",
          "name": "AppIDs",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of associated domains that the device uses with the Enrollment SSO extension.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "16.0.0",
          "name": "AssociatedDomains",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true,",
                  "type": "codeVoice"
                },
                {
                  "text": " allows the domain to directly verify site association, instead of at Apple’s servers. Use this verification only with domains that are inaccessible on the public Internet.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "16.0.0",
          "name": "AssociatedDomainsEnableDirectDownloads",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The profile containing an ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/ExtensibleSingleSignOn",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " payload that specifies the SSO extension in the downloaded app prior to enrollment. This profile may contain certificate payloads.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "16.0.0",
          "name": "ConfigurationProfile",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "data"
            }
          ]
        },
        {
          "attributes": [],
          "content": [],
          "introducedVersion": "16.0.0",
          "name": "Declarations",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[data]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The iTunes Store ID of the app to download prior to enrollment, to support Enrollment SSO during enrollment. Using developer mode ignores this key.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "16.0.0",
          "name": "iTunesStoreID",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
