    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of storage area network (SAN) File System Name Server coordinators. The list should contain the same addresses in the same order as the metadata controller (MDC) ",
                  "type": "text"
                },
                {
                  "code": "/Library/Preferences/Xsan/fsnameservers",
                  "type": "codeVoice"
                },
                {
                  "text": " file.  Xsan SAN clients automatically receive updates to the ",
                  "type": "text"
                },
                {
                  "code": "fsnameservers",
                  "type": "codeVoice"
                },
                {
                  "text": " list from the SAN configuration servers whenever this list changes. StorNext administrators should update their profile whenever the ",
                  "type": "text"
                },
                {
                  "code": "fsnameservers",
                  "type": "codeVoice"
                },
                {
                  "text": " list changes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This key is required for StorNext SANs.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.10.0",
          "name": "fsnameservers",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "auth_secret"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The authentication method for the SAN. This key is required for all Xsan SANs. It’s optional for StorNext SANs but should be set if the StorNext SAN uses an ",
                  "type": "text"
                },
                {
                  "code": "auth_secret",
                  "type": "codeVoice"
                },
                {
                  "text": " file.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Only one value is accepted: ",
                  "type": "text"
                },
                {
                  "code": "auth_secret",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.10.0",
          "name": "sanAuthMethod",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of LDAP URLs where Xsan systems can obtain SAN configuration updates. This key is required for all Xsan SANs. There should be one entry for each Xsan MDC.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Example URL: ",
                  "type": "text"
                },
                {
                  "code": "ldaps://mdc1.example.com:389",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.10.0",
          "name": "sanConfigURLs",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the SAN. This key is required for all Xsan SANs. The name must exactly match the name of the SAN defined in the metadata server.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.10.0",
          "name": "sanName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The shared secret used for Xsan network authentication. This key is required when the ",
                  "type": "text"
                },
                {
                  "code": "sanAuthMethod",
                  "type": "codeVoice"
                },
                {
                  "text": " key is present. The value should equal the content of the MDC’s ",
                  "type": "text"
                },
                {
                  "code": "/Library/Preferences/Xsan/.auth_secret",
                  "type": "codeVoice"
                },
                {
                  "text": " file.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.10.0",
          "name": "sharedSecret",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
