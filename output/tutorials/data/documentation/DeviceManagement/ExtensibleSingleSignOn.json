          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "ExtensibleSingleSignOn"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Password",
                "UserSecureEnclaveKey"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Platform SSO authentication method the extension uses. Requires that the SSO Extension also supports the method. Available in macOS 13 and later, and deprecated in macOS 14.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "introducedVersion": "13.0.0",
          "name": "AuthenticationMethod",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of bundle identifiers of apps that don’t use SSO provided by this extension. Available in iOS 15 and later, and macOS 12 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "DeniedBundleIdentifiers",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary of arbitrary data passed through to the app extension.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "ExtensionData",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/ExtensibleSingleSignOn/ExtensionData-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:ExtensibleSingleSignOn.ExtensionData",
              "text": "ExtensibleSingleSignOn.ExtensionData"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle identifier of the app extension that performs SSO for the specified URLs.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "ExtensionIdentifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of host or domain names that apps can authenticate through the app extension.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Required for ",
                  "type": "text"
                },
                {
                  "code": "Credential",
                  "type": "codeVoice"
                },
                {
                  "text": " payloads. Ignored for ",
                  "type": "text"
                },
                {
                  "code": "Redirect",
                  "type": "codeVoice"
                },
                {
                  "text": " payloads.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The system:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Matches host or domain names case-insensitively",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Requires that all the host and domain names of all installed Extensible SSO payloads are unique",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "Host names that begin with a “.” are wildcard suffixes that match all subdomains; otherwise the host name needs be an exact match.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "name": "Note",
              "style": "note",
              "type": "aside"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "Hosts",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dictionary to configure Platform SSO.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "PlatformSSO",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/ExtensibleSingleSignOn/PlatformSSO-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:ExtensibleSingleSignOn.PlatformSSO",
              "text": "ExtensibleSingleSignOn.PlatformSSO"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The realm name for ",
                  "type": "text"
                },
                {
                  "code": "Credential",
                  "type": "codeVoice"
                },
                {
                  "text": " payloads. Use proper capitalization for this value. Ignored for ",
                  "type": "text"
                },
                {
                  "code": "Redirect",
                  "type": "codeVoice"
                },
                {
                  "text": " payloads.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "Realm",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The token this device uses for registration with Platform SSO. Use it for silent registration with the Identity Provider. Requires that ",
                  "type": "text"
                },
                {
                  "code": "AuthenticationMethod",
                  "type": "codeVoice"
                },
                {
                  "text": " isn’t empty. Available in macOS 13 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "RegistrationToken",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "Cancel"
            },
            {
              "kind": "allowedValues",
              "values": [
                "Cancel",
                "DoNotHandle"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If set to ",
                  "type": "text"
                },
                {
                  "code": "Cancel",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system cancels authentication requests when the screen is locked. If set to ",
                  "type": "text"
                },
                {
                  "code": "DoNotHandle",
                  "type": "codeVoice"
                },
                {
                  "text": ", the request continues without SSO instead. This doesn’t apply to requests where ",
                  "type": "text"
                },
                {
                  "code": "userInterfaceEnabled",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", or for background ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/urlsession",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " requests. Available in iOS 15 and later, and macOS 12 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "ScreenLockedBehavior",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The team identifier of the app extension. This key is required on macOS and ignored elsewhere.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "TeamIdentifier",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Credential",
                "Redirect"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of SSO.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "Type",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of URL prefixes of identity providers where the app extension performs SSO.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Required for ",
                  "type": "text"
                },
                {
                  "code": "Redirect",
                  "type": "codeVoice"
                },
                {
                  "text": " payloads. Ignored for ",
                  "type": "text"
                },
                {
                  "code": "Credential",
                  "type": "codeVoice"
                },
                {
                  "text": " payloads.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The URLs need to begin with ",
                  "type": "text"
                },
                {
                  "code": "http://",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "https://",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The system:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Matches scheme and host name case-insensitively",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Doesn’t allow query parameters and URL fragments",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Requires that the URLs of all installed Extensible SSO payloads are unique",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "URLs",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
