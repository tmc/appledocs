          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of operation the declaration is requesting. This key must be one of these values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "tokens",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": For fetching synchronization tokens from the server",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "declaration-items",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": For fetching the declaration manifest from the server",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "status",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": For sending a status report to the server",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "declaration/…/…",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": For fetching a specific declaration from the server. Include the declaration type and identifier separated by forward slashes (",
                          "type": "text"
                        },
                        {
