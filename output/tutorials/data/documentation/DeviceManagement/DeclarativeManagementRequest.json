    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "Data",
              "type": "codeVoice"
            },
            {
              "text": " field is optional, depending on the ",
              "type": "text"
            },
            {
              "code": "Endpoint",
              "type": "codeVoice"
            },
            {
              "text": " value, as described below:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The client uses the ",
                        "type": "text"
                      },
                      {
                        "code": "tokens",
                        "type": "codeVoice"
                      },
                      {
                        "text": " endpoint to request the current synchronization tokens from the server. The ",
                        "type": "text"
                      },
                      {
                        "code": "Data",
                        "type": "codeVoice"
                      },
                      {
                        "text": " field isn’t used. A successful response to this request is a ",
                        "type": "text"
                      },
                      {
                        "code": "200 OK HTTP",
                        "type": "codeVoice"
                      },
                      {
                        "text": " status, with a response body that’s a JSON object conforming to the ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/TokensResponse",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " schema.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "tokens",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The client uses the ",
                        "type": "text"
                      },
                      {
                        "code": "declaration-items",
                        "type": "codeVoice"
                      },
                      {
                        "text": " endpoint to request the current declaration manifest from the server. The ",
                        "type": "text"
                      },
                      {
                        "code": "Data",
                        "type": "codeVoice"
                      },
                      {
                        "text": " field isn’t used. A successful response to this request is a ",
                        "type": "text"
                      },
                      {
                        "code": "200 OK HTTP",
                        "type": "codeVoice"
                      },
                      {
                        "text": " status, with a response body that’s a JSON object conforming to the ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/DeclarationItemsResponse",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " schema.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "declaration-items",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The client uses the ",
                        "type": "text"
                      },
                      {
                        "code": "declaration/…/…",
                        "type": "codeVoice"
                      },
                      {
                        "text": " endpoint to request a specific declaration from the server. The ",
                        "type": "text"
                      },
                      {
                        "code": "Data",
                        "type": "codeVoice"
                      },
                      {
                        "text": " field isn’t used.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "declaration/…/…",
                    "type": "codeVoice"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "inlineContent": [
            {
              "text": "The endpoint value is a path with 3 segments separated by a / character. The first segment is always ",
              "type": "text"
            },
            {
              "code": "declaration",
              "type": "codeVoice"
            },
            {
              "text": ". The second segment indicates the declaration type and is one of ",
              "type": "text"
            },
            {
              "code": "activation",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "asset",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "configuration",
              "type": "codeVoice"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "code": "management",
              "type": "codeVoice"
            },
            {
              "text": ". The third segment is the ",
              "type": "text"
            },
            {
              "code": "Identifier",
              "type": "codeVoice"
            },
            {
              "text": " of the declaration to fetch.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A successful response to this request is a ",
              "type": "text"
            },
            {
              "code": "200 OK HTTP",
              "type": "codeVoice"
            },
            {
              "text": " status, with a response body that’s a JSON object representing the requested declaration. If the declaration isn’t present on the server, it must return a ",
              "type": "text"
            },
            {
              "code": "404 Not Found HTTP",
              "type": "codeVoice"
            },
            {
              "text": " status response to the device. The device then considers that declaration to no longer exist on the server and removes any corresponding declaration present on the device.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The client uses the ",
                        "type": "text"
                      },
                      {
                        "code": "status",
                        "type": "codeVoice"
                      },
                      {
                        "text": " endpoint to send a status report to the server. The ",
                        "type": "text"
                      },
                      {
                        "code": "Data",
                        "type": "codeVoice"
                      },
                      {
                        "text": " field must be present and set to a base64-encoded JSON object conforming to the ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/StatusReport",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " schema. A successful response to this request is a ",
                        "type": "text"
                      },
                      {
                        "code": "200 OK HTTP",
                        "type": "codeVoice"
                      },
                      {
                        "text": " status, with an empty response body.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "status",
                    "type": "codeVoice"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        }
      ],
      "kind": "content"
    }
