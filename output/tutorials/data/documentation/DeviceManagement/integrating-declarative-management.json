{
  "abstract": [
    {
      "text": "Use the declarative management protocol to manage MDM features such as device enrollment and un-enrollment and device and user authentication.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.devicemanagement/documentation/DeviceManagement"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "data",
    "url": "doc://com.apple.devicemanagement/documentation/DeviceManagement/integrating-declarative-management"
  },
  "kind": "article",
