  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: VPP License Management"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "VppLicense"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier for a product in the iTunes Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "adamId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier for a product in the iTunes Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "adamIdStr",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The client user ID for the user to whom this license is assigned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientUserIdStr",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If true, once a license is assigned for specified adamId, the license may not be revoked or disassociated.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isIrrevocable",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The hash of the iTunes Store ID for the user to whom this license is assigned. The itsIdHash field is included only when the user is associated with an iTunes Store account.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "itsIdHash",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier of the assigned license.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "licenseIdStr",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The quality of a product in the iTunes Store. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "STDQ",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Standard quality",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "PLUS",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": High quality",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "pricingParam",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of a product. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "7",
                          "type": "codeVoice"
                        },
                        {
                          "text": " = macOS software",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "8",
                          "type": "codeVoice"
                        },
                        {
                          "text": " = iOS or macOS app from the App Store",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "10",
                          "type": "codeVoice"
                        },
                        {
                          "text": " = Book",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "productTypeId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the product type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "productTypeName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The device serial number to which this license is assigned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "serialNumber",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The current state of the license. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Associated",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Available",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Refunded",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "status",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier assigned by the VPP for the user to whom this license is assigned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "userId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The string representation of the unique identifier assigned by the VPP for the user to whom this license is assigned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "userIdStr",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
