      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the AirPlay Mirroring destination.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "7.0.0",
          "name": "DestinationName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the device must be network-tethered to run the command.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "7.0.0",
          "name": "RequestRequiresNetworkTether",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "RequestMirroring"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The request type to prompt the user to share their screen using AirPlay Mirroring.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "7.0.0",
          "name": "RequestType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The hardware address of the AirPlay Mirroring destination that identifies the device, in the format ",
                  "type": "text"
                },
                {
                  "code": "xx:xx:xx:xx:xx",
                  "type": "codeVoice"
                },
                {
                  "text": ". This value isn’t case-sensitive.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "7.0.0",
          "name": "DestinationDeviceID",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The screen-sharing password that the device uses when connecting to the destination.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "7.0.0",
          "name": "Password",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of seconds, from ",
                  "type": "text"
                },
                {
                  "code": "10",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "300",
                  "type": "codeVoice"
                },
                {
                  "text": ", for the device to spend searching for the destination. The default value is ",
                  "type": "text"
                },
                {
                  "code": "30",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "7.0.0",
          "name": "ScanTime",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
