    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The organization’s address. Use the LF character (",
                  "type": "text"
                },
                {
                  "code": "&#10",
                  "type": "codeVoice"
                },
                {
                  "text": ") to insert line breaks.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "5.0.0",
          "name": "OrganizationAddress",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The orgnization’s support email address.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "5.0.0",
          "name": "OrganizationEmail",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier for the various services a single organization manages.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "5.0.0",
          "name": "OrganizationMagic",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that describes the organization operating the MDM server for display to the user during certain operations, such as purchasing or installing apps.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "5.0.0",
          "name": "OrganizationName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The organization’s phone number.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "5.0.0",
          "name": "OrganizationPhone",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A shorter version of ",
                  "type": "text"
                },
                {
                  "code": "OrganizationName",
                  "type": "codeVoice"
                },
                {
                  "text": ", preferably a single word or abbreviation, suitable for display to the user in places where a very short name is necessary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "5.0.0",
          "name": "OrganizationShortName",
