    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary that contains the attributes to apply to the app. Omit this setting to remove existing attributes. This setting is available in iOS 7 and later, and tvOS 10.2 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "5.0.0",
          "name": "Attributes",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SettingsCommand/Command-data.dictionary/Settings-data.dictionary/ApplicationAttributes-data.dictionary/Attributes-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-commands:SettingsCommand.Command.Settings.ApplicationAttributes.Attributes",
              "text": "SettingsCommand.Command.Settings.ApplicationAttributes.Attributes"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle identifier of the app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "",
                      "type": "text"
