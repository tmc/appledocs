    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The code signature designated requirement of the app using the per-app VPN.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "DesignatedRequirement",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle identifier of the app using the per-app VPN.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "Identifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of dictionaries. Each dictionary specifies a per-app VPN rule. Use this property to restrict this per-app VPN rule to only match the app’s spawned ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "helper tool",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " network traffic.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "For example, to match network traffic that the ",
                  "type": "text"
                },
                {
                  "code": "curl",
                  "type": "codeVoice"
                },
                {
                  "text": " command generates when run from the Terminal.app, create an app mapping payload for Terminal.app and set the payload’s ",
                  "type": "text"
                },
                {
                  "code": "MatchTools",
                  "type": "codeVoice"
                },
                {
                  "text": " key to an array that contains a dictionary that matches the ",
                  "type": "text"
                },
                {
                  "code": "curl",
                  "type": "codeVoice"
                },
                {
                  "text": " command-line tool.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you don’t specify the ",
                  "type": "text"
                },
                {
                  "code": "MatchTools",
                  "type": "codeVoice"
                },
                {
                  "text": " key, this per-app VPN rule matches all network traffic that the matching app and its spawned helper tools generate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "MatchTools",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/AppToAppLayerVPNMapping/AppLayerVPNMappingItem/MatchToolsItem",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:AppToAppLayerVPNMapping.AppLayerVPNMappingItem.MatchToolsItem",
              "text": "AppToAppLayerVPNMapping.AppLayerVPNMappingItem.MatchToolsItem"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The file-system path of the executable using the per-app VPN.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "Path",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The code signature signing identifier of the app using the per-app VPN.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "SigningIdentifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier of the per-app VPN payload, which defines the per-app VPN that the app uses. See the ",
                  "type": "text"
                },
                {
                  "code": "VPNUUID",
                  "type": "codeVoice"
                },
                {
                  "text": " key of the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/AppLayerVPN",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " payload.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "VPNUUID",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
