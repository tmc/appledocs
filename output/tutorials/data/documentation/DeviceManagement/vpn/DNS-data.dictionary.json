  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: iOS",
            "Unsupported OS: macOS",
            "Unsupported OS: tvOS",
            "Unsupported OS: visionOS"
          ],
          "tokens": [
            {
              "kind": "text",
