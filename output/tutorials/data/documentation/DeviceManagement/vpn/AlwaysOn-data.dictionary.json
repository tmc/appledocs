    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", allows traffic from all captive networking apps outside the VPN tunnel to perform captive network handling.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "AllowAllCaptiveNetworkPlugins",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", allows traffic from Captive Web Sheet outside the VPN tunnel.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "AllowCaptiveWebSheet",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The array of captive networking apps whose traffic is allowed outside the VPN tunnel, to perform captive network handling. Used only when ",
                  "type": "text"
                },
                {
                  "code": "AllowAllCaptiveNetworkPlugins",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "AllowedCaptiveNetworkPlugins",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VPN/AlwaysOn-data.dictionary/AllowedCaptiveNetworkPluginElement",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:VPN.AlwaysOn.AllowedCaptiveNetworkPluginElement",
              "text": "VPN.AlwaysOn.AllowedCaptiveNetworkPluginElement"
