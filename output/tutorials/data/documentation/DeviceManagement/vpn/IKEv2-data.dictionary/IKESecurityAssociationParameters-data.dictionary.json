          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Diffie-Hellman group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "For ",
                  "type": "text"
                },
                {
                  "code": "AlwaysOn",
                  "type": "codeVoice"
                },
                {
                  "text": " VPN in iOS 14.2 and later, the minimum allowed value is ",
                  "type": "text"
