    {
      "items": [
        {
          "attributes": [],
          "content": [],
          "introducedVersion": "4.0.0",
          "name": "AuthName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [],
          "introducedVersion": "4.0.0",
          "name": "AuthPassword",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "Password"
            },
            {
              "kind": "allowedValues",
              "values": [
                "Password",
                "Certificate",
                "Password+Certificate"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The authentication method to use.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "AuthenticationMethod",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", disconnects after an on-demand connection idles.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "DisconnectOnIdle",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length of time to wait, in seconds, before disconnecting an on-demand connection. In watchOS, the maximum allowed value is ",
                  "type": "text"
                },
                {
                  "code": "15",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "DisconnectOnIdleTimer",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", all the VPN’s non-default routes take precedence over any locally defined routes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "IncludeAllNetworks",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system ignores the value of ",
                  "type": "text"
                },
                {
                  "code": "EnforceRoutes",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Available in iOS 14.2 and later, and macOS 11 and later. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "EnforceRoutes",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "1"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "IncludeAllNetworks",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", then the system excludes the network traffic for the Apple Push Notification service (APNs) from the tunnel. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ExcludeAPNs",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "1"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "IncludeAllNetworks",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", then the system excludes internet-routable network traffic for cellular services (VoLTE, Wi-Fi Calling, IMS, MMS, Visual Voicemail, etc.) from the tunnel. Note that some cellular carriers route cellular services traffic directly to the carrier network, bypassing the internet. Such cellular services traffic is always excluded from the tunnel. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ExcludeCellularServices",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "1"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ExcludeDeviceCommunication",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "IncludeAllNetworks",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", routes all local network traffic outside the VPN. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ExcludeLocalNetworks",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", routes all traffic through the VPN. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "IncludeAllNetworks",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", enables VPN On Demand.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "OnDemandEnabled",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of domain names. The system treats associated domain names as though they’re associated with the ",
                  "type": "text"
                },
                {
                  "code": "OnDemandMatchDomainsOnRetry",
                  "type": "codeVoice"
                },
                {
                  "text": " key. This behavior can be overridden by ",
                  "type": "text"
                },
                {
                  "code": "OnDemandRules",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "In iOS 7 and later, this key is deprecated (but still supported) in favor of ",
                  "type": "text"
                },
                {
                  "code": "EvaluateConnection",
                  "type": "codeVoice"
                },
                {
                  "text": " actions in the ",
                  "type": "text"
                },
                {
                  "code": "OnDemandRules",
                  "type": "codeVoice"
                },
                {
                  "text": " dictionaries.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "introducedVersion": "4.0.0",
          "name": "OnDemandMatchDomainsAlways",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of domain names. If the host name ends with one of these domain names, the system doesn’t start the VPN automatically. The system uses this value to exclude a subdomain within an included domain.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "In iOS 7 and later, this key is deprecated (but still supported) in favor of ",
                  "type": "text"
                },
                {
                  "code": "EvaluateConnection",
                  "type": "codeVoice"
                },
                {
                  "text": " actions in the ",
                  "type": "text"
                },
                {
                  "code": "OnDemandRules",
                  "type": "codeVoice"
                },
                {
                  "text": " dictionaries.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "introducedVersion": "4.0.0",
          "name": "OnDemandMatchDomainsNever",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of domain names. If the host name ends with one of these domain names and a DNS query for that domain name fails, the system starts the VPN automatically.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "In iOS 7 and later, this key is deprecated (but still supported) in favor of ",
                  "type": "text"
                },
                {
                  "code": "EvaluateConnection",
                  "type": "codeVoice"
                },
                {
                  "text": " actions in the ",
                  "type": "text"
                },
                {
                  "code": "OnDemandRules",
                  "type": "codeVoice"
                },
                {
                  "text": " dictionaries.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "introducedVersion": "4.0.0",
          "name": "OnDemandMatchDomainsOnRetry",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of dictionaries defining On Demand Rules.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "OnDemandRules",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VPN/VPN-data.dictionary/OnDemandRulesElement",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:VPN.VPN.OnDemandRulesElement",
              "text": "VPN.VPN.OnDemandRulesElement"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            },
            {
              "kind": "allowedValues",
              "values": [
                "0",
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ", the Connect On Demand toggle in Settings is disabled for this configuration. Available in iOS 14 and later. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "OnDemandUserOverrideDisabled",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UUID of the certificate payload within the same profile to use for account credentials.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PayloadCertificateUUID",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle identifier for the VPN provider. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ProviderBundleIdentifier",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If the VPN provider is implemented as a system extension, this field is required. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ProviderDesignatedRequirement",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "packet-tunnel"
            },
            {
              "kind": "allowedValues",
              "values": [
                "packet-tunnel",
                "app-proxy"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of VPN service. If the value is ",
                  "type": "text"
                },
                {
                  "code": "app-proxy",
                  "type": "codeVoice"
                },
                {
                  "text": ", the service tunnels traffic at the app level. If the value is ",
                  "type": "text"
                },
                {
                  "code": "packet-tunnel",
                  "type": "codeVoice"
                },
                {
                  "text": ", the service tunnels traffic at the IP layer. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ProviderType",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [],
          "introducedVersion": "4.0.0",
          "name": "RemoteAddress",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The system uses this dictionary when the ",
              "type": "text"
            },
            {
              "code": "VPNType",
              "type": "codeVoice"
            },
            {
              "text": " is ",
              "type": "text"
            },
            {
              "code": "VPN",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "IPSec",
              "type": "codeVoice"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "code": "IKEv2",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
