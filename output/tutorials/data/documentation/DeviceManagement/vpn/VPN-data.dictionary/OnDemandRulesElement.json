  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: iOS",
            "Unsupported OS: macOS",
            "Unsupported OS: tvOS",
            "Unsupported OS: visionOS"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "VPN.VPN.OnDemandRulesElement"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Allow",
                "Connect",
                "Disconnect",
                "EvaluateConnection",
                "Ignore"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The action to take if this dictionary matches the current network. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Allow",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Deprecated. Allow VPN On Demand to connect if triggered.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Connect",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Unconditionally initiate a VPN connection on the next network attempt.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Disconnect",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Tear down the VPN connection and don’t reconnect on demand as long as this dictionary matches.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "EvaluateConnection",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Evaluate the ActionParameters array for each connection attempt.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Ignore:",
                          "type": "codeVoice"
                        },
                        {
                          "text": " Leave any existing VPN connection up, but don’t reconnect on demand as long as this dictionary matches.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "Only the ",
                  "type": "text"
                },
                {
                  "code": "Disconnect",
                  "type": "codeVoice"
                },
                {
                  "text": " action is available on watchOS 10 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "Action",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of dictionaries that provides rules similar to the ",
                  "type": "text"
                },
                {
                  "code": "OnDemandRules",
                  "type": "codeVoice"
                },
                {
                  "text": " dictionary, but evaluated on each connection instead of when the network changes. This value is only for use with dictionaries in which the ",
                  "type": "text"
                },
                {
                  "code": "Action",
                  "type": "codeVoice"
                },
                {
                  "text": " value is ",
                  "type": "text"
                },
                {
                  "code": "EvaluateConnection",
                  "type": "codeVoice"
                },
                {
                  "text": ". The system evaluates these dictionaries in order and the first dictionary that matches determines the behavior. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ActionParameters",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VPN/VPN-data.dictionary/OnDemandRulesElement/ActionParameter",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:VPN.VPN.OnDemandRulesElement.ActionParameter",
              "text": "VPN.VPN.OnDemandRulesElement.ActionParameter"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of domain names. This rule matches if any of the domain names in the specified list matches any domain in the device’s search domains list.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The system supports a wildcard (",
                  "type": "text"
                },
                {
                  "code": "*",
                  "type": "codeVoice"
                },
                {
                  "text": ") prefix. For example, ",
                  "type": "text"
                },
                {
                  "code": "*.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": " matches against either ",
                  "type": "text"
                },
                {
                  "code": "mydomain.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "yourdomain.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "DNSDomainMatch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of IP addresses. This rule matches if any of the network’s specified DNS servers match any entry in the array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The system supports matching with a single wildcard. For example, ",
                  "type": "text"
                },
                {
                  "code": "17.*",
                  "type": "codeVoice"
                },
                {
                  "text": " matches any DNS server in the ",
                  "type": "text"
                },
                {
                  "code": "17.0.0.0/8",
                  "type": "codeVoice"
                },
                {
                  "text": " subnet.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "DNSServerAddressMatch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Ethernet",
                "WiFi",
                "Cellular"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An interface type. If specified, this rule matches only if the primary network interface hardware matches the specified type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "InterfaceTypeMatch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of SSIDs to match against the current network. If the network isn’t a Wi-Fi network or if the SSID doesn’t appear in this array, the match fails.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Omit this key and the corresponding array to match against any SSID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "SSIDMatch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A URL to probe. This rule matches when this URL is successfully fetched (returns a ",
                  "type": "text"
                },
                {
                  "code": "200",
                  "type": "codeVoice"
                },
                {
                  "text": " HTTP status code) without redirection. Not available in watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "URLStringProbe",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
