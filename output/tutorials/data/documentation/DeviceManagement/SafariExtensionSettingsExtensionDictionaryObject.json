          "content": [
            {
              "inlineContent": [
                {
                  "text": "Controls the domains and sub-domains the extension is granted access to. Any non-prefixed domains take precedence over prefixed domains, and ",
                  "type": "text"
                },
                {
                  "code": "DeniedDomains",
                  "type": "codeVoice"
                },
                {
                  "text": " takes precedence over ",
                  "type": "text"
                },
                {
                  "code": "AllowedDomains",
                  "type": "codeVoice"
                },
                {
                  "text": ". Any domains not specified in ",
                  "type": "text"
                },
                {
                  "code": "AllowedDomains",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "DeniedDomains",
                  "type": "codeVoice"
                },
                {
                  "text": " are configurable by the user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "18.0.0",
          "name": "AllowedDomains",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Controls the domains and sub-domains the extension isnâ€™t allowed to access. Any non-prefixed domains take precedence over prefixed domains, and ",
                  "type": "text"
                },
                {
                  "code": "DeniedDomains",
                  "type": "codeVoice"
                },
                {
                  "text": " takes precedence over ",
                  "type": "text"
                },
                {
                  "code": "AllowedDomains",
                  "type": "codeVoice"
                },
                {
                  "text": ". Any domains not specified in ",
                  "type": "text"
