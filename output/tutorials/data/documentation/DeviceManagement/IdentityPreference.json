    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The email address (in RFC 822 format), DNS host name, or other name that uniquely identifies a service requiring this identity.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.12.0",
          "name": "Name",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UUID of the certificate payload within the same profile to use for the identity credential.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.12.0",
          "name": "PayloadCertificateUUID",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Specify ",
              "type": "text"
            },
            {
              "code": "com.apple.security.identitypreference",
              "type": "codeVoice"
            },
            {
              "text": " as the payload type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This payload specifies an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/IdentityPreference",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " item in the user’s keychain that references an identity payload included in the same profile. It can only appear in a user profile, not in a device profile.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can include multiple ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/IdentityPreference",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " payloads as needed.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "See also ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/CertificatePreference",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for setting up certificate preferences.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
