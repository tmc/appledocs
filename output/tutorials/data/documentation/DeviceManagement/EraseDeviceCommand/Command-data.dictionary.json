  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: iOS",
            "Unsupported OS: macOS",
            "Unsupported OS: tvOS",
            "Unsupported OS: visionOS",
            "Unsupported OS: watchOS"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "EraseDeviceCommand.Command"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", disable Proximity Setup on the next reboot and skip the pane in Setup Assistant. This value is available in iOS 11 and later. Prior to iOS 14, don’t use this option with any other option.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "DisallowProximitySetup",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The six-character PIN for Find My. This value is available in macOS 10.8 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PIN",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", preserve the data plan on an iPhone or iPad with eSIM functionality, if one exists. This value is available in iOS 11 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "PreserveDataPlan",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the device must be network-tethered to run the command.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "RequestRequiresNetworkTether",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "EraseDevice"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The request type to erase a device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "RequestType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Default",
                "DoNotObliterate",
                "ObliterateWithWarning",
                "Always"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This key defines the fallback behavior for erasing a device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "In macOS 12 and later, this command uses Erase All Content and Settings (EACS) on Mac computers with the Apple M1 chip or the Apple T2 Security Chip. On those devices, if EACS can’t run, the device can use obliteration (macOS 11.x behavior). This key has no effect on machines prior to the T2 chip. For a list of supported macs, see ",
                  "type": "text"
                },
                {
                  "identifier": "https://support.apple.com/en-us/HT208862",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Upon receiving this command, the device performs preflight checks to determine if the device is in a state that allows EACS. The ",
                  "type": "text"
                },
                {
                  "code": "status",
                  "type": "codeVoice"
                },
                {
                  "text": " of the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/EraseDeviceResponse",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " is either ",
                  "type": "text"
                },
                {
                  "code": "Acknowledged",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "Error",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The following values define the device’s fallback behavior:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "If EACS preflight fails, the device responds to the server with an ",
                            "type": "text"
                          },
                          {
                            "code": "Error",
                            "type": "codeVoice"
                          },
                          {
                            "text": " status and doesn’t attempt to erase itself.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "If EACS preflight succeeds but EACS fails, then the device doesn’t attempt to erase itself.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "DoNotObliterate",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "If EACS preflight fails, the device responds with an ",
                            "type": "text"
                          },
                          {
                            "code": "Acknowledged",
                            "type": "codeVoice"
                          },
                          {
                            "text": " status and then attempts to erase itself.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "If EACS preflight succeeds but EACS fails, then the device attempts to erase itself.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "ObliterateWithWarning",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "The system doesn’t attempt EACS. T2 and later devices always obliterate.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "Always",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "If EACS preflight fails, the device responds to the server with an ",
                            "type": "text"
                          },
                          {
                            "code": "Error",
                            "type": "codeVoice"
                          },
                          {
                            "text": " status and then attempts to erase itself.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "If EACS preflight succeeds but EACS fails, then the device attempts to erase itself.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "Default",
                        "type": "codeVoice"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ObliterationBehavior",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The configuration settings for Return to Service. This value is available in iOS 17 and later and with Shared iPad and tvOS 18 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "ReturnToService",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/EraseDeviceCommand/Command-data.dictionary/ReturnToService-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-commands:EraseDeviceCommand.Command.ReturnToService",
              "text": "EraseDeviceCommand.Command.ReturnToService"
            }
          ]
        }
      ],
