  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: iOS",
            "Unsupported OS: macOS",
            "Unsupported OS: visionOS"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "DNSSettings.OnDemandRulesElement"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Connect",
                "Disconnect",
                "EvaluateConnection"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The action to take if this dictionary matches the current network. Allowed values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Connect",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Apply DNS Settings when the dictionary matches.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Disconnect",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Don’t apply DNS Settings when the dictionary matches.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "EvaluateConnection",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Apply DNS Settings with per-domain exceptions when the dictionary matches.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "Action",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of dictionaries that provide per-connection rules. The system uses this array only for settings where the ",
                  "type": "text"
                },
                {
                  "code": "Action",
                  "type": "codeVoice"
                },
                {
                  "text": " value is",
                  "type": "text"
                },
                {
                  "code": "EvaluateConnection",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "ActionParameters",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/DNSSettings/OnDemandRulesElement/ActionParameter",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:DNSSettings.OnDemandRulesElement.ActionParameter",
              "text": "DNSSettings.OnDemandRulesElement.ActionParameter"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of domain names. This rule matches if any of the domain names in the specified list matches any domain in the device’s search domains list.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The system supports a single wildcard (",
                  "type": "text"
                },
                {
                  "code": "*",
                  "type": "codeVoice"
                },
                {
                  "text": ") prefix, but it’s not required. For example, both ",
                  "type": "text"
                },
                {
                  "code": "*.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "example.com",
                  "type": "codeVoice"
                },
                {
                  "text": " match against ",
                  "type": "text"
                },
                {
                  "code": "mydomain.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "your.domain.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": ", but don’t match against ",
                  "type": "text"
                },
                {
                  "code": "mydomain-example.com",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "DNSDomainMatch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of IP addresses. This rule matches if any of the network’s specified DNS servers match any entry in the array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The system supports matching with a single wildcard. For example, ",
                  "type": "text"
                },
                {
                  "code": "17.*",
                  "type": "codeVoice"
                },
                {
                  "text": " matches any DNS server in the 17.0.0.0/8 subnet.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "DNSServerAddressMatch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Ethernet",
                "WiFi",
                "Cellular"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An interface type. If specified, this rule matches only if the primary network interface hardware matches the specified type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "InterfaceTypeMatch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of SSIDs to match against the current network. If the network isn’t a Wi-Fi network or if the SSID doesn’t appear in this array, the match fails. Omit this key and the corresponding array to match against any SSID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "SSIDMatch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A URL to probe. This rule matches if this URL is successfully fetched and returns a 200 HTTP status code without redirection.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "URLStringProbe",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
