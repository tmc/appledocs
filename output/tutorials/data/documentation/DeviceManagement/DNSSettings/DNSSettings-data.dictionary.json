      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "HTTPS",
                "TLS"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encrypted transport protocol used to communicate with the DNS server.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "DNSProtocol",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An unordered list of DNS server IP address strings. These IP addresses can be a mixture of IPv4 and IPv6 addresses.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "ServerAddresses",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The hostname of a DNS-over-TLS server used to validate the server certificate, as defined in RFC 7858. If no ",
                  "type": "text"
                },
                {
                  "code": "ServerAddresses",
                  "type": "codeVoice"
                },
                {
                  "text": " are provided, the system uses the hostname to determine the server addresses. This key must be present only if the DNSProtocol is ",
                  "type": "text"
                },
                {
                  "code": "TLS",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "ServerName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URI template of a DNS-over-HTTPS server, as defined in RFC 8484. This URL needs to use the ",
                  "type": "text"
                },
                {
                  "code": "https://",
                  "type": "codeVoice"
                },
                {
                  "text": " scheme, and the system uses the hostname or address in the URL to validate the server certificate. If no ",
                  "type": "text"
                },
                {
                  "code": "ServerAddresses",
                  "type": "codeVoice"
                },
                {
                  "text": " are provided, the system uses the hostname or address in the URL to determine the server addresses. Required if ",
                  "type": "text"
                },
                {
                  "code": "DNSProtocol",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "HTTPS",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "ServerURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of domain strings used to determine which DNS queries use the DNS server. If not set, all domains use the DNS server.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The system supports a single wildcard (",
                  "type": "text"
                },
                {
                  "code": "*",
                  "type": "codeVoice"
                },
                {
                  "text": ") prefix, but it’s not required. For example, both ",
                  "type": "text"
                },
                {
                  "code": "*.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "example.com",
                  "type": "codeVoice"
                },
                {
                  "text": " match against ",
                  "type": "text"
                },
                {
                  "code": "mydomain.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "your.domain.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": ", but don’t match against ",
                  "type": "text"
                },
                {
                  "code": "mydomain-example.com",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "14.0.0",
          "name": "SupplementalMatchDomains",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
