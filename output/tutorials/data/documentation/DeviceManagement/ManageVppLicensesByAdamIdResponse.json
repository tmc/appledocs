  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: VPP License Management"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "ManageVppLicensesByAdamIdResponse"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier for a product in the iTunes Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "adamIdStr",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of dictionaries representing successful and failed associations. If an association succeeds, its dictionary contains the license and either a client-user ID or the serial number of the device associated with the license. If an association fails, its dictionary contains the error message and number, and either the client-user ID or the serial number of the device that couldn’t be associated with the license.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "associations",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VppAssociation",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-apps:vpp:VppAssociation",
              "text": "VppAssociation"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value currently associated with the provided sToken. This field is only included in the response when a value has been set via the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Client-Configuration",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " endpoint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/protecting-your-vpp-account",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientContext",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of dictionaries representing successful and failed disassociations. If a disassociation succeeds, its dictionary contains a license and either a client-user ID or the serial number of the device disassociated. The license may not be the license ID that was just disassociated; rather the license ID is any currently available license at the time of disassociation. If the disassociation fails, its dictionary contains the error message and number, and either the client-user ID or the serial number of the device that couldn’t be disassociated from the license.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "disassociations",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VppAssociation",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-apps:vpp:VppAssociation",
              "text": "VppAssociation"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The human-readable explanation of the error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "errorMessage",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The numeric code of the error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "errorNumber",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UNIX epoch timestamp, in milliseconds, when the account’s sToken or password expires (whichever is earlier).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "expirationMillis",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", licenses for the specified product can’t be revoked and reassigned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isIrrevocable",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The location associated with the provided sToken. This field is only returned when a location token is used with an Apple School Manager account.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "location",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VppLocation",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-apps:vpp:VppLocation",
              "text": "VppLocation"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The quality of a product in the iTunes Store. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "STDQ: Standard quality",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "PLUS: High quality",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pricingParam",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of a product. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "7 = macOS software",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "8 = iOS or macOS app from the App Store",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "10 = Book",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "productTypeId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the product type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "productTypeName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The status code for the response. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " = Success. All requested associations and disassociations have succeeded.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "-1 = Failure. All requested associations and disassociations have failed. An errorNumber, errorCode, and errorMessage is returned for each failed association and disassociation. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VppAssociation",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " and  ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/interpreting-error-codes",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "-3",
                  "type": "codeVoice"
                },
                {
                  "text": " = The licenses can’t be changed as requested. Some of the requested associations and disassociations have succeeded and others have failed. An errorNumber, errorCode, and errorMessage is returned for each failed association and disassociation. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/VppAssociation",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/interpreting-error-codes",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "status",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique library identifier. When querying records using multiple tokens that may share libraries, use the ",
                  "type": "text"
                },
                {
                  "code": "uId",
                  "type": "codeVoice"
                },
                {
                  "text": " field to filter duplicates. In this way, you can avoid double-counting records when duplicate tokens are uploaded by different content managers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "uId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
