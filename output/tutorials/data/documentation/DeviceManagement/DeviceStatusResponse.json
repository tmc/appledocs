    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary of devices. Each key in this dictionary is the serial number of a device in the original request. Each value is one of the following values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "SUCCESS",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": The device was successfully disowned.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "NOT_ACCESSIBLE",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": The device with the specified ID was not accessible by this MDM server.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "FAILED",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Disowning the device failed for an unexpected reason. If three retries fail, the user should contact Apple support.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "If no devices were provided in the original request, this dictionary may be absent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "devices",
          "required": false,
