      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Specify ",
              "type": "text"
            },
            {
              "code": "com.apple.vpn.managed.applayer",
              "type": "codeVoice"
            },
            {
              "text": " as the payload type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This profile defines per-app VPN behavior and only applies to VPN services of type ",
              "type": "text"
            },
            {
              "code": "VPN",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "IPsec",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "IKEv2",
              "type": "codeVoice"
            },
            {
              "text": ". All the properties of VPN apply to the top level of this profile as well.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
