  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: macOS"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "InstallEnterpriseApplicationCommand.Command"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Managed"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The change management state. This value doesn’t work with the User Enrollment feature introduced in iOS 13. The only possible value is:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "Take management of the app if the user installed it already and ",
                            "type": "text"
                          },
                          {
                            "code": "InstallAsManaged",
                            "type": "codeVoice"
                          },
                          {
                            "text": " is ",
                            "type": "text"
                          },
                          {
                            "code": "true",
                            "type": "codeVoice"
                          },
                          {
                            "text": ".",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "Managed",
                        "type": "codeVoice"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            },
            {
              "inlineContent": [
                {
                  "text": "Available in macOS 11 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "ChangeManagementState",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary that contains the initial configuration of the app, if you choose to provide it. Available in macOS 11 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "Configuration",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/InstallEnterpriseApplicationCommand/Command-data.dictionary/Configuration-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-commands:InstallEnterpriseApplicationCommand.Command.Configuration",
              "text": "InstallEnterpriseApplicationCommand.Command.Configuration"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", install the app as a managed app. Otherwise, the system installs the app as unmanaged. If you reinstall a manged app and omit this value or set it to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", the app becomes unmanaged.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "For manifest-based installs, if ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system only considers apps installed in ",
                  "type": "text"
                },
                {
                  "code": "/Applications",
                  "type": "codeVoice"
                },
                {
                  "text": " as managed. In macOS 11 through 13, the system requires that the ",
                  "type": "text"
                },
                {
                  "code": "pkg",
                  "type": "codeVoice"
                },
                {
                  "text": " only contains a single signed app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Available in macOS 11 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "InstallAsManaged",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the app is an iOS app that can run on an Apple silicon in macOS 11 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "iOSApp",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "1"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The management flags. The possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "If ",
                            "type": "text"
                          },
                          {
                            "code": "InstallAsManaged",
                            "type": "codeVoice"
                          },
                          {
                            "text": " is ",
                            "type": "text"
                          },
                          {
                            "code": "true",
                            "type": "codeVoice"
                          },
                          {
                            "text": ", remove the app upon removal of the MDM profile.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "1",
                        "type": "codeVoice"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            },
            {
              "inlineContent": [
                {
                  "text": "Available in macOS 11 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "ManagementFlags",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary that specifies where to download the app. This value is backward-compatible with the manifest for the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/InstallApplicationCommand",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": "; however, it also allows you to specify ",
                  "type": "text"
                },
                {
                  "code": "sha256s",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "sha256-size",
                  "type": "codeVoice"
                },
                {
                  "text": " for SHA-256 hashes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "Manifest",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/InstallEnterpriseApplicationCommand/Command-data.dictionary/Manifest-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-commands:InstallEnterpriseApplicationCommand.Command.Manifest",
              "text": "InstallEnterpriseApplicationCommand.Command.Manifest"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL of the app manifest, which needs to begin with ",
                  "type": "text"
                },
                {
                  "code": "https:",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "ManifestURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of DER-encoded certificates to pin the connection when fetching the ",
                  "type": "text"
                },
                {
                  "code": "ManifestURL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "ManifestURLPinningCerts",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[data]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", certificate revocation checks require a positive response when using certificate pinning with ",
                  "type": "text"
                },
                {
                  "code": "ManifestURLPinningCerts",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "PinningRevocationCheckRequired",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the device needs to be network-tethered to run the command.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "RequestRequiresNetworkTether",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "InstallEnterpriseApplication"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The request type to install an enterprise managed app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.13.6",
          "name": "RequestType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
