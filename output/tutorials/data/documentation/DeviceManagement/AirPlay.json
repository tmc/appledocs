          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If present, only AirPlay destinations in this list are available to the device. This allow list applies to supervised devices.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "7.0.0",
          "name": "AllowList",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/AirPlay/AllowListItem",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:AirPlay.AllowListItem",
              "text": "AirPlay.AllowListItem"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If present, sets passwords for known AirPlay destinations. Using multiple entries for the same destination, whether within the same payload or across multiple installed payloads, is an error and results in undefined behavior.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "7.0.0",
          "name": "Passwords",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/AirPlay/PasswordsItem",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:AirPlay.PasswordsItem",
              "text": "AirPlay.PasswordsItem"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Use ",
                  "type": "text"
                },
                {
                  "code": "AllowList",
                  "type": "codeVoice"
                },
                {
                  "text": " instead. This key is deprecated in iOS 14.5 and macOS 11.3.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
