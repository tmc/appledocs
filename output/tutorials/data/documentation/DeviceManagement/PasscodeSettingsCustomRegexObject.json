          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary with supported OS language IDs for the keys (such as ",
                  "type": "text"
                },
                {
                  "code": "en-US",
                  "type": "codeVoice"
                },
                {
                  "text": "), and values that represent a localized description of the policy that the regular expression enforces. Use the special ",
                  "type": "text"
                },
                {
                  "code": "default",
                  "type": "codeVoice"
                },
                {
                  "text": " key for languages that the dictionary doesn’t contain.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "15.0.0",
          "name": "Description",
          "required": false,
