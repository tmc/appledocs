  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: macOS"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "SetAutoAdminPasswordCommand.Command"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier of the local administrator account. If this value doesn’t match the GUID of an administrator account that MDM created during Device Enrollment Program (DEP) enrollment, the command returns an error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.11.0",
          "name": "GUID",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The precreated salted SHA-512 PBKDF2 password hash for the account.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Create this hash on the server using the CommonCrypto libraries, or equivalent, as a salted SHA-512 PBKDF2 dictionary that contains these elements:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "entropy",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": The derived key from the password hash; for example, from ",
                          "type": "text"
                        },
                        {
                          "code": "CCKeyDerivationPBKDF()",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "salt",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": The 32-byte randomized salt; for example, from ",
                          "type": "text"
                        },
                        {
                          "code": "CCRandomCopyBytes()",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "iterations:",
                          "type": "codeVoice"
                        },
                        {
                          "text": " The number of iterations; for example, from ",
                          "type": "text"
                        },
                        {
                          "code": "CCCalibratePBKDF()",
                          "type": "codeVoice"
                        },
                        {
                          "text": " using a minimum hash time of 100 milliseconds, or if unknown, a number in the range of 20,000 to 40,000 iterations",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "Place the dictionary that contains these elements into an outer dictionary with the key ",
                  "type": "text"
                },
                {
                  "code": "SALTED-SHA512-PBKDF2",
                  "type": "codeVoice"
                },
                {
                  "text": ". Convert this dictionary to binary data before setting it as the value for ",
                  "type": "text"
                },
                {
                  "code": "passwordHash",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.11.0",
          "name": "passwordHash",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "data"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the device must be network-tethered to run the command.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.11.0",
          "name": "RequestRequiresNetworkTether",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "SetAutoAdminPassword"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The request type to change the password of a local administrator account.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.11.0",
          "name": "RequestType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
