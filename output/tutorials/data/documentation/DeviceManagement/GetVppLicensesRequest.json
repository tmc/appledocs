  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: VPP License Management"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
