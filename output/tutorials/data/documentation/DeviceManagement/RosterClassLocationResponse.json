      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A hex string that should be used for the next request to paginate. This field data type has a maximum length of 512 UTF-8 characters.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cursor",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Provides information about locations, sorted in lexical order by a location ",
                  "type": "text"
                },
                {
                  "code": "source_system_identifier",
                  "type": "codeVoice"
                },
                {
                  "text": ". The organization must provide this identifier to Apple.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "locations",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/RosterLocation",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-services:dep:RosterLocation",
              "text": "RosterLocation"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether the request’s limit and cursor values resulted in only a partial list of classes. If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the Mobile Device Management server should then make another request (starting from the newly returned cursor) to obtain additional records.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "more_to_follow",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        }
      ],
