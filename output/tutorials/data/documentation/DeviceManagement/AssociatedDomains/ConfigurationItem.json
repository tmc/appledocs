    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app identifier to associate the domains with.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.15.0",
          "name": "ApplicationIdentifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The domains to associate with the app. Each string is in the form of “",
                  "type": "text"
                },
                {
                  "code": "service:domain",
                  "type": "codeVoice"
                },
                {
                  "text": "”. Use fully qualified hostnames, such as ",
                  "type": "text"
                },
                {
                  "code": "www.example.com",
                  "type": "codeVoice"
                },
                {
                  "text": ". See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/Xcode/supporting-associated-domains",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.15.0",
          "name": "AssociatedDomains",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system enables direct download of data for this domain instead of through a CDN. Set the entitlement value for this domain to ",
                  "type": "text"
                },
                {
                  "code": "service:domain?mode=managed",
                  "type": "codeVoice"
                },
                {
                  "text": "; otherwise, the system ignores this value. Available in macOS 11 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.15.0",
          "name": "EnableDirectDownloads",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
