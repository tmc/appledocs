    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier supplied by the client when registering a user. Either ",
                  "type": "text"
                },
                {
                  "code": "clientUserIdStr",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "userId",
                  "type": "codeVoice"
                },
                {
                  "text": " is required. If both ",
                  "type": "text"
                },
                {
                  "code": "clientUserIdStr",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "userId",
                  "type": "codeVoice"
                },
                {
                  "text": " are supplied, ",
                  "type": "text"
                },
                {
                  "code": "userId",
                  "type": "codeVoice"
                },
                {
                  "text": " takes precedence.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientUserIdStr",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user’s email address. The ",
                  "type": "text"
                },
                {
                  "code": "email",
                  "type": "codeVoice"
                },
                {
                  "text": " field is updated only if the value is provided in the request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "email",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The hash of the user’s iTunes Store ID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "itsIdHash",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Apple ID associated with the user. This ID’s organization must match that of the provided sToken.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/associating-an-apple-id-with-a-volume-purchase-program-vpp-user",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "managedAppleIDStr",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The authentication token. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/managing-apps-and-books-through-web-services-legacy#Authentication",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sToken",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier assigned by the VPP when registering the user. Either ",
                  "type": "text"
                },
                {
                  "code": "clientUserIdStr",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "userId",
                  "type": "codeVoice"
                },
                {
                  "text": " is required. If both ",
                  "type": "text"
                },
                {
                  "code": "clientUserIdStr",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "userId",
                  "type": "codeVoice"
                },
                {
                  "text": " are supplied, ",
                  "type": "text"
                },
                {
                  "code": "userId",
                  "type": "codeVoice"
                },
                {
                  "text": " takes precedence.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "userId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
