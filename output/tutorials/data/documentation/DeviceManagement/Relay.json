      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of domain strings to exclude from routing through the servers in ",
                  "type": "text"
                },
                {
                  "code": "Relays",
                  "type": "codeVoice"
                },
                {
                  "text": ". Any connection that matches a domain in the list exactly or is a subdomain of the listed domain won’t use the relay server.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "ExcludedDomains",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of domain strings that the system uses to determine which connection to route through the servers in ",
                  "type": "text"
                },
                {
                  "code": "Relays",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Any connection that matches a domain in the list exactly or is a subdomain of the listed domain uses the relay servers, unless it matches a domain in ",
                  "type": "text"
                },
                {
                  "code": "ExcludedDomains",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If this list is empty, the system routes traffic to all domains to the relay servers, except those that match an excluded domain.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "MatchDomains",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of dictionaries that describe one or more relay servers that the system can chain together.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Relays",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Relay/Relay",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:Relay.Relay",
              "text": "Relay.Relay"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A globally-unique identifier for this relay configuration. The system uses this UUID to route managed apps through the servers in ",
                  "type": "text"
                },
                {
                  "code": "Relays",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "RelayUUID",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [],
          "introducedVersion": "17.0.0",
          "name": "Excluded FQDNs",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [],
          "introducedVersion": "17.0.0",
          "name": "MatchFQDNs",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        }
      ],
