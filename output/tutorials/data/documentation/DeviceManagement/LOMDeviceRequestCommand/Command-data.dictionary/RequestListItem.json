    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The DNS name of the device. This should match the ",
                  "type": "text"
                },
                {
                  "code": "dNSName",
                  "type": "codeVoice"
                },
                {
                  "text": " in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SCEP/PayloadContent-data.dictionary/SubjectAltName-data.dictionary",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or an equivalent in a PKCS12 identity.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "DeviceDNSName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "PowerON",
                "PowerOFF",
                "Reset"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The requested action to perform on the device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "DeviceRequestType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier of the request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "DeviceRequestUUID",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The LOM protocol version that the device supports. Provide the same value that ",
                  "type": "text"
                },
                {
                  "code": "LOMProtocolVersion",
                  "type": "codeVoice"
                },
                {
                  "text": " receives in the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/LOMSetupRequestResponse",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "LOMProtocolVersion",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array that contains the IPv6 addresses for primary LOM-compatible Ethernet interfaces for the device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "PrimaryIPv6AddressList",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array that contains the IPv6 addresses for secondary LOM-compatible Ethernet interfaces for the device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "11.0.0",
          "name": "SecondaryIPv6AddressList",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
