  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: iOS",
            "Unsupported OS: macOS",
            "Unsupported OS: tvOS"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "SoftwareUpdateSettings"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "default",
              "value": "true"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If set to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", a standard user can perform Major and Minor Software Updates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If set to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", only administrators can perform Major and Minor Software Updates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "18.0.0",
          "name": "AllowStandardUserOSUpdates",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This object configures various automatic Software Update functionality.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "18.0.0",
          "name": "AutomaticActions",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SoftwareUpdateSettingsAutomaticActionsObject",
              "kind": "typeIdentifier",
              "preciseIdentifier": "rmdm-declarations:SoftwareUpdateSettingsAutomaticActionsObject",
              "text": "SoftwareUpdateSettingsAutomaticActionsObject"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This object configures the beta program settings for a device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "18.0.0",
          "name": "Beta",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SoftwareUpdateSettingsBetaObject",
              "kind": "typeIdentifier",
              "preciseIdentifier": "rmdm-declarations:SoftwareUpdateSettingsBetaObject",
              "text": "SoftwareUpdateSettingsBetaObject"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This object configures the deferral of software updates. Rapid Security Responses aren’t considered within ‘Major’, ‘Minor’, or ‘System’ deferral mechanism.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "18.0.0",
          "name": "Deferrals",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SoftwareUpdateSettingsDeferralsObject",
              "kind": "typeIdentifier",
              "preciseIdentifier": "rmdm-declarations:SoftwareUpdateSettingsDeferralsObject",
              "text": "SoftwareUpdateSettingsDeferralsObject"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "true"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If set to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the device shows all software update enforcement notifications.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If set to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", the device only shows notifications triggered one hour before the enforcement deadline, and the restart countdown notification.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "18.0.0",
          "name": "Notifications",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "These configurations set user access to interacting with Rapid Security Responses (RSRs).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "18.0.0",
          "name": "RapidSecurityResponse",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SoftwareUpdateSettingsRapidSecurityResponseObject",
              "kind": "typeIdentifier",
              "preciseIdentifier": "rmdm-declarations:SoftwareUpdateSettingsRapidSecurityResponseObject",
              "text": "SoftwareUpdateSettingsRapidSecurityResponseObject"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "All",
                "Oldest",
                "Newest"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This string specifies how the device shows software updates to the user. When more than one update is available update, the device behaves as follows:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "All",
                          "type": "codeVoice"
                        },
                        {
                          "text": " - Shows all software update versions.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Oldest",
                          "type": "codeVoice"
                        },
                        {
                          "text": " - Shows only the oldest (lower numbered) software update version.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "Newest",
                          "type": "codeVoice"
                        },
                        {
                          "text": " - Shows only the newest (highest numbered) software update version.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "introducedVersion": "18.0.0",
          "name": "RecommendedCadence",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
