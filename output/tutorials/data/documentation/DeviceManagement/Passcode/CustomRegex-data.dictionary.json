    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Contains a dictionary of keys for supported OS language IDs (for example, “en-US”), and whose values represent a localized description of the policy enforced by the regular expression. Use the special ",
                  "type": "text"
                },
                {
                  "code": "default",
                  "type": "codeVoice"
                },
                {
                  "text": " key can for languages that aren’t contained in the dictionary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "passwordContentDescription",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Passcode/CustomRegex-data.dictionary/PasswordContentDescription-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:Passcode.CustomRegex.PasswordContentDescription",
              "text": "Passcode.CustomRegex.PasswordContentDescription"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A regular expression string that the system matches against the password to determine whether it complies with a policy. The regular expression uses the ICU syntax (",
                  "type": "text"
                },
                {
                  "identifier": "https://unicode-org.github.io/icu/userguide/strings/regexp.html",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": "). The string must not exceed 2048 characters in length.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "4.0.0",
          "name": "passwordContentRegex",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
