    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The display name for the account in notifications and authentication requests.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "AccountDisplayName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of created groups that don’t have administrator access.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "AdditionalGroups",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of groups to use for administrator access. The system requests membership during authentication.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "AdministratorGroups",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount of time after a ",
                  "type": "text"
                },
                {
                  "code": "FileVaultPolicy",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "LoginPolicy",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "UnlockPolicy",
                  "type": "codeVoice"
                },
                {
                  "text": " is received or updated that unregistered local accounts can be used. Required when ",
                  "type": "text"
                },
                {
                  "code": "AllowAuthenticationGracePeriod",
                  "type": "codeVoice"
                },
                {
                  "text": " is set. Available in macOS 15 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "AuthenticationGracePeriod",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Password",
                "UserSecureEnclaveKey",
                "SmartCard"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Platform SSO authentication method to use with the extension. Requires that the SSO Extension also support the method.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "AuthenticationMethod",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The pairing of Authorization Rights to group names. The system updates the Authorization Right to use the group when used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "AuthorizationGroups",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/ExtensibleSingleSignOn/PlatformSSO-data.dictionary/AuthorizationGroups-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-profiles:ExtensibleSingleSignOn.PlatformSSO.AuthorizationGroups",
              "text": "ExtensibleSingleSignOn.PlatformSSO.AuthorizationGroups"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Enables using identity provider accounts at authorization prompts. Requires that ",
                  "type": "text"
                },
                {
                  "code": "UseSharedDeviceKeys",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ". The system assigns groups using ",
                  "type": "text"
                },
                {
                  "code": "AdministratorGroups",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "AdditionalGroups",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "AuthorizationGroups",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "EnableAuthorization",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Enables creating new users at the login window with an ",
                  "type": "text"
                },
                {
                  "code": "AuthenticationMethod",
                  "type": "codeVoice"
                },
                {
                  "text": " of either ",
                  "type": "text"
                },
                {
                  "code": "Password",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "SmartCard",
                  "type": "codeVoice"
                },
                {
                  "text": ". Requires that ",
                  "type": "text"
                },
                {
                  "code": "UseSharedDeviceKeys",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "EnableCreateUserAtLogin",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "AttemptAuthentication",
                "RequireAuthentication",
                "AllowOfflineGracePeriod",
                "AllowAuthenticationGracePeriod"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The policy to apply when using Platform SSO at FileVault unlock on Apple Silicon Macs. Applies when ",
                  "type": "text"
                },
                {
                  "code": "AuthenticationMethod",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "Password",
                  "type": "codeVoice"
                },
                {
                  "text": ". Available in macOS 15 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "FileVaultPolicy",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "64800"
            },
            {
              "kind": "minimum",
              "value": "3600"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The duration, in seconds, until the system requires a full login instead of a refresh. The default value is 64,800 (18 hours). The minimum value is 3600 (1 hour).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "LoginFrequency",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "AttemptAuthentication",
                "RequireAuthentication",
                "AllowOfflineGracePeriod",
                "AllowAuthenticationGracePeriod"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The policy to apply when using Platform SSO at the login window. Applies when ",
                  "type": "text"
                },
                {
                  "code": "AuthenticationMethod",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "Password",
                  "type": "codeVoice"
                },
                {
                  "text": ". Available in macOS 15 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "13.0.0",
          "name": "LoginPolicy",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Standard",
                "Admin",
                "Groups"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The permission to apply to newly created accounts at login. Allowed values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "The account is a standard user.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "Standard",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "The system adds the account to the local administrators group.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "Admin",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "The system assigns group to the account using ",
                            "type": "text"
                          },
                          {
                            "code": "AdministratorGroups",
                            "type": "codeVoice"
                          },
                          {
                            "text": ", ",
                            "type": "text"
                          },
                          {
                            "code": "AdditionalGroups",
                            "type": "codeVoice"
                          },
                          {
                            "text": ", or ",
                            "type": "text"
                          },
                          {
                            "code": "AuthorizationGroups",
                            "type": "codeVoice"
                          },
                          {
                            "text": ".",
                            "type": "text"
