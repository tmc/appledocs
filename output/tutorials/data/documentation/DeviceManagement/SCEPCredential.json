        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The fingerprint of the Certificate Authority certificate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "CAFingerprint",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A preshared secret.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Challenge",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "RSA"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key type, which always has the value ",
                  "type": "text"
                },
                {
                  "code": "RSA",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Key Type",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A bitmask that specifies the use of the key: ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": " is signing, ",
                  "type": "text"
                },
                {
                  "code": "4",
                  "type": "codeVoice"
                },
                {
                  "text": " is encryption, and ",
                  "type": "text"
                },
                {
                  "code": "5",
                  "type": "codeVoice"
                },
                {
                  "text": " is both signing and encryption. Some certificate authorities, such as Windows CA, support only encryption or signing, but not both at the same time.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Key Usage",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "1024"
            },
            {
              "kind": "allowedValues",
              "values": [
                "1024",
                "2048",
                "4096"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key size in bits, either ",
                  "type": "text"
                },
                {
                  "code": "1024",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "2048",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "4096",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Keysize",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Any string that the SCEP server recognizes. For example, it could be a domain name such as ",
                  "type": "text"
                },
                {
                  "code": "example.org",
                  "type": "codeVoice"
                },
                {
                  "text": ". If a certificate authority has multiple CA certificates, you can use this field to specify the required certificate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "3"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of times the device should retry if the server sends a ",
                  "type": "text"
                },
                {
                  "code": "PENDING",
                  "type": "codeVoice"
                },
                {
                  "text": " response.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Retries",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "10"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of seconds to wait between subsequent retries. The system makes the first retry without this delay.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "RetryDelay",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The representation of an X.500 name is an array of OID and value. For example, ",
                  "type": "text"
                },
                {
                  "code": "/C=US/O=Apple Inc./CN=foo/1.2.5.3=bar",
                  "type": "codeVoice"
                },
                {
                  "text": " corresponds to:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "[ [ [\"C\", \"US\"] ], [ [\"O\", \"Apple Inc.\"] ], [ [ \"CN\", \"foo\"] ], [ [ \"1.2.5.3\", \"bar\" ] ] ]",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You can represent OIDs as dotted numbers or use shortcuts for country (",
                  "type": "text"
                },
                {
                  "code": "C",
                  "type": "codeVoice"
                },
                {
                  "text": "), locality (",
                  "type": "text"
                },
                {
                  "code": "L",
                  "type": "codeVoice"
                },
                {
                  "text": "), state (",
                  "type": "text"
                },
                {
                  "code": "ST",
                  "type": "codeVoice"
                },
                {
                  "text": "), organization (",
                  "type": "text"
                },
                {
                  "code": "O",
                  "type": "codeVoice"
                },
                {
                  "text": "), organizational unit (",
                  "type": "text"
                },
                {
                  "code": "OU",
                  "type": "codeVoice"
                },
                {
                  "text": "), and common name (",
                  "type": "text"
                },
                {
                  "code": "CN",
                  "type": "codeVoice"
                },
                {
                  "text": ").",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "Subject",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[[[string]]]"
            }
          ],
          "typeDetails": [
            {
              "arrayMode": true,
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The subject’s alternative name for the certificate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "SubjectAltName",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/SCEPCredentialSubjectAltNameObject",
              "kind": "typeIdentifier",
              "preciseIdentifier": "rmdm-declarations:SCEPCredentialSubjectAltNameObject",
              "text": "SCEPCredentialSubjectAltNameObject"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The SCEP URL.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "17.0.0",
          "name": "URL",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
