              "inlineContent": [
                {
                  "text": "The bundle identifier of the app. This setting is required if the type is ",
                  "type": "text"
                },
                {
                  "code": "Application",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
