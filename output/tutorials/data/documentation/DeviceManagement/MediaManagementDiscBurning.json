          "content": [
            {
              "inlineContent": [
                {
                  "text": "Configure disc-burn. Allowed values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "The system disables disc burning.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "off",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "The system allows normal default operation. Setting this key to ",
                            "type": "text"
                          },
                          {
                            "code": "on",
                            "type": "codeVoice"
                          },
                          {
                            "text": " doesn’t enable disc burn support if other mechanisms or preferences disabled it. Needs to be enabled with the ",
                            "type": "text"
                          },
                          {
                            "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/Finder",
                            "isActive": true,
                            "type": "reference"
                          },
                          {
                            "text": " profile.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "on",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "The system requires authentication.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "authenticate",
                        "type": "codeVoice"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            }
