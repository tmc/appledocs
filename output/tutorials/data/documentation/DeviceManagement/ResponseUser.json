        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The hash of the user’s unique store identifier. The ",
                  "type": "text"
                },
                {
                  "code": "idHash",
                  "type": "codeVoice"
                },
                {
                  "text": " field is only present when the user has an associated Apple ID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "idHash",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The invitation code that associates an Apple ID to a user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inviteCode",
          "required": false,
          "type": [
            {
              "kind": "text",
