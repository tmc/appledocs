    {
      "items": [
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the peer content cache power source is AC; otherwise, an internal battery provides its power.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.15.4",
          "name": "ac-power",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum amount of disk space, in bytes, available to the peer content cache.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.15.4",
          "name": "cache-size",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary that describes the capabilities of the peer content cache.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.15.4",
          "name": "capabilities",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/ContentCachingInformationResponse/StatusResponse-data.dictionary/PeersItem/Details-data.dictionary/Capabilities-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-commands:ContentCachingInformationResponse.StatusResponse.PeersItem.Details.Capabilities",
              "text": "ContentCachingInformationResponse.StatusResponse.PeersItem.Details.Capabilities"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the peer content cache computer is portable; for example, a laptop.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.15.4",
          "name": "is-portable",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
