    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary that contains FileVault unlock options.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "FileVaultUnlock",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.devicemanagement/documentation/DeviceManagement/RotateFileVaultKeyCommand/Command-data.dictionary/FileVaultUnlock-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "mdm-commands:RotateFileVaultKeyCommand.Command.FileVaultUnlock",
              "text": "RotateFileVaultKeyCommand.Command.FileVaultUnlock"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "personal",
                "institutional"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of FileVault key you want to change the password for. Set this value to ",
                  "type": "text"
                },
                {
                  "code": "personal",
                  "type": "codeVoice"
                },
                {
                  "text": " and set a value for ",
                  "type": "text"
                },
                {
                  "code": "Password",
                  "type": "codeVoice"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "code": "FileVaultUnlock",
                  "type": "codeVoice"
                },
                {
                  "text": " dictionary to enable unlocking a device with a password. Set this value to ",
                  "type": "text"
                },
                {
                  "code": "institutional",
                  "type": "codeVoice"
                },
                {
                  "text": " and set values for ",
                  "type": "text"
                },
                {
                  "code": "PrivateKeyExport",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "PrivateKeyExportPassword",
                  "type": "codeVoice"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "code": "FileVaultUnlock",
                  "type": "codeVoice"
                },
                {
                  "text": " dictionary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "KeyType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A DER-encoded certificate for creating a new institutional recovery key, which the system requires if ",
                  "type": "text"
                },
                {
                  "code": "KeyType",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "institutional",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "NewCertificate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "data"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A DER-encoded certificate for encrypting the new personal recovery key in a wrapper conforming to the IETF Cryptographic Message Syntax (CMS) standard.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "ReplyEncryptionCertificate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "data"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the device must be network-tethered to run the command.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "RequestRequiresNetworkTether",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "RotateFileVaultKey"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The request type to change the FileVault primary password on a device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "introducedVersion": "10.9.0",
          "name": "RequestType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
