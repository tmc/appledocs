    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ‘rrggbb’ field that defines the color of the gradient. The textColor* fields are not used to define the gradient color when this field is present.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "color",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A height between 0 and 1 that indicates where the gradient starts, measured from the bottom. If y1 is not present, it defaults to 0.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "y1",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A height between 0 and 1 that indicates where the gradient stops, measured from the bottom of the image.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "y2",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
