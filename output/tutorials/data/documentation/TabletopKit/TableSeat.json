    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To represent seats in your game, follow these steps:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Create a structure that conforms to this protocol.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Set the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.tabletopkit/documentation/TabletopKit/TableSeat/State",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " type alias to ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.tabletopkit/documentation/TabletopKit/TableSeatState",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Declare the ",
                      "type": "text"
                    },
                    {
                      "code": "id",
                      "type": "codeVoice"
                    },
                    {
                      "text": " property as a ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.tabletopkit/documentation/TabletopKit/TableSeatIdentifier",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " structure.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Declare the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.tabletopkit/documentation/TabletopKit/TableSeat/initialState",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " property as a ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.tabletopkit/documentation/TabletopKit/TableSeat/State",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " structure.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Implement an initializer that sets these properties.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "orderedList"
        },
        {
          "code": [
            "struct Seat: TableSeat {",
            "    typealias State = TableSeatState",
            "    var id: TableSeatIdentifier",
            "    var initialState: State",
            "",
            "    init(index: Int = 0, position: TableVisualState.Point2D) {",
            "        id = .seat(index)",
            "        initialState = .init(pose: .init(position: position, rotation: .init()))",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Then add the seats to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.tabletopkit/documentation/TabletopKit/TableSetup",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object using one of its add methods. For example, use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.tabletopkit/documentation/TabletopKit/TableSetup/add(seat:)-a9qw",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method to specify a position for the seat.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var setup = TableSetup(tabletop: table)",
            "setup.add(seat: Seat(index: 0, position: .init(x: 0, z: -0.5)))"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "To render seats using RealityKit, conform to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.tabletopkit/documentation/TabletopKit/EntityTableSeat",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " protocol instead.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
