    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The accessibility activation point to apply.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "activationPoint"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If true the accessibility activation point is applied;",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "otherwise the accessibility activation point is unchanged.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isEnabled"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to ensure that the activation point for a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "small element remains accurate even if you present a larger",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "version of the element to VoiceOver.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If an activation point is not provided, an activation point",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "will be derived from one of the accessibility elements",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "decedents or from the center of the accessibility frame.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
