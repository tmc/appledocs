{
  "abstract": [
    {
      "text": "Obtain authorization to use location services and manage changes to your app’s authorization status.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
