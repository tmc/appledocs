    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Boolean value to add to the message.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "boolean"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The format to apply to the Boolean value. You format Boolean values as either true/false or yes/no strings. If you don’t specify this parameter, the default format uses true/false strings. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.os/documentation/os/OSLogBoolFormat",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "format"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The privacy level of the information. If you don’t specify this parameter, the system uses the default rules to determine whether to show the information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "privacy"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Don’t call this function directly. The system calls it automatically when interpolating values of this type. When specifying the value in your string, you may include any of the indicated parameters to change the default presentation of that value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
