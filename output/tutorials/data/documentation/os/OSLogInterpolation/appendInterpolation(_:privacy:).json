    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The object with the description you want to add to the message. This function calls the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/nsobjectprotocol/1418746-description",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " method of the object and incorporates that value into the message string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "argumentObject"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The privacy level of the information. If you don’t specify this parameter, the default rules redact the object description.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "privacy"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Don’t call this function directly. The system calls it automatically when interpolating values of this type. When specifying the value in your string, you may include any of the indicated parameters to change the default presentation of that value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
