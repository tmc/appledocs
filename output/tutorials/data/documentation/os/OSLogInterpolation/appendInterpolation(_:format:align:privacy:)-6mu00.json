    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The double value to add to the message.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "number"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The format to apply to the double value. You format floating-point numbers as fixed-point, hexadecimal, exponential, or hybrid values. If you don’t specify this parameter, the default format uses a fixed-point value. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.os/documentation/os/OSLogFloatFormatting",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "format"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The alignment to apply to the value. Use this parameter to specify the width of the column containing the data, and the alignment of the data within that column. If you don’t specify this parameter, the system doesn’t align the value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "align"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The privacy level of the information. If you don’t specify this parameter, the system uses the default rules to determine whether to show the information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "privacy"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Don’t call this function directly. The system calls it automatically when interpolating values of this type. When specifying the value in your string, you may include any of the indicated parameters to change the default presentation of that value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
