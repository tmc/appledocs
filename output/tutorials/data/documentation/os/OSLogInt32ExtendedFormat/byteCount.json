    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this option to format the interpolated value as bytes, such as ",
              "type": "text"
            },
            {
              "code": "100 kB",
              "type": "codeVoice"
            },
            {
              "text": ". The following example applies the ",
              "type": "text"
            },
            {
              "code": "byteCount",
              "type": "codeVoice"
            },
            {
              "text": " formatter to the 32-bit integer value ",
              "type": "text"
            },
            {
              "code": "100_000",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Create a logger with the specified subsystem and category.",
            "let logger = Logger(subsystem: \"com.example.OSLogValueFormatting\",",
            "                    category: \"Formatter Output\")",
            "                ",
            "// Assign the value to interpolate.",
            "let value: Int32 = 100_000",
            "                ",
            "// Write the value to the log using the specified format.",
            "logger.info(\".byteCount output is \\(value, format: .byteCount)\")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "And the system writes the folllowing message to the log:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "[Formatter Output] .byteCount output is 100 kB"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
