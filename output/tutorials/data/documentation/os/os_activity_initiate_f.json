      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A C string that describes the activity. The description must be a constant string within the calling executable or library.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "description"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Flags that define how the activity should be created. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.os/documentation/os/os_activity_flag_t",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "flags"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A context to pass to the function. This may be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "context"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A function to call within the context of the given activity. This function must match the signature defined by ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.os/documentation/os/os_function_t",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "function"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function creates a new activity object implicitly as a child of the current activity object on the current thread. The object persists for the duration of the activity block’s execution.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
