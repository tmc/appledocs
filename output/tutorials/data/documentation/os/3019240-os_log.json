          "content": [
            {
              "inlineContent": [
                {
                  "text": "The log level to assign to the message. For more information about log levels, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.os/documentation/os/OSLogType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A custom log object. If unspecified, system uses the shared default log.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "log"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A constant string or format string that produces a human-readable log message.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "message"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "message",
                  "type": "codeVoice"
                },
                {
                  "text": " is a constant string, do not specify arguments. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "message",
                  "type": "codeVoice"
                },
                {
                  "text": " is a format string, pass the expected number of arguments in the order that they appear in the string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "args"
        }
      ]
    },
    {
      "content": [
