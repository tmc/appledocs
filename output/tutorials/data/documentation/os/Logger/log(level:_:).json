            {
              "inlineContent": [
                {
                  "text": "The message’s log level, which determines the severity of the message and whether the system persists it to disk. For possible values, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.os/documentation/os/OSLogType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "level"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The interpolated string that the logger writes to the log. Each of the message’s interpolations can specify individual formatting and privacy options. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.os/documentation/os/message-argument-formatters",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "message"
        }
      ]
    },
