      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether to display a plus (",
                  "type": "text"
                },
                {
                  "code": "+",
                  "type": "codeVoice"
                },
                {
                  "text": ") sign in front of positive numbers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "explicitPositiveSign"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether to uppercase letters that are part of the floating-point number. For example, it determines the capitalization of the exponent indicator ",
                  "type": "text"
                },
                {
                  "code": "e",
                  "type": "codeVoice"
                },
                {
                  "text": " in the number ",
                  "type": "text"
                },
                {
                  "code": "1.0e9",
                  "type": "codeVoice"
                },
                {
                  "text": ", or the letters in special values such as ",
                  "type": "text"
                },
                {
                  "code": "NaN",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "Inf",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "uppercase"
        }
      ]
