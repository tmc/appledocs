            },
            {
              "kind": "identifier",
              "text": "Stations.Attributes"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The radio station artwork.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artwork",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/Artwork",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:Artwork",
              "text": "Artwork"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The duration of the stream. This value isn’t emitted for ‘live’ or programmed stations.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "durationInMillis",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The notes about the station that appear in Apple Music.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "editorialNotes",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/EditorialNotes",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:EditorialNotes",
              "text": "EditorialNotes"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The episode number of the station. This value appears when the station represents an episode of a show or other content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "episodeNumber",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "clean",
                "explicit"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The rating of the content possibly heard while playing the station. The possible values for this rating are ",
                  "type": "text"
                },
                {
                  "code": "clean",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "explicit",
                  "type": "codeVoice"
                },
                {
                  "text": ". No value means no rating.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contentRating",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether the station is a livestream.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isLive",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "audio",
                "video"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The media kind for the station. It can have value ",
                  "type": "text"
                },
                {
                  "code": "audio",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "video",
                  "type": "codeVoice"
                },
                {
                  "text": " depending on whether it has video stream or audio stream.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mediaKind",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localized name of the station.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "When present, this attribute indicates that the radio station or episode is available to play with an Apple Music subscription. The value map may be used to initiate playback of the station. Live radio stations and episodes initiate streaming playback. Track-based stations initiate playback of individual tracks.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "playParams",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/PlayParameters",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:PlayParameters",
              "text": "PlayParameters"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the entity that provided the station, when specified.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stationProviderName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL for sharing the station in Apple Music.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "url",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
