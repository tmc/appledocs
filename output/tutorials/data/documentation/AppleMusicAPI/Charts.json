                },
                {
                  "text": " object specified by ",
                  "type": "text"
                },
                {
                  "code": "storefront",
                  "type": "codeVoice"
                },
                {
                  "text": ". Otherwise, the default is ",
                  "type": "text"
                },
                {
                  "code": "defaultLanguageTag",
                  "type": "codeVoice"
                },
                {
                  "text": " in ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
