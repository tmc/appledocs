    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The library albums associated with the artist. By default, ",
                  "type": "text"
                },
                {
                  "code": "albums",
                  "type": "codeVoice"
                },
                {
                  "text": " not included. It’s available only when fetching a single library artist resource by ID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Fetch limits: 25 default, 100 maximum",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "albums",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/LibraryArtists/Relationships-data.dictionary/LibraryArtistsAlbumsRelationship",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:LibraryArtists.Relationships.LibraryArtistsAlbumsRelationship",
              "text": "LibraryArtists.Relationships.LibraryArtistsAlbumsRelationship"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The artist in the Apple Music catalog the library artist is associated with, when known.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Fetch limits: None (associated with, at most, one catalog artist).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "catalog",
          "required": false,
          "type": [
            {
