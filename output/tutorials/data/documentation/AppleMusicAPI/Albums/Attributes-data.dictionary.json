    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the primary artist associated with the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artistName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "(Extended)",
                      "type": "text"
                    }
                  ],
                  "type": "strong"
                },
                {
                  "text": " The URL of the artist for this content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artistUrl",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The artwork for the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artwork",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/Artwork",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:Artwork",
              "text": "Artwork"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "dolby-atmos",
                "dolby-audio",
                "hi-res-lossless",
                "lossless",
                "lossy-stereo"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "(Extended)",
                      "type": "text"
                    }
                  ],
                  "type": "strong"
                },
                {
                  "text": " Indicates the specific audio variant for the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "Use badges to indicate the audio variant in Apple Music. For more information, see ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://assetcenter.dolby.com/",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " for identifying tracks with the Dolby Atmos logo and ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://developer.apple.com/go/?id=lossless-badges",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " for identifying tracks with the specific Apple Lossless glyph.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "name": "Important",
              "style": "important",
              "type": "aside"
            }
          ],
          "name": "audioVariants",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "clean",
                "explicit"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Recording Industry Association of America (RIAA) rating of the content. No value means no rating.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contentRating",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The copyright text.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "copyright",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The notes about the album that appear in the iTunes Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "editorialNotes",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/EditorialNotes",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:EditorialNotes",
              "text": "EditorialNotes"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The names of the genres associated with the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "genreNames",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether the album is marked as a compilation. If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the album is a compilation; otherwise, it’s not.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isCompilation",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether the album is complete. If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the album is complete; otherwise, it’s not. An album is complete if it contains all its tracks and songs.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isComplete",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether the response delivered the album as an ",
                  "type": "text"
                },
                {
                  "identifier": "https://www.apple.com/itunes/mastered-for-itunes/",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isMasteredForItunes",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether the album contains a single song.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isSingle",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localized name of the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "When present, this attribute indicates that one or more tracks on the album are available to play with an Apple Music subscription. The value map may be used to initiate playback of available tracks on the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "playParams",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/PlayParameters",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:PlayParameters",
              "text": "PlayParameters"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the record label for the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "recordLabel",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The release date of the album, when known, in YYYY-MM-DD or YYYY format. Prerelease content may have an expected release date in the future.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "releaseDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of tracks for the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trackCount",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Universal Product Code for the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "upc",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL for sharing the album in Apple Music.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "url",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
