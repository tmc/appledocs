    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The International Standard Recording Code (ISRC) values for the songs. You can substitute ",
                  "type": "text"
                },
                {
                  "code": "filter[isrc]",
                  "type": "codeVoice"
                },
                {
                  "text": " for ",
                  "type": "text"
                },
                {
                  "code": "ids",
                  "type": "codeVoice"
                },
                {
                  "text": ", or use it in conjunction with ",
                  "type": "text"
                },
                {
                  "code": "ids",
                  "type": "codeVoice"
                },
                {
                  "text": " for additional filtering. Note that one ISRC value may return more than one song. The maximum fetch limit is 25.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "filter[isrc]",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localization to use, specified by a language tag. The possible values are in the ",
                  "type": "text"
                },
                {
                  "code": "supportedLanguageTags",
                  "type": "codeVoice"
                },
                {
                  "text": " array belonging to the ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": " object specified by ",
                  "type": "text"
                },
                {
                  "code": "storefront",
                  "type": "codeVoice"
                },
                {
                  "text": ". Otherwise, the default is ",
                  "type": "text"
                },
                {
                  "code": "defaultLanguageTag",
                  "type": "codeVoice"
                },
                {
                  "text": " in ",
                  "type": "text"
                },
                {
