        },
        {
          "inlineContent": [
            {
              "text": "A rating indicates whether a user likes ",
              "type": "text"
            },
            {
              "code": "(1)",
              "type": "codeVoice"
            },
            {
              "text": " or dislikes ",
              "type": "text"
            },
            {
              "code": "(-1)",
              "type": "codeVoice"
            },
            {
              "text": " the station. These are the only two ratings supported.",
              "type": "text"
            }
