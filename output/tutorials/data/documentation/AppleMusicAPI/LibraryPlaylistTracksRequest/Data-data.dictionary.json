              "values": [
                "library-music-videos",
                "library-songs",
                "music-videos",
                "songs"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of the track to be added. The possible values are ",
                  "type": "text"
                },
                {
                  "code": "library-music-videos",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "library-songs",
                  "type": "codeVoice"
                },
                {
                  "text": ",  ",
                  "type": "text"
                },
                {
                  "code": "music-videos",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "songs",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
