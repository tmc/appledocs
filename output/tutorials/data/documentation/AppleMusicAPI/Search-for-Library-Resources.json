    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The text for the search, with a plus sign (",
                  "type": "text"
                },
                {
                  "code": "+",
                  "type": "codeVoice"
                },
                {
                  "text": ") between each word to replace spaces (for example, ",
                  "type": "text"
                },
                {
                  "code": "term=james+br",
                  "type": "codeVoice"
                },
                {
                  "text": ").",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "term",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "library-albums",
                "library-artists",
                "library-music-videos",
                "library-playlists",
                "library-songs"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of the types of resources to include in the results. The possible values are ",
                  "type": "text"
                },
                {
                  "code": "library-albums",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "library-songs",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "library-playlists",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "library-artists",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "library-music-videos",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "types",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "5"
            },
            {
              "kind": "maximum",
              "value": "25"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of objects or number of objects in the specified relationship returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "limit",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The next page or group of objects to fetch.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offset",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localization to use, specified by a language tag. The possible values are in the ",
                  "type": "text"
                },
                {
                  "code": "supportedLanguageTags",
                  "type": "codeVoice"
                },
                {
                  "text": " array belonging to the ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": " object specified by ",
                  "type": "text"
                },
                {
                  "code": "storefront",
                  "type": "codeVoice"
                },
                {
                  "text": ". Otherwise, the default is ",
                  "type": "text"
                },
                {
                  "code": "defaultLanguageTag",
                  "type": "codeVoice"
                },
                {
                  "text": " in ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "l",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
