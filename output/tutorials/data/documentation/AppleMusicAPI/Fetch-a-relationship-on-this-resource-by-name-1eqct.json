          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier for the activity.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
