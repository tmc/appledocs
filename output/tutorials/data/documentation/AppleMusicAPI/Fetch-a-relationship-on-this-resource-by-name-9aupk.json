          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the relationship you want to fetch for this resource.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "relationship",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An iTunes Store territory, specified by an ISO 3166 alpha-2 country code. The possible values are the ",
                  "type": "text"
                },
                {
                  "code": "id",
                  "type": "codeVoice"
                },
                {
                  "text": " attributes of ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": " objects.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "storefront",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
