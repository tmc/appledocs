        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique catalog identifiers for the resources. To indicate the type of resource to add, follow the ",
                  "type": "text"
                },
                {
                  "code": "ids",
                  "type": "codeVoice"
                },
                {
                  "text": " with one of the allowed values. Add multiple types in the same request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ids",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localization to use, specified by a language tag. The possible values are in the ",
                  "type": "text"
                },
                {
                  "code": "supportedLanguageTags",
                  "type": "codeVoice"
                },
                {
                  "text": " array belonging to the ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": " object that ",
                  "type": "text"
                },
                {
                  "code": "storefront",
                  "type": "codeVoice"
                },
                {
                  "text": " specifies. Otherwise, the default is ",
                  "type": "text"
                },
                {
                  "code": "defaultLanguageTag",
                  "type": "codeVoice"
                },
                {
                  "text": " in ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "l",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
