          "content": [
            {
              "inlineContent": [
                {
                  "text": "The playlist artwork.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artwork",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/Artwork",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:Artwork",
              "text": "Artwork"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The display name of the curator.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "curatorName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A description of the playlist.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "description",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/DescriptionAttribute",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:DescriptionAttribute",
              "text": "DescriptionAttribute"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether the playlist represents a popularity chart.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isChart",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date the playlist was last modified.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lastModifiedDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localized name of the playlist.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "editorial",
                "external",
                "personal-mix",
                "replay",
                "user-shared"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of playlist. Possible values are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Editorial: A playlist created by an Apple Music curator.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "External: A playlist created by a non-Apple curator or brand.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Personal-mix: A personalized playlist for an Apple Music user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Replay: A personalized Apple Music Replay playlist for an Apple Music user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "User-shared: A playlist created and shared by an Apple Music user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "playlistType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value map may be used to initiate playback of available tracks in the playlist.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "playParams",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/PlayParameters",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:PlayParameters",
              "text": "PlayParameters"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL for sharing the playlist in Apple Music.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "url",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "music-videos",
                "songs"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "(Extended)",
                      "type": "text"
                    }
                  ],
                  "type": "strong"
                },
                {
                  "text": " The resource types that are present in the tracks of the playlists.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trackTypes",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        }
      ],
