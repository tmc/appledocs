    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the album the song appears on.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "albumName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The artist’s name.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artistName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "(Extended)",
                      "type": "text"
                    }
                  ],
                  "type": "strong"
                },
                {
                  "text": " The URL of the artist for the content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artistUrl",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The album artwork.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artwork",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/Artwork",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:Artwork",
              "text": "Artwork"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "(Classical music only) The name of the artist or composer to attribute the song with.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "attribution",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "dolby-atmos",
                "dolby-audio",
                "hi-res-lossless",
                "lossless",
                "lossy-stereo"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "(Extended)",
                      "type": "text"
                    }
                  ],
                  "type": "strong"
                },
                {
                  "text": " Indicates the specific audio variant for a song.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "Use badges to indicate the audio variant in Apple Music. For more information, see ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://assetcenter.dolby.com/",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " for identifying tracks with the Dolby Atmos logo and ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://developer.apple.com/go/?id=lossless-badges",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " for identifying tracks with the specific Apple Lossless glyph.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "name": "Important",
              "style": "important",
              "type": "aside"
            }
          ],
          "name": "audioVariants",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The song’s composer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "composerName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "clean",
                "explicit"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Recording Industry Association of America (RIAA) rating of the content. No value means no rating.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contentRating",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The disc number of the album the song appears on.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "discNumber",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The duration of the song in milliseconds.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "durationInMillis",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The notes about the song that appear in the Apple Music catalog.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "editorialNotes",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/EditorialNotes",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:EditorialNotes",
              "text": "EditorialNotes"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The genre names the song is associated with.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "genreNames",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether the song has lyrics available in the Apple Music catalog. If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the song has lyrics available; otherwise, it doesn’t.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "hasLyrics",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether the response delivered the song as an ",
                  "type": "text"
                },
                {
                  "identifier": "https://www.apple.com/apple-music/apple-digital-masters/",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isAppleDigitalMaster",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The International Standard Recording Code (ISRC) for the song.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isrc",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "(Classical music only) The movement count of the song.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "movementCount",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "(Classical music only) The movement name of the song.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "movementName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "(Classical music only) The movement number of the song.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "movementNumber",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localized name of the song.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "When present, this attribute indicates that the song is available to play with an Apple Music subscription. The value map may be used to initiate playback. Previews of the song audio may be available with or without an Apple Music subscription.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "playParams",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/PlayParameters",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:PlayParameters",
              "text": "PlayParameters"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The preview assets for the song.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "previews",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/Preview",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:Preview",
              "text": "Preview"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The release date of the song, when known, in YYYY-MM-DD or YYYY format. Prerelease songs may have an expected release date in the future.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "releaseDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of the song in the album’s track list.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trackNumber",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL for sharing the song in Apple Music.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "url",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "(Classical music only) The name of the associated work.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "workName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
