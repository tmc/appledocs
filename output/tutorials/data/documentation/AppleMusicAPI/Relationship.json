      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A URL subpath that fetches the relationship resources as the primary object. This member is only present in responses.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "href",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Link to the next page of resources in the relationship. Contains the ",
                  "type": "text"
                },
                {
                  "code": "offset",
                  "type": "codeVoice"
                },
                {
                  "text": " query parameter that specifies the next page. See ",
                  "type": "text"
                },
                {
                  "code": "Fetch Resources by Page",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "next",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "One or more destination objects.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data",
          "required": true,
          "type": [
