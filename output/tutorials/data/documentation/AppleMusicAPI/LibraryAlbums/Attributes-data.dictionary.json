    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The artistâ€™s name.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artistName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The album artwork.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "artwork",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/Artwork",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:Artwork",
              "text": "Artwork"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "clean",
                "explicit"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Recording Industry Association of America (RIAA) rating of the content. The possible values for this rating are ",
                  "type": "text"
                },
                {
                  "code": "clean",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "explicit",
                  "type": "codeVoice"
                },
                {
                  "text": ". No value means no rating.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contentRating",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date the album was added to the library, in YYYY-MM-DD or YYYY format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dateAdded",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localized name of the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "When present, this attribute indicates that tracks from the album are available to play.  The value map may be used to initiate playback of available tracks on the album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "playParams",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemusicapi/documentation/AppleMusicAPI/PlayParameters",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:music_api:PlayParameters",
              "text": "PlayParameters"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The release date of the album, when known, in YYYY-MM-DD or YYYY format. Pre-release albums may have an expected release date in the future.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "releaseDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of tracks.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trackCount",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The names of the genres associated with this album.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "genreNames",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
