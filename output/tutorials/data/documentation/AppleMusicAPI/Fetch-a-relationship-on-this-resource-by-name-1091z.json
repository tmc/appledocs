      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier for the curator.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "playlists"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the relationship you want to fetch for this resource.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "relationship",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An iTunes Store territory, specified by an ISO 3166 alpha-2 country code. The possible values are the ",
                  "type": "text"
                },
                {
                  "code": "id",
                  "type": "codeVoice"
                },
                {
                  "text": " attributes of ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": " objects.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "storefront",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "restParameters",
      "source": "path",
      "title": "Path Parameters"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localization to use, specified by a language tag. The possible values are in the ",
                  "type": "text"
                },
                {
                  "code": "supportedLanguageTags",
                  "type": "codeVoice"
                },
                {
                  "text": " array belonging to the ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": " object specified by ",
                  "type": "text"
                },
                {
                  "code": "storefront",
                  "type": "codeVoice"
                },
                {
                  "text": ". Otherwise, the default is ",
                  "type": "text"
                },
                {
                  "code": "defaultLanguageTag",
                  "type": "codeVoice"
                },
                {
                  "text": " in ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
