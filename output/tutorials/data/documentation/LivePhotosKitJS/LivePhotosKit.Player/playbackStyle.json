      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This property can take one of two values:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "LivePhotosKit.PlaybackStyle.FULL",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "LivePhotosKit.PlaybackStyle.HINT",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "code": "FULL",
              "type": "codeVoice"
            },
            {
              "text": " refers to the full-featured Live Photo animation consisting of a brief animation forward from the photo. This is followed by a fade transition to the beginning of the video and a subsequent full play through before transitioning back to the photo.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "code": "HINT",
              "type": "codeVoice"
            },
            {
              "text": " refers to a shortened animation that simply plays forward from the photo through the end of the video, before transitioning back to the photo.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
