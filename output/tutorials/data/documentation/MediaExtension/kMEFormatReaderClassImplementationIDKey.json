      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Format the string similar to the bundle identifier of the format reader. Start with the reverse domain identifier of your developer account, followed by ",
              "type": "text"
            },
            {
              "code": ".formatreader.",
              "type": "codeVoice"
            },
            {
              "text": " and the name of the media format. If you plan to create multiple variants of the same media format, include an additional component to make each format reader identifier unique; for example ",
              "type": "text"
            },
            {
              "code": "com.mycompany.formatreader.mymediaformat.formatvariant",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
