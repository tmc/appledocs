      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Format the string similar to the bundle identifier of the video decoder. Start with the reverse domain identifier of your developer account, followed by ",
              "type": "text"
            },
            {
              "code": ".videodecoder.",
              "type": "codeVoice"
            },
            {
              "text": " and the name of the codec. If you plan to create multiple variants of the same video decoder, include an additional component to make each video decoder identifier unique; for example ",
              "type": "text"
            },
            {
              "code": "com.mycompany.videodecoder.mycodec.codecvariant",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
