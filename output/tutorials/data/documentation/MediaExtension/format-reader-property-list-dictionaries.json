  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "An ",
              "type": "text"
            },
            {
              "code": "ExtensionKit",
              "type": "codeVoice"
            },
            {
              "text": " format reader needs to include one required dictionary and can also include an optional dictionary in its ",
              "type": "text"
            },
            {
              "code": "Info.plist",
              "type": "codeVoice"
            },
            {
              "text": " file:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "EXAppExtensionAttributes",
                      "type": "codeVoice"
                    },
                    {
                      "text": ": A required dictionary that describes the format reader and contains the following keys and values:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "orderedList"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.mediaextension/documentation/MediaExtension/kMEFormatReaderClassImplementationIDKey",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ": The identifier for the format reader. Format similarly to the bundle identifier.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "EXExtensionPointIdentifier",
                      "type": "codeVoice"
                    },
                    {
                      "text": ": The extension point name for format readers. Set to the value for ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.mediaextension/documentation/MediaExtension/kMEFormatReaderExtensionPointName",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "EXPrincipalClass",
                      "type": "codeVoice"
                    },
                    {
                      "text": ": The name of the format reader factory class that conforms to the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.mediaextension/documentation/MediaExtension/MEFormatReaderExtension",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " protocol.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.mediaextension/documentation/MediaExtension/kMEFormatReaderFileNameExtensionArrayKey",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ": An array of strings that specify the file name extensions that the format reader supports.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.mediaextension/documentation/MediaExtension/kMEFormatReaderUTTypeArrayKey",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ": A key to the array of Uniform Type Identifiers that the format reader supports.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.mediaextension/documentation/MediaExtension/kMEFormatReaderObjectNameKey",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ": A user-readable string that describes the format reader.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/BundleResources/Information-Property-List/UTExportedTypeDeclarations",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ": An optional dictionary that registers the formats from the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.mediaextension/documentation/MediaExtension/kMEFormatReaderUTTypeArrayKey",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " array that the format reader supports, so the system can recognize those media files and open them in the appropriate app. It contains the following keys and values:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "start": 2,
          "type": "orderedList"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/BundleResources/Information-Property-List/UTExportedTypeDeclarations/UTTypeDescription",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ": A user-readable string that specifies the media kind, which Finder displays.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/BundleResources/Information-Property-List/UTExportedTypeDeclarations/UTTypeConformsTo",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ": An array of ",
                      "type": "text"
                    },
                    {
                      "code": "UTType",
                      "type": "codeVoice"
                    },
                    {
                      "text": " identifiers, with the first entry set to ",
                      "type": "text"
                    },
                    {
                      "code": "com.apple.mediaextension-content",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". This type also conforms to higher level abstract types such as ",
                      "type": "text"
                    },
                    {
                      "code": "public.movie",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "public.audiovisual-content",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". This array can include additional types as needed.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/BundleResources/Information-Property-List/UTExportedTypeDeclarations/UTTypeIdentifier",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ": This value needs to match the value for ",
                      "type": "text"
                    },
                    {
                      "code": "kMEFormatReaderUTTypeArrayKey",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in the ",
                      "type": "text"
                    },
                    {
                      "code": "EXAppExtensionAttributes",
                      "type": "codeVoice"
                    },
                    {
                      "text": " dictionary.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/BundleResources/Information-Property-List/UTExportedTypeDeclarations/UTTypeTagSpecification",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "public.filename-extension",
                      "type": "codeVoice"
                    },
                    {
                      "text": " key: This value needs to match the value for ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.mediaextension/documentation/MediaExtension/kMEFormatReaderFileNameExtensionArrayKey",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " in the ",
                      "type": "text"
                    },
                    {
                      "code": "EXAppExtensionAttributes",
                      "type": "codeVoice"
                    },
                    {
                      "text": " dictionary.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "public.mime-type",
                      "type": "codeVoice"
                    },
                    {
                      "text": ": This is an optional key to a MIME type that corresponds to the format.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "For general information on how to make a ",
              "type": "text"
            },
            {
              "code": "UTExportedTypeDeclarations",
              "type": "codeVoice"
            },
            {
              "text": " dictionary, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/BundleResources/Information-Property-List/UTExportedTypeDeclarations",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
