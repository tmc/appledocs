          "value": {
            "kind": "parameters",
            "parameters": [
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "An allocator for the session.  Pass ",
                        "type": "text"
                      },
                      {
                        "code": "NULL",
                        "type": "codeVoice"
                      },
                      {
                        "text": " to use the default allocator.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "allocator"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Specifies where to put the backing file for the multipass storage object. If you pass ",
                        "type": "text"
                      },
                      {
                        "code": "NULL",
                        "type": "codeVoice"
                      },
                      {
                        "text": " for ",
                        "type": "text"
                      },
                      {
                        "code": "fileURL",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", the video toolbox will pick a unique temporary file name.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "fileURL"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Gives a hint to the multipass storage about valid time stamps for data. You can pass ",
                        "type": "text"
                      },
                      {
                        "code": "kCMTimeRangeInvalid",
                        "type": "codeVoice"
                      },
                      {
                        "text": " if you do not want to provide a time range hint.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "timeRange"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "If the file did not exist when the storage was created, the file will be deleted when the multipass storage object is finalized, unless you set the ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.videotoolbox/documentation/VideoToolbox/kVTMultiPassStorageCreationOption_DoNotDelete",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " option to ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfbooleantrue",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " in the ",
                        "type": "text"
                      },
                      {
                        "code": "options",
                        "type": "codeVoice"
                      },
                      {
                        "text": " dictionary.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "options"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A pointer to the newly created multipass storage object.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "multiPassStorageOut"
              }
            ]
          }
