    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Depending on the scaling mode, scaling may take into account:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The full image buffer width and height of the source and destination",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The clean aperture attachment (",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/CoreVideo/kCVImageBufferCleanApertureKey",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ") on the source image buffer",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The pixel aspect ratio attachment (",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/CoreVideo/kCVImageBufferPixelAspectRatioKey",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ") on the source image buffer",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The destination clean aperture (",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.videotoolbox/documentation/VideoToolbox/kVTPixelTransferPropertyKey_DestinationCleanAperture",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ")",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The destination pixel aspect ratio (",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.videotoolbox/documentation/VideoToolbox/kVTPixelTransferPropertyKey_DestinationPixelAspectRatio",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ")",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "The destination image bufferâ€™s clean aperture and pixel aspect ratio attachments are not taken into account, and are overwritten.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
