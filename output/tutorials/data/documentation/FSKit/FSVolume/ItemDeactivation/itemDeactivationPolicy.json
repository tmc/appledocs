        {
          "inlineContent": [
            {
              "text": "Tell FSKit for which type of items (if at all) ",
              "type": "text"
            },
            {
              "code": "deactivateItem:",
              "type": "codeVoice"
            },
            {
              "text": " should be called.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "FSKit reads this value after the file system module replies to ",
              "type": "text"
            },
            {
              "code": "loadResource:",
              "type": "codeVoice"
            },
            {
              "text": ". Changing it during the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "runtime of the volume wonâ€™t have an effect.",
              "type": "text"
            }
          ],
