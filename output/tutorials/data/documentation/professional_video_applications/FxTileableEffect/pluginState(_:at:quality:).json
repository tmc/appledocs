            "parameters": [
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The state of the plug-in. Your plug-in should return an autoreleased ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/foundation/nsdata",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " that the host retains. Once created, your plug-in should not hold on to this pointer. The host returns this data (or a copy of it) to your plug-in during other methods, such as the ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.professionalvideoapplications/documentation/professional_video_applications/FxTileableEffect/renderDestinationImage(_:sourceImages:pluginState:at:)",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " method and ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.professionalvideoapplications/documentation/professional_video_applications/FxTileableEffect/scheduleInputs(_:withPluginState:at:)",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " methods.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "pluginState"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The rational time at which your plug-in is being asked to gather its rendering state.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "renderTime"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The requested render quality (",
                        "type": "text"
                      },
                      {
                        "code": "kFxQuality_LOW",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", ",
                        "type": "text"
                      },
                      {
                        "code": "kFxQuality_MEDIUM",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", or ",
                        "type": "text"
                      },
                      {
                        "code": "kFxQuality_HIGH",
                        "type": "codeVoice"
                      },
                      {
                        "text": ").",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "qualityLevel"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Description of the problem if you are unable to add all of your parameters. After describing the problem in this ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/foundation/nserror",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": ", return ",
                        "type": "text"
                      },
                      {
                        "code": "NO",
                        "type": "codeVoice"
                      },
                      {
                        "text": ". Use the ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.professionalvideoapplications/documentation/professional_video_applications/FxPlugErrorDomain",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " when creating the error.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "error"
              }
            ]
