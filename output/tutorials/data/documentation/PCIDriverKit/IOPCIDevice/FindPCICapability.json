      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A PCI capability ID. PCI Express devices may support extended capabilities in configuraiton space, starting at offset ",
                  "type": "text"
                },
                {
                  "code": "0x100",
                  "type": "codeVoice"
                },
                {
                  "text": ". To search this space, pass an ID that is the negated value of the PCI-SIG assigned ID for the extended capability.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "capabilityID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The offset into configuration space at which to start the search.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "searchOffset"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A variable in which you want to store the resulting offset value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "foundCapabilityOffset"
        }
      ]
    },
