          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the properties of this structure to create a monthly weather statistics query. You can combine multiple queries into a single ",
              "type": "text"
            },
            {
              "code": "WeatherService",
              "type": "codeVoice"
            },
            {
              "text": " request.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Hereâ€™s how to get monthly weather statistics for New York City:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let (monthlyPrecipitationStatistics, monthlyTemperatureStatistics) = try await service.monthlyStatistics(for: newYork, spanning: interval, including: .precipitation, .temperature)"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
