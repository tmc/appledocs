    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The content request event that occured that caused this method to be invoked.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "request"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A file URL to the local manifest that was pre-downloaded and specified in your App’s Info.plist under the ",
                  "type": "text"
                },
                {
                  "code": "BAManifestURL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The file is read-only and will be deleted after this method returns. If you wish to keep it, you must copy it out.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "manifestURL"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Information about your extension that may be useful for making a decision about what content needs to be scheduled.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "extensionInfo"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A set of unique ",
              "type": "text"
            },
            {
              "code": "BAURLDownload",
              "type": "codeVoice"
            },
            {
              "text": " objects that the extension should schedule for download.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "You cannot use ",
                  "type": "text"
                },
                {
                  "code": "BADownloadManager",
                  "type": "codeVoice"
                },
                {
                  "text": " to manually schedule downloads using this method. All downloads to enqueue must be returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Warning",
          "style": "warning",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
