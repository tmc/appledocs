      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The effective top-level domain and one more preceding path component (eTLD+1) representation of the ad network that seeks ad attribution.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source_domain",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A key that describes the type of user action that leads to the ad impression. The value is ",
                  "type": "text"
                },
                {
                  "code": "\"click_to_App_Store\"",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source_engagement_type",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Base64URL-encoded string representation of a UUID. This value is the same value the ad network provides as the ",
                  "type": "text"
                },
                {
                  "code": "attributionSourceNonce",
                  "type": "codeVoice"
                },
                {
                  "text": " in the attributable ad link.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source_nonce",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The SKAdNetwork version of ",
                  "type": "text"
                },
                {
                  "code": "\"4.0\"",
                  "type": "codeVoice"
                },
                {
                  "text": " or later. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/StoreKit/skadnetwork-release-notes",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for version information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "version",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "attributes": [
        {
          "kind": "allowedTypes",
          "values": []
        }
      ],
      "kind": "attributes",
      "title": "Attributes"
    },
    {
      "kind": "mentions",
      "mentions": [
        "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/creating-an-attributable-ad-link"
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The device generates the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/AdImpressionRequest",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and sends it in the body of a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/Get-a-signed-SKAdNetwork-ad-payload-for-a-web-ad.",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " request. The device uses the value from the ",
              "type": "text"
            },
            {
              "code": "attributionSourceNonce",
              "type": "codeVoice"
            },
            {
              "text": " in a web ad link for the ",
              "type": "text"
            },
            {
              "code": "source_nonce",
              "type": "codeVoice"
            },
            {
              "text": " value in this request. The ad network looks up the fully signed web ad impression using the ",
              "type": "text"
            },
            {
              "code": "source_nonce",
              "type": "codeVoice"
            },
            {
              "text": " value, which it creates when it serves the attributable ad link.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "attributionSourceNonce",
                  "type": "codeVoice"
                },
                {
                  "text": " in a web ad, the ",
                  "type": "text"
                },
                {
                  "code": "source_nonce",
                  "type": "codeVoice"
                },
                {
                  "text": " in this request, and the ",
                  "type": "text"
                },
                {
                  "code": "nonce",
                  "type": "codeVoice"
                },
                {
                  "text": " in a corresponding ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/AdImpressionResponse",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " all represent the same UUID, but the encoding varies.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
