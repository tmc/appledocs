          "content": [
            {
              "inlineContent": [
                {
                  "text": "The SKAdNetwork version. Use version ",
                  "type": "text"
                },
                {
                  "code": "“4.0”",
                  "type": "codeVoice"
                },
                {
                  "text": " or later. For version information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/StoreKit/skadnetwork-release-notes",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "version",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ad network ID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You receive an ad network ID when you register to use SKAdNetwork. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/StoreKit/registering-an-ad-network",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ad_network_id",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A four-digit value you use to measure the aspects of an advertising effort or campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source_identifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The App Store ID of the app that the ad impression advertises. This is the same value the ad network provides in the attributable ad link. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/creating-an-attributable-ad-link",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "itunes_item_id",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A UUID you generate to include in the signature. This value needs to match the value of ",
                  "type": "text"
                },
                {
                  "code": "attributionSourceNonce",
                  "type": "codeVoice"
                },
                {
                  "text": " in the original ad link. This value needs to be in the ",
                  "type": "text"
                },
                {
                  "code": "UUID",
                  "type": "codeVoice"
                },
                {
                  "text": " string format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Provide the dash-separated representation of the ",
                  "type": "text"
                },
                {
                  "code": "attributionSourceNonce",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nonce",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The effective top-level domain and one more preceding path component (eTLD+1) representation of the ad network that seeks ad attribution.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source_domain",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The fidelity type value for web ads is ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fidelity_type",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An integer that represents the UNIX time, in milliseconds, of the ad impression.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timestamp",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        }
      ],
