        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ad network ID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You receive an ad network ID when you register to use SKAdNetwork. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/StoreKit/registering-an-ad-network",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ad_network_id",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The App Store app ID that the ad impression advertises. This is the same value the ad network provides in the attributable ad link. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/creating-an-attributable-ad-link",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "itunes_item_id",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This value needs to match the value of ",
                  "type": "text"
                },
                {
                  "code": "source_nonce",
                  "type": "codeVoice"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/AdImpressionRequest",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". The value needs to be in the ",
                  "type": "text"
                },
                {
                  "code": "UUID",
                  "type": "codeVoice"
                },
                {
                  "text": " string format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Provide the dash-separated representation of the ",
                  "type": "text"
                },
                {
                  "code": "source_nonce",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nonce",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The cryptographic signature the ad network generates to sign the web ad. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/generating-a-signature-for-attributable-web-ads",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signature",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/signature",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:skadnetwork:signature",
              "text": "signature"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The effective top-level domain and one more preceding path component (eTLD+1) representation of the ad network serving the ad. This value needs to match the ",
                  "type": "text"
                },
                {
                  "code": "source_domain",
                  "type": "codeVoice"
                },
                {
                  "text": " value you receive in the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/AdImpressionRequest",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source_domain",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A four-digit value you use to measure the aspects of an advertising effort or campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source_identifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An integer that represents the UNIX time, in milliseconds, that you create this ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.skadnetworkforwebads/documentation/SKAdNetworkforWebAds/AdImpressionResponse",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timestamp",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The SKAdNetwork version. Use version ",
                  "type": "text"
                },
                {
                  "code": "\"4.0\"",
                  "type": "codeVoice"
                },
                {
                  "text": " or later. For version information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/StoreKit/skadnetwork-release-notes",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
