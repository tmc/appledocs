            {
              "inlineContent": [
                {
                  "text": "A completion handler that the system calls after the user responds to a request to authorize use of personal voices, which receives the authorization status as an argument.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
