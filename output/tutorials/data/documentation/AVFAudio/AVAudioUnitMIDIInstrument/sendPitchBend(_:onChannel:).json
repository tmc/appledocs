      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Value of the pitchbend. The valid range of values is ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "16383",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pitchbend"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The channel number to send the event to. The valid range of values is ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "15",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "channel"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If this method isn’t invoked, then the system uses the default pitch bend value of ",
              "type": "text"
            },
            {
              "code": "8192",
              "type": "codeVoice"
            },
            {
              "text": " (no pitch).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
