    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You specify the blend as a percentage. The default value is ",
              "type": "text"
            },
            {
              "code": "100%",
              "type": "codeVoice"
            },
            {
              "text": ". The valid range of values is ",
              "type": "text"
            },
            {
              "code": "0%",
              "type": "codeVoice"
            },
            {
              "text": " through ",
              "type": "text"
            },
            {
              "code": "100%",
              "type": "codeVoice"
            },
            {
              "text": ", where ",
              "type": "text"
            },
            {
              "code": "0%",
              "type": "codeVoice"
            },
            {
              "text": " represents all dry.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
