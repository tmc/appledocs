{
  "abstract": [
    {
      "text": "Use a resource state encoder to allocate and deallocate sparse tiles for a sparse texture.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
