            }
          ],
          "name": "buffers"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of offsets, each of which specifies where the data begins, in bytes, from the start of its corresponding buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offsets"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The argument table indices to bind each of the ",
                  "type": "text"
                },
                {
                  "code": "buffers",
                  "type": "codeVoice"
                },
                {
                  "text": " to, in the order they appear.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "range"
        }
      ]
    },
