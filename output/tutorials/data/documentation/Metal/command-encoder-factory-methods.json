  "topicSections": [
    {
      "abstract": [
        {
          "text": "A render encoder creates a render pass that draws graphics with a GPU.",
          "type": "text"
        }
      ],
      "anchor": "Creating-Render-Encoders",
      "identifiers": [
        "doc://com.apple.metal/documentation/Metal/MTLCommandBuffer/makeRenderCommandEncoder(descriptor:)"
      ],
      "title": "Creating Render Encoders"
    },
    {
      "abstract": [
        {
          "text": "A parallel render encoder creates multiple render encoders that all contribute to a single render pass that draws graphics with a GPU.",
          "type": "text"
        }
      ],
      "anchor": "Creating-Parallel-Render-Encoders",
      "identifiers": [
        "doc://com.apple.metal/documentation/Metal/MTLCommandBuffer/makeParallelRenderCommandEncoder(descriptor:)"
      ],
      "title": "Creating Parallel Render Encoders"
    },
    {
      "abstract": [
        {
          "text": "An acceleration structure encoder creates a pass that builds or refits data for ray tracing with a GPU.",
          "type": "text"
        }
      ],
      "anchor": "Creating-Acceleration-Structure-Encoders",
      "identifiers": [
        "doc://com.apple.metal/documentation/Metal/MTLCommandBuffer/makeAccelerationStructureCommandEncoder(descriptor:)",
        "doc://com.apple.metal/documentation/Metal/MTLCommandBuffer/makeAccelerationStructureCommandEncoder()"
      ],
      "title": "Creating Acceleration Structure Encoders"
    },
    {
      "abstract": [
        {
          "text": "A compute encoder creates a pass that runs computations in parallel with the GPU.",
          "type": "text"
        }
      ],
      "anchor": "Creating-Compute-Encoders",
      "identifiers": [
        "doc://com.apple.metal/documentation/Metal/MTLCommandBuffer/makeComputeCommandEncoder(descriptor:)",
