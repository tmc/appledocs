    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An indirect command buffer the command copies from.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "buffer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The range of commands in the source buffer to copy.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The source range needs to start on a valid execution point.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sourceRange"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Another indirect command buffer the command copies to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destination"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An index in ",
                  "type": "text"
                },
                {
                  "code": "destination",
                  "type": "codeVoice"
                },
                {
                  "text": " where the command copies content from ",
                  "type": "text"
                },
                {
                  "code": "source",
                  "type": "codeVoice"
                },
                {
                  "text": " to. The destination index needs to be a valid execution point with enough remaining space in ",
                  "type": "text"
                },
                {
                  "code": "destination",
                  "type": "codeVoice"
                },
                {
                  "text": " to accommodate ",
                  "type": "text"
                },
                {
                  "code": "sourceRange.count",
                  "type": "codeVoice"
                },
                {
                  "text": " indexes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destinationIndex"
        }
      ]
    },
    {
      "content": [
