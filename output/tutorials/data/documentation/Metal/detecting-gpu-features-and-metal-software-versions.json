{
  "abstract": [
    {
      "text": "Use the device object’s properties to determine how you perform tasks in Metal.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
