      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The default value is ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ". The ",
              "type": "text"
            },
            {
              "code": "stepRate",
              "type": "codeVoice"
            },
            {
              "text": " value, in conjunction with the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.metal/documentation/Metal/MTLVertexBufferLayoutDescriptor/stepFunction",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property, determines how often the function fetches new attribute data. The ",
              "type": "text"
            },
            {
              "code": "stepRate",
              "type": "codeVoice"
            },
            {
              "text": " property is generally used when ",
              "type": "text"
            },
            {
              "code": "stepFunction",
              "type": "codeVoice"
            },
            {
              "text": " is ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.metal/documentation/Metal/MTLVertexStepFunction/perInstance",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". If ",
              "type": "text"
            },
            {
              "code": "stepRate",
              "type": "codeVoice"
            },
            {
              "text": " is equal to ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ", new attribute data is fetched for every instance; if ",
              "type": "text"
            },
            {
              "code": "stepRate",
              "type": "codeVoice"
            },
            {
              "text": " is equal to ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": ", new attribute data is fetched for every two instances, and so forth.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
