      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "usesTabsForIndentation",
              "type": "codeVoice"
            },
            {
              "text": " property determines whether tab characters are used to indent text instead of space characters when possible. When the indentation width isnâ€™t a multiple of the tab width, space characters are used to pad the indentation to the appropriate width.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, consider an ",
              "type": "text"
            },
            {
              "code": "XCSourceTextBuffer",
              "type": "codeVoice"
            },
            {
              "text": " instance that has a tab width of eight, an indentation width of four, and the ",
              "type": "text"
            },
            {
              "code": "usesTabsForIndentation",
              "type": "codeVoice"
            },
            {
              "text": " property set to ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ". The first indentation level is represented by four space characters, the second by a tab character, the third by a tab followed by four space characters, the fourth by two tab characters, and so on.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "media-2902679",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
