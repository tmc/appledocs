          "content": [
            {
              "inlineContent": [
                {
                  "text": "The network address of the desired host.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "address"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length, in bytes, of the address.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "addrlen"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to memory that, on output, will be filled with a set of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.systemconfiguration/documentation/SystemConfiguration/SCNetworkConnectionFlags",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " values detailing the reachability of the specified address",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "flags"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "",
              "type": "text"
            },
            {
              "code": "TRUE",
              "type": "codeVoice"
            },
            {
              "text": " if the network connection flags are valid; ",
              "type": "text"
            },
            {
              "code": "FALSE",
              "type": "codeVoice"
            },
            {
              "text": " if the status could not be determined.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function is deprecated, but you can get equivalent results using the following code:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "SCNetworkReachabilityRef target;",
            "SCNetworkConnectionFlags flags = 0;",
            "Boolean ok;",
            "target = SCNetworkReachabilityCreateWithAddress(NULL, address);",
            "ok = SCNetworkReachabilityGetFlags(target, &flags);",
            "CFRelease(target);"
          ],
          "syntax": "objc",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
