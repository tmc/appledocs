{
  "abstract": [
    {
      "text": "Configure your CallKit or LiveCommunicationKit app so people can set it as the default calling app on their device.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.callkit/documentation/CallKit"
      ]
    ]
  },
