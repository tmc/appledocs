      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A content type of the returned data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contentType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether the file should be opened in place, if possible.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allowOpenInPlace"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that accepts the file URL as a parameter.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The file is written to a temporary destination and removed",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "right after the closure returns.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fileHandler"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the file is not opened in place, the system removes it after the fileHandler closure returns.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let image = try await intentFile.withFile(contentType: .png) { url, _ in",
            "    let data = try Data(contentsOf: url)",
            "    return CGImage(pngData: data)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
