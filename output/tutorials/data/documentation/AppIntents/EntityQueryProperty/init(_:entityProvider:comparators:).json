              "inlineContent": [
                {
                  "text": "The keypath to the property that this EntityQueryProperty applies to. The target",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "property type determines which comparator modifiers will be available.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "keyPath"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Closure which, given a ",
                  "type": "text"
                },
                {
                  "code": "Entity",
                  "type": "codeVoice"
                },
                {
                  "text": " instance, returns the appropriate",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "Subject",
                  "type": "codeVoice"
                },
                {
                  "text": " instance to apply ",
                  "type": "text"
                },
                {
                  "code": "EntityQueryComparator",
                  "type": "codeVoice"
                },
                {
                  "text": "s to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "entityProvider"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The set of ",
                  "type": "text"
                },
                {
                  "code": "EntityQueryComparators",
                  "type": "codeVoice"
                },
                {
                  "text": " that this property supports being queried by.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
