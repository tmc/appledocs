    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The value used should be unique across all entities of the given type. Entities which are",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "relevant across executions of the application should have stable identifiers that persist",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "across executions.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Entities, by default, conform to the ",
              "type": "text"
            },
            {
              "code": "Identifiable",
              "type": "codeVoice"
            },
            {
              "text": " protocol. Use a type for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the ",
              "type": "text"
            },
            {
              "code": "id",
              "type": "codeVoice"
            },
            {
              "text": " that conforms to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.AppIntents/documentation/AppIntents/EntityIdentifierConvertible",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". Default implementations for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "String",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "UUID",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "Int",
              "type": "codeVoice"
            },
            {
              "text": " are provided.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Song: AppEntity {",
            "    let id = UUID()",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
