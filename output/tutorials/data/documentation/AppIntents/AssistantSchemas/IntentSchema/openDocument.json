    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To integrate your app’s functionality with Siri and Apple Intelligence,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "you use Swift macros that generate additional properties and add protocol conformance",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for your app intent implementation.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For more information about the ",
              "type": "text"
            },
            {
              "code": ".reader",
              "type": "codeVoice"
            },
            {
              "text": " app intent domain, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.AppIntents/documentation/AppIntents/Making-document-reader-actions-available-to-siri-and-apple-intelligence",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For general information about app intent domains, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.AppIntents/documentation/AppIntents/Integrating-actions-with-siri-and-apple-intelligence",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows an app intent that conforms to the ",
              "type": "text"
            },
            {
              "code": ".reader.openDocument",
              "type": "codeVoice"
            },
            {
              "text": " schema:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@AssistantIntent(schema: .reader.openDocument)",
            "struct ReaderOpenDocumentsIntent: AppIntent {",
            "    @Parameter",
            "    var files: [IntentFile]",
            "",
            "    func perform() async throws -> some IntentResult {",
            "        return .result()",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
