      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The requested behavior.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "behavior"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "By default, when a person swipes up on a scroll view in a resizable",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "presentation, the presentation grows to the next detent. A scroll view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "embedded in the presentation only scrolls after the presentation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "reaches its largest size. Use this modifier to control which action",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "takes precedence.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, you can request that swipe gestures scroll content first,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "resizing the sheet only after hitting the end of the scroll view, by",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "passing the ",
              "type": "text"
            },
            {
              "code": "PresentationContentInteraction/scrolls",
              "type": "codeVoice"
            },
            {
              "text": " value to this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    @State private var showSettings = false",
            "",
            "    var body: some View {",
            "        Button(\"View Settings\") {",
            "            showSettings = true",
            "        }",
            "        .sheet(isPresented: $showSettings) {",
            "            SettingsView()",
            "                .presentationDetents([.medium, .large])",
            "                .presentationContentInteraction(.scrolls)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "People can always resize your presentation using the drag indicator.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
