    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/Swift/Hashable",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "value to use as the view’s tag.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tag"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If the tag value for ",
                  "type": "text"
                },
                {
                  "code": "Optional<V>",
                  "type": "codeVoice"
                },
                {
                  "text": " should",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "also be set.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "includeOptional"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view with the specified tag set.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to differentiate among certain selectable views,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "like the possible values of a ",
              "type": "text"
            },
            {
              "code": "Picker",
              "type": "codeVoice"
            },
            {
              "text": " or the tabs of a ",
              "type": "text"
            },
            {
              "code": "TabView",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Tag values can be of any type that conforms to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Swift/Hashable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " protocol.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier will write the tag value for the type ",
              "type": "text"
            },
            {
              "code": "V",
              "type": "codeVoice"
            },
            {
              "text": ", as well as",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Optional<V>",
              "type": "codeVoice"
            },
            {
              "text": " if ",
              "type": "text"
            },
            {
              "code": "includeOptional",
              "type": "codeVoice"
            },
            {
              "text": " is enabled. Containers checking for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "tags of either type will see the value as set.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, the ",
              "type": "text"
            },
            {
              "code": "ForEach",
              "type": "codeVoice"
            },
            {
              "text": " loop in the ",
              "type": "text"
            },
            {
              "code": "Picker",
              "type": "codeVoice"
            },
            {
              "text": " view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "builder iterates over the ",
              "type": "text"
            },
            {
              "code": "Flavor",
              "type": "codeVoice"
            },
            {
              "text": " enumeration. It extracts the string",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value of each enumeration element for use in constructing the row",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "label, and uses the enumeration value as input to the ",
              "type": "text"
            },
            {
              "code": "tag(_:)",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct FlavorPicker: View {",
            "    enum Flavor: String, CaseIterable, Identifiable {",
            "        case chocolate, vanilla, strawberry",
            "        var id: Self { self }",
            "    }",
            "",
            "    @State private var selectedFlavor: Flavor? = nil",
            "",
            "    var body: some View {",
            "        Picker(\"Flavor\", selection: $selectedFlavor) {",
            "            ForEach(Flavor.allCases) { flavor in",
            "                Text(flavor.rawValue)",
            "                    .tag(flavor)",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The selection type of the ",
              "type": "text"
            },
            {
              "code": "Picker",
              "type": "codeVoice"
            },
            {
              "text": " is an ",
              "type": "text"
            },
            {
              "code": "Optional<Flavor>",
              "type": "codeVoice"
            },
            {
              "text": " and so it",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "will look for tags on its contents of ",
              "type": "text"
            },
            {
              "code": "Optional<Flavor>",
              "type": "codeVoice"
            },
            {
              "text": "type. Since the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "tag modifier defaults to having ",
              "type": "text"
            },
            {
              "code": "includeOptional",
              "type": "codeVoice"
            },
            {
              "text": " enabled, even though",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the tag for each option is a non-optional ",
              "type": "text"
            },
            {
              "code": "Flavor",
              "type": "codeVoice"
            },
            {
              "text": ", the tag modifier",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "writes values for both the non-optional, and optional versions of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value, allowing the contents to be selectable by the ",
              "type": "text"
            },
            {
              "code": "Picker",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "ForEach",
              "type": "codeVoice"
            },
            {
              "text": " automatically applies a default tag to each enumerated",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view using the ",
              "type": "text"
            },
            {
              "code": "id",
              "type": "codeVoice"
            },
            {
              "text": " parameter of the corresponding element. If",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the element’s ",
              "type": "text"
            },
            {
              "code": "id",
              "type": "codeVoice"
            },
            {
              "text": " parameter and the picker’s ",
              "type": "text"
            },
            {
              "code": "selection",
              "type": "codeVoice"
            },
            {
              "text": " input",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "have exactly the same type, or the same type but optional, you can omit",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the explicit tag modifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To see examples that don’t require an explicit tag, see ",
              "type": "text"
            },
            {
              "code": "Picker",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
