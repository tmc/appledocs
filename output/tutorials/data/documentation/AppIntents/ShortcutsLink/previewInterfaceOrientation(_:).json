    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "By default, device previews appear right side up, using orientation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "InterfaceOrientation/portrait",
              "type": "codeVoice"
            },
            {
              "text": ". You can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "change the orientation of a preview using one of the values in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the ",
              "type": "text"
            },
            {
              "code": "InterfaceOrientation",
              "type": "codeVoice"
            },
            {
              "text": " structure:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct CircleImage_Previews: PreviewProvider {",
            "    static var previews: some View {",
            "        CircleImage()",
            "            .previewInterfaceOrientation(.landscapeRight)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
