            {
              "inlineContent": [
                {
                  "text": "A Boolean value that determines whether users can",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "select this view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isDisabled"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to control the selectability of views in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "selectable containers like ",
              "type": "text"
            },
            {
              "code": "List",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "Table",
              "type": "codeVoice"
            },
            {
              "text": ". In the example,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "below, the user can’t select the first item in the list.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@Binding var selection: Item.ID?",
            "@Binding var items: [Item]",
            "",
            "var body: some View {",
            "    List(selection: $selection) {",
            "        ForEach(items) { item in",
            "            ItemView(item: item)",
            "                .selectionDisabled(item.id == items.first?.id)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can also use this modifier to specify the selectability of views",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "within a ",
              "type": "text"
            },
            {
              "code": "Picker",
              "type": "codeVoice"
            },
            {
              "text": ". The following example represents a flavor picker",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that disables selection on flavors that are unavailable.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Picker(\"Flavor\", selection: $selectedFlavor) {",
            "    ForEach(Flavor.allCases) { flavor in",
            "        Text(flavor.rawValue.capitalized)",
            "            .selectionDisabled(isSoldOut(flavor))",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
