    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The string to use as a type select equivalent for a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "view in a collection.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "string"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "By default, a type select equivalent is automatically derived from any",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Text",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "TextField",
              "type": "codeVoice"
            },
            {
              "text": " content in a list or table. In the below example,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "type select can be used to select a person, even though no explicit",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value has been set.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "List(people, selection: $selectedPersonID) { person in",
            "    Label {",
            "        Text(person.name)",
            "    } icon: {",
            "        person.avatar",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "An explicit type select value should be set when there is no textual",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "content or when a different value is desired compared to what’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "displayed in the view. Explicit values also provide a more performant",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for complex view types. In the below example, type select is explicitly",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "set to allow selection of views that otherwise only display an image.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "List(people, selection: $selectedPersonID) { person in",
            "    person.avatar",
            "        .accessibilityLabel(person.name)",
            "        .typeSelectEquivalent(person.name)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Setting an empty string value disables text selection for the view,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and a value of ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " results in the view using its default value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
