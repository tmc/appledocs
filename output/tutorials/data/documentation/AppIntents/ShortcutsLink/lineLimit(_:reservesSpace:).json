          "content": [
            {
              "inlineContent": [
                {
                  "text": "The line limit.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "limit"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether text reserves space so that",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "it always occupies the height required to display the specified",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "number of lines.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reservesSpace"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to specify a limit to the lines that a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Text",
              "type": "codeVoice"
            },
            {
              "text": " or a vertical ",
              "type": "text"
            },
            {
              "code": "TextField",
              "type": "codeVoice"
            },
            {
              "text": " may occupy. If passed a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value of true for the ",
              "type": "text"
            },
            {
              "code": "reservesSpace",
              "type": "codeVoice"
            },
            {
              "text": " parameter, and the text of such",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "views occupies less space than the provided limit, that view expands",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to occupy the minimum number of lines. When the text occupies",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "more space than the provided limit, a ",
              "type": "text"
            },
            {
              "code": "Text",
              "type": "codeVoice"
            },
            {
              "text": " view truncates its",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "content while a ",
              "type": "text"
            },
            {
              "code": "TextField",
              "type": "codeVoice"
            },
            {
              "text": " becomes scrollable.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "GroupBox {",
            "    Text(\"Title\")",
            "        .font(.headline)",
            "        .lineLimit(2, reservesSpace: true)",
            "    Text(\"Subtitle\")",
            "        .font(.subheadline)",
            "        .lineLimit(4, reservesSpace: true)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
