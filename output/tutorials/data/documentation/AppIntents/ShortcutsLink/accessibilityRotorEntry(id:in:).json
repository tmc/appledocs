          "content": [
            {
              "inlineContent": [
                {
                  "text": "An arbitrary hashable identifier. Pass this same value",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "when initializing an AccessibilityRotorEntry.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A namespace created with ",
                  "type": "text"
                },
                {
                  "code": "@Namespace()",
                  "type": "codeVoice"
                },
                {
                  "text": ". Pass this",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "same namespace when initializing an ",
                  "type": "text"
                },
                {
                  "code": "AccessibilityRotorEntry",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "namespace"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this when creating an AccessibilityRotorEntry without a namespace",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "does not allow SwiftUI to automatically find and reveal the element,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or when the Rotor entry should be associated with a sub-element of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a complex view generated in a ForEach, for example.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
