    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Localized text describing to the user what",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "is contained in this additional information entry. For example:",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "“orientation”.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "labelKey"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Text value for the additional accessibility",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "information. For example: “landscape.”",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Importance of the accessibility information.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "High-importance information gets read out immediately, while",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "default-importance information must be explicitly asked for by the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "importance"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to add information you want accessibility users to be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "able to access about this element, beyond the basics of label, value,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and hint. For example, ",
              "type": "text"
            },
            {
              "code": "accessibilityCustomContent",
              "type": "codeVoice"
            },
            {
              "text": " can be used to add",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "information about the orientation of a photograph, or the number of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "people found in the picture.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Repeated calls of ",
                  "type": "text"
                },
                {
                  "code": "accessibilityCustomContent",
                  "type": "codeVoice"
                },
                {
                  "text": " with different",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "labels will create new entries of additional information. Calling",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "accessibilityAdditionalContent",
                  "type": "codeVoice"
                },
                {
                  "text": " repeatedly with the same label will",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "instead replace the previous value and importance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
