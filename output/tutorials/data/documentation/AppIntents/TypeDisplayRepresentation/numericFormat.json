    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In your ",
              "type": "text"
            },
            {
              "code": "numericFormat",
              "type": "codeVoice"
            },
            {
              "text": " implementation, use ",
              "type": "text"
            },
            {
              "code": "StringInterpolation",
              "type": "codeVoice"
            },
            {
              "text": " placeholders and provide a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": ".stringsdict",
              "type": "codeVoice"
            },
            {
              "text": " file with all possible pluralizations for the type, such as “0 books”, “1 book”,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and “2 books”.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TypeDisplayRepresentation(",
            "    name: LocalizedStringResource(\"Book\"),",
            "    numericFormat: LocalizedStringResource(\"\\(placeholder: .int) books\")",
            ")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "code": [
            "<dict>",
            "   <key>Book</key>",
            "   <dict>",
            "     <key>NSStringLocalizedFormatKey</key>",
            "     <string>%#@VARIABLE@</string>",
            "     <key>VARIABLE</key>",
            "     <dict>",
            "       <key>NSStringFormatSpecTypeKey</key>",
            "       <string>NSStringPluralRuleType</string>",
            "       <key>one</key>",
            "       <string>Book</string>",
            "       <key>other</key>",
            "       <string>Books</string>",
            "     </dict>",
            "   </dict>",
            "   <key>%lld books</key>",
            "   <dict>",
            "     <key>NSStringLocalizedFormatKey</key>",
            "     <string>%#@count@</string>",
            "     <key>count</key>",
            "     <dict>",
            "       <key>NSStringFormatSpecTypeKey</key>",
            "       <string>NSStringPluralRuleType</string>",
            "       <key>NSStringFormatValueTypeKey</key>",
            "       <string>lld</string>",
            "       <key>zero</key>",
            "       <string>%lld books</string>",
            "       <key>one</key>",
            "       <string>%lld book</string>",
            "       <key>other</key>",
            "       <string>%lld books</string>",
            "     </dict>",
            "   </dict>",
            " </dict>"
          ],
