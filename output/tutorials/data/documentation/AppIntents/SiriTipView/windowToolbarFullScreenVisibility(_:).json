          "content": [
            {
              "inlineContent": [
                {
                  "text": "The visibility to use for the window toolbar in",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "full screen mode.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "visibility"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "By default, the window toolbar will show at the top of the display,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "above the window’s contents.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this modifier to override the default behavior.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, you can specify that the window toolbar should be hidden",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by default, and only show once the mouse moves into the area occupied",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by the menu bar:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct RootView: View {",
            "    var body: some View {",
            "        ContentView()",
            "            .toolbar {",
            "                ...",
            "            }",
            "            .windowToolbarFullScreenVisibility(.onHover)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
