          "content": [
            {
              "inlineContent": [
                {
                  "text": "The heading level to associate with this element",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "from the available ",
                  "type": "text"
                },
                {
                  "code": "AccessibilityHeadingLevel",
                  "type": "codeVoice"
                },
                {
                  "text": " levels.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "level"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to set the level of this heading in relation to other headings. The system speaks",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the level number of levels ",
              "type": "text"
            },
            {
              "code": "AccessibilityHeadingLevel/h1",
              "type": "codeVoice"
            },
            {
              "text": " through",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "AccessibilityHeadingLevel/h6",
              "type": "codeVoice"
            },
            {
              "text": " alongside the text.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The default heading level if you don’t use this modifier",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is ",
              "type": "text"
            },
            {
              "code": "AccessibilityHeadingLevel/unspecified",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
