    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding to a value that updates when the user rotates",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the  Digital Crown.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "binding"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Lower end of the range reported.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "minValue"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Upper end of the range reported.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxValue"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value settles on multiples of ",
                  "type": "text"
                },
                {
                  "code": "stride",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stride"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "How much the user needs to rotate the  Digital Crown",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to move between two integer numbers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sensitivity"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Controls if the value reported stops at ",
                  "type": "text"
                },
                {
                  "code": "minValue",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "and ",
                  "type": "text"
                },
                {
                  "code": "maxValue",
                  "type": "codeVoice"
                },
                {
                  "text": ", or if it should wrap around. Default is ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isContinuous"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Controls the generation of haptic feedback",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "when turning the Digital Crown. Default is ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isHapticFeedbackEnabled"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A block that is called as the Digital Crown is rotated.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onChange"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A block that is called when the Digital Crown has settled to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "an idle state.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onIdle"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to receive values on a binding you provides as",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the user turns the Digital Crown on Apple Watch. The example below",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "receives changes to the binding value, starting at the ",
              "type": "text"
            },
            {
              "code": "minValue",
              "type": "codeVoice"
            },
            {
              "text": " of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " up to the ",
              "type": "text"
            },
            {
              "code": "maxValue",
              "type": "codeVoice"
            },
            {
              "text": " of ",
              "type": "text"
            },
            {
              "code": "100",
              "type": "codeVoice"
            },
            {
              "text": " in steps of ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": " incrementing or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "decrementing depending on the direction that the user turns the Digital",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Crown, rolling over if the user exceeds the specified boundary values.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The binding will always be updated to a value that is a multiple of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "stride that is provided:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct DigitalCrown: View {",
            "    @State private var crownValue = 0.0",
            "    @State private var selected = 0",
            "    @State private var minValue = 0",
            "    @State private var maxValue = 100",
            "    @State private var stepAmount = 1",
            "    @State private var velocity = 0.0",
            "    @State private var isIdle = true",
            "",
            "    var body: some View {",
            "        Text(\"Received Value:\\(crownValue, specifier: \"%.2f\")\")",
            "            .focusable()",
            "            .digitalCrownRotation(detent: $selected,",
            "                                  from: minValue,",
            "                                  through: maxValue,",
            "                                  by: stepAmount,",
            "                                  sensitivity: .low,",
            "                                  isContinuous: true",
            "            ) { crownEvent in",
            "                isIdle = false",
            "                crownValue = crownEvent.offset",
            "                velocity = crownEvent.velocity",
            "            } onIdle: {",
            "                isIdle = true",
            "            }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
