    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "By default, search fields on visionOS will automatically start",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "dictation when looking at the dictation button in the search field.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "You can change this behavior by providing a value of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "TextInputDictationBehavior/preventDictation",
              "type": "codeVoice"
            },
            {
              "text": " to this modifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "See the ",
              "type": "text"
            },
            {
              "code": "TextInputDictationBehavior",
              "type": "codeVoice"
            },
            {
              "text": " type for more information",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "on the available dictation behaviors.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
