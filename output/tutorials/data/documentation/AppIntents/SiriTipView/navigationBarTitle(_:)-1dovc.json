      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A key to a localized description of this view to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "display in the navigation bar.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "titleKey"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "navigationBarTitle(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to set the title of the navigation bar",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using a ",
              "type": "text"
            },
            {
              "code": "LocalizedStringKey",
              "type": "codeVoice"
            },
            {
              "text": " that will be used to search for a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "matching localized string in the application’s localizable strings",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "assets.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier only takes effect when this view is inside of and visible",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "within a ",
              "type": "text"
            },
            {
              "code": "NavigationView",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, a string constant is used to access a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "LocalizedStringKey",
              "type": "codeVoice"
            },
            {
              "text": " that will be resolved at run time to provide a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "title for the navigation bar. If the localization key cannot be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "resolved, the text of the key name will be used as the title text.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct FlavorView: View {",
            "    let items = [\"Chocolate\", \"Vanilla\", \"Strawberry\", \"Mint Chip\",",
            "                 \"Pistachio\"]",
            "    var body: some View {",
            "        NavigationView {",
            "            List(items, id: \\.self) {",
            "                Text($0)",
            "            }",
            "            .navigationBarTitle(\"Today's Flavors\")",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
