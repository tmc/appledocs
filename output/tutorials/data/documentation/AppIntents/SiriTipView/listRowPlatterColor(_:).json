        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "View/listRowPlatterColor(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to set the underlying row",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "background color in a list.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, the ",
              "type": "text"
            },
            {
              "code": "Flavor",
              "type": "codeVoice"
            },
            {
              "text": " enumeration provides content for list",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "items. The SwiftUI ",
              "type": "text"
            },
            {
              "code": "List",
              "type": "codeVoice"
            },
            {
              "text": " builder iterates over the ",
              "type": "text"
            },
            {
              "code": "Flavor",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "enumeration and extracts the raw value of each of its elements using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "resulting text to create each list row item. After the list builder",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "finishes, the ",
              "type": "text"
            },
            {
              "code": "listRowPlatterColor(_:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier sets the underlying row",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "background color to the ",
              "type": "text"
            },
            {
              "code": "Color",
              "type": "codeVoice"
            },
            {
              "text": " you specify.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    enum Flavor: String, CaseIterable, Identifiable {",
            "        var id: String { self.rawValue }",
            "        case vanilla, chocolate, strawberry",
            "    }",
            "",
            "    var body: some View {",
            "        List {",
            "            ForEach(Flavor.allCases) {",
            "                Text($0.rawValue)",
            "                    .listRowPlatterColor(.green)",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
