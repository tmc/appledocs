    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The quick action will automatically become active when the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view appears. If the view is disabled, the action will defer",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "becoming active until the view is no longer disabled.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows how to add a quick action to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "pause and resume a workout, with the ",
              "type": "text"
            },
            {
              "code": "AccessibilityQuickActionStyle/prompt",
              "type": "codeVoice"
            },
            {
              "text": " style.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@State private var isPaused = false",
            "",
            "var body: some View {",
            "    WorkoutView(isPaused: $isPaused)",
            "        .accessibilityQuickAction(style: .prompt) {",
            "            Button(isPaused ? \"Resume\" : \"Pause\") {",
            "                isPaused.toggle()",
            "            }",
            "        }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows how to add a quick action to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "play and pause music, with the ",
              "type": "text"
            },
            {
              "code": "AccessibilityQuickActionStyle/outline",
              "type": "codeVoice"
            },
            {
              "text": " style.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@State private var isPlaying = false",
            "",
            "var body: some View {",
            "    PlayButton(isPlaying: $isPlaying)",
            "        .contentShape(.focusEffect, Circle())",
            "        .accessibilityQuickAction(style: .outline) {",
            "            Button(isPlaying ? \"Pause\" : \"Play\") {",
            "                isPlaying.toggle()",
            "            }",
            "        }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
