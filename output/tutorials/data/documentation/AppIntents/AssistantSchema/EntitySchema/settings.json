    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To integrate your app’s functionality with Siri and Apple Intelligence,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "you use Swift macros that generate additional properties and add protocol conformance",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for your app entity implementation.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For more information about the ",
              "type": "text"
            },
            {
              "code": ".books",
              "type": "codeVoice"
            },
            {
              "text": " app intent domain, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.AppIntents/documentation/AppIntents/Making-ebook-actions-available-to-siri-and-apple-intelligence",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For general information about app intent domains, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.AppIntents/documentation/AppIntents/Integrating-actions-with-siri-and-apple-intelligence",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows an app entity that conforms to the ",
              "type": "text"
            },
            {
              "code": ".books.settings",
              "type": "codeVoice"
            },
            {
              "text": " schema:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@AssistantEntity(schema: .books.settings)",
            "struct BookSettingsEntity: AppEntity {",
            "    struct Query: EntityStringQuery {",
            "        func entities(for identifiers: [BookSettingsEntity.ID]) async throws -> [BookSettingsEntity] { [] }",
            "        func entities(matching string: String) async throws -> [BookSettingsEntity] { [] }",
            "    }",
            "",
            "    static var defaultQuery = Query()",
            "    var displayRepresentation: DisplayRepresentation { \"Book Settings\" }",
            "",
            "    let id = UUID()",
            "",
            "    @Property",
            "    var font: BookFont",
            "",
            "    @Property",
            "    var fontSize: BookFontSize",
            "",
            "    @Property",
            "    var theme: BookTheme",
            "",
            "    @Property",
            "    var pageNavigationSetting: BookPageNavigationSetting",
            "",
            "    @Property",
            "    var isTextJustified: Bool",
            "",
            "    @Property",
            "    var isAllowMultipleColumns: Bool}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
