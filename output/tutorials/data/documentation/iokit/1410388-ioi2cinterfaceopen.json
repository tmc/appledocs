          "content": [
            {
              "inlineContent": [
                {
                  "text": "An I2C bus interface (see IOFBCopyI2CInterfaceForBus). The interface may be released after this call is made.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "interface"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Pass kNilOptions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The opaque IOI2CConnectRef is returned, for use with IOI2CSendRequest() and IOI2CInterfaceClose().",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "connect"
