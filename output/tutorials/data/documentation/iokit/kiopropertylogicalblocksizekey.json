      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This key is used to define the logical block size of a hard disk drive.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Requirement: Mandatory for hard disk drives with logical block size other than 512 bytes or that does not match its physical block size.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " ",
            " ",
            "<dict>",
            "   <key>Device Characteristics</key>",
            "   <dict>",
            "   \t<key>Vendor Name</key>",
            "   \t<string>Apple</string>",
            "   \t<key>Product Name</key>",
            "   \t<string>iPod</string>",
            "   \t<key>Product Revision Level</key>",
            "   \t<string>1.0</string>",
            "   \t<key>Physical Block Size</key>",
            "   \t<integer>4096</integer>",
            "   \t<key>Logical Block Size</key>",
            "   \t<integer>512</integer>",
            "   </dict>",
            "</dict>"
          ],
