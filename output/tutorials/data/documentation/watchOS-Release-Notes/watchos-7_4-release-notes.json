  "metadata": {
    "role": "article",
    "roleHeading": "Article",
    "title": "watchOS 7.4 Release Notes"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The watchOS 7.4 SDK provides support to develop watchOS apps for Apple Watch devices running watchOS 7.4. The SDK comes bundled with Xcode 12.5, available from the Mac App Store. For information on the compatibility requirements for Xcode 12.5, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Xcode-Release-Notes/xcode-12_5-release-notes",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Accessibility",
          "level": 3,
          "text": "Accessibility",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Many SF Symbols now have default accessibility labels. (70305995)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Combine",
          "level": 3,
          "text": "Combine",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Using ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Combine/Published",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " in a subclass of a type conforming to ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Combine/ObservableObject",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " now correctly publishes changes. (71816443)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "SwiftUI",
          "level": 3,
          "text": "SwiftUI",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/TitleAndIconLabelStyle",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", a new style for ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Label",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " views that shows both the title and icon of the label using a system-standard layout. In most cases, labels show both title and icon by default. However, some containers might apply a different default label style to their content, such as only showing icons within toolbars on macOS and iOS. To opt in to showing both the title and the icon, apply the title and icon label style: ",
                      "type": "text"
                    },
                    {
                      "code": "Label(\"Lightning\", systemImage: \"bolt.fill\").labelStyle(TitleAndIconLabelStyle())",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". (64646578)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Types conforming to any style protocol, such as ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/ButtonStyle",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " or ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/ToggleStyle",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", are now enforced to be value types. Styles must be structures or enumerations, not classes, and conforming a class to a style protocol may trigger an assertion. This is the same restriction that the system has always enforced on types conforming to ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/View",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ". (62886135)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Dynamic properties such as ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/State",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Environment",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", and others now work correctly in ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/ButtonStyle",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " instances. (62886135)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/ProgressView",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " instances initialized with a ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/progress",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " object now correctly track updates to the ",
                      "type": "text"
                    },
                    {
                      "code": "Progress",
                      "type": "codeVoice"
                    },
                    {
                      "text": " object from background threads, and no longer issue a “not allowed” console warning. (69999449)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/InlinePickerStyle",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " now resolves as an in-line section if applied to a ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Picker",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " within a ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/List",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " on iOS, watchOS, and tvOS, using a checkmark to indicate the selected option. (71383311)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/AppStorage",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " property wrappers now work as expected when contained inside an ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Combine/ObservableObject",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", causing the system to emit the ",
                      "type": "text"
                    },
                    {
                      "text": "doc://com.apple.documentation/documentation/combine/observableobject/objectwillchange-2oa5v",
                      "type": "text"
                    },
                    {
                      "text": " publisher. (65562845)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Using ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/ScrollViewProxy/scrollTo(_:anchor:)",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " without specifying an anchor now scrolls the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/List",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " the minimum amount to make it visible. (70184639)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/TabView",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/PageTabViewStyle",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " now correctly invokes ",
                      "type": "text"
                    },
                    {
                      "text": "doc://com.apple.documentation/documentation/swiftui/anyview/onappear(perform:)",
                      "type": "text"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "text": "doc://com.apple.documentation/documentation/swiftui/anyview/ondisappear(perform:)",
                      "type": "text"
                    },
                    {
                      "text": " for its tabs. (71225006)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
