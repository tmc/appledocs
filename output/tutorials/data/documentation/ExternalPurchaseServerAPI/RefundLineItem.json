          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier for the transaction, that you determine. The value must be unique per app. Using UUIDs is recommended. Reuse a ",
                  "type": "text"
                },
                {
                  "code": "lineItemId",
                  "type": "codeVoice"
                },
                {
                  "text": " only to submit a correction for a previously submitted line item.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineItemId",
          "required": true,
          "type": [
            {
              "identifier": "doc://org.swift.docc.ExternalPurchaseServerAPI/documentation/ExternalPurchaseServerAPI/lineItemId",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:external_purchase_server_api:lineItemId",
              "text": "lineItemId"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://org.swift.docc.ExternalPurchaseServerAPI/documentation/ExternalPurchaseServerAPI/lineItemId",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " of the initial purchase transaction that receives a refund.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "referenceLineItemId",
          "required": true,
          "type": [
            {
