        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "eventType",
              "type": "codeVoice"
            },
            {
              "text": " field describes the type of transaction of the line item. Specify an ",
              "type": "text"
            },
            {
              "code": "eventType",
              "type": "codeVoice"
