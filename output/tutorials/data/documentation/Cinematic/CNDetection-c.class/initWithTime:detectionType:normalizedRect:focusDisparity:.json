      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The presentation time of the frame that the detection occurred.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "time"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of object detected, such as the face, torso, cat, dog, and so on.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "detectionType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The rectangle within the image where the object occurs, normalized such that (0.0, 0.0) is the top-left and (1.0, 1.0) is the bottom-right.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "normalizedRect"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The disparity to use in order to focus on the object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "focusDisparity"
        }
      ]
