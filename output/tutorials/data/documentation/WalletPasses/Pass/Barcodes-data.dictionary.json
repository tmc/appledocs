    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The text to display near the barcode. For example, a human-readable version of the barcode data in case the barcode doesn’t scan.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The alternative text isn’t displayed for watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "altText",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "PKBarcodeFormatQR",
                "PKBarcodeFormatPDF417",
                "PKBarcodeFormatAztec",
                "PKBarcodeFormatCode128"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The format of the barcode.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The barcode format PKBarcodeFormatCode128 isn’t supported for watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "format",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The message or payload to display as a barcode.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "message",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The IANA character set name of the text encoding to use to convert ",
                  "type": "text"
                },
                {
                  "code": "message",
                  "type": "codeVoice"
                },
                {
                  "text": " from a string representation to a data representation that the system renders as a barcode, such as ",
                  "type": "text"
                },
                {
                  "code": "“iso-8859-1”",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "messageEncoding",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
