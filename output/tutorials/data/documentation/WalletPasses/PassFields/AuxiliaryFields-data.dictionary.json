          "content": [
            {
              "inlineContent": [
                {
                  "text": "A number you use to add a row to the auxiliary field in an event ticket pass type. Set the value to ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": " to add an auxiliary row. Each row displays up to four fields.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "row",
          "required": false,
