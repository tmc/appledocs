      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value of the field, including HTML markup for links. The only supported tag is the ",
                  "type": "text"
                },
                {
                  "code": "<a>",
                  "type": "codeVoice"
                },
                {
                  "text": " tag and its ",
                  "type": "text"
                },
                {
                  "code": "href",
                  "type": "codeVoice"
                },
                {
                  "text": " attribute. The value of this key overrides that of the ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": " key.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "For example, the following is a key-value pair that specifies a link with the text “Edit my profile”:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "”attributedValue”: “<a href=’http://example.com/customers/123’>Edit my profile</a>”",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The attributed value isn’t used for watchOS; use the ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": " field instead.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "attributedValue",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "localizable string, ISO 8601 date, or number"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A format string for the alert text to display when the pass updates. The format string needs to contain the escape ",
                  "type": "text"
                },
                {
                  "code": "%@",
                  "type": "codeVoice"
                },
                {
                  "text": ", which the field’s new value replaces. For example, ",
                  "type": "text"
                },
                {
                  "code": "Gate changed to %@",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You need to provide a value for the system to show a change notification.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field isn’t used for watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "changeMessage",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "Localizable format string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The currency code to use for the value of the field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "currencyCode",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "ISO 4217 currency code as a string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "PKDataDetectorTypePhoneNumber",
                "PKDataDetectorTypeLink",
                "PKDataDetectorTypeAddress",
                "PKDataDetectorTypeCalendarEvent"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The data detectors to apply to the value of a field on the back of the pass. The default is to apply all data detectors. To use no data detectors, specify an empty array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You don’t use data detectors for fields on the front of the pass.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field isn’t used for watchOS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dataDetectorTypes",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "PKDateStyleNone",
                "PKDateStyleShort",
                "PKDateStyleMedium",
                "PKDateStyleLong",
                "PKDateStyleFull"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style of the date to display in the field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dateStyle",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that controls the time zone for the time and date to display in the field. The default value is ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", which displays the time and date using the current device’s time zone. Otherwise, the time and date appear in the time zone associated with the date and time of ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This key doesn’t affect the pass relevance calculation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ignoresTimeZone",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that controls whether the date appears as a relative date. The default value is ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", which displays the date as an absolute date.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This key doesn’t affect  the pass relevance calculation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isRelative",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique key that identifies a field in the pass; for example, ",
                  "type": "text"
                },
                {
                  "code": "“departure-gate”",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "key",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The text for a field label.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "label",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "localizable string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "PKNumberStyleDecimal",
                "PKNumberStylePercent",
                "PKNumberStyleScientific",
                "PKNumberStyleSpellOut"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style of the number to display in the field. Formatter styles have the same meaning as the formats with corresponding names in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/numberformatter/style",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "numberStyle",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "PKTextAlignmentLeft",
                "PKTextAlignmentCenter",
                "PKTextAlignmentRight",
                "PKTextAlignmentNatural"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The alignment for the content of a field. The default is natural alignment, which aligns the text based on its script direction. This key is invalid for primary and back fields.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "textAlignment",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "PKDateStyleNone",
                "PKDateStyleShort",
                "PKDateStyleMedium",
                "PKDateStyleLong",
                "PKDateStyleFull"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style of the time displayed in the field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timeStyle",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value to use for the field; for example, ",
                  "type": "text"
                },
                {
                  "code": "42",
                  "type": "codeVoice"
                },
                {
                  "text": ". A date or time value needs to include a time zone.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "localizable string, ISO 8601 date, or number"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
