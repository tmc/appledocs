      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user-entered email address for the user of the personalized pass.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "emailAddress",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The family name for the user of the personalized pass, parsed from the full name. The name can indicate membership in a group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "familyName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user-entered full name for the user of the personalized pass.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fullName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The given name for the user of the personalized pass, parsed from the full name. The system uses the name to differentiate the individual from other members who share the same family name. In some locales, the given name is also known as a ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "forename",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "first name",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "givenName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ISO country code. The system sets this key when it’s known; otherwise, it doesn’t include the key.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ISOCountryCode",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user-entered phone number for the user of the personalized pass.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "phoneNumber",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user-entered postal code for the user of the personalized pass.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "postalCode",
          "required": false,
