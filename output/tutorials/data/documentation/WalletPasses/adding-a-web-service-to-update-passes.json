{
  "abstract": [
    {
      "text": "Implement a web server to register, update, and unregister a pass on a device.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.walletpasses/documentation/WalletPasses"
      ]
    ]
