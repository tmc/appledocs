    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "DataFrame",
                  "type": "codeVoice"
                },
                {
                  "text": " containing keypoints and labels.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dataFrame"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the column containing session identifiers. Defaults to “session_id”.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sessionIdColumn"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the column containing the labels. Defaults to “label”.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "labelColumn"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the column containing the keypoints. Defaults to “keypoints”.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "featureColumn"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The data frame must contain a column of session identifiers, a column of labels, and a column of keypoints.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Each set of keypoints must be a multi-dimensional 1x3x18 array containing the x, y, z coordinates of each",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of the 18 keypoints. See ",
              "type": "text"
            },
            {
              "code": "VNRecognizedPointsObservation",
              "type": "codeVoice"
            },
            {
              "text": " for more details.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
