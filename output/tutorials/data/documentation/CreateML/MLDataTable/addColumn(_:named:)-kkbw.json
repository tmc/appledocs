    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to add a data column to a data table.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of elements in the column must equal the number",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of rows in the data table. Otherwise, the data table will be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "invalidated.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "As an example, start with a data table variable.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let data: [String: MLDataValueConvertible] = [",
            "    \"Title\": [\"Alice in Wonderland\", \"Hamlet\", \"Treasure Island\", \"Peter Pan\"],",
            "    \"Author\": [\"Lewis Carroll\", \"William Shakespeare\", \"Robert L. Stevenson\", \"J. M. Barrie\"],",
            "    \"Pages\": [124, 98, 280, 94],",
            "]",
            "",
            "var bookTable = try MLDataTable(dictionary: data)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Then use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CreateML/documentation/CreateML/MLDataTable/addColumn(_:named:)-kkbw",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to add a column to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "table.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let pagesColumn = MLDataColumn([124, 98, 280, 94])",
            "bookTable.addColumn(pagesColumn, named: \"Pages\")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
