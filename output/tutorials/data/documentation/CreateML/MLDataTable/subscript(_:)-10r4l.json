      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An untyped column indicating whether rows should be removed (a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "default value) or included (any nondefault value) in the derived table.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mask"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A new data table.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this untyped column–based subscript to create a new table by masking",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a subset of the table rows. The derived table will not include rows",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "where ",
              "type": "text"
            },
            {
              "code": "mask",
              "type": "codeVoice"
            },
            {
              "text": " contains a default value for its underlying type, such as:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "0",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in untyped ",
                      "type": "text"
                    },
                    {
                      "code": "Int",
                      "type": "codeVoice"
                    },
                    {
                      "text": " columns",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "0.0",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in untyped ",
                      "type": "text"
                    },
                    {
                      "code": "Double",
                      "type": "codeVoice"
                    },
                    {
                      "text": " columns",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "An empty string in untyped ",
                      "type": "text"
                    },
                    {
                      "code": "String",
                      "type": "codeVoice"
                    },
                    {
                      "text": " columns",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "The derived data table includes rows where the masking column has any",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "other (nondefault) value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "MLDataTable-subscript(_:)-10r4l-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, to filter the values in a data table as shown above, begin",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by creating a table with the original data.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let data: [String: MLDataValueConvertible] = [",
            "    \"Title\": [\"Alice in Wonderland\", \"Hamlet\", \"Dracula\", \"Peter Pan\", \"The Great Gatsby\"],",
            "    \"Genre\": [\"Fantasy\", \"Drama\", \"Adventure\", \"Fantasy\", \"Drama\"],",
            "    \"Pages\": [124, 98, 0, 94, 0]",
            "]",
            "",
            "guard let table = try? MLDataTable(dictionary: data) else {",
            "    fatalError(\"Invalid dictionary data\")",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "After you create the table, use the ",
              "type": "text"
            },
            {
              "code": "String",
              "type": "codeVoice"
            },
            {
              "text": "-based",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CreateML/documentation/CreateML/MLDataTable/subscript(_:)-3wjk",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to extract a column.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Create a new untyped column \"pagesColumn\" copied from the table’s \"Pages\" column",
            "let pagesColumn = table[\"Pages\"]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "subscript(mask: MLUntypedColumn)",
              "type": "codeVoice"
            },
            {
              "text": " with an untyped column–row mask",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to create a filtered table. The subscript uses the untyped values to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "determine whether to keep a row.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In this example, the untyped column ",
              "type": "text"
            },
            {
              "code": "pagesColumn",
              "type": "codeVoice"
            },
            {
              "text": " has an underlying type",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of ",
              "type": "text"
            },
            {
              "code": "Int",
              "type": "codeVoice"
            },
            {
              "text": ". The subscript removes any row with a value of ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " (the default",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value for ",
              "type": "text"
            },
            {
              "code": "Int",
              "type": "codeVoice"
            },
            {
              "text": ") and keeps all other rows.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Filter the table using pagesColumn.",
            "// Each row with a value of 0 is removed.",
            "let validPageCountTable = table[pagesColumn]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
