    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A thread-safe row transformation function.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The implementation of your transform must accept a row from the data table and return",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "a type that conforms to ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.CreateML/documentation/CreateML/MLDataValueConvertible",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lazyTransform"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A new ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CreateML/documentation/CreateML/MLDataColumn",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to create a new column derived from the existing data in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the table. The closure you pass evaluates lazily only when the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "transformed values are needed for a subsequent operation. Your",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "implementation should accept a data table row and must be thread-safe",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "because the framework may invoke the closure concurrently on unspecified",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "threads.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "MLDataTable-map(_:)-92wrj-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, to perform the column derivation operation shown above,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "begin by creating a table of data.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let data: [String: MLDataValueConvertible] = [",
            "    \"Day\": [\"Monday\", \"Tuesday\", \"Wednesday\", \"Thursday\", \"Friday\"],",
            "    \"Temperature\": [91.3, 85.8, 79.5, 83.4, 72.2]",
            "]",
            "",
            "var table = try MLDataTable(dictionary: data)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "After creating the table, use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CreateML/documentation/CreateML/MLDataTable/map(_:)-92wrj",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to create a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "new column of data from the original data. The example closure",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "implementation below is stateless and safe to invoke concurrently on any",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "thread, so no synchronization is necessary.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let derivedColumn = table.map { row -> String in",
            "    guard let day = row[\"Day\"]?.stringValue,",
            "          let temperature = row[\"Temperature\"]?.doubleValue else {",
            "            fatalError(\"Missing or invalid columns in row.\")",
            "    }",
            "    return String(format: \"%@ %.1fº\", day, temperature)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Then use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CreateML/documentation/CreateML/MLDataTable/addColumn(_:named:)-kkbw",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to add the derived",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "column to a table.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "table.addColumn(derivedColumn, named: \"Description\")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
