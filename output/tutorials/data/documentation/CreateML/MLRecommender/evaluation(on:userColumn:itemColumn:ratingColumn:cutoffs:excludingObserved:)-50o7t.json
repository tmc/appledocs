    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A MLDataTable containing testing data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "testingData"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Name of the Int or String typed column in the testing data containing user identifiers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "userColumn"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Name of the Int or String typed column in the testing data containing item identifiers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "itemColumn"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Name of an Int or Double typed column optionally in the testing data containing scores or",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "ratings. The default is nil, which corresponds to no rating column.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ratingColumn"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of Ints corresponding to each value at which the precision and recall will be evaluated.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The default is [1,2,3,4,5].",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Definition: Let P𝑘 be a vector of the first k items recommended by the model for a particular user and",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "let A be the set of items in the provided testingData for the same user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The “precision at cutoff k” for this user is: ",
                  "type": "text"
                },
                {
                  "code": "precision(k) = |A ∩ P𝑘| / k",
                  "type": "codeVoice"
                },
                {
                  "text": " while “recall at cutoff k” for",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "this user is: ",
                  "type": "text"
                },
                {
                  "code": "recall(k) = |A ∩ P𝑘| / |A|",
                  "type": "codeVoice"
                },
                {
                  "text": " where ",
                  "type": "text"
                },
                {
                  "code": "|A ∩ P𝑘|",
                  "type": "codeVoice"
                },
                {
                  "text": " is the number of elements in the intersection",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of A and P𝑘 and |A| is the number of elements in A.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cutoffs"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies whether user-item interactions observed in the training data are excluded when",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "generating evaluation result. The default is true.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "excludingObserved"
        }
      ]
    }
  ],
