          "content": [
            {
              "inlineContent": [
                {
                  "text": "The location in the file system where the session stores its progress.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sessionDirectory"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of iterations the session completes before it reports its progress.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reportInterval"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of iterations the session completes before it saves a checkpoint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "checkpointInterval"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
