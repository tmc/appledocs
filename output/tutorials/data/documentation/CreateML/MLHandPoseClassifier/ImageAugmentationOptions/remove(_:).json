            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the ",
              "type": "text"
            },
            {
              "code": "options",
              "type": "codeVoice"
            },
            {
              "text": " option set. Attempting to remove the same shipping option",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a second time results in ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", because ",
              "type": "text"
            },
