    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to create a column of arrays of strings from another column. Start by creating a column",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that is convertible to a column of arrays of strings.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let stringArrayString = \"[\\\"Array\\\", \\\"of\\\", \\\"strings\\\", \\\"1\\\"]\"",
            "let stringArrayString2 = \"[\\\"Array\\\", \\\"of\\\", \\\"strings\\\", \\\"2\\\"]\"",
            "let stringsColumn = MLDataColumn([stringArrayString, stringArrayString2])",
            "",
            "print(stringsColumn) // Prints [\"[\"Array\", \"of\", \"strings\", \"1\"]\", \"[\"Array\", \"of\", \"strings\", \"2\"]\"]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Then use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CreateML/documentation/CreateML/MLDataColumn/init(column:)-8uzuq",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to convert the column to a column of arrays of strings.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let stringArrayColumn = MLDataColumn<[String]>(column: stringsColumn)",
            "print(stringArrayColumn) // Prints [[\"Array\", \"of\", \"strings\", \"1\"], [\"Array\", \"of\", \"strings\", \"2\"]]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
