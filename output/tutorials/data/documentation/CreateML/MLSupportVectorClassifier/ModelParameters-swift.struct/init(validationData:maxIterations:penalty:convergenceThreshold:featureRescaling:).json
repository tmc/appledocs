    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dataset used to monitor how well the model is generalizing.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " which will use an automatically sampled validation set.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "validationData"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of passes through the data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is 11.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxIterations"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Weight of the regularizer. The larger the penalty the less variance in the model.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is 1.0.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "penalty"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The threshold with which to determine if the model has converged. Consider",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "reducing this value for higher training accuracy, but beware of overfitting.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is 0.01.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "convergenceThreshold"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Determines if the features should be preprocessed to ensure all features are on the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "same scale.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is true.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "featureRescaling"
        }
      ]
    }
  ],
