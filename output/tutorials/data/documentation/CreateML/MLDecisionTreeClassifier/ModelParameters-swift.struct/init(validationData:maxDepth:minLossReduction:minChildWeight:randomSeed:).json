    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dataset used to monitor how well the model is generalizing.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " which will use an automatically sampled validation set.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "validationData"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum depth of the tree. Must be a value of at least 1.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is 6.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxDepth"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The minimum amount of reduction to the loss function that is required to make another",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "node to split the data. Larger values help prevent overfitting.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is 0.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "minLossReduction"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Determines the minimum weight of each leaf node of the tree. Larger values help prevent",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "overfitting.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is 0.1.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "minChildWeight"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A seed for internal random operations. Set this value to ensure reproducible results.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is 42.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "randomSeed"
        }
      ]
    }
  ],
