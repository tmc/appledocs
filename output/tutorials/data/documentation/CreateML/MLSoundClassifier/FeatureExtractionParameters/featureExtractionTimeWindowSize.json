      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The time-window size defaults to ",
              "type": "text"
            },
            {
              "code": "0.975",
              "type": "codeVoice"
            },
            {
              "text": " seconds and must be in the range ",
              "type": "text"
            },
            {
              "code": "[0.5, 15.0]",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Feature-extraction sessions that use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CreateML/documentation/CreateML/MLSoundClassifier/ModelParameters-swift.struct/FeatureExtractorType/vggish(revision:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "ignore this value and always use a time-window size of ",
              "type": "text"
            },
            {
              "code": "0.975",
              "type": "codeVoice"
            },
            {
              "text": " seconds.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
