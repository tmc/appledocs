      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A validation dataset represented by an",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.CreateML/documentation/CreateML/MLSoundClassifier/ModelParameters-swift.struct/ValidationData",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " instance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "validation"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The largest number of iterations the training session can use to train the sound",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "classifier.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxIterations"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A proportion of overlap the training session uses to analyze two consecutive windows in",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the audio data. The proportion must be in the range ",
                  "type": "text"
                },
                {
                  "code": "[0.0, 1.0)",
                  "type": "codeVoice"
                },
                {
                  "text": ". Higher proportions generate more",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "training data, but also increases the training time.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The default value is ",
                  "type": "text"
                },
                {
                  "code": "0.5",
                  "type": "codeVoice"
                },
                {
                  "text": ", which represents a 50% overlap.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "overlapFactor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The algorithm the training session uses to train the sound classifier.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "algorithm"
        }
      ]
    }
