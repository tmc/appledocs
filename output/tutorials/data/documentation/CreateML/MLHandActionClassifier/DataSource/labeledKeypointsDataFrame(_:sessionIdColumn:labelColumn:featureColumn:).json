    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A data frame that contains the hand-joint locations and annotations for a set of hand",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "actions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dataFrame "
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the column in the data frame that contains the session identifiers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sessionIdColumn"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the column in the data frame that contains the hand action label names.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "labelColumn"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the column in the data frame that contains the hand-joint location data.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Each entry in the column must be a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/TabularData/ShapedData",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "instance that contains three dimensions:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "The first dimension has a size of one.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "The second dimension has three channels: the x-coordinate, the y-coordinate, and the confidence",
                          "type": "text"
                        },
                        {
                          "text": " ",
                          "type": "text"
                        },
                        {
                          "text": "value, respectively.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "The third dimension has 21 channels, one for each hand joint.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "featureColumn"
        }
      ]
    }
