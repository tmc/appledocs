        {
          "inlineContent": [
            {
              "text": "This frame should override the app’s default window position and size.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Individual components (e.g., ",
              "type": "text"
            },
            {
              "code": "origin.x",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "size.width",
              "type": "codeVoice"
            },
            {
              "text": ") will be ",
              "type": "text"
            },
            {
              "code": "NaN",
              "type": "codeVoice"
            },
            {
              "text": " if not specified.",
              "type": "text"
            }
