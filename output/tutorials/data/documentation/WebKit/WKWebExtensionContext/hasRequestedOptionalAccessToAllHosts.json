    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If this property is ",
              "type": "text"
            },
            {
              "code": "YES",
              "type": "codeVoice"
            },
            {
              "text": ", the extension has asked for access to all hosts in a call to ",
              "type": "text"
            },
            {
              "code": "browser.runtime.permissions.request()",
              "type": "codeVoice"
            },
            {
              "text": ", and future permission checks will present discrete hosts for approval as being implicitly requested. This value should be saved and restored as needed by the app.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
