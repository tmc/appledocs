      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tab that has become active.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "activatedTab"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tab that was active before. This parameter can be \\c nil if there was no previously active tab.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "previousTab"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method informs all loaded extensions of the tab activation, ensuring consistent state awareness across extensions.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If the intention is to inform only a specific extension, use the respective method on that extension’s context instead.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
