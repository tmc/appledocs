      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To indicate that no user gestures are required to play media, use an empty set of audio/visual media types, indicated by the empty array literal, ",
              "type": "text"
            },
            {
              "code": "[]",
              "type": "codeVoice"
            },
            {
              "text": ". For example, ",
              "type": "text"
            },
            {
              "code": "let myAudiovisualMediaType: WKAudiovisualMediaTypes = []",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
