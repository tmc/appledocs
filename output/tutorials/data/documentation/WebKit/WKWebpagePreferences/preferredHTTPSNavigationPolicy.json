    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Used when performing a top-level navigation to a webpage.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The default value is WKWebpagePreferencesUpgradeToHTTPSPolicyKeepAsRequested.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The stated preference is ignored on subframe navigation, and it may be ignored based on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "system configuration. The upgradeKnownHostsToHTTPS property on WKWebViewConfiguration",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "supercedes this policy for known hosts.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
