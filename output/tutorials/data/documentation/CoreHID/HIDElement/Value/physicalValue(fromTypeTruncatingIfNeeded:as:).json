    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type to cast the underlying bytes to before bounding, shifting and scaling; truncating or extending the bytes as necessary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fromTypeTruncatingIfNeeded"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type to cast the calculated floating point result to before returning.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "as"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The calculated value cast as the requested type, or nil if out of bounds.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CoreHID/documentation/CoreHID/HIDElement/Value/logicalValue(asTypeTruncatingIfNeeded:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " is first retrieved with the specified type. If the logical value is undefined, such as when the raw value is out of bounds, the physical value is also undefined.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "More information and example calculations for physical values can be found in the HID specification: See the HID specification for more details: ",
              "type": "text"
            },
            {
              "identifier": "https://www.usb.org/hid",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
