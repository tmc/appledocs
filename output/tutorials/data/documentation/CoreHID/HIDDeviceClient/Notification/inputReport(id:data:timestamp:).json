      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ID of the report. This may not be populated if the descriptor contains only one report.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The raw bytes of the report. If there are multiple reports, the first byte is the report ID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data"
        },
        {
          "content": [
            {
