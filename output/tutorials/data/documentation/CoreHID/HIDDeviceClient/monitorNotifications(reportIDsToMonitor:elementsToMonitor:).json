    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Notifications come in asynchronously from the device at arbitrary times when relevant events occur.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Example usage:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "for await notification in try await client.monitorNotifications(reportIDsToMonitor: [HIDReportID.allReports], elementsToMonitor: await client.elements) {",
            "switch notification {",
            "case .inputReport(let reportID, let report, let timestamp):",
            "    break",
            "case .elementUpdates(let values):",
            "    break",
            "case .deviceSeized:",
            "    break",
            "case .deviceUnseized:",
            "    break",
            "case .deviceRemoved:",
            "    break",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.CoreHID/documentation/CoreHID/HIDDeviceError",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " if there is an issue with setup.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Throws",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
