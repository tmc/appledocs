      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Skip over all whitespace that does not abut non-whitespace character data. In other words, given “",
              "type": "text"
            },
            {
              "code": "<foo>  <bar> blah </bar></foo>",
              "type": "codeVoice"
            },
            {
              "text": ",” the whitespace between foo's open tag and bar's open tag would be suppressed, but the whitespace around ",
              "type": "text"
            },
            {
              "code": "blah",
              "type": "codeVoice"
            },
            {
              "text": " would be preserved.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
