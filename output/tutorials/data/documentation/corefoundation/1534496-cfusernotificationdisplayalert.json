    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount of time to wait for the user to dismiss the notification dialog before the dialog dismisses itself. Pass ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to have the dialog never time out.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timeout"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A set of flags describing the type of notification dialog to display. The value is normally just the alert level from ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfusernotification/1534483-alert_levels",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". If you don’t want a default button displayed, perform a bitwise-OR operation with the alert level and the constant ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfusernotificationnodefaultbuttonflag",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "flags"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A file URL pointing to the icon to display in the dialog. If ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", a default icon is used based on the notification’s alert level specified in ",
                  "type": "text"
                },
                {
                  "code": "flags",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "iconURL"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Not used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "soundURL"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A file URL pointing to a bundle that contains localized versions of the strings displayed in the dialog. Can be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "localizationURL"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The title of the notification dialog. Cannot be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alertHeader"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The message string to display in the dialog. Can be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alertMessage"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The title of the default button. If ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", the string ",
                  "type": "text"
                },
                {
                  "code": "OK",
                  "type": "codeVoice"
                },
                {
                  "text": " is used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "defaultButtonTitle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The title of an optional alternate button. Can be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alternateButtonTitle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The title of an optional third button. Can be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "otherButtonTitle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, contains flags identifying how the notification was dismissed. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfusernotification/1534504-response_codes",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for details.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "responseFlags"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " if the cancel was successful; a non-",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " value otherwise.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function blocks the current thread’s execution until the dialog is dismissed, either by the user or by timing out.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
