      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The socket signature for the name server. If ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", this function contacts the default server, which is assumed to be a local process using TCP/IP to listen on the port number returned from ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1541492-cfsocketgetdefaultnameregistrypo",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". If ",
                  "type": "text"
                },
                {
                  "code": "nameServerSignature",
                  "type": "codeVoice"
                },
                {
                  "text": " is incomplete, the missing values are replaced with the default serverâ€™s values, if appropriate. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nameServerSignature"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time to wait for the server to accept a connection and to reply to the registration request. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timeout"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name with which to register ",
                  "type": "text"
                },
                {
                  "code": "signature",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The socket signature to register. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signature"
        }
      ]
    },
    {
      "content": [
        {
