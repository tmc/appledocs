    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle to examine. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the requested resource. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resourceName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The abstract type of the requested resource. The type is expressed as a filename extension, such as ",
                  "type": "text"
                },
                {
                  "code": "jpg",
                  "type": "codeVoice"
                },
                {
                  "text": ". Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if ",
                  "type": "text"
                },
                {
                  "code": "resourceName",
                  "type": "codeVoice"
                },
                {
                  "text": " is the complete name of the file you’re looking for, including any extension.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resourceType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the subdirectory of the bundle’s resources directory to search. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " to search the standard CFBundle resource locations.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "subDirName"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A CFURL object describing the location of the requested resource, or ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": " if the resource cannot be found. Ownership follows the ",
              "type": "text"
            },
            {
              "identifier": "devLink-1698003",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, if a bundle contains a subdirectory ",
              "type": "text"
            },
            {
              "code": "WaterSounds",
              "type": "codeVoice"
            },
            {
              "text": " that includes a file ",
              "type": "text"
            },
            {
              "code": "Water1.aiff",
              "type": "codeVoice"
            },
            {
              "text": ", you can retrieve the URL for the file using:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "CFBundleCopyResourceURL(bundle, CFSTR(\"Water1\"), CFSTR(\"aiff\"), CFSTR(\"WaterSounds\"));"
          ],
          "metadata": {
            "anchor": "2556081",
            "title": "Listing 1"
          },
          "syntax": "occ",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
