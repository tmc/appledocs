      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The preference key whose value to obtain. The key must specify a preference whose value is of type ",
                  "type": "text"
                },
                {
                  "code": "Boolean",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "key"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier of the application whose preferences are searched, typically ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencescurrentapplication",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". Do not pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfpreferencesanyapplication",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". Takes the form of a Java package name, such as ",
                  "type": "text"
                },
                {
                  "code": "com.foosoft",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "applicationID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " if the preference value for the specified key was located and found to be of type ",
                  "type": "text"
                },
                {
                  "code": "Boolean",
                  "type": "codeVoice"
                },
                {
                  "text": ", otherwise ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "keyExistsAndHasValidFormat"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The preference data for the specified key and application, or if no value was located, ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": ". ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
