      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dictionary to modify. If this parameter is a fixed-capacity dictionary and it is full before this operation, and the key does not exist in the dictionary, the behavior is undefined. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "theDict"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key of the value to set in ",
                  "type": "text"
                },
                {
                  "code": "theDict",
                  "type": "codeVoice"
                },
                {
                  "text": ". If a key which matches ",
                  "type": "text"
                },
                {
                  "code": "key",
                  "type": "codeVoice"
                },
                {
                  "text": " is already present in the dictionary, only the value for the key is changed (\"add if absent, replace if present\"). If no key matches ",
                  "type": "text"
                },
                {
                  "code": "key",
                  "type": "codeVoice"
                },
                {
                  "text": ", the key-value pair is added to the dictionary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If a key-value pair is added, both ",
                  "type": "text"
                },
                {
                  "code": "key",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": " are retained by the dictionary, using the retain callback provided when ",
                  "type": "text"
                },
                {
                  "code": "theDict",
                  "type": "codeVoice"
                },
                {
                  "text": " was created. ",
                  "type": "text"
                },
                {
                  "code": "key",
                  "type": "codeVoice"
                },
                {
                  "text": " must be of the type expected by the key retain callback. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "key"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value to add to or replace in ",
                  "type": "text"
                },
                {
                  "code": "theDict",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": " is retained using the value retain callback provided when ",
                  "type": "text"
                },
                {
                  "code": "theDict",
                  "type": "codeVoice"
                },
                {
                  "text": " was created, and the previous value if any is released. ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": " must be of the type expected by the retain and release callbacks.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        }
      ]
    }
