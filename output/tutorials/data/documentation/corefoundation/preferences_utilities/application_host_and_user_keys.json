          "inlineContent": [
            {
              "text": "Not all combinations of application, host, and user are supported preference domains. Specifically, ",
              "type": "text"
            },
            {
              "code": "kCFPreferencesAnyHost",
              "type": "codeVoice"
            },
            {
              "text": " is not supported in any combination with other options.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
