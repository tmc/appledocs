      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The callback types for which a callback is made is determined when the CFSocket object is created, such as with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1543527-cfsocketcreate",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", or later with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1543029-cfsocketenablecallbacks",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1542983-cfsocketdisablecallbacks",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "kCFSocketReadCallBack",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "kCFSocketAcceptCallBack",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "kCFSocketDataCallBack",
              "type": "codeVoice"
            },
            {
              "text": " callbacks are mutually exclusive.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "1819302",
          "level": 3,
          "text": "Version-Notes",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "kCFSocketWriteCallBack",
              "type": "codeVoice"
            },
            {
              "text": " is available in macOS 10.2 and later.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
