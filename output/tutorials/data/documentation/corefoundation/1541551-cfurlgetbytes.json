      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL object to convert to a byte representation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "anURL"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The buffer where you want the bytes to be placed. If the buffer is of insufficient size, returns ",
                  "type": "text"
                },
                {
                  "code": "-1",
                  "type": "codeVoice"
                },
                {
                  "text": " and no bytes are placed in buffer. If ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " the needed length is computed and returned. The returned bytes are the original bytes from which the URL was created (",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "not",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " including the base URL). If the URL was created from a string, the bytes are the bytes of the string encoded via UTF-8.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "buffer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of bytes in ",
                  "type": "text"
                },
                {
                  "code": "buffer",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bufferLength"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Returns the number of bytes in ",
              "type": "text"
            },
            {
              "code": "buffer",
              "type": "codeVoice"
            },
            {
              "text": " that were filled. If the buffer is of insufficient size, returns ",
              "type": "text"
            },
            {
              "code": "-1",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
