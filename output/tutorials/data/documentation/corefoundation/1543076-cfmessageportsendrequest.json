      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The message port to which ",
                  "type": "text"
                },
                {
                  "code": "data",
                  "type": "codeVoice"
                },
                {
                  "text": " should be sent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "remote"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An arbitrary integer value that you can send with the message.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "msgid"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The data to send to ",
                  "type": "text"
                },
                {
                  "code": "remote",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time to wait for ",
                  "type": "text"
                },
                {
                  "code": "data",
                  "type": "codeVoice"
                },
                {
                  "text": " to be sent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sendTimeout"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time to wait for a reply to be returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rcvTimeout"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The run loop mode in which the function should wait for a reply. If the message is a ",
                  "type": "text"
                },
                {
                  "code": "oneway",
                  "type": "codeVoice"
                },
                {
                  "text": " (so no response is expected), then ",
                  "type": "text"
                },
                {
                  "code": "replyMode",
                  "type": "codeVoice"
                },
                {
                  "text": " should be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ". If ",
                  "type": "text"
                },
                {
                  "code": "replyMode",
                  "type": "codeVoice"
                },
                {
                  "text": " is non-",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", the function runs the run loop waiting for a reply, in that mode. ",
                  "type": "text"
                },
                {
                  "code": "replyMode",
                  "type": "codeVoice"
                },
                {
                  "text": " can be any string name of a run loop mode, but it should be one with input sources installed. You should use the ",
                  "type": "text"
                },
                {
                  "code": "kCFRunLoopDefaultMode",
                  "type": "codeVoice"
                },
                {
                  "text": " constant unless you have a specific reason to use a different mode.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "replyMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Upon return, contains a CFData object containing the reply data. Ownership follows the ",
                  "type": "text"
                },
                {
                  "identifier": "devLink-1698003",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "returnData"
        }
      ]
    },
