  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "When you use the ",
                  "type": "text"
                },
                {
                  "code": "CFStream",
                  "type": "codeVoice"
                },
                {
                  "text": " API for networking, read and write operations on sockets can block. To prevent blocking:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Call ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.documentation/documentation/corefoundation/1539670-cfreadstreamsetclient",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " and ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.documentation/documentation/corefoundation/1539678-cfwritestreamsetclient",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " to register to receive stream-related event notifications.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Call ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.documentation/documentation/corefoundation/1539611-cfreadstreamschedulewithrunloop",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " and ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.documentation/documentation/corefoundation/1539621-cfwritestreamschedulewithrunloop",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " to schedule the stream on a run loop for receiving stream-related event notifications.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Call ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.documentation/documentation/corefoundation/1539743-cfreadstreamopen",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " and ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.documentation/documentation/corefoundation/1539753-cfwritestreamopen",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " to open each stream.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Read only after receiving a ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstreameventtype/1539600-hasbytesavailable",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " notification. Write only after receiving a ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstreameventtype/1539627-canacceptbytes",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " notification.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "orderedList"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
