      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string containing a ISO 4217 3-letter currency code. For example, AUD for Australian Dollars, EUR for Euros.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "currencyCode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Upon return, contains the number of fraction digits that should be displayed for the currency specified by ",
                  "type": "text"
                },
                {
                  "code": "currencyCode",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "defaultFractionDigits"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Upon return, contains the rounding increment for the currency specified by ",
                  "type": "text"
                },
                {
                  "code": "currencyCode",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "0.0",
                  "type": "codeVoice"
                },
                {
                  "text": " if no rounding is done by the currency. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "roundingIncrement"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " if the information was obtained successfully, otherwise ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": " (for example, if the currency code is unknown or the information is not available).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The returned values are not localized because these are properties of the currency.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
