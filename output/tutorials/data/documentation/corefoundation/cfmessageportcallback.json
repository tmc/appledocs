        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The local message port that received the message. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An arbitrary integer value assigned to the message by the sender.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "msgid"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The message data. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "info",
                  "type": "codeVoice"
                },
                {
                  "text": " member of the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfmessageportcontext",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " structure that was used when creating ",
                  "type": "text"
                },
                {
                  "code": "local",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "info"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Data to send back to the sender of the message. The system releases the returned CFData object. Return ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": " if you want an empty reply returned to the sender. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If you want the message data to persist beyond this callback, you must explicitly create a copy of ",
              "type": "text"
            },
            {
              "code": "data",
              "type": "codeVoice"
            },
            {
              "text": " rather than merely retain it; the contents of ",
              "type": "text"
            },
            {
              "code": "data",
              "type": "codeVoice"
            },
            {
              "text": " will be deallocated after the callback exits.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
