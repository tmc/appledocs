    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The first string to use in the comparison.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "theString1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The second string to use in the comparison. The full range of this string is used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "theString2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The range of characters in ",
                  "type": "text"
                },
                {
                  "code": "theString1",
                  "type": "codeVoice"
                },
                {
                  "text": " to be used in the comparison to ",
                  "type": "text"
                },
                {
                  "code": "theString2",
                  "type": "codeVoice"
                },
                {
                  "text": ". To use the whole string, pass the range ",
                  "type": "text"
                },
                {
                  "code": "CFRangeMake(0, CFStringGetLength(theString1))",
                  "type": "codeVoice"
                },
                {
                  "text": ". The specified range must not exceed the bounds of the string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rangeToCompare"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Flags that select different types of comparisons, such as case-insensitive comparison and non-literal comparison. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstring/string_comparison_flags",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for the available flags.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Specifying the ",
                  "type": "text"
                },
                {
                  "code": "kCFCompareBackwards",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "kCFCompareAnchored",
                  "type": "codeVoice"
                },
                {
                  "text": " option has no effect.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Specifying the ",
                  "type": "text"
                },
                {
                  "code": "kCFCompareLocalized",
                  "type": "codeVoice"
                },
                {
                  "text": " option and passing ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " for ",
                  "type": "text"
                },
                {
                  "code": "locale",
                  "type": "codeVoice"
                },
                {
                  "text": " causes the current locale (the return value of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/1542508-cflocalecopycurrent",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ") to be used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "compareOptions"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The locale to use for the comparison, which affects both equality and ordering algorithms. For example, in some locales, accented characters are ordered immediately after the base; other locales order them after “z”.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " and the ",
                  "type": "text"
                },
                {
                  "code": "kCFCompareLocalized",
                  "type": "codeVoice"
                },
                {
                  "text": " option is not specified for ",
                  "type": "text"
                },
                {
                  "code": "compareOptions",
                  "type": "codeVoice"
                },
                {
                  "text": ", the comparison is nonlocalized.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "locale"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfcomparisonresult",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " value that indicates whether ",
              "type": "text"
            },
            {
              "code": "theString1",
              "type": "codeVoice"
            },
            {
              "text": " is equal to, less than, or greater than ",
              "type": "text"
            },
            {
              "code": "theString2",
              "type": "codeVoice"
            },
            {
              "text": ". ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
