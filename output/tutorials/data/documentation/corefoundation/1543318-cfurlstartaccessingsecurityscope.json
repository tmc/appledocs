          "inlineContent": [
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " if the request to access the resource succeeded; otherwise, ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": ".",
