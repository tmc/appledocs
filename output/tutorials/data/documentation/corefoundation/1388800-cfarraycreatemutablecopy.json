      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The allocator to use to allocate memory for the new array and its storage for values. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfallocatordefault",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to use the current default allocator.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allocator"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of values that can be contained by the new array. The array starts with the same number of values as ",
                  "type": "text"
                },
                {
                  "code": "theArray",
                  "type": "codeVoice"
                },
                {
                  "text": " and can grow to this number of values (and it can have less).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Pass ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to specify that the maximum capacity is not limited. If non-",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "capacity",
                  "type": "codeVoice"
                },
                {
                  "text": " must be greater than or equal to the count of ",
                  "type": "text"
                },
                {
                  "code": "theArray",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "capacity"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The array to copy. The pointer values from the array are copied into the new array. However, the values are also retained by the new array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "theArray"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A new mutable array that contains the same values as ",
              "type": "text"
            },
            {
              "code": "theArray",
              "type": "codeVoice"
            },
            {
              "text": ". The new array has the same count as the ",
              "type": "text"
            },
            {
              "code": "theArray",
              "type": "codeVoice"
            },
            {
              "text": " and uses the same callbacks. Ownership follows the ",
              "type": "text"
            },
            {
              "identifier": "devLink-1698003",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
