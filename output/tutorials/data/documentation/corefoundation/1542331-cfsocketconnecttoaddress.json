      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The CFSocket object with which to connect to ",
                  "type": "text"
                },
                {
                  "code": "address",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "s"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A CFData object containing a ",
                  "type": "text"
                },
                {
                  "code": "struct sockaddr",
                  "type": "codeVoice"
                },
                {
                  "text": " appropriate for the protocol family of ",
                  "type": "text"
                },
                {
                  "code": "s",
                  "type": "codeVoice"
                },
                {
                  "text": " (",
                  "type": "text"
                },
                {
                  "code": "struct sockaddr_in",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "struct sockaddr_in6",
                  "type": "codeVoice"
                },
                {
                  "text": ", for example), indicating the remote address to which to connect. This data object is used only for the duration of the function call.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "address"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time to wait for a connection to succeed. If a negative value is used, this function does not wait for the connection and instead lets the connection attempt happen in the background. If ",
                  "type": "text"
                },
                {
                  "code": "s",
                  "type": "codeVoice"
                },
                {
                  "text": " requested a ",
                  "type": "text"
                },
                {
                  "code": "kCFSocketConnectCallBack",
                  "type": "codeVoice"
                },
                {
                  "text": ", you will receive a callback when the background connection succeeds or fails. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timeout"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "An error code indicating success or failure of the connection attempt. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
