        },
        {
          "inlineContent": [
            {
              "text": "The scheme of ",
              "type": "text"
            },
            {
              "code": "anURL",
              "type": "codeVoice"
            },
            {
              "text": ". Ownership follows the create rule. See ",
              "type": "text"
            },
            {
              "identifier": "devLink-1698003",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
