    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The allocator to use to allocate memory for the new object. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/kcfallocatordefault",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to use the current default allocator. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allocator"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The protocol family for the socket. If negative or 0 is passed, the socket defaults to ",
                  "type": "text"
                },
                {
                  "code": "PF_INET",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "protocolFamily"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The socket type to create. If ",
                  "type": "text"
                },
                {
                  "code": "protocolFamily",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "PF_INET",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "socketType",
                  "type": "codeVoice"
                },
                {
                  "text": " is negative or 0, the socket type defaults to ",
                  "type": "text"
                },
                {
                  "code": "SOCK_STREAM",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "socketType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The protocol for the socket. If ",
                  "type": "text"
                },
                {
                  "code": "protocolFamily",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "PF_INET",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "protocol",
                  "type": "codeVoice"
                },
                {
                  "text": " is negative or 0, the socket protocol defaults to ",
                  "type": "text"
                },
                {
                  "code": "IPPROTO_TCP",
                  "type": "codeVoice"
                },
                {
                  "text": " if ",
                  "type": "text"
                },
                {
                  "code": "socketType",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "SOCK_STREAM",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "IPPROTO_UDP",
                  "type": "codeVoice"
                },
                {
                  "text": " if ",
                  "type": "text"
                },
                {
                  "code": "socketType",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "SOCK_DGRAM",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "protocol"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A bitwise-OR combination of the types of socket activity that should cause ",
                  "type": "text"
                },
                {
                  "code": "callout",
                  "type": "codeVoice"
                },
                {
                  "text": " to be called. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfsocketcallbacktype",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for the possible activity values. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "callBackTypes"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The function to call when one of the activities indicated by ",
                  "type": "text"
                },
                {
                  "code": "callBackTypes",
                  "type": "codeVoice"
                },
                {
                  "text": " occurs. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "callout"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A structure holding contextual information for the CFSocket object. The function copies the information out of the structure, so the memory pointed to by ",
                  "type": "text"
                },
                {
                  "code": "context",
                  "type": "codeVoice"
                },
                {
                  "text": " does not need to persist beyond the function call. Can be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "context"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The new CFSocket object, or ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": " if an error occurred. Ownership follows the ",
              "type": "text"
            },
            {
              "identifier": "devLink-1698003",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
