      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "CFURL",
                  "type": "codeVoice"
                },
                {
                  "text": " object whose native file system representation you want to obtain. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "url"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Pass ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " to return an absolute path name. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resolveAgainstBase"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a character buffer. On return, the buffer holds the native file system's representation of ",
                  "type": "text"
                },
                {
                  "code": "url",
                  "type": "codeVoice"
                },
                {
                  "text": ". The buffer is null-terminated. This parameter must be at least ",
                  "type": "text"
                },
                {
                  "code": "maxBufLen",
                  "type": "codeVoice"
                },
                {
                  "text": " in size for the file system in question to avoid failures for insufficiently large buffers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "buffer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of characters that can be written to ",
                  "type": "text"
                },
                {
                  "code": "buffer",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxBufLen"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " if successful, ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": " if an error occurred. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "No more than ",
              "type": "text"
            },
            {
              "code": "maxBufLen",
              "type": "codeVoice"
            },
            {
              "text": " bytes are written to ",
              "type": "text"
            },
            {
              "code": "buffer",
              "type": "codeVoice"
            },
            {
              "text": ". If ",
              "type": "text"
            },
            {
              "code": "url",
              "type": "codeVoice"
            },
            {
              "text": " requires more than ",
              "type": "text"
            },
            {
              "code": "maxBufLen",
              "type": "codeVoice"
            },
            {
              "text": " bytes to represent itself, including the terminating null byte, this function returns ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": ". To avoid this possible failure, you should pass a buffer with size of at least the maximum path length for the file system in question.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
