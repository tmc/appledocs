          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that represents a double value. The only allowed characters are the ASCII digit characters (ASCII ",
                  "type": "text"
                },
                {
                  "code": "0x30",
                  "type": "codeVoice"
                },
                {
                  "text": " - ",
                  "type": "text"
                },
                {
                  "code": "0x39",
                  "type": "codeVoice"
                },
                {
                  "text": "), the plus sign (ASCII ",
                  "type": "text"
                },
                {
                  "code": "0x2B",
                  "type": "codeVoice"
                },
                {
                  "text": "), the minus sign (ASCII ",
                  "type": "text"
                },
                {
                  "code": "0x2D",
                  "type": "codeVoice"
                },
                {
                  "text": "), and the period character (ASCII ",
                  "type": "text"
                },
                {
                  "code": "0x2E",
                  "type": "codeVoice"
                },
                {
                  "text": "). ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "str"
        }
      ]
    },
    {
      "content": [
