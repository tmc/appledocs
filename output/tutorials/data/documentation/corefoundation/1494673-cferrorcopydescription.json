      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cferror",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to examine. If this is not a valid ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cferror",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": ", the behavior is undefined.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
