          "content": [
            {
              "inlineContent": [
                {
                  "text": "The development language version of the string. This string is used as the search key to locate the localized version of the string. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "key"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the strings file to search. The name should not include the ",
                  "type": "text"
                },
                {
                  "code": "strings",
                  "type": "codeVoice"
                },
                {
                  "text": " filename extension. The case of the string must match that of the file name, even on file systems (such as HFS+) that are not case sensitive with regards to file names",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tableName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A comment to provide the translators with contextual information necessary for proper translation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "comment"
        }
      ]
    },
