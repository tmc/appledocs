      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The stream to modify.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stream"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The set of events for which the client should receive callbacks. The events are listed in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstreameventtype",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". If you pass ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfstreameventtype/kcfstreameventnone",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", the current client for ",
                  "type": "text"
                },
                {
                  "code": "stream",
                  "type": "codeVoice"
                },
                {
                  "text": " is removed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "streamEvents"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The client callback function to call when one of the events requested in ",
                  "type": "text"
                },
                {
                  "code": "streamEvents",
                  "type": "codeVoice"
                },
                {
                  "text": " occurs. If ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", the current client for ",
                  "type": "text"
                },
                {
                  "code": "stream",
                  "type": "codeVoice"
                },
                {
                  "text": " is removed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientCB"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A structure holding contextual information for the stream client. The function copies the information out of the structure, so the memory pointed to by ",
                  "type": "text"
                },
                {
                  "code": "clientContext",
                  "type": "codeVoice"
                },
                {
                  "text": " does not need to persist beyond the function call. If ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", the current client for ",
                  "type": "text"
                },
                {
                  "code": "stream",
                  "type": "codeVoice"
                },
                {
                  "text": " is removed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientContext"
        }
