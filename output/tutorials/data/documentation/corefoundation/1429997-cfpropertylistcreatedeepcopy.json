      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The allocator to use to allocate memory for the new property list. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " or kCFAllocatorDefault to use the current default allocator. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allocator"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The property list to copy. This may be any of the standard property list objects, for example a CFArray or a CFDictionary object. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "propertyList"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A constant that specifies the degree of mutability of the returned property list. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfpropertylist/property_list_mutability_options",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for descriptions of possible values. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mutabilityOption"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A new property list that is a copy of ",
              "type": "text"
            },
            {
              "code": "propertyList",
              "type": "codeVoice"
            },
            {
              "text": ". Ownership follows the ",
              "type": "text"
            },
            {
              "identifier": "devLink-1698003",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Recursively creates a copy of the given property list so nested arrays and dictionaries are copied as well as the top-most container.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
