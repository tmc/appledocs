      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The notification center to modify. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "center"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The observer. This value must not be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "observer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the notification to stop observing. If ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "observer",
                  "type": "codeVoice"
                },
                {
                  "text": " stops receiving callbacks for all notifications posted by ",
                  "type": "text"
                },
                {
                  "code": "object",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The object to stop observing. For distributed notifications, ",
                  "type": "text"
                },
                {
                  "code": "object",
                  "type": "codeVoice"
                },
                {
                  "text": " must be a CFString object. If ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "observer",
                  "type": "codeVoice"
                },
                {
                  "text": " stops receiving callbacks for all objects posting notifications named ",
                  "type": "text"
                },
                {
                  "code": "name",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "center",
                  "type": "codeVoice"
                },
                {
                  "text": " is a Darwin notification center, this value is ignored.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "object"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If both ",
              "type": "text"
            },
            {
              "code": "name",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "object",
              "type": "codeVoice"
            },
            {
              "text": " are ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": ", this function unregisters ",
              "type": "text"
            },
            {
              "code": "observer",
              "type": "codeVoice"
            },
            {
              "text": " from all the notifications for which it had previously registered with ",
              "type": "text"
            },
            {
              "code": "center",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
