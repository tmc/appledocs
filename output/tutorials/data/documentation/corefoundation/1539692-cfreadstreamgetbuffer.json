    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The stream to examine.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stream"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of bytes to read. If greater than ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "maxBytesToRead",
                  "type": "codeVoice"
                },
                {
                  "text": " limits the number of bytes read; if ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " or less, all available bytes are read.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxBytesToRead"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, contains the length of returned buffer. If ",
                  "type": "text"
                },
                {
                  "code": "stream",
                  "type": "codeVoice"
                },
                {
                  "text": " is not open or has encountered an error, ",
                  "type": "text"
                },
                {
                  "code": "numBytesRead",
                  "type": "codeVoice"
                },
                {
                  "text": " is set to ",
                  "type": "text"
                },
                {
                  "code": "-1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "numBytesRead"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A pointer to the internal buffer of unread data for ",
              "type": "text"
            },
            {
              "code": "stream",
              "type": "codeVoice"
            },
            {
              "text": ", if possible; ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": " otherwise. The buffer is good only until the next stream operation called on the stream. You should neither change the contents of the returned buffer nor attempt to deallocate the buffer; it is still owned by the stream. The bytes returned in the buffer are considered read from the stream.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
