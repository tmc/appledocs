    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A descriptor for your application service. To create an application service descriptor, call ",
                  "type": "text"
                },
                {
                  "code": "NWBrowser.Descriptor.applicationService(name:options:)",
                  "type": "codeVoice"
                },
                {
                  "text": " and provide a name for the service.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "browseDescriptor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that the system calls when the user selects a device in the picker view, or cancels the view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onSelect"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A label displayed by the network device picker.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "label"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A view that the system displays if the current device doesn’t support device discovery.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fallback"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Parameters for your network connection. Use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/Network/NWParameters/applicationService",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to create a default set of parameters that create an encrypted connection with the other devices. You can also add ",
                  "type": "text"
                },
                {
                  "code": "a",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/Network/NWProtocolFramer",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to provide an application-level messaging protocol.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "parameters"
        }
      ]
    }
  ],
