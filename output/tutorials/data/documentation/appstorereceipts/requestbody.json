          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Base64-encoded receipt data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "receipt-data",
          "required": true,
          "type": [
            {
              "kind": "text",
