    {
      "kind": "possibleValues",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The subscription will renew at the end of the current subscription period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The customer has turned off automatic renewal for the subscription.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "0"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This field is returned in the JSON response, in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/responsebody/pending_renewal_info",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " array.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The value for this field should not be interpreted as the customer’s subscription status. You can use this value to display an alternative subscription product in your app, such as a lower-level subscription plan to which the user can downgrade from their current plan.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Consider presenting an attractive upgrade or downgrade offer in the same subscription group, if the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/auto_renew_status",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " value is ",
              "type": "text"
            },
            {
              "code": "“0”",
              "type": "codeVoice"
            },
            {
              "text": ". See ",
              "type": "text"
            },
            {
              "identifier": "link-3230736",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " from WWDC 2018 for more information.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
