  "primaryContentSections": [
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "title": "Possible values",
              "values": [
                "Sandbox",
                "Production"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The environment the system generates the receipt for.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "environment",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An indicator when an error occurs during the request. A value of ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": " indicates a temporary issue; retry validation for this receipt at a later time. A value of ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " indicates an unresolvable issue; don’t retry validation for this receipt. This is applicable only to status codes ",
                  "type": "text"
                },
                {
                  "code": "21100–21199",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "is_retryable",
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ],
          "typeDetails": [
            {
              "baseType": "boolean"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The latest Base64-encoded app receipt. This only returns for receipts that contain auto-renewable subscriptions. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "latest_receipt",
          "type": [
            {
              "kind": "text",
              "text": "byte"
            }
          ],
          "typeDetails": [
            {
              "baseType": "byte"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array that contains all in-app purchase transactions. This excludes transactions for consumable products that your app marks as finished. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "latest_receipt_info",
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/responsebody/latest_receipt_info",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:responseBody.Latest_receipt_info",
              "text": "responseBody.Latest_receipt_info"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ],
          "typeDetails": [
            {
              "arrayMode": true,
              "baseType": "dictionary"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "In the JSON file, an array where each element contains the pending renewal information for each auto-renewable subscription the ",
                  "type": "text"
                },
                {
                  "code": "product_id",
                  "type": "codeVoice"
                },
                {
                  "text": " identifies. This only returns for app receipts that contain auto-renewable subscriptions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "pending_renewal_info",
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/responsebody/pending_renewal_info",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:responseBody.Pending_renewal_info",
              "text": "responseBody.Pending_renewal_info"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ],
          "typeDetails": [
            {
              "arrayMode": true,
              "baseType": "dictionary"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A JSON representation of the receipt that you send for verification. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "receipt",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/responsebody/receipt",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:responseBody.Receipt",
              "text": "responseBody.Receipt"
            }
          ],
          "typeDetails": [
            {
              "baseType": "dictionary"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Either ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " if the receipt is valid, or a status code if there’s an error. The status code reflects the status of the app receipt as a whole. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/status",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for possible status codes and descriptions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "status",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/status",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:status",
              "text": "status"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
