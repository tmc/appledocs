  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "App Store Receipts"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "string expiration_intent"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "kind": "possibleValues",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The customer canceled their subscription.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Billing error; for example, the customer’s payment information is no longer valid.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The customer didn’t consent to an auto-renewable subscription price increase that requires customer consent, allowing the subscription to expire.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "3"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The product wasn’t available for purchase at the time of renewal.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "4"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The subscription expired for some other reason.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "5"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This field is returned in the JSON response, in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/responsebody/pending_renewal_info",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " array.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this value to do the following:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the value is ",
                      "type": "text"
                    },
                    {
                      "code": "\"1\"",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", decide whether to survey the subscribers who have opted in to an account on your system or show alternative subscription products within the same group. Decide whether to present a subscription offer to win back the user.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the value is ",
                      "type": "text"
                    },
                    {
                      "code": "\"2\"",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", decide whether to show the same or alternative subscription products because the user didn't actively make the choice to unsubscribe. ",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "For more information, see ",
              "type": "text"
            },
            {
              "identifier": "link-3230735",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " from WWDC 2018 and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.storekit/documentation/StoreKit/implementing-promotional-offers-in-your-app",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
