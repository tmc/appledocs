  "primaryContentSections": [
    {
      "items": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value for this key corresponds to the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.storekit/documentation/StoreKit/SKPayment/productIdentifier",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " property of the product that the customerâ€™s subscription renews. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "auto_renew_product_id",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The current renewal status for the auto-renewable subscription. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/auto_renew_status",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "auto_renew_status",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/auto_renew_status",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:auto_renew_status",
              "text": "auto_renew_status"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The reason a subscription expired. This field is present only for a receipt that contains an expired auto-renewable subscription. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "expiration_intent",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/expiration_intent",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:expiration_intent",
              "text": "expiration_intent"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time at which the grace period for subscription renewals expires, in a date-time format similar to the ISO 8601.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "grace_period_expires_date",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time at which the grace period for subscription renewals expires, in UNIX epoch time format, in milliseconds. This key is present only for apps that have Billing Grace Period enabled and when the user experiences a billing error at the time of renewal. Use this time format for processing dates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "grace_period_expires_date_ms",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time at which the grace period for subscription renewals expires, in the Pacific Time zone.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "grace_period_expires_date_pst",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A flag that indicates Apple is attempting to renew an expired subscription automatically. This field is present only if an auto-renewable subscription is in the billing retry state. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/is_in_billing_retry_period",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "is_in_billing_retry_period",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/is_in_billing_retry_period",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:is_in_billing_retry_period",
              "text": "is_in_billing_retry_period"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The reference name of a subscription offer that you configured in App Store Connect. This field is present when a customer redeemed a subscription offer code. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/offer_code_ref_name",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "offer_code_ref_name",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/offer_code_ref_name",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:offer_code_ref_name",
              "text": "offer_code_ref_name"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The transaction identifier of the original purchase. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "original_transaction_id",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/original_transaction_id",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:original_transaction_id",
              "text": "original_transaction_id"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "title": "Possible values",
              "values": [
                "1",
                "0"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The price consent status for an auto-renewable subscription price increase that requires customer consent. This field is present only if the App Store requested customer consent for a price increase that requires customer consent. The default value is ",
                  "type": "text"
                },
                {
                  "code": "\"0\"",
                  "type": "codeVoice"
                },
                {
                  "text": " and changes to ",
                  "type": "text"
                },
                {
                  "code": "\"1\"",
                  "type": "codeVoice"
                },
                {
                  "text": " if the customer consents.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "price_consent_status",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier of the product purchased. You provide this value when creating the product in App Store Connect, and it corresponds to the ",
                  "type": "text"
                },
                {
                  "code": "productIdentifier",
                  "type": "codeVoice"
                },
                {
                  "text": " property of the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.storekit/documentation/StoreKit/SKPayment",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object stored in the transaction's payment property.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "product_id",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier of the promotional offer for an auto-renewable subscription that the user redeemed. You provide this value in the Promotional Offer Identifier field when you create the promotional offer in App Store Connect.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "promotional_offer_id",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/promotional_offer_id",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:promotional_offer_id",
              "text": "promotional_offer_id"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "title": "Possible values",
              "values": [
                "1",
                "0"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The status that indicates if an auto-renewable subscription is subject to a price increase.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The price increase status is ",
                  "type": "text"
                },
                {
                  "code": "\"0\"",
                  "type": "codeVoice"
                },
                {
                  "text": " when the App Store has requested consent for an auto-renewable subscription price increase that requires customer consent, and the customer hasn't yet consented.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The price increase status is ",
                  "type": "text"
                },
                {
                  "code": "\"1\"",
                  "type": "codeVoice"
                },
                {
                  "text": " if the customer has consented to a price increase that requires customer consent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The price increase status is also ",
                  "type": "text"
                },
                {
                  "code": "\"1\"",
                  "type": "codeVoice"
                },
                {
                  "text": " if the App Store has notified the customer of the auto-renewable subscription price increase that doesn't require customer consent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "price_increase_status",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In the JSON file, ",
              "type": "text"
            },
            {
              "code": "pending_renewal_info",
              "type": "codeVoice"
            },
            {
              "text": " is an array in which each element contains the pending renewal information for each auto-renewable subscription identified by the ",
              "type": "text"
            },
            {
              "code": "product_id",
              "type": "codeVoice"
            },
            {
              "text": ". A pending renewal may refer to a renewal that the system scheduled in the future or a renewal that failed in the past for some reason.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use this value to get critical information about any pending renewal transactions for an auto-renewable subscription.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "pending_renewal_info",
              "type": "codeVoice"
            },
            {
              "text": " array is returned only for app receipts that contain auto-renewable subscriptions. If customers voluntarily cancel a subscription renewal while in the grace period, the App Store pauses billing retry, and removes the transaction from ",
              "type": "text"
            },
            {
              "code": "pending_renewal_info",
              "type": "codeVoice"
            },
            {
              "text": ". The subscription is in the grace period if the key ",
              "type": "text"
            },
            {
              "code": "grace_period_expires_date_ms",
              "type": "codeVoice"
            },
            {
              "text": " is present and the expiration date hasn't passed.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
