  "primaryContentSections": [
    {
      "items": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "code": "app_item_id",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "adam_id",
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Generated by App Store Connect and used by the App Store to uniquely identify the app purchased. Apps are assigned this identifier only in production. Treat this value as a 64-bit long integer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "app_item_id",
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app’s version number. The app's version number corresponds to the value of ",
                  "type": "text"
                },
                {
                  "code": "CFBundleVersion",
                  "type": "codeVoice"
                },
                {
                  "text": " (in iOS) or ",
                  "type": "text"
                },
                {
                  "code": "CFBundleShortVersionString",
                  "type": "codeVoice"
                },
                {
                  "text": " (in macOS) in the ",
                  "type": "text"
                },
                {
                  "code": "Info.plist",
                  "type": "codeVoice"
                },
                {
                  "text": ". In production, this value is the current version of the app on the device based on the ",
                  "type": "text"
                },
                {
                  "code": "receipt_creation_date_ms",
                  "type": "codeVoice"
                },
                {
                  "text": ". In the sandbox, the value is always ",
                  "type": "text"
                },
                {
                  "code": "\"1.0\"",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "application_version",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle identifier for the app to which the receipt belongs. You provide this string on App Store Connect. This corresponds to the value of ",
                  "type": "text"
                },
                {
                  "code": "CFBundleIdentifier",
                  "type": "codeVoice"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "code": "Info.plist",
                  "type": "codeVoice"
                },
                {
                  "text": " file of the app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "bundle_id",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier for the app download transaction.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "download_id",
          "type": [
            {
              "kind": "text",
              "text": "long"
            }
          ],
          "typeDetails": [
            {
              "baseType": "long"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the receipt expires for apps purchased through the Volume Purchase Program, in a date-time format similar to the ISO 8601.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "expiration_date",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the receipt expires for apps purchased through the Volume Purchase Program, in UNIX epoch time format, in milliseconds. If this key is not present for apps purchased through the Volume Purchase Program, the receipt does not expire. Use this time format for processing dates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "expiration_date_ms",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the receipt expires for apps purchased through the Volume Purchase Program, in the Pacific Time zone. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "expiration_date_pst",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array that contains the in-app purchase receipt fields for all in-app purchase transactions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "in_app",
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/appstorereceipts/responsebody/receipt/in_app",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_receipts:responseBody.Receipt.In_app",
              "text": "responseBody.Receipt.In_app"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ],
          "typeDetails": [
            {
              "arrayMode": true,
              "baseType": "dictionary"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The version of the app that the user originally purchased. This value does not change, and corresponds to the value of ",
                  "type": "text"
                },
                {
                  "code": "CFBundleVersion",
                  "type": "codeVoice"
                },
                {
                  "text": " (in iOS) or ",
                  "type": "text"
                },
                {
                  "code": "CFBundleShortVersion",
                  "type": "codeVoice"
                },
                {
                  "text": "String (in macOS) in the ",
                  "type": "text"
                },
                {
                  "code": "Info.plist",
                  "type": "codeVoice"
                },
                {
                  "text": " file of the original purchase. In the sandbox environment, the value is always ",
                  "type": "text"
                },
                {
                  "code": "\"1.0\"",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "original_application_version",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time of the original app purchase, in a date-time format similar to ISO 8601. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "original_purchase_date",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time of the original app purchase, in UNIX epoch time format, in milliseconds. Use this time format for processing dates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "original_purchase_date_ms",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time of the original app purchase, in the Pacific Time zone.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "original_purchase_date_pst",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the user ordered the app available for pre-order, in a date-time format similar to ISO 8601.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "preorder_date",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the user ordered the app available for pre-order, in UNIX epoch time format, in milliseconds. This field is only present if the user pre-orders the app. Use this time format for processing dates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "preorder_date_ms",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the user ordered the app available for pre-order, in the Pacific Time zone.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "preorder_date_pst",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the App Store generated the receipt, in a date-time format similar to ISO 8601.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "receipt_creation_date",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the App Store generated the receipt, in UNIX epoch time format, in milliseconds. Use this time format for processing dates. This value does not change.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "receipt_creation_date_ms",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the App Store generated the receipt, in the Pacific Time zone. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "receipt_creation_date_pst",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "title": "Possible values",
              "values": [
                "Production",
                "ProductionVPP",
                "ProductionSandbox",
                "ProductionVPPSandbox"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of receipt generated. The value corresponds to the environment in which the app or VPP purchase was made.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "receipt_type",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the request to the ",
                  "type": "text"
                },
                {
                  "code": "verifyReceipt",
                  "type": "codeVoice"
                },
                {
                  "text": " endpoint was processed and the response was generated, in a date-time format similar to ISO 8601.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "request_date",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the request to the ",
                  "type": "text"
                },
                {
                  "code": "verifyReceipt",
                  "type": "codeVoice"
                },
                {
                  "text": " endpoint was processed and the response was generated, in UNIX epoch time format, in milliseconds. Use this time format for processing dates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "request_date_ms",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the request to the ",
                  "type": "text"
                },
                {
                  "code": "verifyReceipt",
                  "type": "codeVoice"
                },
                {
                  "text": " endpoint was processed and the response was generated, in the Pacific Time zone.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "request_date_pst",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An arbitrary number that identifies a revision of your app. In the sandbox, this key's value is ",
                  "type": "text"
                },
                {
                  "code": "“0”",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "version_external_identifier",
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
