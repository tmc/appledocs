            "parameters": [
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "An asset containing meshes whose volume is to be modeled as voxels.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "asset"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The number of divisions to create along the y-axis of the asset’s bounding box; that is, the height in voxels of the resulting voxel array.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "divisions"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The number of concentric shells of voxels to generate inside the object’s surface.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "interiorShells"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The number of concentric shells of voxels to generate outside the object’s surface.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "exteriorShells"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The largest radius, in world space units, of patches to apply to create closed volumes from meshes in the asset.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "patchRadius"
              }
            ]
          }
