  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This sample code project demonstrates how to create and display 3D objects using predefined methods in ",
              "type": "text"
            },
            {
              "code": "MeshResource",
              "type": "codeVoice"
            },
            {
              "text": ", such as ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/RealityKit/MeshResource/generateBox(size:cornerRadius:)-8em0v",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for creating a box and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/RealityKit/MeshResource/generateSphere(radius:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for creating a sphere. Additionally, it shows how to use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/RealityKit/SimpleMaterial",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to apply a white material. By combining the mesh and material, you can create a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/RealityKit/ModelComponent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for your visionOS app with RealityKit.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The sample app draws the following entities in its main view:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A box",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A rounded box",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A sphere",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A cone",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A cylinder",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "identifier": "sample-3d-shapes-1-main-view.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "RealityKit defines the ",
              "type": "text"
            },
            {
              "code": "Box",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "Sphere",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "Cone",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "Cylinder",
              "type": "codeVoice"
            },
            {
              "text": " types, and the app creates a ",
              "type": "text"
            },
            {
              "code": "roundedBox",
              "type": "codeVoice"
            },
            {
              "text": " type by adding a ",
              "type": "text"
            },
            {
              "code": "cornerRadius",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "Box",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Add-3D-shapes-to-the-view",
          "level": 3,
          "text": "Add 3D shapes to the view",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The appâ€™s main view renders the entity using ",
              "type": "text"
            },
            {
              "code": "RealityView",
              "type": "codeVoice"
            },
            {
              "text": " and adds the shape using the ",
              "type": "text"
            },
            {
              "code": "addGeometryShapes(to:)",
              "type": "codeVoice"
            },
            {
              "text": " method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "import SwiftUI",
            "import RealityKit",
            "",
            "struct ShapesView: View {",
            "    var body: some View {",
            "        RealityView { content in",
            "            addGeometryShapes(to: content)",
            "        }",
            "    }",
            "",
            "    // Positions and adds all geometric shapes to the scene.",
            "    func addGeometryShapes(to content: RealityViewContent) {",
            "        /// An array of all the 3D entities representing different shapes.",
            "        let allGeometryEntities = [",
            "            Primitives3DView.boxEntity,",
            "            Primitives3DView.roundedBoxEntity,",
            "            Primitives3DView.sphereEntity,",
            "            Primitives3DView.coneEntity,",
            "            Primitives3DView.cylinderEntity",
            "        ]",
            "",
            "        var xOffset: Float = -0.25",
            "",
            "        // Position the entities along the x-axis, and add them to the content.",
            "        for entity in allGeometryEntities {",
            "            entity.position.x = xOffset",
            "            content.add(entity)",
            "            xOffset += 0.125",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "addGeometryShapes(to:)",
              "type": "codeVoice"
            },
            {
              "text": " method sets an array of 3D entities in a row along the x-axis, including a box, a rounded box, a sphere, a cone, and a cylinder, with an offset of 0.125 between each.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Create-a-primitives-3D-view-extension",
          "level": 3,
          "text": "Create a primitives 3D view extension",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "Primitives3DView",
              "type": "codeVoice"
            },
            {
              "text": " extension defines the entity types for the box, sphere, cone, and cylinder with white material by:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Creating a material with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/RealityKit/SimpleMaterial",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Generating a mesh with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/RealityKit/MeshResource",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Rendering a 3D model with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/RealityKit/ModelComponent",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "code": [
            "import RealityKit",
            "",
            "extension ShapesView {",
            "    /// The white material that responds to lighting.",
            "    static let whiteMaterial = SimpleMaterial(color: .white, isMetallic: false)",
            "",
            "    /// The entity with a box geometry.",
            "    static let boxEntity: Entity = {",
            "        // Create an entity instance.",
            "        let entity = Entity()",
            "",
            "        // Create a mesh resource.",
            "        let boxSize: Float = 0.1",
            "        let boxMesh = MeshResource.generateBox(size: boxSize)",
            "",
            "        // Add the mesh resource to a model component, and add it to the entity.",
            "        entity.components.set(ModelComponent(mesh: boxMesh, materials: [whiteMaterial]))",
            "",
            "        return entity",
            "    }()",
            "",
            "    // ...",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In this sample code project, the ",
              "type": "text"
            },
            {
              "code": "boxEntity",
              "type": "codeVoice"
            },
            {
              "text": " property creates an entity and generates a box mesh with a length of 0.1 units. Use the mesh and the white material to create a ",
              "type": "text"
            },
            {
              "code": "ModelComponent",
              "type": "codeVoice"
            },
            {
              "text": ", and then set it as a component and return the entity.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Similarly, you can create 3D shapes using other preset meshes, such as those in the example below (a sphere, cone, and cylinder):",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "import RealityKit",
            "",
            "extension Primitives3DView {",
            "    // ...",
            "",
            "    /// The entity with a spherical geometry.",
            "    static let sphereEntity: Entity = {",
            "        // Create an entity instance.",
            "        let entity = Entity()",
            "",
            "        // Create a mesh resource.",
            "        let sphereRadius: Float = 0.05",
            "        let boxMesh = MeshResource.generateSphere(radius: sphereRadius)",
            "",
            "        // Add the mesh resource to a model component, and add it to the entity.",
            "        entity.components.set(ModelComponent(mesh: boxMesh, materials: [whiteMaterial]))",
            "",
            "        return entity",
            "    }()",
            "",
            "    /// The entity with a conical geometry.",
            "    static let coneEntity: Entity = {",
            "        // Create an entity instance.",
            "        let entity = Entity()",
            "",
            "        // Create a mesh resource.",
            "        let coneHeight: Float = 0.1",
            "        let coneRadius: Float = 0.05",
            "        let roundedBoxMesh = MeshResource.generateCone(height: coneHeight, radius: coneRadius)",
            "",
            "        // Add the mesh resource to a model component, and add it to the entity.",
            "        entity.components.set(ModelComponent(mesh: roundedBoxMesh, materials: [whiteMaterial]))",
            "",
            "        return entity",
            "    }()",
            "",
            "    /// The entity with a cylindrical geometry.",
            "    static let cylinderEntity: Entity = {",
            "        // Create an entity instance.",
            "        let entity = Entity()",
            "",
            "        // Create a mesh resource.",
            "        let cylinderHeight: Float = 0.1",
            "        let cylinderRadius: Float = 0.05",
            "        let roundedBoxMesh = MeshResource.generateCylinder(height: cylinderHeight, radius: cylinderRadius)",
            "",
            "        // Add the mesh resource to a model component, and add it to the entity.",
            "        entity.components.set(ModelComponent(mesh: roundedBoxMesh, materials: [whiteMaterial]))",
            "",
            "        return entity",
            "    }()",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
