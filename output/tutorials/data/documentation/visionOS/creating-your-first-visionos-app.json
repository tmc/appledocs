    "doc://com.apple.visionOS/documentation/visionOS/creating-fully-immersive-experiences": {
      "abstract": [
        {
          "text": "Build fully immersive experiences by combining spaces with",
          "type": "text"
        },
        {
          "text": " ",
          "type": "text"
        },
        {
