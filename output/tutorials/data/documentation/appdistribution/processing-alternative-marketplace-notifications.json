  "identifier": {
    "interfaceLanguage": "swift",
    "url": "doc://com.apple.ManagedAppDistribution/documentation/appdistribution/processing-alternative-marketplace-notifications"
  },
  "kind": "article",
  "legalNotices": {
    "copyright": "Copyright &copy; 2025 Apple Inc. All rights reserved.",
