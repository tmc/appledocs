    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The error’s identity.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "errorCode",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "minimumLength",
              "value": "0"
            },
            {
              "kind": "maximumLength",
              "value": "500"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The error’s domain. This is ",
                  "type": "text"
                },
                {
                  "code": "com.apple.sirikitcloudmedia.errorDomain",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "errorDomain",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "minimumLength",
              "value": "0"
            },
            {
              "kind": "maximumLength",
              "value": "2000"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL of the content the client can’t play.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "url",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object that, when present, provides additional information about why the content fails to play.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "underlyingError",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.sirikitcloudmedia/documentation/SiriKitCloudMedia/UnderlyingError",
