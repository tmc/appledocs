      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Move to the next track while playing content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nextTrack",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Move to the beginning of the current track, or to the previous track, while playing content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "previousTrack",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Move forward by a number of seconds.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "skipForward",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Move backward by a number of seconds.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "skipBackward",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Display a skip forward control in the remote UI instead of a next track control when the ",
                  "type": "text"
                },
                {
                  "code": "skipForward",
                  "type": "codeVoice"
                },
                {
                  "text": " control is available.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "preferSkipForward",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Display a skip backward control in the remote UI instead of a next track control when the ",
                  "type": "text"
                },
                {
                  "code": "skipBackward",
                  "type": "codeVoice"
                },
                {
                  "text": " control is available.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "preferSkipBackward",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Scrub to any point in the content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "seekToPlaybackPosition",
          "required": false,
          "type": [
            {
              "kind": "text",
