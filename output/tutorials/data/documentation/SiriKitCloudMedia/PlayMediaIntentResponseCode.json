      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Your service can play the media item. When the client receives this ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.sirikitcloudmedia/documentation/SiriKitCloudMedia/InvocationResponse",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", it sends a request to ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.sirikitcloudmedia/documentation/SiriKitCloudMedia/playMedia-1onzj",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " with the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.sirikitcloudmedia/documentation/SiriKitCloudMedia/UserActivity",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " from this response.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "success"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Your service can’t play the media item because it’s restricted content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "failureRestrictedContent"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user asks to resume playback, but there isn’t any unplayed content in the playback queue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "failureNoUnplayedContent"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Your service doesn’t know how to play media items of this type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "failureUnknownMediaType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user needs to launch your app on their iOS device to resolve a problem.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "failureRequiringAppLaunch"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A failure occurs while handling the intent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "failure"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An unspecified response code.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "unspecified"
        }
      ]
    }
