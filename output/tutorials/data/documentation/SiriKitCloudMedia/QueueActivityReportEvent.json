      ],
      "kind": "declarations"
    },
    {
      "kind": "possibleValues",
      "title": "Possible Values",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A periodic report of playback progress. Use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.sirikitcloudmedia/documentation/SiriKitCloudMedia/PlayMediaControlActivity",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to schedule these reports.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.elapsed"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user pauses playback.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.paused"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user resumes playback after a pause.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.continued"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user fast-forwards the content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.fastForward"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user rewinds the content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.fastRewind"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user uses the ",
                  "type": "text"
                },
                {
                  "code": "seekToPlaybackPosition",
                  "type": "codeVoice"
                },
                {
                  "text": " control.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.scrub"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user skips forward to the next content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.transitioned.skip_next"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user skips backward to the previous content or to the start of the current content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.transitioned.skip_previous"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Playback ends because the user skips forward while playing the last content in the queue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.stopped.skip_past_end"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The current content starts playing because the previous content ends.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.transitioned.naturally"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The client receives a new queue from your service and starts playing that queue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.playing.transitioned.queue_replaced"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The last content in the playback queue finishes playing.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.stopped.naturally"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user selects the like control.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.command.like"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user selects the dislike control.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.command.dislike"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user bookmarks the content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.command.bookmark"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "local.stopped.content_failure"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Events represent a user controlling playback or a natural transition at the end of a piece of content. The client handles playback controls, such as pausing and resuming, then sends these event reports to your service.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To customize which controls are available during playback of a piece of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.sirikitcloudmedia/documentation/SiriKitCloudMedia/Content",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", set its ",
              "type": "text"
            },
            {
              "code": "control",
              "type": "codeVoice"
            },
            {
              "text": " property to a scheme you define in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.sirikitcloudmedia/documentation/SiriKitCloudMedia/QueueControlMapping",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The like and dislike events above indicate when the user presses a corresponding control. These are separate from the user telling Siri they like or dislike some content, which the client reports to the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.sirikitcloudmedia/documentation/SiriKitCloudMedia/updateMediaAffinity",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " endpoint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
