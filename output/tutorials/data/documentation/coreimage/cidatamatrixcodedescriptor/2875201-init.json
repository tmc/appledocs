      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The data to encode in the Data Matrix code.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "errorCorrectedPayload"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of rows in the matrix.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rowCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of columns in the matrix.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "columnCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The code's ECC version.  Valid values are 000, 050, 080, 100, 140, and 200. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Any symbol with an even number of rows and columns will be ECC 200.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "eccVersion"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A Data Matrix code descriptor encoding the specified payload in the specified row and column count under the specified ECC version.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "CIBarcodeGenerator",
              "type": "codeVoice"
            },
            {
              "text": " filter can recreate a Data Matrix code given the descriptor created using this method. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
