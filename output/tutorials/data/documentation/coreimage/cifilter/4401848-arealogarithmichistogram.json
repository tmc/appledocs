    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A 1-pixel-high image containing the calculated histogram",
              "type": "text"
            },
            {
              "code": ".",
              "type": "codeVoice"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This filter calculates histograms of the ",
              "type": "text"
            },
            {
              "code": "red,",
              "type": "codeVoice"
            },
            {
              "code": "green,",
              "type": "codeVoice"
            },
            {
              "code": "blue,",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "alpha",
              "type": "codeVoice"
            },
            {
              "text": " colors for the specified area of an image. A base two-logarithm function is applied to the values before binning. The ",
              "type": "text"
            },
            {
              "code": "count",
              "type": "codeVoice"
            },
            {
              "text": " property controls the number of bins (or width) of the histogram. The histogram is scaled so that all the values sum to ",
              "type": "text"
            },
            {
              "code": "scale",
              "type": "codeVoice"
            },
            {
              "text": ". ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "An image with the type ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "inputImage",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/corefoundation/cgrect",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " that specifies the subregion of the image you want to process. ",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "extent",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The scale value for the histogram values. If the scale is ",
                        "type": "text"
                      },
                      {
                        "code": "1",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", then the bins in the resulting image sum to ",
                        "type": "text"
                      },
                      {
                        "code": "1",
                        "type": "codeVoice"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "scale",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The number of bins for the histogram. This value determines the width of the output image. Minimum value ",
                        "type": "text"
                      },
                      {
                        "code": "1",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", and maximum value ",
                        "type": "text"
                      },
                      {
                        "code": "2048",
                        "type": "codeVoice"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "count",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The minimum of the range of color channel values in the logarithmic histogram image. Defaults to ",
                        "type": "text"
                      },
                      {
                        "code": "-10",
                        "type": "codeVoice"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "minimumStop",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The maximum of the range of color channel values in the logarithmic histogram image. Defaults to 4.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "maximumStop",
                    "type": "codeVoice"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "inlineContent": [
            {
              "text": "The following code creates a filter that results in a 1-pixel-tall image with a width of 256. The pixel color components contain the logarithmic histogram values:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "func areaLogarithmicHistogram(inputImage: CIImage) -> CIImage {",
            "    let filter = CIFilter.areaLogarithmicHistogram()",
            "    filter.inputImage = inputImage",
            "    filter.count = 256",
            "    filter.scale = 15",
            "    filter.extent = CGRect(",
            "        x: inputImage.extent.width/2-250,",
            "        y: inputImage.extent.height/2-250,",
            "        width: 500,",
            "        height: 500)",
            "    return filter.outputImage!",
            "}"
          ],
          "metadata": {
            "anchor": "4407330",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cifilter/3547122-histogramdisplay",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " filter to display the histogram:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "func logarithmicHistogramDisplay(inputImage: CIImage) -> CIImage {",
            "    let filter = CIFilter.histogramDisplay()",
            "    filter.inputImage = areaLogarithmicHistogram(inputImage: inputImage)",
            "    filter.highLimit = 1",
            "    filter.height = 100",
            "    filter.lowLimit = 0",
            "    return filter.outputImage!",
            "}"
          ],
          "metadata": {
            "anchor": "4407339",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
