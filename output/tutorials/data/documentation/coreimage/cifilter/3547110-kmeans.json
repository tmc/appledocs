      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A one-dimensional ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " containing the colors.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This filter uses the k-means clustering algorithm to find the most common colors in an input image. The result is a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " with ",
              "type": "text"
            },
            {
              "code": "count",
              "type": "codeVoice"
            },
            {
              "text": " x 1 dimensions. Each ",
              "type": "text"
            },
            {
              "code": "RGBA",
              "type": "codeVoice"
            },
            {
              "text": " pixel in the result image represents the center of a k-means cluster. The ",
              "type": "text"
            },
            {
              "code": "RGB",
              "type": "codeVoice"
            },
            {
              "text": " components contain the color and the alpha component represents the weight of the color. You typically use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cifilter/3547110-kmeans",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " filter in conjunction with the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cifilter/3228378-palettize",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " filter to produce an image with a reduced number of colors.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage",
                        "isActive": false,
                        "type": "reference"
                      },
                      {
                        "text": " to process.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "inputImage",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/corefoundation/cgrect",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " specifying the area of the image to analyze. ",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "extent",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "An optional ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " containing a set of colors to use as seeds for the k-means clustering. ",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "means",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The number of k-means color clusters that should be created. Maximum is ",
                        "type": "text"
                      },
                      {
                        "code": "128",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", and default is ",
                        "type": "text"
                      },
                      {
                        "code": "8",
                        "type": "codeVoice"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "count",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The number of k-means passes that should run. Maximum is ",
                        "type": "text"
                      },
                      {
                        "code": "20",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", and default is ",
                        "type": "text"
                      },
                      {
                        "code": "5",
                        "type": "codeVoice"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "passes",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Whether the k-means color palette should use a perceptual color space.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "perceptual",
                    "type": "codeVoice"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The colors in the result of the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreimage/cifilter/3547110-kmeans",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": " filter have an alpha component that indicates the weight of the color. You should set this value one using ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage/1645891-settingalphaone",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " before using the palette.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "style": "tip",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "The following code example uses the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cifilter/3547110-kmeans",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " filter followed by the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cifilter/3228378-palettize",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " filter to reduce the colors in the image to four:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "func kMeans(inputImage: CIImage) -> CIImage {",
            "    let filter = CIFilter.kMeans()",
            "    filter.inputImage = inputImage",
            "    filter.extent = inputImage.extent",
            "    filter.count = 4",
            "    filter.passes = 5",
            "    return filter.outputImage!",
            "}",
            "",
            "func palettize(inputImage: CIImage, paletteImage: CIImage) -> CIImage {",
            "    let palettize = CIFilter.palettize()",
            "    palettize.inputImage = inputImage",
            "    palettize.paletteImage = paletteImage",
            "    return palettize.outputImage!",
            "}",
            "",
            "let palette = kMeans(inputImage: image)",
            "let palettized = palettize(inputImage: image, palette.settingAlphaOne(in: palette.extent))"
          ],
          "metadata": {
            "anchor": "4332442",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
