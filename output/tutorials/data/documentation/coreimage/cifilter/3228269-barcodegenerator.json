    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method generates a custom barcode as an image. The effect uses barcode descriptors to specify properties of the generated barcode.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The barcode generator uses the following property:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "An instance of ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/coreimage/cibarcodedescriptor",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " with the input parameters supplied.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "barcodeDescriptor",
                    "type": "codeVoice"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "inlineContent": [
            {
              "text": "The following code creates a filter that generates a QR code containing the text ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "Johnny Appleseed.",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "func barcode(inputMessage: Data) -> CIImage {",
            "   let barcodeGenerator = CIFilter.barcodeGenerator()",
            "    barcodeGenerator.barcodeDescriptor = CIQRCodeDescriptor(payload: inputMessage, symbolVersion: 1, maskPattern: 4, errorCorrectionLevel: .levelL)!",
            "    return barcodeGenerator.outputImage!",
            "}",
            "",
            "let johnnyAppleseed: [UInt8] = [0x41, 0x04, 0xA6, 0xF6, 0x86, 0xE6, 0xE7, 0x92, 0x04, 0x17, 0x07, 0x06, 0xC6, 0x57, 0x36, 0x56, 0x56, 0x40, 0xEC]",
            "let data = Data(johnnyAppleseed)",
            "",
            "let bImage = barcode(inputMessage: data)"
          ],
          "metadata": {
            "anchor": "3546203",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
