        },
        {
          "inlineContent": [
            {
              "text": "An asynchronous ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreimage/cirendertask",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to render the image to the specified destination.",
              "type": "text"
