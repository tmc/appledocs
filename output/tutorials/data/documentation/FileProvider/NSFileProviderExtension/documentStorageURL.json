    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This property contains the URL ",
              "type": "text"
            },
            {
              "code": "<container URL>/File Provider Storage",
              "type": "codeVoice"
            },
            {
              "text": ", where ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "container URL",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": " is the value returned by the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/filemanager/1412643-containerurl",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The container URL refers to an app group container directory used by the ",
              "type": "text"
            },
            {
              "code": "NSFileProviderExtension",
              "type": "codeVoice"
            },
            {
              "text": " extension. You can specify this shared container using the ",
              "type": "text"
            },
            {
              "code": "NSExtensionFileProviderDocumentGroup",
              "type": "codeVoice"
            },
            {
              "text": " key in the File Provider extension’s ",
              "type": "text"
            },
            {
              "code": "info.plist",
              "type": "codeVoice"
            },
            {
              "text": " file.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
