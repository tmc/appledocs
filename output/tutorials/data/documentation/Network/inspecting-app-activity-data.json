{
  "abstract": [
    {
      "text": "Verify that your app accesses only the user data and network resources that you expect it to access.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.network/documentation/Network",
