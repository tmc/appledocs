    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An integer that represents the fine conversion value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fineConversionValue"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the system can lock the postback, reducing the system time",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to deliver a signal.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lockPostback"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional ",
                  "type": "text"
                },
                {
                  "code": "CoarseConversionValue",
                  "type": "codeVoice"
                },
                {
                  "text": " enumeration value that represents the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "coarse conversion value. Defaults to ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "coarseConversionValue"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional array of conversion types the system uses to determine which postbacks to update.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Defaults to ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " and the system updates all types of postbacks by default.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "conversionTypes"
        }
      ]
    }
