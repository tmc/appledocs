    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Subject analysis begins automatically without calling this method moments after the overlay view’s image displays onscreen. The framework ignores calls to this method if subject analysis is already in progress or complete.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "For subject analysis to begin, ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.VisionKit/documentation/VisionKit/ImageAnalysisOverlayView/preferredInteractionTypes",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " needs to contain a subject-related option, such as ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.VisionKit/documentation/VisionKit/ImageAnalysisOverlayView/InteractionTypes/automatic",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.VisionKit/documentation/VisionKit/ImageAnalysisOverlayView/InteractionTypes/imageSubject",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.VisionKit/documentation/VisionKit/ImageAnalysisOverlayView/InteractionTypes/visualLookUp",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
