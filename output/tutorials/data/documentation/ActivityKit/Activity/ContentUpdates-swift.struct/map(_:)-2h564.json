          "content": [
            {
              "inlineContent": [
                {
                  "text": "A mapping closure. ",
                  "type": "text"
                },
                {
                  "code": "transform",
                  "type": "codeVoice"
                },
                {
                  "text": " accepts an element",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of this sequence as its parameter and returns a transformed value of the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "same or of a different type. ",
                  "type": "text"
                },
                {
                  "code": "transform",
                  "type": "codeVoice"
                },
                {
                  "text": " can also throw an error, which",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "ends the transformed sequence.",
                  "type": "text"
                }
              ],
