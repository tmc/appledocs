            },
            {
              "identifier": "doc://com.apple.ActivityKit/documentation/ActivityKit/Activity/request(attributes:contentState:pushType:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " function to specify that you want to use a broadcast channel instead of a token. You can only specify one ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.ActivityKit/documentation/ActivityKit/PushType",
              "isActive": true,
