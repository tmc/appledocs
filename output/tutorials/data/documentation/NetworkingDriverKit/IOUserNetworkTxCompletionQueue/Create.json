          "content": [
            {
              "inlineContent": [
                {
                  "text": "The buffer pool containing the memory for the network packets. This parameter must not be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pool"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The object to use as the owner of the queue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "owner"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of packets in the queue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "capacity"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An identifier for you to use to distinguish the queue from other queues. Specify ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " if you donâ€™t use this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "queueId"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dispatch queue on which to execute tasks.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dispatchQueue"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, the new queue object. It is a programmer error to specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " for this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
