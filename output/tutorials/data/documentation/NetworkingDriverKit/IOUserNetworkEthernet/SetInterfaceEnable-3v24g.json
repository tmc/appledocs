                {
                  "code": "YES",
                  "type": "codeVoice"
                },
                {
                  "text": " to enable the service or ",
                  "type": "text"
                },
                {
                  "code": "NO",
                  "type": "codeVoice"
                },
                {
                  "text": " to disable it.",
                  "type": "text"
