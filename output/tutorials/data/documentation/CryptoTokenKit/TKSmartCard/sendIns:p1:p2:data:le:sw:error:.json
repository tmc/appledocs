    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The instruction code.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ins"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The first parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "p1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The second parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "p2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The data field of the APDU, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " if no input data field should be presentâ€”for example, a ",
                  "type": "text"
                },
                {
                  "code": "case1",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "case2",
                  "type": "codeVoice"
                },
                {
                  "text": " APDU.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The length of the data serves as ",
                  "type": "text"
                },
                {
                  "code": "Lc",
                  "type": "codeVoice"
                },
                {
                  "text": " field of the APDU.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "requestData"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The expected number of bytes to be returned, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " if no output data are expectedâ€”for example, a ",
                  "type": "text"
                },
                {
                  "code": "case1",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "case3",
                  "type": "codeVoice"
                },
                {
                  "text": " APDU. Pass ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to accept as many bytes as the card provides.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "le"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, contains the result code as represented by the first two bytes (",
                  "type": "text"
                },
                {
                  "code": "SW1SW2",
                  "type": "codeVoice"
                },
                {
                  "text": ") of the returned data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sw"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, if an error occurred when attempting to create a session or execute the block, contains details about the error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "error"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The returned data without the first two bytes (",
              "type": "text"
            },
            {
              "code": "SW1SW2",
              "type": "codeVoice"
            },
            {
              "text": "), or ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " if an error occurred.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
