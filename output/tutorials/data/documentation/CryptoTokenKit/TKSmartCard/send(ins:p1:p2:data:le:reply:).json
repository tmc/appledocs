    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The instruction code.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ins"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The first parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "p1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The second parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "p2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The data field, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " if no input data field is present—for example, a ",
                  "type": "text"
                },
                {
                  "code": "case1",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "case2",
                  "type": "codeVoice"
                },
                {
                  "text": " APDU.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The length of the data serves as ",
                  "type": "text"
                },
                {
                  "code": "Lc",
                  "type": "codeVoice"
                },
                {
                  "text": " field of the APDU.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The expected number of bytes to be returned, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " if no output data are expected—for example, a ",
                  "type": "text"
                },
                {
                  "code": "case1",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "case3",
                  "type": "codeVoice"
                },
                {
                  "text": " APDU. Pass ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to accept as many bytes as the card provides.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "le"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure to call when the response is returned from the card.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "The returned data without the first two bytes (",
                            "type": "text"
                          },
                          {
                            "code": "SW1SW2",
                            "type": "codeVoice"
                          },
                          {
                            "text": "), or ",
                            "type": "text"
                          },
                          {
                            "code": "nil",
                            "type": "codeVoice"
                          },
                          {
                            "text": " if an error occurred.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "text": "replyData",
                        "type": "text"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "The result code as represented by the first two bytes (",
                            "type": "text"
                          },
                          {
                            "code": "SW1SW2",
                            "type": "codeVoice"
                          },
                          {
                            "text": ") of the returned data.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "text": "sw",
                        "type": "text"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "If a communication error occurred or the result code is anything other than ",
                            "type": "text"
                          },
                          {
                            "code": "0x9000",
                            "type": "codeVoice"
                          },
                          {
                            "text": ", contains details about the error.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "text": "error",
                        "type": "text"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            }
          ],
          "name": "reply"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This call handles all ISO7816-4 APDU cases, translating to proper the sequences according to the protocol.  It consults the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cryptotokenkit/documentation/CryptoTokenKit/TKSmartCard/useExtendedLength",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cryptotokenkit/documentation/CryptoTokenKit/TKSmartCard/useCommandChaining",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " properties and uses these modes whenever appropriate for sending the APDU request.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
