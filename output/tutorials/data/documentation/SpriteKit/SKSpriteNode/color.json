            {
              "identifier": "doc://com.apple.spritekit/documentation/SpriteKit/SKSpriteNode/texture",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", the color (including the alpha component) is used to draw a single-color rectangle.",
              "type": "text"
            }
          ],
