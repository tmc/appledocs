{
  "abstract": [
    {
      "text": "Shape a physics body to your graphics for the right blend of collision accuracy and performance.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
