    "doc://com.apple.spritekit/documentation/SpriteKit/SKRepeatMode/clamp": {
      "abstract": [
        {
          "text": "When a sample is calculated, the time value is clamped to the range of time values found in the sequence. For example, if the last keyframe’s time value is ",
          "type": "text"
        },
        {
          "code": "0.5",
          "type": "codeVoice"
        },
        {
          "text": ", a sample at any time value from ",
          "type": "text"
        },
        {
          "code": "0.5",
          "type": "codeVoice"
        },
        {
          "text": " to ",
          "type": "text"
        },
        {
          "code": "1.0",
          "type": "codeVoice"
        },
        {
          "text": " returns the last keyframe’s value.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "keyword",
          "text": "case"
        },
        {
          "kind": "text",
          "text": " "
        },
        {
          "kind": "identifier",
