      "abstract": [
        {
          "text": "A ",
          "type": "text"
        },
        {
          "code": "SafariExtensionCompanion",
          "type": "codeVoice"
        },
        {
          "text": " object is an app extension for your macOS app that receives and responds to secure messages from your Safari extension. Sending a message to a Safari extension companion is similar to sending a message to an injected script; simply call the ",
          "type": "text"
        },
        {
          "code": "dispatchMessage",
          "type": "codeVoice"
        },
        {
          "text": " method on the extension companion object. See ",
          "type": "text"
        },
        {
          "text": " for more information on extension companions.",
          "type": "text"
        }
      ],
