  "abstract": [
    {
      "text": "Instances of the ",
      "type": "text"
    },
    {
      "code": "SafariExtensionsSettingsChangeEvent",
      "type": "codeVoice"
    },
    {
      "text": " class are used to provide a description of a change made to the extensionâ€™s settings. A settings change event is sent when your extension changes a setting as well as when a setting is changed outside your extension (for example, a setting changed in the Safari preferences).",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "This API is deprecated in Safari 13 and later. Add features to the Safari browser with Safari web extensions or Safari app extensions instead. To convert your Legacy Safari Extension to a Safari app extension, see ",
