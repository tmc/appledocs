{
  "abstract": [
    {
      "text": "Instances of the ",
      "type": "text"
    },
    {
      "code": "SafariBrowserWindow",
      "type": "codeVoice"
    },
    {
      "text": " class represent browser windows. Each window contains one or more tabs, which display web content.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
