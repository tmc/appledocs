{
  "abstract": [
    {
      "text": "The ",
      "type": "text"
    },
    {
      "code": "SafariExtensionsSettings",
      "type": "codeVoice"
    },
    {
      "text": " class provides a place for your extension to store settings that should persist between sessions.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "This API is deprecated in Safari 13 and later. Add features to the Safari browser with Safari web extensions or Safari app extensions instead. To convert your Legacy Safari Extension to a Safari app extension, see ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.safariservices/documentation/SafariServices/converting-a-legacy-safari-extension-to-a-safari-app-extension",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": ". To start developing Safari web extensions, see ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.safariservices/documentation/SafariServices/safari-web-extensions",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": ". To start developing Safari app extensions, see ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.safariservices/documentation/SafariServices/building-a-safari-app-extension",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": ".",
          "type": "text"
        }
      ],
      "type": "paragraph"
    }
  ],
