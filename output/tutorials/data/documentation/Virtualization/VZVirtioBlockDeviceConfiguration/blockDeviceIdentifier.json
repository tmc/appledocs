    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "blockDeviceIdentifier",
              "type": "codeVoice"
            },
            {
              "text": " to name devices so theyâ€™re more discoverable in the Linux guest. The identifier must be an ASCII encodable string of 20 bytes or less.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Validate the identifier string using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.virtualization/documentation/Virtualization/VZVirtioBlockDeviceConfiguration/validateBlockDeviceIdentifier(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " before attempting to set this property, for example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let idString = \"ProjectData\"",
            "do {",
            "    try VZVirtioBlockDeviceConfiguration.validateBlockDeviceIdentifier(idString)",
            "    blockDeviceIdentifier = idString",
            "} catch {",
            "    // Handle error as appropriate.",
            "    throw error",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Setting the identifier to an invalid string results in a fatal error that terminates the app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Warning",
          "style": "warning",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "In a Linux guest, device identifiers are visible in the ",
              "type": "text"
            },
            {
              "code": "/dev/disk/by-id/",
              "type": "codeVoice"
            },
            {
              "text": " directory.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
