    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/filehandle",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to a block device to attach to this VM.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fileHandle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether this disk attachment is read-only; otherwise, if the file handle allows writes, the device can write data into it.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "readOnly"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.virtualization/documentation/Virtualization/VZDiskSynchronizationMode",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " value that defines how the disk synchronizes with the underlying storage when the guest operating system flushes data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "synchronizationMode"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The disk attachment retains the file handle, and the handle must be open when the virtual machine starts.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "readOnly",
              "type": "codeVoice"
            },
            {
              "text": " parameter affects how the Virtualization framework exposes the disk to the guest operating system by the storage controller. If you intend to use the disk in read-only mode, itâ€™s also a best practice to open the file handle as read-only.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
