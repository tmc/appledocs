      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Because playground pages and the always-on live view run in separate processes, you use a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/playgroundsupport/playgroundremoteliveviewproxy",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " as a wrapper for communicating with the always-on live view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "An instance of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/playgroundsupport/playgroundremoteliveviewproxy",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " is automatically created when you've configured the always-on live view in a playground page's ",
              "type": "text"
            },
            {
              "code": "LiveView.swift",
              "type": "codeVoice"
            },
            {
              "text": " file. It's assigned as the value of ",
              "type": "text"
            },
            {
              "code": "PlaygroundPage.current.liveView",
              "type": "codeVoice"
            },
            {
              "text": " only when using always-on live view. Otherwise, that property is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " or a local view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "guard let remoteView = PlaygroundPage.current.liveView as? PlaygroundRemoteLiveViewProxy else {",
            "    fatalError(\"The always-on live view wasn't configured in this page's LiveView.swift.\")",
            "}"
          ],
          "metadata": {
            "anchor": "3036822",
            "title": "Listing 1"
          },
