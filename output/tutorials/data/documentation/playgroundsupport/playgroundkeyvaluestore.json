      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The key-value store is available only in Swift Playgrounds and is persistent only in playground books.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below sets the value of the key “animal” in the playground key-value store to “Llama” and then retrieves the value of that key back from the key-value store.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Store a value.",
            "PlaygroundKeyValueStore.current.keyValueStore[\"animal\"] = .string(\"Llama\")",
            "",
            "// Retreive that same value.\t ",
            "var theAnimal: String? = nil",
            "if let keyValue = PlaygroundKeyValueStore.current.keyValueStore[\"animal\"],",
            "    case .string(let animalType) = keyValue {",
            "    theAnimal = animalType",
            "}"
          ],
          "metadata": {
            "anchor": "3034545",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
