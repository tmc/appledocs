        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "colorScheme(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to set the color scheme for the view to which you",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "apply it and any subviews. If you want to set the color scheme for all",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "views in the presentation, use ",
              "type": "text"
            },
            {
              "code": "View/preferredColorScheme(_:)",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
