        {
          "inlineContent": [
            {
              "text": "To set a specific style for all menu instances within a view, use the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "menuStyle(_:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Menu(\"PDF\") {",
            "    Button(\"Open in Preview\", action: openInPreview)",
            "    Button(\"Save as PDF\", action: saveAsPDF)",
            "}",
            ".menuStyle(ButtonMenuStyle())"
          ],
          "syntax": "swift",
