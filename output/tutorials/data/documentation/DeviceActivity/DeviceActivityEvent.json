      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Device activity is the amount of time an application, category, or web domain is frontmost on the screen and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "accumulates based on the time zone of the scheduled start date. Web domain activity includes domains visited in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Safari or any third-party browser that contributes web usage via a ",
              "type": "text"
            },
            {
              "code": "STWebpageController",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
