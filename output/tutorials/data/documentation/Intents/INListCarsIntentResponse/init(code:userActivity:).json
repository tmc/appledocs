      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The response code that indicates success or failure when confirming or handling the intent. For a list of possible values, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.sirikit/documentation/Intents/INListCarsIntentResponseCode",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "code"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A user activity that the system passes to your app if it launches as a result of this response. Use an activity if you need to provide additional information specific to your app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you specify ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system automatically:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Creates a user activity for you.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Sets the activity’s type to the class name of the intent it’s handling.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Sets the activity’s ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.documentation/documentation/foundation/nsuseractivity/1690346-interaction",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " property to an instance of ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.sirikit/documentation/Intents/INInteraction",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " that contains the intent and your response.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "userActivity"
