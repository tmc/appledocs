      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The account containing the funds to transfer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fromAccount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The account to receive the funds.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "toAccount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount of money to transfer between the accounts. When transferring money between accounts, you typically specify a fixed amount of money to transfer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transactionAmount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date on which to transfer the money.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transactionScheduledDate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional note to include with the transaction.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transactionNote"
        }
      ]
    },
