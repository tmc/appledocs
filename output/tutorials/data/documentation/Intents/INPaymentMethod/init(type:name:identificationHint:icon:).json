                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The payment method type. For a list of possible values, see ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.sirikit/documentation/Intents/INPaymentMethodType",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": ".",
