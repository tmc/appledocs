    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A user-defined name that identifies the account in your app. Never include account numbers or other potentially sensitive user information in this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nickname"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string to help the user differentiate between similar accounts. Typically, you use this parameter to specify only the last few digits of the user’s account. Never specify the user’s full account number.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "number"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of the account. Typical account types include checking accounts, savings accounts, credit accounts, and many others. For a complete list, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.sirikit/documentation/Intents/INAccountType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "accountType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The organization that provides the account for the user. For example, this parameter might contain the name of the user’s bank. Never include account numbers or other potentially sensitive user information in this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "organizationName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The primary balance for the account.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "balance"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A secondary balance associated with the account. For example, a credit card account might have points or frequent flier miles based on the user’s purchases.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "secondaryBalance"
        }
      ]
    },
    {
      "content": [
        {
