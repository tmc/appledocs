            {
              "text": "Use the value in this property to limit the total number of reservations you include in your response to this intent. If this property is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", include all of the reservations you found.",
              "type": "text"
            }
          ],
