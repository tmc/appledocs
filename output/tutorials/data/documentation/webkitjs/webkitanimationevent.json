        {
          "code": "elapsedTime",
          "type": "codeVoice"
        },
        {
          "text": " is ",
          "type": "text"
        },
        {
          "code": "0",
          "type": "codeVoice"
        },
        {
          "text": ".",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/webkitjs/webkitanimationevent/1633014-elapsedtime",
