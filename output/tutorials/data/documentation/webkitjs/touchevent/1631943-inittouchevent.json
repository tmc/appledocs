    {
      "kind": "parameters",
      "languages": [
        "data"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of event that occurred.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether an event can bubble. If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the event can bubble; otherwise, it cannot.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "canBubble"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates whether an event can have its default action prevented. If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the default action can be prevented; otherwise, it cannot.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cancelable"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The view (DOM window) in which the event occurred.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "view"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies some detail information about the event depending on the type of event.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "detail"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The x-coordinate of the event’s location in screen coordinates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "screenX"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The y-coordinate of the event’s location in screen coordinates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "screenY"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The x-coordinate of the event’s location relative to the window’s viewport.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientX"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The y-coordinate of the event’s location relative to the window’s viewport.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientY"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the control key is pressed; otherwise, it is not.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ctrlKey"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the alt key is pressed; otherwise, it is not.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "altKey"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the shift key is pressed; otherwise, it is not.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shiftKey"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the meta key is pressed; otherwise, it is not.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "metaKey"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A collection of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/webkitjs/touch",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " objects representing all touches associated with this event.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "touches"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A collection of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/webkitjs/touch",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " objects representing all touches associated with this target.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "targetTouches"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A collection of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/webkitjs/touch",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " objects representing all touches that changed in this event.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "changedTouches"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The distance between two fingers since the start of an event as a multiplier of the initial distance. The initial value is ",
                  "type": "text"
                },
                {
                  "code": "1.0",
                  "type": "codeVoice"
                },
                {
                  "text": ". If less than ",
                  "type": "text"
                },
                {
                  "code": "1.0",
                  "type": "codeVoice"
                },
                {
                  "text": ", the gesture is pinch close (to zoom out). If greater than ",
                  "type": "text"
                },
                {
                  "code": "1.0",
                  "type": "codeVoice"
                },
                {
                  "text": ", the gesture is pinch open (to zoom in).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "scale"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The delta rotation since the start of an event, in degrees, where clockwise is positive and counter-clockwise is negative. The initial value is ",
                  "type": "text"
                },
                {
                  "code": "0.0",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rotation"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to programmatically create a ",
              "type": "text"
            },
            {
              "code": "TouchEvent",
              "type": "codeVoice"
            },
            {
              "text": " object.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
