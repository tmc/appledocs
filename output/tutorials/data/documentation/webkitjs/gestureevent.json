  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Safari Desktop",
            "Safari Mobile"
          ],
          "tokens": [
            {
              "kind": "keyword",
              "text": "interface"
            },
            {
              "kind": "text",
              "text": " "
            },
            {
              "kind": "identifier",
              "text": "GestureEvent"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "GestureEvent",
              "type": "codeVoice"
            },
            {
              "text": " objects are high-level events that encapsulate the low-level ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/webkitjs/touchevent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " objects. Both ",
              "type": "text"
            },
            {
              "code": "GestureEvent",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "TouchEvent",
              "type": "codeVoice"
            },
            {
              "text": " events are sent during a multi-touch sequence. Gesture events contain scaling and rotation information allowing gestures to be combined, if supported by the platform. If not supported, one gesture ends before another starts. Listen for ",
              "type": "text"
            },
            {
              "code": "GestureEvent",
              "type": "codeVoice"
            },
            {
              "text": " events if you want to respond to gestures only, not process the low-level ",
              "type": "text"
            },
            {
              "code": "TouchEvent",
              "type": "codeVoice"
            },
            {
              "text": " objects.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The different types of ",
              "type": "text"
            },
            {
              "code": "GestureEvent",
              "type": "codeVoice"
            },
            {
              "text": " objects that can occur are:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Sent when two or more fingers touch the surface.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "gesturestart",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Sent when fingers are moved during a gesture.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "gesturechange",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Sent when the gesture ends (when there are 1 or 0 fingers touching the surface).",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "gestureend",
                    "type": "codeVoice"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "inlineContent": [
            {
              "text": "For example, for a two finger multi-touch gesture, the events occur in the following sequence:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "touchstart",
                      "type": "codeVoice"
                    },
                    {
                      "text": " for finger 1. Sent when the first finger touches the surface.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "gesturestart",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". Sent when the second finger touches the surface.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "touchstart",
                      "type": "codeVoice"
                    },
                    {
                      "text": " for finger 2. Sent immediately after ",
                      "type": "text"
                    },
                    {
                      "code": "gesturestart",
                      "type": "codeVoice"
                    },
                    {
                      "text": " when the second finger touches the surface.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "gesturechange",
                      "type": "codeVoice"
                    },
                    {
                      "text": " for current gesture. Sent when both fingers move while still touching the surface.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "gestureend",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". Sent when the second finger lifts from the surface.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "touchend",
                      "type": "codeVoice"
                    },
                    {
                      "text": " for finger 2. Sent immediately after ",
                      "type": "text"
                    },
                    {
                      "code": "gestureend",
                      "type": "codeVoice"
                    },
                    {
                      "text": " when the second finger lifts from the surface.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "touchend",
                      "type": "codeVoice"
                    },
                    {
                      "text": " for finger 1. Sent when the first finger lifts from the surface.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "orderedList"
        },
        {
          "inlineContent": [
            {
              "text": "See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/webkitjs/touchevent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " if you want to process just low-level ",
              "type": "text"
            },
            {
              "code": "TouchEvent",
              "type": "codeVoice"
            },
            {
              "text": " objects.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
