{
  "abstract": [
    {
      "text": "The ",
      "type": "text"
    },
    {
      "code": "GestureEvent",
      "type": "codeVoice"
    },
    {
      "text": " class encapsulates information about a multi-touch gesture.",
      "type": "text"
    }
  ],
