      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string containing the text to draw.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "text"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The x-coordinate of the ",
                  "type": "text"
                },
                {
                  "code": "textAlign",
                  "type": "codeVoice"
                },
                {
                  "text": " point (the left edge, right edge, or center of the text).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "x"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The y-coordinate of the ",
                  "type": "text"
                },
                {
                  "code": "textBaseline",
                  "type": "codeVoice"
                },
                {
                  "text": " point.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "y"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum width of the string, in pixels. This parameter is optional. If omitted, the current font size is used and text is clipped if it falls outside the canvas’s clipping region. If a maximum width is specified, the font size is scaled down if necessary to fit the text inside the specified width.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxWidth"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Text is rendered in the current font, with the current ",
              "type": "text"
            },
            {
              "code": "textBaseline",
              "type": "codeVoice"
            },
            {
              "text": " aligned with the y-coordinate and the current ",
              "type": "text"
            },
            {
              "code": "textAlign",
              "type": "codeVoice"
            },
            {
              "text": " point (left, center, or right) aligned with the x-coordinate.  The ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "y",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "maxWidth",
              "type": "codeVoice"
            },
            {
              "text": " parameter values are in the canvas’s current coordinate system, subject to the current transformation matrix (rotation, scale, and so on).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
