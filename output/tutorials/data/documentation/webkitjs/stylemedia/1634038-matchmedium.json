    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " if the media query is logically true; otherwise, ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, ",
              "type": "text"
            },
            {
              "code": "window.styleMedia.matchMedium(\"(color)\")",
              "type": "codeVoice"
            },
            {
              "text": " returns ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " if the display device is a color device. You can also use this method to check whether the browser supports 3D transforms as follows:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "if ('styleMedia' in window && window.styleMedia.matchMedium(\"(-webkit-transform-3d)\")) {",
            "  // Insert 3D code here",
            "}"
          ],
          "metadata": {
            "anchor": "2557277",
            "title": "Listing 1"
          },
          "syntax": "javascript",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Or check to see whether the browser supports animations as follows:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "if ('styleMedia' in window && window.styleMedia.matchMedium(\"(-webkit-animation)\")) {",
            "  // Insert animation code here",
            "}"
          ],
          "metadata": {
            "anchor": "2557280",
            "title": "Listing 2"
          },
          "syntax": "javascript",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
