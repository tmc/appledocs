{
  "abstract": [
    {
      "text": "Help users discover your App Clip by using an NFC-integrated or scan-only App Clip Code.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.appclip/documentation/AppClip"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "swift",
    "url": "doc://com.apple.appclip/documentation/AppClip/creating-app-clip-codes"
  },
  "kind": "article",
  "legalNotices": {
