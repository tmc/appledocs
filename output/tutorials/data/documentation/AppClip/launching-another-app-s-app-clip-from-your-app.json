{
  "abstract": [
    {
      "text": "Enable people to launch another app’s App Clip from your app with App Clip links and offer a rich preview of it with the Link Presentation framework.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
