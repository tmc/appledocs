{
  "abstract": [
    {
      "text": "Debug App Clip invocations, test the launch experience, and verify the configuration of your released App Clip.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.appclip/documentation/AppClip"
      ]
    ]
