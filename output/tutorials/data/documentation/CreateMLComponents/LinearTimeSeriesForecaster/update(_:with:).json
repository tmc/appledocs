    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The model to update.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "model"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A shaped array of windowed features. The shape should be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "[batchSize, inputWindowSize, featureSize]",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "input"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CreateMLComponents/documentation/CreateMLComponents/TimeSeriesForecasterBatches",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to convert a shaped array of features into batches of windowed",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "features and annotations. Here is an example of training a forecaster:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let estimator = LinearTimeSeriesForecaster<Float>(configuration: configuration)",
            "var model = estimator.makeTransformer()",
            "",
            "let batches = try TimeSeriesForecasterBatches(",
            "    features: features,       // shape [N, featureSize]",
            "    annotations: annotations, // shape [N, annotationSize]",
            "    batchSize: 32,",
            "    inputWindowSize: configuration.inputWindowSize,",
            "    forecastWindowSize: configuration.forecastWindowSize,",
            "    shufflesBatches: true",
            ")",
            "",
            "for iteration in 0 ..< configuration.maximumIterationCount {",
            "    for batch in batches {",
            "        let loss = try await estimator.update(&model, with: batch)",
            "        print(\"Loss: \\(loss)\")",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
