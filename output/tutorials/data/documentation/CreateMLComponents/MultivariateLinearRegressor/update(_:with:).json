    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A model to update.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "model"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An annotated batch containing the features and annotations. The last dimension of the features is",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the model’s input size and the last dimension of the annotations is the model’s output size. All the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "leading dimensions of the features must match all leading dimensions of the annotations. For example, the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "feature shape can be ",
                  "type": "text"
                },
                {
                  "code": "[N, X]",
                  "type": "codeVoice"
                },
                {
                  "text": " and the annotation shape can be ",
                  "type": "text"
                },
                {
                  "code": "[N, Y]",
                  "type": "codeVoice"
                },
                {
                  "text": " for ",
                  "type": "text"
                },
                {
                  "code": "N",
                  "type": "codeVoice"
                },
                {
                  "text": " examples where ",
                  "type": "text"
                },
                {
                  "code": "X",
                  "type": "codeVoice"
                },
                {
                  "text": " is the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "input size and ",
                  "type": "text"
                },
                {
                  "code": "Y",
                  "type": "codeVoice"
                },
                {
                  "text": " is the output size.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "input"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The mean squared error for the batch, also known as the batch loss.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
