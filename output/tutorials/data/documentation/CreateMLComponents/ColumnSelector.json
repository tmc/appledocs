    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This estimator applies a non-tabular estimator to a selection of columns. Hereâ€™s an example of normalizing",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "numeric values within each column using a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CreateMLComponents/documentation/CreateMLComponents/StandardScaler",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let numericalScaling = ColumnSelector(",
            "    columns: [\"volume\", \"price\"],",
            "    estimator: NumericImputer<Float>(.mean)",
            "        .appending(StandardScaler<Float>())",
            ")"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In most cases, an inputer must handle missing values.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
