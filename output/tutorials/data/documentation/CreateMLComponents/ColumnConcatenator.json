    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resulting concatenated column contains ",
              "type": "text"
            },
            {
              "code": "MLShapedArray<Scalar>",
              "type": "codeVoice"
            },
            {
              "text": " elements. For example",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "┏━━━┳━━━━━━━━━━┳━━━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┓",
            "┃   ┃ label    ┃ price   ┃ rooms ┃ A     ┃ B     ┃ C     ┃",
            "┃   ┃ <String> ┃ <Int>   ┃ <Int> ┃ <Int> ┃ <Int> ┃ <Int> ┃",
            "┡━━━╇━━━━━━━━━━╇━━━━━━━━━╇━━━━━━━╇━━━━━━━╇━━━━━━━╇━━━━━━━┩",
            "│ 0 │ good     │ 850,000 │     4 │     1 │     0 │     0 │",
            "│ 1 │ bad      │ 700,000 │     3 │     0 │     1 │     0 │",
            "│ 2 │ bad      │ 650,000 │     3 │     0 │     0 │     1 │",
            "│ 3 │ good     │ 600,000 │     2 │     0 │     1 │     0 │",
            "└───┴──────────┴─────────┴───────┴───────┴───────┴───────┘"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "would be concatenated as:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "┏━━━┳━━━━━━━━━━┳━━━━━━━━━━━━━━━━━━━━━━━━┓",
            "┃   ┃ label    ┃ features               ┃",
            "┃   ┃ <String> ┃ <MLShapedArray<Float>> ┃",
            "┡━━━╇━━━━━━━━━━╇━━━━━━━━━━━━━━━━━━━━━━━━┩",
            "│ 0 │ good     │ [850,000, 4, 1, 0, 0]  │",
            "│ 1 │ bad      │ [700,000, 3, 0, 1, 0]  │",
            "│ 2 │ bad      │ [650,000, 3, 0, 0, 1]  │",
            "│ 3 │ good     │ [600,000, 2, 0, 1, 0]  │",
            "└───┴──────────┴────────────────────────┘"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Non-numerical columns are left in the data frame unchanged. Supported numeric types are ",
              "type": "text"
            },
            {
              "code": "Int",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "UInt8",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "Float",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and ",
              "type": "text"
            },
            {
              "code": "Double",
              "type": "codeVoice"
            },
            {
              "text": ". Arrays and shaped arrays of those types as supported, but every array in a given column must have the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "same shape and shaped arrays across columns must have the same shape except for the last dimension.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
