    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Take for example this directory structure:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "/",
            "    fold1-foo-file1.png",
            "    fold1-foo-file2.png",
            "    fold2-foo-file3.png",
            "    fold1-bar-file4.png",
            "    fold1-bar-file5.png",
            "    fold2-bar-file6.png"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When we specify separator as “-” and index as 1, it would produce two labels (foo and bar) with three",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "URLs each.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
