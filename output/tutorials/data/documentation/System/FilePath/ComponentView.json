    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "ComponentView provides access to standard ",
              "type": "text"
            },
            {
              "code": "BidirectionalCollection",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "algorithms for accessing components from the front or back, as well as",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "standard ",
              "type": "text"
            },
            {
              "code": "RangeReplaceableCollection",
              "type": "codeVoice"
            },
            {
              "text": " algorithms for modifying the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "file path using component or range of components granularity.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var path: FilePath = \"/./home/./username/scripts/./tree\"",
            "let scriptIdx = path.components.lastIndex(of: \"scripts\")!",
            "path.components.insert(\"bin\", at: scriptIdx)",
            "// path is \"/./home/./username/bin/scripts/./tree\"",
            "",
            "path.components.removeAll { $0.kind == .currentDirectory }",
            "// path is \"/home/username/bin/scripts/tree\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
