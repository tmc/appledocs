          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure with a pointer parameter",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that points to a null-terminated platform string.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "body",
                  "type": "codeVoice"
                },
                {
                  "text": " has a return value,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that value is also used as the return value for this method.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "body"
        }
      ]
    },
    {
      "content": [
