    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Components can be one of the special directory components (",
              "type": "text"
            },
            {
              "code": ".",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "..",
              "type": "codeVoice"
            },
            {
              "text": ")",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or a file or directory name. Components are never empty and never",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "contain the directory separator.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var path: FilePath = \"/tmp\"",
            "let file: FilePath.Component = \"foo.txt\"",
            "file.kind == .regular           // true",
            "file.extension                  // \"txt\"",
            "path.append(file)               // path is \"/tmp/foo.txt\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
