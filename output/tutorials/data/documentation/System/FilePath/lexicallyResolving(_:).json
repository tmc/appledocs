        {
          "inlineContent": [
            {
              "code": "subpath",
              "type": "codeVoice"
            },
            {
              "text": " will be lexically normalized (see ",
              "type": "text"
            },
            {
              "code": "lexicallyNormalize",
              "type": "codeVoice"
            },
            {
              "text": ") as",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "part of resolution, meaning any contained ",
              "type": "text"
            },
            {
              "code": ".",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "..",
              "type": "codeVoice"
            },
            {
              "text": " components will",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "be collapsed without resolving symlinks. Any root in ",
              "type": "text"
            },
            {
              "code": "subpath",
              "type": "codeVoice"
            },
            {
              "text": " will be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "ignored.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Returns ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " if the result would “escape” from ",
              "type": "text"
            },
            {
              "code": "self",
              "type": "codeVoice"
            },
            {
              "text": " through use of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the special directory component ",
              "type": "text"
            },
            {
              "code": "..",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This is useful for protecting against arbitrary path traversal from an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "untrusted subpath: the result is guaranteed to be lexically contained",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "within ",
              "type": "text"
            },
            {
              "code": "self",
              "type": "codeVoice"
            },
            {
              "text": ". Since this operation does not consult the file system to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "resolve symlinks, any escaping symlinks nested inside of ",
              "type": "text"
            },
            {
              "code": "self",
              "type": "codeVoice"
            },
            {
              "text": " can still",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "be targeted by the result.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let staticContent: FilePath = \"/var/www/my-website/static\"",
            "let links: [FilePath] =",
            "  [\"index.html\", \"/assets/main.css\", \"../../../../etc/passwd\"]",
            "links.map { staticContent.lexicallyResolving($0) }",
            "  // [\"/var/www/my-website/static/index.html\",",
            "  //  \"/var/www/my-website/static/assets/main.css\",",
            "  //  nil]"
          ],
