    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Normally, file descriptors remain open",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "across calls to the ",
              "type": "text"
            },
            {
              "code": "exec(2)",
              "type": "codeVoice"
            },
            {
              "text": " family of functions.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "If you specify this option,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the file descriptor is closed when replacing this process",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with another process.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The state of the file",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "descriptor flags can be inspected using ",
              "type": "text"
            },
            {
              "code": "F_GETFD",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as described in the ",
              "type": "text"
            },
            {
              "code": "fcntl(2)",
              "type": "codeVoice"
            },
            {
              "text": " man page.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The corresponding C constant is ",
              "type": "text"
            },
            {
              "code": "O_CLOEXEC",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
