      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "connect(2)",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "connectx(2)",
              "type": "codeVoice"
            },
            {
              "text": " request was made",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "on an already connected socket,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or a ",
              "type": "text"
            },
            {
              "code": "sendto(2)",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "sendmsg(2)",
              "type": "codeVoice"
            },
            {
              "text": " request was made",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "on a connected socket specified a destination when already connected.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The corresponding C error is ",
              "type": "text"
            },
            {
              "code": "EISCONN",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
