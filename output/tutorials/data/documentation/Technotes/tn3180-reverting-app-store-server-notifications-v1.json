              "isActive": true,
              "overridingTitle": "Modify an App",
              "overridingTitleInlineContent": [
                {
                  "text": "Modify an App",
                  "type": "text"
                }
              ],
              "type": "reference"
            },
            {
              "text": " endpoint.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
