{
  "abstract": [
    {
      "text": "Learn how to configure your App Clip to prevent it from being unavailable in the App Store.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
