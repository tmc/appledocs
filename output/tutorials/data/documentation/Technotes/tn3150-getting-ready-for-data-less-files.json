{
  "abstract": [
    {
      "text": "Understand dataless files and how to minimize the performance impact as the system materializes them.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.technotes/documentation/Technotes"
