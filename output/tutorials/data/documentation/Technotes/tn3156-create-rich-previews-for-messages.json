{
  "abstract": [
    {
      "text": "Learn best practices for creating rich text and image previews for display in the Messages app.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
