{
  "abstract": [
    {
      "text": "Declare the tracking domains you use in your app or third-party SDK in a privacy manifest.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
