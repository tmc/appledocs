    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, a pointer to the memory buffer for storing interrupt-related data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ifmd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, a pointer to the memory buffer for storing data received from the device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rxqmd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, a pointer to the memory buffer for storing data ready to transmit to the device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "txqmd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An existing memory buffer to configure and return in the ",
                  "type": "text"
                },
                {
                  "code": "rxqmd",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter. Specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if you want this method to create the memory buffer for you. If you specify your own buffer object, this method retains it.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "in_rxqmd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An existing memory buffer to configure and return in the ",
                  "type": "text"
                },
                {
                  "code": "txqmd",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter. Specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if you want this method to create the memory buffer for you. If you specify your own buffer object, this method retains it.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "in_txqmd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The offset, in bytes, to the beginning of the ",
                  "type": "text"
                },
                {
                  "code": "in_rxqmd",
                  "type": "codeVoice"
                },
                {
                  "text": " buffer. The offset represents the location at which the system starts accessing the buffer data. Specify ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to indicate the beginning of the memory buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "in_rxqoffset"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The offset, in bytes, to the beginning of the ",
                  "type": "text"
                },
                {
                  "code": "in_txqmd",
                  "type": "codeVoice"
                },
                {
                  "text": " buffer. The offset represents the location at which the system starts accessing the buffer data. Specify ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to indicate the beginning of the memory buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "in_txqoffset"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The base-2 logarithmic size of the buffer in the ",
                  "type": "text"
                },
                {
                  "code": "in_rxqmd",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter. For example, specify ",
                  "type": "text"
                },
                {
                  "code": "14",
                  "type": "codeVoice"
                },
                {
                  "text": " for a buffer that is 16 kilobytes (2^14 bytes) in size. If ",
                  "type": "text"
                },
                {
                  "code": "in_rxqmd",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", this method creates a 16 kilobyte buffer regardless of the value in this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "in_rxqlogsz"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The base-2 logarithmic size of the buffer in the ",
                  "type": "text"
                },
                {
                  "code": "in_txqmd",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter. For example, specify ",
                  "type": "text"
                },
                {
                  "code": "14",
                  "type": "codeVoice"
                },
                {
                  "text": " for a buffer that is 16 kilobytes (2^14 bytes) in size. If ",
                  "type": "text"
                },
                {
                  "code": "in_txqmd",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", this method creates a 16 kilobyte buffer regardless of the value in this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "in_txqlogsz"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
