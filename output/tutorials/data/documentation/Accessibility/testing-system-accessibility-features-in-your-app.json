{
  "abstract": [
    {
      "text": "Confirm that your app provides a good experience for everyone",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "by testing system accessibility settings in Accessibility Inspector.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Accessibility/documentation/Accessibility",
        "doc://com.apple.Accessibility/documentation/Accessibility/accessibility-inspector"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "swift",
    "url": "doc://com.apple.Accessibility/documentation/Accessibility/testing-system-accessibility-features-in-your-app"
  },
