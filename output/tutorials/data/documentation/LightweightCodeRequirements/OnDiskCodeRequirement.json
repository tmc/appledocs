    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "identifier": "doc://com.apple.LightweightCodeRequirements/documentation/LightweightCodeRequirements/OnDiskCodeRequirement",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " objects can only be built using constraints that conform to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.LightweightCodeRequirements/documentation/LightweightCodeRequirements/OnDiskConstraint",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " protocol.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Specifically ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.LightweightCodeRequirements/documentation/LightweightCodeRequirements/OnDiskCodeRequirement",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " objects will be matched against a SecStaticCodeRef. A SecStaticCodeRef may reference",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "any of the following:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The main executable of a bundle",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A specific file on disk",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The region of a Mach-O pertaining to a specific architecture (e.g. x86_64, arm64, arm64e).",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
