      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether to make the palette visible when ",
                  "type": "text"
                },
                {
                  "code": "responder",
                  "type": "codeVoice"
                },
                {
                  "text": " becomes active. Specify ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/swift/true",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to show the palette when the object becomes the first responder.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "visible"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A responder object associated with the tool pickerâ€™s window. Typically, you specify a view capable of becoming the first responder.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "responder"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Each time you call this method with the ",
              "type": "text"
            },
            {
              "code": "visible",
              "type": "codeVoice"
            },
            {
              "text": " parameter set to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/true",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", the tool picker adds ",
              "type": "text"
            },
            {
              "code": "responder",
              "type": "codeVoice"
            },
            {
              "text": " to a list of objects to monitor. When any object in the list becomes the first responder, the tool picker displays the palette. Calling this method with the ",
              "type": "text"
            },
            {
              "code": "visible",
              "type": "codeVoice"
            },
            {
              "text": " parameter set to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/false",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " removes ",
              "type": "text"
            },
            {
              "code": "responder",
              "type": "codeVoice"
            },
            {
              "text": " from the list of monitored objects.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
