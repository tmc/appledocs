    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Compares values from vector ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " to limit scalar ",
              "type": "text"
            },
            {
              "code": "*B",
              "type": "codeVoice"
            },
            {
              "text": ". For inputs greater than or equal to ",
              "type": "text"
            },
            {
              "code": "*B",
              "type": "codeVoice"
            },
            {
              "text": ", scalar ",
              "type": "text"
            },
            {
              "code": "*C",
              "type": "codeVoice"
            },
            {
              "text": " is written to ",
              "type": "text"
            },
            {
              "code": "D",
              "type": "codeVoice"
            },
            {
              "text": " . For inputs  less than ",
              "type": "text"
            },
            {
              "code": "*B",
              "type": "codeVoice"
            },
            {
              "text": ", the negated value of scalar ",
              "type": "text"
            },
            {
              "code": "*C",
              "type": "codeVoice"
            },
            {
              "text": " is written to vector  ",
              "type": "text"
            },
            {
              "code": "D",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This calculates the following:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    for (n = 0; n < N; ++n)",
            "        if (*B <= A[n*IA])",
            "            D[n*ID] = *C;",
            "        else",
            "            D[n*ID] = -(*C);"
          ],
          "syntax": "objc",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
