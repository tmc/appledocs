            {
              "inlineContent": [
                {
                  "text": "The descriptor of the input.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "i_desc"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The descriptor of the output.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "o_desc"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The probability that the layer drops out an element or a group of elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The seed for the random number generator that the layer ignores if zero.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "seed"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An 8-bit bit mask that indicates the dimension of the grouping of the dropout decision.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "control"
        }
      ]
    },
    {
