                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Dropout layers only support arrays with a data type of ",
                  "type": "text"
                },
                {
                  "code": "float",
                  "type": "codeVoice"
                },
                {
                  "text": ". The input shape must be equal to the output shape.",
                  "type": "text"
                }
              ],
