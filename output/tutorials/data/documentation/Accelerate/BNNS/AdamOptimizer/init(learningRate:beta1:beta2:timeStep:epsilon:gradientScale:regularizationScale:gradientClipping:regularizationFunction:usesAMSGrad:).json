      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the learning rate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "learningRate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the first-moment constant, in the range ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "beta1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the second-moment constant, in the range ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "beta2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that’s at least ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": " and represents the optimizer’s current time.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timeStep"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The epsilon value you use to improve numerical stability.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "epsilon"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the gradient scaling factor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "gradientScale"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the regularization scaling factor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "regularizationScale"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The gradient clipping function and bounds.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "gradientClipping"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the regularization function.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "regularizationFunction"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that specifies whether the optimizer should use the AMSGrad variant.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "usesAMSGrad"
        }
      ]
    }
