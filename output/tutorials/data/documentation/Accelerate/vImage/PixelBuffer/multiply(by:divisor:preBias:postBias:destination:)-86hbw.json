      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The 4 x 4 multiplication matrix values in row-major order.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "matrix"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that the function divides the result by.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "divisor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Values that the function adds to the source before multiplication.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "preBias"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that the function adds to the result after multiplication.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "postBias"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The destination pixel buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destination"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function applies the following operation to each pixel:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "p = (source.0 + preBias.0) * matrix.0 +",
            "    (source.1 + preBias.1) * matrix.1 +",
            "    (source.2 + preBias.2) * matrix.2 +",
            "    (source.3 + preBias.3) * matrix.3",
            "destination = (p + postBias)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
