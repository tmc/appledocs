      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that specifies whether the function locks the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/CoreVideo/CVPixelBuffer",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " with the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corevideo/cvpixelbufferlockflags/1457186-readonly",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " flag. If the closure doesnâ€™t modify the data, set this parameter to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "readOnly"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure with a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/CoreVideo/CVPixelBuffer",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " parameter that points to the underlying pixel buffer image data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "body"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this function to pass a vImage pixel buffer to other frameworks. For example, the following code creates a Core Image ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreimage/ciimage",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instance from a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/CoreVideo/CVPixelBuffer",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " that shares its underlying storage with a pixel buffer:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let src = vImage.PixelBuffer<vImage.Interleaved8x4>(",
            "    size: vImage.Size(width: 64, height: 64))",
            "",
            "src.withCVPixelBuffer(readOnly: false) { cvPixelBuffer in",
            "",
            "    let ciImage = CIImage(cvImageBuffer: cvPixelBuffer)",
            "",
            "    // Core Image workflow using `ciImage`",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
