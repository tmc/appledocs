    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The source pixel buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "interleavedBuffer"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this function to deinterleave a pixel buffer and store the result as homogeneous planes that are represented by multiple underlying vImage buffers.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code creates a new ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.accelerate/documentation/Accelerate/vImage/PlanarFx3",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " pixel buffer from a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.accelerate/documentation/Accelerate/vImage/InterleavedFx3",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " pixel buffer:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let src = vImage.PixelBuffer<vImage.InterleavedFx3>(",
            "    pixelValues: [0.25, 0.5, 0.75] as [Float],",
            "    size: vImage.Size(width: 1, height: 1))",
            "",
            "let dest = vImage.PixelBuffer<vImage.PlanarFx3>(interleavedBuffer: src)",
            "",
            "// Prints \"[0.25] [0.5] [0.75]\"",
            "dest.withUnsafePixelBuffers { pixelBuffers in",
            "    for pixelBuffer in pixelBuffers {",
            "        print(pixelBuffer.array)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
