      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The source pixel buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "interleavedBuffer"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function treats floating point pixels represented by the range ",
              "type": "text"
            },
            {
              "code": "0 ... 1",
              "type": "codeVoice"
            },
            {
              "text": " as the ",
              "type": "text"
            },
            {
              "text": "doc://com.apple.documentation/documentation/kernel/uint8",
              "type": "text"
            },
            {
              "text": " range ",
              "type": "text"
            },
            {
              "code": "0 ... 255",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code initializes an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.accelerate/documentation/Accelerate/vImage/InterleavedFx4",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " pixel buffer from a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.accelerate/documentation/Accelerate/vImage/Interleaved8x4",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " pixel buffer:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let src = vImage.PixelBuffer<vImage.Interleaved8x4>(",
            "    pixelValues: [255 / 17,",
            "                  255 / 15,",
            "                  255 / 5,",
            "                  255 / 3] as [UInt8],",
            "    size: vImage.Size(width: 1,",
            "                      height: 1))",
            "",
            "let dest = vImage.PixelBuffer<vImage.InterleavedFx4>(interleavedBuffer: src)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
