    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The multiplication factor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "factor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that the function divides the result by. The function treats ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " as ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "divisor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that the function adds to the source before multiplication.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "preBias"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that the function adds to the result after multiplication.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "postBias"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The destination pixel buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destination"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function applies the following operation to each pixel:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "destination = (((source + preBias) * factor) + postBias) / divisor"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code multiplies each pixel value in an 8-bit planar buffer by ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": " and adds ",
              "type": "text"
            },
            {
              "code": "5",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "  let buffer = vImage.PixelBuffer<vImage.Planar8>(",
            "       pixelValues: [10, 20, 30, 40,",
            "                     50, 60, 70, 80],",
            "       size: vImage.Size(width: 4,",
            "                         height: 2))",
            "",
            "   buffer.multiply(by: 2,",
            "                   divisor: 1,",
            "                   preBias: 0, postBias: 5,",
            "                   destination: buffer)",
            "",
            "   // Prints",
            "   // [  25,  45,  65,  85,",
            "   //   105, 125, 145, 165 ]",
            "   print(buffer.array)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
