      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A tuple of four 8-bit integers with the values 0, 1, 2, and 3, in some order.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "permuteMap"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The destination pixel buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destination"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code reverses the channel ordering of a pixel buffer:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let buffer = vImage.PixelBuffer<vImage.Interleaved16Fx4>(",
            "    pixelValues: [10, 20, 30, 40],",
            "    size: vImage.Size(width: 1,",
            "                      height: 1))",
            "",
            "buffer.permuteChannels(to: (3, 2, 1, 0),",
            "                       destination: buffer)",
            "",
            "// Prints \"[40, 30, 20, 10]\".",
            "print(buffer.array)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
