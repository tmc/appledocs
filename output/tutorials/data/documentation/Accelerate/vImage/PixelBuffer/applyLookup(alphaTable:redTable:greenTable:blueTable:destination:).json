    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A lookup table for the alpha channel that contains 256 ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.accelerate/documentation/Accelerate/Pixel_8",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " values. Pass ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to specify that the function copies the alpha channel unchanged to the destination buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alphaTable"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A lookup table for the red channel that contains 256 ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.accelerate/documentation/Accelerate/Pixel_8",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " values. Pass ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to specify that the function copies the red channel unchanged to the destination buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "redTable"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A lookup table for the green channel that contains 256 ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.accelerate/documentation/Accelerate/Pixel_8",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " values. Pass ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to specify that the function copies the green channel unchanged to the destination buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "greenTable"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A lookup table for the blue channel that contains 256 ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.accelerate/documentation/Accelerate/Pixel_8",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " values. Pass ",
                  "type": "text"
