    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The channel ordering of the source buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "channelOrdering"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function multiplies the color values in each pixel ",
              "type": "text"
            },
            {
              "code": "self",
              "type": "codeVoice"
            },
            {
              "text": " by the corresponding alpha value and copies the alpha value to the destination unchanged. The function treats the values ",
              "type": "text"
            },
            {
              "code": "0 ... 255",
              "type": "codeVoice"
            },
            {
              "text": " in the pixel buffer as the values ",
              "type": "text"
            },
            {
              "code": "0 ... 1",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code multiplies the RGB values ",
              "type": "text"
            },
            {
              "code": "[32, 64, 128]",
              "type": "codeVoice"
            },
            {
              "text": " by the alpha value ",
              "type": "text"
            },
            {
              "code": "128",
              "type": "codeVoice"
            },
            {
              "text": " (interpreted as ",
              "type": "text"
            },
            {
              "code": "0.5",
              "type": "codeVoice"
            },
            {
              "text": "):",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let src = vImage.PixelBuffer<vImage.Interleaved8x4>(",
            "    pixelValues: [127, 32, 64, 128],",
            "    size: vImage.Size(width: 1, height: 1))",
            "",
            "src.premultiply(channelOrdering: .ARGB)",
            "",
            "// Prints \"[127, 16, 32, 64]\".",
            "print(src.array)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
