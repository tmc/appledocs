      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The type-III DCT uses the following operation for a discrete cosine transform:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// `N` is the length given in the setup.",
            "// `h` is the input array that contains real numbers.",
            "// `H` is the output array that contains real numbers.",
            "",
            "For 0 <= k < N",
            "    Or[k] = Ir[0]/2",
            "        + sum(Ir[j] * cos((k+1/2) * j * pi / N), 1 <= j < N)"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
