    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function calculates the division of the first ",
              "type": "text"
            },
            {
              "code": "N",
              "type": "codeVoice"
            },
            {
              "text": " elements of input vectors ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "B",
              "type": "codeVoice"
            },
            {
              "text": ", and writes the result to output vector ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": ". ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " for (n = 0; n < N; ++n)",
            "    C[n] = A[n] / B[n];"
          ],
          "metadata": {
            "anchor": "4336920",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
