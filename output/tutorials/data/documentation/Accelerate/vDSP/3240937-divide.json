        {
          "inlineContent": [
            {
              "text": "The following code shows an example of using this function:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let a: [Float] = [10, 20, 30, 40, 50]",
            "    let b: [Float] = [ 1,  2,  3,  4,  5]",
            "    ",
            "    let c = vDSP.divide(a, b)",
            "    ",
            "    // Prints \"[10.0, 10.0, 10.0, 10.0, 10.0]\".",
            "    print(c)"
