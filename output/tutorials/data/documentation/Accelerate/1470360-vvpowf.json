          "code": [
            "var x: [Float] = [3, 2, 10, 6]",
            "var y: [Float] = [2, 4, 3, 2]",
            "var z = [Float](repeating: 0, count: x.count)",
            "var n = Int32(x.count)",
            " ",
            "vvpowf(&z, &y, &x, &n)",
            " ",
            "print(z) // [9.0, 16.0, 1000.0, 36.0]"
