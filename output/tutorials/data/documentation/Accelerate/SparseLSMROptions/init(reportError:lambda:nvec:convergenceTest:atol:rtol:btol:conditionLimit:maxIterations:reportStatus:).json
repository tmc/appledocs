    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional status-reporting routine.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reportError"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The damping parameter lambda for regularized least squares.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lambda"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of vectors to use for local reorthogonalization.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nvec"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The convergence test to use for iterative solve methods.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "convergenceTest"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The absolute tolerance (default test) or ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "A",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " tolerance (Fong-Saunders test).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "atol"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The relative convergence tolerance (default test only).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rtol"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "B",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " tolerance (Fong-Saunders test only).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "btol"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The condition number limit (Fong-Saunders test only).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "conditionLimit"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of iterations.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxIterations"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional error-reporting routine.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reportStatus"
        }
      ]
    }
