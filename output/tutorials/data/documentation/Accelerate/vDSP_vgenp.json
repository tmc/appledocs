    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following code creates a five-element vector from two values using linear interpolation. The last index in ",
              "type": "text"
            },
            {
              "code": "indices",
              "type": "codeVoice"
            },
            {
              "text": " determines the length of the operation result.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let values: [Float] = [0, 100]",
            "    let indices: [Float] = [0, 4]",
            "    ",
            "    let n = Int(indices.last! + 1)",
            "    let m = values.count",
            "    ",
            "    let stride = 1",
            "    ",
            "    let interpolatedValues = [Float](unsafeUninitializedCapacity: n) {",
            "        buffer, initializedCount in",
            "        ",
            "        vDSP_vgenp(values, stride,",
            "                   indices, stride,",
            "                   buffer.baseAddress!, stride,",
            "                   vDSP_Length(n),",
            "                   vDSP_Length(m))",
            "        ",
            "        initializedCount = n",
            "    }",
            "    ",
            "    // Prints \"[0.0, 25.0, 50.0, 75.0, 100.0]\".",
            "    print(interpolatedValues)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
