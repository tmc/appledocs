    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the learning rate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "learning_rate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the first moment constant in the range 0 to 1.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "beta1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the second moment constant in the range 0 to 1.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "beta2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that’s at least 1 and represents the optimizer’s current time.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "time_step"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An addition for the division in the parameter update stage.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "epsilon"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the gradient scaling factor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "gradient_scale"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies the regularization scaling factor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "regularization_scale"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The variable that specifies the regularization function. The AdamW optimizer ignores this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "regularization_func"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The clipping function.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clipping_func"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The minimum clipping value for clipping by value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clip_gradients_min"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum clipping value for clipping by value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clip_gradients_max"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum Euclidean norm for clipping by norm and clipping by global norm.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clip_gradients_max_norm"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional value for a known Euclidean norm for clipping by global norm. Set to ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to specify that the function computes the norm.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clip_gradients_use_norm"
        }
      ]
    }
  ],
