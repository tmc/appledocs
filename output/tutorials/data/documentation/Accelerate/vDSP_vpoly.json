    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code evaluates the polynomial with the coefficients ",
              "type": "text"
            },
            {
              "code": "[10.0, 20.0, 30.0]",
              "type": "codeVoice"
            },
            {
              "text": " and the variables ",
              "type": "text"
            },
            {
              "code": "[7.0, 5.0]",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let stride = 1",
            "    ",
            "    let coefficients: [Float] = [10, 20, 30]",
            "    let variables: [Float] = [7, 5]",
            "    ",
            "    let result = [Float](",
            "        unsafeUninitializedCapacity: variables.count) {",
            "            buffer, initializedCount in",
            "            ",
            "            vDSP_vpoly(coefficients, stride, ",
            "                       variables, stride,",
            "                       buffer.baseAddress!, stride,",
            "                       vDSP_Length(variables.count),",
            "                       vDSP_Length(coefficients.count - 1))",
            "            ",
            "            initializedCount = 2",
            "        }",
            "   ",
            "    // Prints \"[660.0, 380.0]\".",
            "    //    result[0] = (10 * 7²) + (20 * 7¹) + (30 * 7⁰) = 660",
            "    //    result[1] = (10 * 5²) + (20 * 5¹) + (30 * 5⁰) = 380",
            "    print(result)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
