    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following code performs a tapered merge between two vectors that represent sine waves at different frequencies:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let count = 1024",
            "    ",
            "    let vectorA: [Double] = (0 ..< count).map {",
            "        return sin(Double($0) * 0.4)",
            "    }",
            "    ",
            "    let vectorB: [Double] = (0 ..< count).map {",
            "        return sin(Double($0) * 0.025)",
            "    }",
            "    ",
            "    let stride = 1",
            "    let n = vDSP_Length(count)",
            "    ",
            "    let tapered = [Double](unsafeUninitializedCapacity: count) {",
            "        buffer, initializedCount in",
            "        ",
            "        vDSP_vtmergD(vectorA, stride,",
            "                     vectorB, stride,",
            "                     buffer.baseAddress!, stride,",
            "                     n)",
            "        ",
            "        initializedCount = count",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following image shows the result of the tapered merge in ",
              "type": "text"
            },
            {
              "code": "tapered",
              "type": "codeVoice"
            },
