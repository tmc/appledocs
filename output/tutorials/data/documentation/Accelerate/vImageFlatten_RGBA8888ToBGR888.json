    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The function uses the following calculation to flatten the source image:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " if( isImagePremultiplied )",
            "     color = (color * 255 + (255 - alpha) * backgroundColor + 127) / 255",
            " else",
            "     color = (color * alpha + (255 - alpha) * backgroundColor + 127) / 255"
          ],
          "syntax": "c",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
