    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "text": "doc://com.apple.documentation/documentation/kernel/vdsp_biquad_setup",
                  "type": "text"
                },
                {
                  "text": " object defining the filter to apply.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__Setup"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of single-precision values initialized with direct-form 1 “past” state data for each section of the biquad. The length of the array should be (2 * ",
                  "type": "text"
                },
                {
                  "code": "M",
                  "type": "codeVoice"
                },
                {
                  "text": ") + 2, where ",
                  "type": "text"
                },
                {
                  "code": "M",
                  "type": "codeVoice"
                },
                {
                  "text": " is the number of sections. For each section ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "m",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "Delay",
                  "type": "codeVoice"
                },
                {
                  "text": "[2*",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "m",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": ":2*",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "m",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": "+1] represent the two delayed input values for section ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "m",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "Delay",
                  "type": "codeVoice"
                },
                {
                  "text": "[2*",
                  "type": "text"
                },
                {
                  "code": "M",
                  "type": "codeVoice"
                },
                {
                  "text": ":2*",
                  "type": "text"
                },
                {
                  "code": "M",
                  "type": "codeVoice"
                },
                {
                  "text": "+1] represent the two delayed output values of the filter.  After this function executes, this array contains the final state data of the filters.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__Delay"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of single-precision input data for the channel.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__X"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Stride for ",
                  "type": "text"
                },
                {
                  "code": "X",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__IX"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array to be filled with single-precision output data for the channel.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__Y"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Stride for",
                  "type": "text"
                },
                {
                  "code": "Y",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__IY"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of elements to filter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__N"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function applies a cascaded biquad filter to the input values in ",
              "type": "text"
            },
            {
              "code": "X",
              "type": "codeVoice"
            },
            {
              "text": ", and places the results in ",
              "type": "text"
            },
            {
              "code": "Y",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The state contained in the ",
              "type": "text"
            },
            {
              "code": "setup",
              "type": "codeVoice"
            },
            {
              "text": " object is updated upon return.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
