    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following code shows an example of gathering the values in ",
              "type": "text"
            },
            {
              "code": "source",
              "type": "codeVoice"
            },
            {
              "text": " using the values in ",
              "type": "text"
            },
            {
              "code": "indices",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let source: [Double] = [10, 20,",
            "                            30, 40,",
            "                            50, 60,",
            "                            70, 80]",
            "    ",
            "    let indices: [Double] = [0, 2, 4, 6]",
            "    ",
            "    let n = indices.count",
            "    let stride = 1",
            "    ",
            "    let result = [Double](unsafeUninitializedCapacity: n) {",
            "        buffer, initializedCount in",
            "        ",
            "        vDSP_vindexD(source,",
            "                     indices, stride,",
            "                     buffer.baseAddress!, stride,",
            "                     vDSP_Length(n))",
            "        ",
            "        initializedCount = n",
            "    }",
            "    ",
            "    // Prints \"[10.0, 30.0, 50.0, 70.0]\".",
            "    print(result)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
