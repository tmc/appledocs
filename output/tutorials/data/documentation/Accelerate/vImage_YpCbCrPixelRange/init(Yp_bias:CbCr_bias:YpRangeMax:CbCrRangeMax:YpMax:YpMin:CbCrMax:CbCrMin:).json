    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encoding for ",
                  "type": "text"
                },
                {
                  "code": "Y' = 0.0",
                  "type": "codeVoice"
                },
                {
                  "text": " for this video format (varies by bit depth).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Yp_bias"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encoding for ",
                  "type": "text"
                },
                {
                  "code": "{Cb, Cr} = 0.0",
                  "type": "codeVoice"
                },
                {
                  "text": " for this video format. This is usually the middle of the range of CbCr, not the low end.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "CbCr_bias"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encoding for ",
                  "type": "text"
                },
                {
                  "code": "Y' = 1.0",
                  "type": "codeVoice"
                },
                {
                  "text": " for this video format. For video range, this is typically less than the maximum representable value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "YpRangeMax"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encoding for ",
                  "type": "text"
                },
                {
                  "code": "{Cb, Cr} = 0.5",
                  "type": "codeVoice"
                },
                {
                  "text": " for this video format. This is usually near the high end of the encodable range (e.g. ",
                  "type": "text"
                },
                {
                  "code": "0xf0",
                  "type": "codeVoice"
                },
                {
                  "text": "), if not the maximum encodable value (e.g. ",
                  "type": "text"
                },
                {
                  "code": "0xff",
                  "type": "codeVoice"
                },
                {
                  "text": ").",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "CbCrRangeMax"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encoding for the maximum allowed Y’ value. All values larger than this will be clamped to this value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "YpMax"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encoding of the minimum allowed Y’ value. All values less than this will be clamped to this value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "YpMin"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encoding of the maximum allowed ",
                  "type": "text"
                },
                {
                  "code": "{Cb, Cr}",
                  "type": "codeVoice"
                },
                {
                  "text": " value. All chroma values greater than this value will be clamped to this value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "CbCrMax"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encoding of the minimum allowed ",
                  "type": "text"
                },
                {
                  "code": "{Cb, Cr}",
                  "type": "codeVoice"
                },
                {
                  "text": " value. All chroma values less than this value will be clamped to this value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "CbCrMin"
        }
      ]
    },
