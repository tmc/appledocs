    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function calculates the sums of the first ",
              "type": "text"
            },
            {
              "code": "N",
              "type": "codeVoice"
            },
            {
              "text": " elements of input vectors ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "B",
              "type": "codeVoice"
            },
            {
              "text": ", and writes the result to output vector ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " for (n = 0; n < N; ++n)",
            "    C[n] = A[n] + B[n];"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "media-4389091",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following code shows an example of using this function:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let stride = 1",
            "    let count = 5",
            "",
            "    let a: [Float] = [ 1,  2,  3,  4,  5]",
            "    let b: [Float] = [10, 20, 30, 40, 50]",
            "",
            "    let c = [Float](unsafeUninitializedCapacity: count) {",
            "        buffer, initializedCount in",
            "        ",
            "        vDSP_vadd(a, stride,",
            "                  b, stride,",
            "                  buffer.baseAddress!, stride,",
            "                  vDSP_Length(count))",
            "        ",
            "        initializedCount = count",
            "    }",
            "   ",
            "    // Prints \"[11.0, 22.0, 33.0, 44.0, 55.0]\".",
            "    print(c)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
