          "content": [
            {
              "inlineContent": [
                {
                  "text": "The first input vector, ",
                  "type": "text"
                },
                {
                  "code": "A",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
