    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The FFT setup structure for this transform. The setup’s structure ",
                  "type": "text"
                },
                {
                  "code": "Log2N",
                  "type": "codeVoice"
                },
                {
                  "text": " must be greater than or equal to this function’s ",
                  "type": "text"
                },
                {
                  "code": "Log2N",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__Setup"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the input-output data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__C"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The stride between the elements in ",
                  "type": "text"
                },
                {
                  "code": "C",
                  "type": "codeVoice"
                },
                {
                  "text": ", set to 1 for best performance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__IC"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The base 2 exponent of the number of elements to process. For example, to process 1024 elements, specify 10 for parameter ",
                  "type": "text"
                },
                {
                  "code": "Log2N",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__Log2N"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A flag that specifies the transform direction. Pass ",
                  "type": "text"
                },
                {
                  "text": "doc://com.apple.documentation/documentation/kernel/1645049-anonymous/kfftdirection_forward",
                  "type": "text"
                },
                {
                  "text": " to transform from the time domain to the frequency domain. Pass ",
                  "type": "text"
                },
                {
                  "text": "doc://com.apple.documentation/documentation/kernel/1645049-anonymous/kfftdirection_inverse",
                  "type": "text"
                },
                {
                  "text": " to transform from the frequency domain to the time domain.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__Direction"
        }
      ]
    },
    {
      "kind": "mentions",
