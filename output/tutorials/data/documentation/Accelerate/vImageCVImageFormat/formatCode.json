    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code prints the format code of a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/CoreVideo/kCVPixelFormatType_420YpCbCr8Planar",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " Core Video image format that equals the expected type:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let cvImageFormat = vImageCVImageFormat.make(",
            "    format: .format420YpCbCr8Planar,",
            "    matrix: kvImage_ARGBToYpCbCrMatrix_ITU_R_709_2.pointee,",
            "    chromaSiting: .center,",
            "    colorSpace: CGColorSpace(name: CGColorSpace.sRGB)!,",
            "    alphaIsOpaqueHint: true)!",
            "",
            "// Prints \"true\".",
            "print(cvImageFormat.formatCode == kCVPixelFormatType_420YpCbCr8Planar)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
