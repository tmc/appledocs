    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function calculates the division of the first ",
              "type": "text"
            },
            {
              "code": "N",
              "type": "codeVoice"
            },
            {
              "text": " elements of input vectors ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "B",
              "type": "codeVoice"
            },
            {
              "text": ", and writes the result to output vector ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": ". ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " for (n = 0; n < N; ++n)",
            "    C[n] = B[n] / A[n];"
          ],
          "metadata": {
            "anchor": "4336933",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "media-4336935",
              "metadata": {
                "anchor": "4336935",
                "title": "Figure 1"
              },
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following code shows an example of using this function:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let stride = 1",
            "    let count = 5",
            "    ",
            "    let b: [Double] = [10, 20, 30, 40, 50]",
            "    let a: [Double] = [ 1,  2,  3,  4,  5]",
            "    ",
            "    let c = [Double](unsafeUninitializedCapacity: count) {",
            "        buffer, initializedCount in",
            "        ",
            "        vDSP_vdivD(a, stride,",
            "                   b, stride,",
            "                   buffer.baseAddress!, stride,",
            "                   vDSP_Length(count))",
            "        ",
            "        initializedCount = count",
            "    }",
            "    ",
            "    // Prints \"[10.0, 10.0, 10.0, 10.0, 10.0]\".",
            "    print(c)"
          ],
          "metadata": {
            "anchor": "4336932",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
