      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Single-precision real input vector. The size of ",
                  "type": "text"
                },
                {
                  "code": "A",
                  "type": "codeVoice"
                },
                {
                  "text": " must be at least ",
                  "type": "text"
                },
                {
                  "code": "DF * (N-1) + P",
                  "type": "codeVoice"
                },
                {
                  "text": " (see below).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__A"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Decimation factor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__DF"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Single-precision real filter vector.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__F"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Single-precision real output vector.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__C"
        },
        {
          "content": [
            {
