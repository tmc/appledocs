    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An existing ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.accelerate/documentation/Accelerate/vDSP_DFT_Setup",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " structure that shares memory with the returned setup structure. Pass ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to create an object with newly initialized and allocated memory.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__Previous"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of complex elements to process. The supported values are ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "f",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " * 2**",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "n",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": ", where ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "f",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " is 1, 3, 5, or 15 and ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "n",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " is at least 3.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__Length"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A flag that specifies the transform direction. Pass ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.accelerate/documentation/Accelerate/vDSP_DFT_Direction/FORWARD",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to transform from the time domain to the frequency domain. Pass ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.accelerate/documentation/Accelerate/vDSP_DFT_Direction/INVERSE",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to transform from the frequency domain to the time domain.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "__Direction"
        }
      ]
    },
    {
      "content": [
