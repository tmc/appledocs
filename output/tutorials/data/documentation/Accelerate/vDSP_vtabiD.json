    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following code shows the result of linearly interpolating a lookup table that contains the values ",
              "type": "text"
            },
            {
              "code": "[-10, 0, 100]",
              "type": "codeVoice"
            },
            {
              "text": " using the offsets ",
              "type": "text"
            },
            {
              "code": "[0, 0.5, 1, 1.5, 2]",
              "type": "codeVoice"
            },
            {
              "text": ".  The integer offsets, ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": ", return the corresponding values in the lookup table, ",
              "type": "text"
            },
            {
              "code": "-10",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": ". The noninteger offsets, ",
              "type": "text"
            },
            {
              "code": "0.5",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "1.5",
              "type": "codeVoice"
            },
            {
              "text": ", return the linearly interpolated values between the lookup table values, ",
              "type": "text"
            },
            {
              "code": "-5",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "50",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let offsets: [Double] = [0, 0.5, 1, 1.5, 2]",
            "    let lookupTable: [Double] = [-10, 0, 100]",
            "    ",
            "    var scale: Double = 1",
            "    var baseOffset: Double = 0",
            "    ",
            "    let n = offsets.count",
            "    let m = lookupTable.count",
            "    ",
            "    let stride = 1",
            "    ",
            "    let values = [Double](unsafeUninitializedCapacity: n) {",
            "        buffer, initializedCount in",
            "        ",
            "        vDSP_vtabiD(offsets, stride,",
            "                    &scale,",
            "                    &baseOffset,",
            "                    lookupTable, vDSP_Length(m),",
            "                    buffer.baseAddress!, stride, vDSP_Length(n))",
            "        ",
            "        initializedCount = n",
            "    }",
            "    ",
            "    // Prints \"[-10.0, -5.0, 0.0, 50.0, 100.0]\".",
            "    print(values)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
