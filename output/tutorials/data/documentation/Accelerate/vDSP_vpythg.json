    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function calculates the length of the hypotenuse of ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "n",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": " number of triangles, where ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "n",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": " is the number of elements in the supplied vectors. The differences between corresponding elements of vectors ",
              "type": "text"
            },
            {
              "code": "x0",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "x1",
              "type": "codeVoice"
            },
            {
              "text": " and vectors ",
              "type": "text"
            },
            {
              "code": "y0",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "y1",
              "type": "codeVoice"
            },
            {
              "text": " define the lengths of the two legs of each triangle.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The functions use the following operation:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "for (n = 0; n < N; ++n)",
            "    E[n] = sqrt((A[n]-B[n])**2 + (C[n]-D[n])**2);"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code calculates the hypotenuse of four Pythagorean triples:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let stride = 1",
            "    ",
            "    let a: [Float] = [3, 6, 5, 9]",
            "    let b: [Float] = [0, 0, 0, 0]",
            "    ",
            "    let c: [Float] = [0, 0, 0, 0]",
            "    let d: [Float] = [4, 8, 12, 12]",
            "    ",
            "    let hypotenuses = [Float](",
            "        unsafeUninitializedCapacity: a.count) {",
            "            buffer, initializedCount in",
            "            ",
            "            vDSP_vpythg(",
            "                a, stride,",
            "                b, stride,",
            "                c, stride,",
            "                d, stride,",
            "                buffer.baseAddress!, stride,",
            "                vDSP_Length(a.count)",
            "            )",
            "   ",
            "            initializedCount = a.count",
            "        }",
            "    ",
            "    // Prints \"[5.0, 10.0, 13.0, 15.0]\".",
            "    print(hypotenuses)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
