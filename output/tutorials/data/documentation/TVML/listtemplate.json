  "metadata": {
    "role": "collectionGroup",
    "title": "listTemplate"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "listTemplate",
              "type": "codeVoice"
            },
            {
              "text": " element to display a list of items; for example, a list of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the user’s favorite movies. Whereas you use the catalog template to display categories",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of a product (action movies, comedies, favorite movies), you use the list template",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to display actual contents of one category, such as a list of the user’s favorite",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "movies. The items are listed on the right side of the screen with like items grouped",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "together in a section. A title providing a brief description of the items is contained",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in a header area directly above the listed items. When an item is selected, information",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "about the item is displayed on the left side of the screen. The following figure",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "shows the basic layout for a ",
              "type": "text"
            },
            {
              "code": "listTemplate",
              "type": "codeVoice"
            },
            {
              "text": " page. The theme for the list template",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "defaults to the system preference.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "listtemplate-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Main-Elements",
          "level": 3,
          "text": "Main Elements",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following listing shows the main elements of the ",
              "type": "text"
            },
            {
              "code": "listTemplate",
              "type": "codeVoice"
            },
            {
              "text": " in TVML format.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<listTemplate>",
            "   <banner>",
            "      …",
            "   </banner>",
            "   <list>",
            "      <header>",
            "         …",
            "      </header>",
            "      <section>",
            "         <listItemLockup>",
            "            <title>…</title>",
            "            <relatedContent>",
            "               …",
            "            </relatedContent>",
            "         </listItemLockup",
            "      </section>",
            "   </list>",
            "</listTemplate>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
          "anchor": "Element-Descriptions",
          "level": 4,
          "text": "Element Descriptions",
          "type": "heading"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Element containing background information and the page title.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/banner",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Information describing what a section contains.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/header",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Element containing all content for the ",
                        "type": "text"
                      },
                      {
                        "code": "listTemplate",
                        "type": "codeVoice"
                      },
                      {
                        "text": " page.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/list",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Element containing all information that pertains to one list item, such as an item title and image, as well as related content.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/listitemlockup",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Information that is displayed on the left side of the screen when an item in the list is highlighted.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/relatedcontent",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "An area of the page that groups related elements together as one element, for layout purposes. In this case, the section contains list items.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/section",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following listing shows the TVML for a ",
              "type": "text"
            },
            {
              "code": "listTemplate",
              "type": "codeVoice"
            },
            {
              "text": " example. An image and a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "description that relate to the highlighted item are displayed on the left side of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the screen.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<document>",
            "   <listTemplate>",
            "      <banner>",
            "         <title>Johnny Appleseed's Movie Collection</title>",
            "      </banner>",
            "      <list>",
            "         <header>",
            "            <title>Favorite Movies</title>",
            "         </header>",
            "         <section>",
            "            <listItemLockup>",
            "               <title>Movie 1</title>",
            "               <relatedContent>",
            "                  <lockup>",
            "                     <img src=\"path to images on your server/Car_Movie_1920x1080.png\" width=\"857\" height=\"482\" />",
            "                     <title>Movie 1</title>",
            "                     <description>A brief description for the first movie should go here.</description>",
            "                  </lockup>",
            "               </relatedContent>",
            "            </listItemLockup>",
            "            <listItemLockup>",
            "               <title>Movie 2</title>",
            "               <relatedContent>",
            "                  <lockup>",
            "                     <img src=\"path to images on your server/Car_Movie_800x600.png\" width=\"857\" height=\"482\" />",
            "                     <title>Movie 2</title>",
            "                     <description>A brief description for the second movie should go here.</description>",
            "                  </lockup>",
            "               </relatedContent>",
            "            </listItemLockup>",
            "            <listItemLockup>",
            "               <title>Movie 3</title>",
            "               <relatedContent>",
            "                  <lockup>",
            "                     <img src=\"path to images on your server/Car_Movie_720x720.png\" width=\"857\" height=\"482\" />",
            "                     <title>Movie 3</title>",
            "                     <description>A brief description for the third movie should go here.</description>",
            "                  </lockup>",
            "               </relatedContent>",
            "            </listItemLockup>",
            "         </section>",
            "      </list>",
            "   </listTemplate>",
            "</document>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following figure shows the output for the above example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "listtemplate-2",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
