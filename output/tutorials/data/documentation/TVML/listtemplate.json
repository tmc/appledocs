{
  "abstract": [
    {
      "text": "Displays a list of items along one side of a page and the corresponding image on",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "the other side.",
      "type": "text"
    }
  ],
