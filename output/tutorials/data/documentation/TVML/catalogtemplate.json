  "metadata": {
    "role": "collectionGroup",
    "title": "catalogTemplate"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "catalogTemplate",
              "type": "codeVoice"
            },
            {
              "text": " element to display information about groups of like products;",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for example, a movie catalog that provides categories for dramas, comedies, and all",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "movies. Each group of products is contained in its own section and displayed along",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the left side of the screen. Images depicting the products contained within a selected",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "group are displayed in the related content area on the right side of the screen.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The following figure shows the basic layout for a ",
              "type": "text"
            },
            {
              "code": "catalogTemplate",
              "type": "codeVoice"
            },
            {
              "text": " page. The theme",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for the catalog template defaults to the system preference.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "catalogtemplate-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Main-Elements",
          "level": 3,
          "text": "Main Elements",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following listing shows the main elements of the ",
              "type": "text"
            },
            {
              "code": "catalogTemplate",
              "type": "codeVoice"
            },
            {
              "text": " element in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "TVML format.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<catalogTemplate>",
            "   <banner>",
            "      <title>…</title>",
            "   </banner>",
            "   <list>",
            "      <section>",
            "         <listItemLockup>",
            "            …",
            "         </listItemLockup>",
            "      </section>",
            "      <section>",
            "         <header>",
            "            …",
            "         </header>",
            "         <listItemLockup>",
            "            …",
            "         </listItemLockup>",
            "      </section>",
            "   </list>",
            "</catalogTemplate>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
          "anchor": "Element-Descriptions",
          "level": 4,
          "text": "Element Descriptions",
          "type": "heading"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Information describing what the catalog contains, such as movies.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/banner",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Information describing what one section of the page contains.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/header",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Element containing all content in the template page, except banner information.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/list",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Element containing all information that relates to one list item on the left side of the page, including the item title and label, as well as related content.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/listitemlockup",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "An area of the page containing related elements that are grouped together as one element for layout purposes.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/section",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following listing shows the TVML for a ",
              "type": "text"
            },
            {
              "code": "catalogTemplate",
              "type": "codeVoice"
            },
            {
              "text": " example. The example",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "displays a title along the top of the screen. Two items representing movie categories,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "All Movies and Comedies, are listed along the left side of the screen. Movie posters",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "are presented on the right in a grid format according to which movie category the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "user selects.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<document>",
            "   <catalogTemplate>",
            "      <banner>",
            "         <title>Movies</title>",
            "      </banner>",
            "      <list>",
            "         <section>",
            "            <listItemLockup>",
            "               <title>All Movies</title>",
            "               <decorationLabel>6</decorationLabel>",
            "               <relatedContent>",
            "                  <grid>",
            "                     <section>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375_A.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 1</title>",
            "                        </lockup>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375_B.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 2</title>",
            "                        </lockup>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375_C.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 3</title>",
            "                        </lockup>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 4</title>",
            "                        </lockup>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375_C.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 5</title>",
            "                        </lockup>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 6</title>",
            "                        </lockup>",
            "                     </section>",
            "                  </grid>",
            "               </relatedContent>",
            "            </listItemLockup>",
            "            <listItemLockup>",
            "               <title>Comedies</title>",
            "               <decorationLabel>4</decorationLabel>",
            "               <relatedContent>",
            "                  <grid>",
            "                     <section>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375_B.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 2</title>",
            "                        </lockup>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375_A.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 1</title>",
            "                        </lockup>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 4</title>",
            "                        </lockup>",
            "                        <lockup>",
            "                           <img src=\"path to images on your server/Car_Movie_250x375_C.png\" width=\"250\" height=\"376\" />",
            "                           <title>Movie 3</title>",
            "                        </lockup>",
            "                     </section>",
            "                  </grid>",
            "               </relatedContent>",
            "            </listItemLockup>",
            "         </section>",
            "      </list>",
            "   </catalogTemplate>",
            "</document>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following figure shows the output for the above example:",
              "type": "text"
            }
          ],
