{
  "abstract": [
    {
      "text": "Displays groups of items along one side of a page and images of a group’s contents",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "on the other side.",
      "type": "text"
    }
  ],
