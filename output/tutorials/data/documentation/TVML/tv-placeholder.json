        {
          "inlineContent": [
            {
              "text": "Here’s an example that displays the default movie image.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<lockup>",
            "   <img style=\"tv-placeholder:movie\" width=\"250\" height=\"376\" />",
            "   <title>Movie 1</title>",
            "</lockup>"
