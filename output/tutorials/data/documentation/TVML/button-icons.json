  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Button icons are badges that are added to a ",
              "type": "text"
            },
            {
              "code": "button",
              "type": "codeVoice"
            },
            {
              "text": " element as a visual clue to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "what the button is used for.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "button-add",
          "level": 3,
          "text": "button-add",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the add button icon is ",
              "type": "text"
            },
            {
              "code": "button-add",
              "type": "codeVoice"
            },
            {
              "text": ". Place an add button icon",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "next to content to indicate that the user can add the item. Place the add icon using",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "<badge src=\"resource://button-add\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "button-icons-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "button-checkmark",
          "level": 3,
          "text": "button-checkmark",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the checkmark button icon is ",
              "type": "text"
            },
            {
              "code": "button-checkmark",
              "type": "codeVoice"
            },
            {
              "text": ". Place a checkmark",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "button icon next to content to indicate that an item has been selected. Place the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "checkmark icon using ",
              "type": "text"
            },
            {
              "code": "<badge src=\"resource://button-checkmark\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
