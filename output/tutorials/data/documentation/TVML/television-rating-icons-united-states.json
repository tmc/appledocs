    "title": "Television Rating Icons (United States)"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "United States television rating icons indicate who a program is designed for. Ratings",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "are assigned by the individual broadcast and cable networks. While there is no legal",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "requirement to rate a television program, almost all programs are rated, with the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "exception of sports and news programs.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "For-Children",
          "level": 3,
          "text": "For Children",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the for children icon for the United States is ",
              "type": "text"
            },
            {
              "code": "tv-y",
              "type": "codeVoice"
            },
            {
              "text": ". Place",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a for children rating icon next to content to indicate that the content is suitable",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to be watched by people of any age, but targeted for children 2-6. Place a for children",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tv-y\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "television-rating-icons-united-states-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Ages-7-and-Older",
          "level": 3,
          "text": "Ages 7 and Older",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the ages 7 and older icon for the United States is ",
              "type": "text"
            },
            {
              "code": "tv-y7",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Place an ages 7 and older rating icon next to content to indicate that the content",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is suitable to be watched by children who are 7 and older. Place an ages 7 and older",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tv-y7\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "television-rating-icons-united-states-2",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Adding the “FV” descriptor indicates that the television program contains fantasy",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "violence. The resource name for this icon is ",
              "type": "text"
            },
            {
              "code": "tv-y7fv",
              "type": "codeVoice"
            },
            {
              "text": ". Place this icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tv-y7fv\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "television-rating-icons-united-states-3",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "All-Ages",
          "level": 3,
          "text": "All Ages",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the all ages icon for the United States is ",
              "type": "text"
            },
            {
              "code": "tv-g",
              "type": "codeVoice"
            },
            {
              "text": ". Place an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "all ages rating icon next to content to indicate that the content is suitable to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "be watched by audiences of any age. Pplace an all ages rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tv-g\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "television-rating-icons-united-states-4",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Parental-Guidance",
          "level": 3,
          "text": "Parental Guidance",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the parental guidance icon for the United States is ",
              "type": "text"
            },
            {
              "code": "tv-pg",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Place a parental guidance rating icon next to content to indicate that parents should",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "review the content before letting young children watch. Place a parental guidance",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tv-pg\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "television-rating-icons-united-states-5",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Parents-Strongly-Cautioned",
          "level": 3,
          "text": "Parents Strongly Cautioned",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the parents strongly cautioned icon for the United States is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "tv-14",
              "type": "codeVoice"
            },
            {
              "text": ". Place a parents strongly cautioned rating icon next to content to indicate",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that parents should review the content before allowing children under the age of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "14 to watch. Place a parents strongly cautioned rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tv-14\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "television-rating-icons-united-states-6",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Mature-Audiences",
          "level": 3,
          "text": "Mature Audiences",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the mature audiences icon for the United States is ",
              "type": "text"
            },
            {
              "code": "tv-ma",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Place a mature audiences rating icon next to content to indicate that the content",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is suitable to be watched by adults and may be unsuitable for children under 17.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Place a mature audiences rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tv-ma\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "television-rating-icons-united-states-7",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
