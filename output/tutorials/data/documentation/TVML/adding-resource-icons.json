  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Apple TV contains several icons that are available when you set the ",
              "type": "text"
            },
            {
              "code": "src",
              "type": "codeVoice"
            },
            {
              "text": " attribute",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "on a ",
              "type": "text"
            },
            {
              "code": "badge",
              "type": "codeVoice"
            },
            {
              "text": " element to ",
              "type": "text"
            },
            {
              "code": "resource://<resourcename>",
              "type": "codeVoice"
            },
            {
              "text": ". Here’s an example that sets",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the preview icon on a button:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<buttonLockup>",
            "    <badge src=\"resource://button-preview\" />",
            "    <title>Preview</title>",
            "</buttonLockup>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Resource icons do not have different variants for dark and light themes. Use the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "tv-tint-color",
              "type": "codeVoice"
            },
            {
              "text": " style to change the color of black and white resource icons. Select",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the appropriate tint for dark and light themes by wrapping the ",
              "type": "text"
            },
            {
              "code": "tv-tint-color",
              "type": "codeVoice"
            },
            {
              "text": " style",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "inside of a media query.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
