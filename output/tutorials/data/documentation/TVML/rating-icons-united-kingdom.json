    "title": "Rating Icons (United Kingdom)"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "United Kingdom movie rating icons indicate who is allowed to watch a movie in the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "United Kingdom. Ratings are assigned by the British Board of Film Classification.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "General-Admission",
          "level": 3,
          "text": "General Admission",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the general admission icon for the United Kingdom is ",
              "type": "text"
            },
            {
              "code": "bbfc-u",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Place a general admission rating icon next to content to indicate that the content",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is suitable to be watched by people of any age. Place a general admissions rating",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://bbfc-u\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rating-icons-united-kingdom-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Parental-Guidance",
          "level": 3,
          "text": "Parental Guidance",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the parental guidance icon for the United Kingdom is ",
              "type": "text"
            },
            {
              "code": "bbfc-pg",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Place a parental guidance rating icon next to content to indicate that the content",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can be watched by people of any age, but parental guidance is recommended. Place",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a parental guidance rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://bbfc-pg\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rating-icons-united-kingdom-2",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Suitable-1212a",
          "level": 3,
          "text": "Suitable 12/12a",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the suitable 12 icon for the United Kingdom is ",
              "type": "text"
            },
            {
              "code": "bbfc-12",
              "type": "codeVoice"
            },
            {
              "text": ". Place",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a suitable 12 rating icon next to content to indicate that no one younger than 12",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can rent or buy the content. Place a suitable 12 rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://bbfc-12\" />.",
              "type": "codeVoice"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rating-icons-united-kingdom-3",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the suitable 12a icon for the United Kingdom is ",
              "type": "text"
            },
            {
              "code": "bbfc-12a",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Place a suitable 12a rating icon next to content to indicate that no one younger",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "than 12 can watch the media unless accompanied by an adult. Place a suitable 12a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://bbfc-12a\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rating-icons-united-kingdom-4",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Suitable-15",
          "level": 3,
          "text": "Suitable 15",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the suitable 15 icon for the United Kingdom is ",
              "type": "text"
            },
            {
              "code": "bbfc-15",
              "type": "codeVoice"
            },
            {
              "text": ". Place",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a suitable 15 rating icon next to content to indicate that no one younger than 15",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can see the film in a theater nor can they rent or buy the content. Place a suitable",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "15 rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://bbfc-15\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rating-icons-united-kingdom-5",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Suitable-18",
          "level": 3,
          "text": "Suitable 18",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the suitable 18 icon for the United Kingdom is ",
              "type": "text"
            },
            {
              "code": "bbfc-18",
              "type": "codeVoice"
            },
            {
              "text": ". Place",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a suitable 18 rating icon next to content to indicate that no one younger than 18",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can see the film in a theater nor can they rent or buy the content. Place a suitable",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "18 rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://bbfc-18\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rating-icons-united-kingdom-6",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Restricted-18",
          "level": 3,
          "text": "Restricted 18",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the restricted 18 icon for the United Kingdom is ",
              "type": "text"
            },
            {
              "code": "bbfc-r18",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Place a restricted 18 rating icon next to content to indicate that it can only be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "shown to adults 18 and over in specially licensed cinemas or sold from specially",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "licensed stores. Place a restricted 18 rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://bbfc-r18\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rating-icons-united-kingdom-7",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
