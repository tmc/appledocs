      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Index titles only appear when the user activates fast scrolling.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Values-for-indexTitles",
          "level": 3,
          "text": "Values for indexTitles",
          "type": "heading"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The indices displayed on the index bar. Each index pair is a space-separated value. For example, ",
                        "type": "text"
                      },
                      {
                        "code": "indexTitles=”0 A 109 D 198 G”",
                        "type": "codeVoice"
                      },
                      {
                        "text": " creates an index bar with three entries, ",
                        "type": "text"
                      },
                      {
                        "code": "A",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", ",
                        "type": "text"
                      },
                      {
                        "code": "D",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", and ",
                        "type": "text"
                      },
                      {
                        "code": "G",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", which fast scrolls the focuses to indexes 0, 109, and 198, respectively.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "String",
