          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "progressBar",
              "type": "codeVoice"
            },
            {
              "text": " element creates an empty bar that is overlaid along the bottom",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of an image. A percentage of the bar is then filled based on the value assigned.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This element is commonly used to denote how much of a media item has already been",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
