      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This element can contain information about a movie’s viewing rating or information",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "from a rating service, such as Rotten Tomatoes. Here’s an example that shows a movie",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "has been rated for general audiences.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<ratingCard>",
            "    <title><ratingBadge src=\"resource://mpaa-g\" />General Admission</title>",
            "    <text>Text</text>",
            "    <description>Description</description>",
            "</ratingCard>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
