  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "anchor": "Closed-Captioning",
          "level": 3,
          "text": "Closed Captioning",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the closed captioning icon is cc. Place a closed captioning",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "icon to indicate that the media item is able to display text on the screen. Place",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a closed captioning icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://cc\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "miscellaneous-icons-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "High-Definition",
          "level": 3,
          "text": "High Definition",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the high definition icon is hd. Place a high definition icon",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "next to media items that provide high definition video streams. Place a high definition",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://hd />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "miscellaneous-icons-2",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "List-Chevron",
          "level": 3,
          "text": "List Chevron",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for a list chevron icon is list-chevron. Place a list chevron icon",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to indicate that the item next to the chevron is playing or selectable. Place a list",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "chevron icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://list-chevron\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "miscellaneous-icons-3",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Overlay-Checkmark",
          "level": 3,
          "text": "Overlay Checkmark",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the overlay checkmark icon is overlay-checkmark. Place an overlay",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "checkmark on top of other images to indicate that the user has selected the item.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Place an overlay checkmark icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://overlay-checkmark\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "miscellaneous-icons-4",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
