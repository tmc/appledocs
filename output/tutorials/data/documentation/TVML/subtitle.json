      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "subtitle",
              "type": "codeVoice"
            },
            {
              "text": " element contains text used to describe a displayed element. It is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "commonly used to provide a small amount of information about a product. Here’s an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example that shows the subtitle for a mongoram.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<monogramLockup>",
            "    <monogram firstName=\"Anne\" lastName=\"Johnson\"/>",
            "    <title>Anne Johnson</title>",
            "    <subtitle>Actor</subtitle>",
            "</monogramLockup>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
