  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "ordinal",
              "type": "codeVoice"
            },
            {
              "text": " element displays a number on the screen and provides proper spacing",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "between the text and the number. Use the ",
              "type": "text"
            },
            {
              "code": "ordinal",
              "type": "codeVoice"
            },
            {
              "text": " element to create an ordered list.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Here’s an example that reserves the spacing for two digits.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<listItemLockup>",
            "    <ordinal minLength=\"2\">1</ordinal>",
            "    <title>Opening sequence</title>",
            "    <decorationLabel>11:14</decorationLabel>",
            "</listItemLockup>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
          "anchor": "Contained-In",
          "level": 3,
          "text": "Contained In",
          "type": "heading"
        },
        {
