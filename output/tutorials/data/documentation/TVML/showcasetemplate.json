  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "showcaseTemplate",
              "type": "codeVoice"
            },
            {
              "text": " element to display a row of images with descriptions associated",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with each image; for example, displaying a set of screenshots to promote a movie.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Users can scroll between images. When an image comes into focus, the size of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "image is increased to be slightly larger than the other images. The following figure",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "shows the basic layout for a showcaseTemplate page. The default theme for a showcase",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "template is ",
              "type": "text"
            },
            {
              "code": "dark",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "showcasetemplate-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Main-Elements",
          "level": 3,
          "text": "Main Elements",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following listing shows the main elements of the ",
              "type": "text"
            },
            {
              "code": "showcaseTemplate",
              "type": "codeVoice"
            },
            {
              "text": " element in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "TVML format.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<showcaseTemplate>",
            "    <background>…</background>",
            "    <banner>",
            "        <title>…</title>",
            "        <row>",
            "            <button>…</button>",
            "        </row>",
            "    </banner>",
            "    <carousel>",
            "        <section>",
            "            <lockup>…</lockup>",
            "        </section>",
            "    </carousel>",
            "</showcaseTemplate>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
          "anchor": "Element-Descriptions",
          "level": 4,
          "text": "Element Descriptions",
          "type": "heading"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Background visual and audio.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/background",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Elements that describe what the page shows and provides buttons for user options.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/banner",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A button providing user options.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/button",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Element containing images and text displayed in a row that the user navigates by swiping left or right on the remote.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/carousel",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "An element containing several elements, such as an image and a title, so that it can be treated as a single element.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/lockup",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A group of elements displayed in a horizontal row.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/row",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A group of ",
                        "type": "text"
                      },
                      {
                        "code": "lockup",
                        "type": "codeVoice"
                      },
                      {
                        "text": " elements.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/section",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Text that describes its containing element.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "identifier": "doc://com.apple.TVML/documentation/TVML/title",
                    "isActive": true,
                    "type": "reference"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following listing shows the TVML for a ",
              "type": "text"
            },
            {
              "code": "showcaseTemplate",
              "type": "codeVoice"
            },
            {
              "text": " example.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<document>",
            "    <showcaseTemplate mode=\"showcase\">",
            "        <background>",
            "            <img src=\"path to images on your server/Car_Movie_1920x1080.png\" />",
            "        </background>",
            "        <banner>",
            "            <title>Scenes</title>",
            "            <row>",
            "                <button>",
            "                    <text>Slideshow</text>",
            "                </button>",
            "                <button>",
            "                    <text>Screensaver</text>",
            "                </button>",
            "            </row>",
            "        </banner>",
            "        <carousel>",
            "            <section>",
            "                <lockup>",
            "                    <img src=\"path to images on your server/Car_Movie_453x255_C.png\" width=\"453\" height=\"255\" />",
            "                    <title>Scene 1</title>",
            "                </lockup>",
            "                <lockup>",
            "                    <img src=\"path to images on your server/Car_Movie_500x600.png\" width=\"500\" height=\"600\" />",
            "                    <title>Scene 2</title>",
            "                </lockup>",
            "                <lockup>",
            "                    <img src=\"path to images on your server/Car_Movie_308x308_B.png\" width=\"308\" height=\"308\" />",
            "                    <title>Scene 3</title>",
            "                </lockup>",
            "                <lockup>",
            "                    <img src=\"path to images on your server/Car_Movie_800x600.png\" width=\"800\" height=\"600\" />",
            "                    <title>Scene 4</title>",
            "                </lockup>",
            "            </section>",
            "        </carousel>",
            "    </showcaseTemplate>",
            "</document>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following figure shows the output for the above example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
