    "title": "Rotten Tomatoes Rating Icons"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Rotten Tomatoes ratings are based on reviews by television and movie critics.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Fresh",
          "level": 3,
          "text": "Fresh",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The fresh rating icon comes in two sizes, which have the resource names ",
              "type": "text"
            },
            {
              "code": "tomato-fresh",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and ",
              "type": "text"
            },
            {
              "code": "tomato-fresh-m",
              "type": "codeVoice"
            },
            {
              "text": ". Rotten Tomatoes assigns a fresh rating to media items that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "have at least 60% positive reviews. Place a fresh rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tomato-fresh\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rotten-tomatoes-rating-icons-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Splat",
          "level": 3,
          "text": "Splat",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The splat rating icon comes in two sizes, which have the resource names ",
              "type": "text"
            },
            {
              "code": "tomato-splat",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and ",
              "type": "text"
            },
            {
              "code": "tomato-splat-m",
              "type": "codeVoice"
            },
            {
              "text": ". Rotten Tomatoes assigns a splat rating to media items that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "have less than 60% positive reviews. Place a splat rating icon is ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tomato-splat\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rotten-tomatoes-rating-icons-2",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Certified-Fresh",
          "level": 3,
          "text": "Certified Fresh",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The certified fresh rating icon comes in two sizes, which have the resource names",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "tomato-certified",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "tomato-certified-m",
              "type": "codeVoice"
            },
            {
              "text": ". Rotten Tomatoes assigns a certified",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "fresh rating to media items that meet the following criteria:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A steady rating score of 75% or higher",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "At least five reviews from Top Critics",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "At least 80 reviews for films in wide release",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "At least 40 reviews for films in limited release",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "At least 20 reviews for an individual season of a television show. Only individual",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "seasons are eligible for the rating.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "Place a certified fresh rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://tomato-certified\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rotten-tomatoes-rating-icons-3",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
