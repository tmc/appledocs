      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "prototypes",
              "type": "codeVoice"
            },
            {
              "text": " element is used to bind data item types, simplifying the template",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "layout. A single binding definition provides the ability to display any number of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "data items. For more information on data binding, see ",
              "type": "text"
            },
            {
              "identifier": "https://developer.apple.com/library/content/documentation/TVMLKitJS/Conceptual/TVMLProgrammingGuide/GeneratingContentForYourApp.html#//apple_ref/doc/uid/TP40016718-CH4-SW1",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Hereâ€™s an example that uses the ",
              "type": "text"
            },
            {
              "code": "prototypes",
              "type": "codeVoice"
            },
            {
              "text": " element to display an undefined number",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of ",
              "type": "text"
            },
            {
              "code": "lockup",
              "type": "codeVoice"
            },
            {
              "text": " elements.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<prototypes>",
            "    <lockup prototype=\"artwork\">",
            "        <img binding=\"@src:{url};\" width=\"200\" height=\"300\"/>",
            "        <title binding=\"textContent:{title};\" />",
            "    </lockup>",
            "</prototypes>",
            "<section binding=\"items:{images};\" />"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
          "anchor": "Subelements-of-prototypes",
          "level": 3,
          "text": "Subelements of prototypes",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "prototypes",
              "type": "codeVoice"
            },
            {
              "text": " can contain any element that can be contained by a ",
              "type": "text"
            },
            {
              "code": "section",
              "type": "codeVoice"
            },
            {
              "text": " element.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The child elements must implement the ",
              "type": "text"
            },
            {
              "code": "prototype",
              "type": "codeVoice"
            },
            {
              "text": " attribute to bind the correct data",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "items.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Elements-that-Use-prototypes",
          "level": 3,
          "text": "Elements that Use prototypes",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "The prototypes",
              "type": "codeVoice"
            },
            {
              "text": " element can be a used by any element.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
