  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Brazil movie rating icons indicate who is allowed to watch a movie in Brazil. Ratings",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "are assigned by an advisory board run by the Brazilian Ministry of Justice.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "General-Audiences",
          "level": 3,
          "text": "General Audiences",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resource name for the general audiences icon for Brazil is ",
              "type": "text"
            },
            {
              "code": "bars-l",
              "type": "codeVoice"
            },
            {
              "text": ". Place a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "general audiences rating icon next to content to indicate that the content is suitable",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to be watched by people of any age. Place a general audiences rating icon using ",
              "type": "text"
            },
            {
              "code": "<img src=\"resource://bars-l\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "rating-icons-brazil-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
