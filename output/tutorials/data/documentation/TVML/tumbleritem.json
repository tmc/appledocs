        {
          "inlineContent": [
            {
              "text": "The user swipes left and right on the remote to display the previous and next ",
              "type": "text"
            },
            {
              "code": "tumblerItem",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "elements inside of the tumbler header. Here’s an example of a tumbler bar containing",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "two tumbler items.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<tumblerBar>",
            "    <tumblerItem>",
            "        <title>Title 1</title>",
            "        <subtitle>5 Items</subtitle>",
            "    </tumblerItem>",
            "    <tumblerItem>",
            "        <title>Title 2</title>",
            "        <subtitle>12 Items</subtitle>",
            "    </tumblerItem>",
