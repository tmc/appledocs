      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "list",
              "type": "codeVoice"
            },
            {
              "text": " element is a generic element that contains nearly all other elements in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the templates that use it. Here’s an example of a ",
              "type": "text"
            },
            {
              "code": "list",
              "type": "codeVoice"
            },
            {
              "text": " inside of a ",
              "type": "text"
            },
            {
              "code": "paradeTemplate",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "list",
              "type": "codeVoice"
            },
            {
              "text": " element contains the information for the page.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<paradeTemplate>",
            "    <list>",
            "        <header>",
            "            <title>Movie Genres</title>",
            "        </header>",
            "        <section>",
            "            <listItemLockup>",
            "                <title>Action</title>",
            "            </listItemLockup>",
            "            <listItemLockup>",
            "                <title>Comedy</title>",
            "            </listItemLockup>",
            "        </section>",
            "        <relatedContent>",
            "            <imgDeck>",
            "                <img src=\"path to images on your server/Car_Movie_250x375.png \"/>",
            "                <img src=\"path to images on your server/Car_Movie_250x375_A.png \"/>",
            "                <img src=\"path to images on your server/Car_Movie_250x375_B.png \"/>",
            "                <img src=\"path to images on your server/Car_Movie_250x375_C.png \"/>",
            "            </imgDeck>",
            "        </relatedContent>",
            "    </list>",
            "</paradeTemplate>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
