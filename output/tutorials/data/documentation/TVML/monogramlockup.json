  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "monogramLockup",
              "type": "codeVoice"
            },
            {
              "text": " element creates a white circle that contains the image or initials",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of the person identified in the title. Use the ",
              "type": "text"
            },
            {
              "code": "src",
              "type": "codeVoice"
            },
            {
              "text": " element to place an image inside",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of the circle. If no image is added, initials are centered in the circle, based on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the ",
              "type": "text"
            },
            {
              "code": "firstName",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "lastName",
              "type": "codeVoice"
            },
            {
              "text": " attributes in the ",
              "type": "text"
            },
            {
              "code": "monogram",
              "type": "codeVoice"
            },
            {
              "text": " element. Here’s an example",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of a monogramLockup displaying an actor’s infomation.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<monogramLockup>",
            "    <monogram firstName=\"Anne\" lastName=\"Johnson\"/>",
            "    <title>Anne Johnson</title>",
            "    <subtitle>Actor</subtitle>",
            "</monogramLockup>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
