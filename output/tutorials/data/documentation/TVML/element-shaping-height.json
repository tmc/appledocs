        {
          "inlineContent": [
            {
              "text": "Hereâ€™s an example that shows how to set the height of an image to 376 points.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<img src=\"path to image\" width=\"250\" height=\"376\" />"
          ],
          "syntax": "xml",
          "type": "codeListing"
