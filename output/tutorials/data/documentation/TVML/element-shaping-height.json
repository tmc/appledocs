        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The height of the element, in points. For example, ",
                        "type": "text"
                      },
                      {
                        "code": "height=\"200\"",
                        "type": "codeVoice"
                      },
                      {
                        "text": " sets the height of the element to 200 points.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
