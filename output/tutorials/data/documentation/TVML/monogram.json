        {
          "inlineContent": [
            {
              "text": "Here’s an example that sets the name Anne Johnson as part of a ",
              "type": "text"
            },
            {
              "code": "monogramLockup",
              "type": "codeVoice"
            },
            {
              "text": " element.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<monogramLockup>",
            "   <monogram firstName=\"Anne\" lastName=\"Johnson\"/>",
            "   <title>Anne Johnson</title>",
            "   <subtitle>Actor</subtitle>",
            "</monogramLockup>"
