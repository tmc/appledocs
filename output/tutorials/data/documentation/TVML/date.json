      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "date",
              "type": "codeVoice"
            },
            {
              "text": " element contains text used to display the associated date. The date string",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "must be in one of the following formats:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "yyyy-MM-dd",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "ISO date format",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "RFC 1123 date format",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "Here’s an example that sets the date to January 19, 2031.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "<date format=\"MMMM dd, yyyy\">2031-01-19</date>"
          ],
          "syntax": "xml",
          "type": "codeListing"
        },
        {
