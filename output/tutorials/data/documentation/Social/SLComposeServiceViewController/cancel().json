        {
          "inlineContent": [
            {
              "text": "When the cancel animation finishes, this method calls ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.social/documentation/Social/SLComposeServiceViewController/didSelectCancel()",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". A subclass shouldnâ€™t need to override ",
              "type": "text"
            },
            {
              "code": "cancel",
              "type": "codeVoice"
            },
            {
              "text": ". In rare cases a subclass may call ",
              "type": "text"
            },
            {
              "code": "cancel",
              "type": "codeVoice"
            },
            {
              "text": ", such as in response to a catastrophic failure during user interaction with the compose view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
