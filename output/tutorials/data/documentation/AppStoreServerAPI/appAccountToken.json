  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: App Store Server API"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "uuid "
            },
            {
              "kind": "identifier",
              "text": "appAccountToken"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "kind": "mentions",
      "mentions": [
        "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/app-store-server-api-changelog"
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When a customer initiates an in-app purchase, you can optionally generate an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/StoreKit/Product/PurchaseOption/appAccountToken(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and send it to the App Store. If you use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/StoreKit/original-api-for-in-app-purchase",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", you may provide a UUID in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/StoreKit/SKMutablePayment/applicationUsername",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property. The App Store returns the same UUID in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/StoreKit/Transaction/appAccountToken",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " in the transaction information after the customer completes the purchase.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/ConsumptionRequest",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " response body requires that you set the ",
              "type": "text"
            },
            {
              "code": "appAccountToken",
              "type": "codeVoice"
            },
            {
              "text": " value to a valid value of either a UUID or an empty string. Set the ",
              "type": "text"
            },
            {
              "code": "appAccountToken",
              "type": "codeVoice"
            },
            {
              "text": " value to the value you received in the ",
              "type": "text"
            },
            {
              "code": "CONSUMPTION_REQUEST",
              "type": "codeVoice"
            },
            {
              "text": " notification, or, if you choose not to provide this information, set the value to an empty string.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If you receive a ",
              "type": "text"
            },
            {
              "code": "CONSUMPTION_REQUEST",
              "type": "codeVoice"
            },
            {
              "text": " notification for a transaction, find its associated ",
              "type": "text"
            },
            {
              "code": "appAccountToken",
              "type": "codeVoice"
            },
            {
              "text": " value as follows:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If you receive App Store Server Notifications version 2, the ",
                      "type": "text"
                    },
                    {
                      "code": "appAccountToken",
                      "type": "codeVoice"
                    },
                    {
                      "text": " value is in ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/AppStoreServerNotifications/JWSTransactionDecodedPayload",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If you receive App Store Server Notifications version 1, the ",
                      "type": "text"
                    },
                    {
                      "code": "appAccountToken",
                      "type": "codeVoice"
                    },
                    {
                      "text": " value is in ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/AppStoreServerNotifications/unified_receipt/Latest_receipt_info-data.dictionary",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "appAccountToken",
              "type": "codeVoice"
            },
            {
              "text": " value may be an empty string if your app doesn’t use app account tokens.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For more information about App Store Server Notifications versions, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/AppStoreServerNotifications/app-store-server-notifications-changelog",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
