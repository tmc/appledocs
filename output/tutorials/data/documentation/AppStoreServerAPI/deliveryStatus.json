    {
      "kind": "possibleValues",
      "title": "Possible Values",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app delivered the consumable in-app purchase and it’s working properly.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "0"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app didn’t deliver the consumable in-app purchase due to a quality issue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app delivered the wrong item.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app didn’t deliver the consumable in-app purchase due to a server outage.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "3"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app didn’t deliver the consumable in-app purchase due to an in-game currency change.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "4"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app didn’t deliver the consumable in-app purchase for other reasons.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "5"
        }
      ]
    }
