    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app’s identifier in the App Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "appAppleId",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/appAppleId",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:appAppleId",
              "text": "appAppleId"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle identifier of the app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundleId",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/bundleId",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:bundleId",
              "text": "bundleId"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The server environment in which you’re making the request, whether sandbox or production.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "environment",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/environment",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:environment",
              "text": "environment"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the App Store has more transactions than it returns in this response. If the value is ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", use the ",
                  "type": "text"
                },
                {
                  "code": "revision",
                  "type": "codeVoice"
                },
                {
                  "text": " token to request the next set of transactions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "hasMore",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/hasMore",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:hasMore",
              "text": "hasMore"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A token you use in a query to request the next set of transactions from the endpoint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "revision",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/revision",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:revision",
              "text": "revision"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of in-app purchase transactions for the customer, signed by Apple, in JSON Web Signature (JWS) format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signedTransactions",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/JWSTransaction",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:JWSTransaction",
              "text": "JWSTransaction"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/GET-v2-history-_transactionId_",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " endpoint returns the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/HistoryResponse",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". This response contains results for all in-app purchase product types: auto-renewable subscriptions, non-renewing subscriptions, non-consumables, and consumables. The result includes transactions in any state, including transactions that are refunded or revoked, and transactions that the app has or hasn’t marked as finished.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The history response returns a maximum of 20 transactions per response. If your customer has more than 20 in-app transactions, the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/HistoryResponse/hasMore",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " value is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ". Each response includes a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/HistoryResponse/revision",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " value. Call ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/GET-v2-history-_transactionId_",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " again with the ",
              "type": "text"
            },
            {
              "code": "revision",
              "type": "codeVoice"
            },
            {
              "text": " token in the query to receive the next set of transactions; use the same query parameters for each subsequent call that includes ",
              "type": "text"
            },
            {
              "code": "revision",
              "type": "codeVoice"
            },
            {
              "text": ". When the App Store has no more transactions to send, the ",
              "type": "text"
            },
            {
              "code": "hasMore",
              "type": "codeVoice"
            },
            {
              "text": " value is ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If a customer upgrades a subscription or the App Store revokes an in-app purchase while you’re receiving transaction history, the App Store updates the relevant transaction. If the response is sorted in ",
              "type": "text"
            },
            {
              "code": "ASCENDING",
              "type": "codeVoice"
            },
            {
              "text": " order, you receive the transaction again, with updated data.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/GET-v1-history-_transactionId_",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " endpoint also returns the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/HistoryResponse",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", however, it doesn’t include consumable in-app purchases that the app has marked as finished. Use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/GET-v2-history-_transactionId_",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " instead.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
