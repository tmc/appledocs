    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The algorithm used for signing the JSON Web Signature (JWS).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alg",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/alg",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:alg",
              "text": "alg"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The X.509 certificate chain that corresponds to the key that the App Store used to secure the JWS.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "x5c",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/x5c",
