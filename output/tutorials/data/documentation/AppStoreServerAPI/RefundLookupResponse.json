          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of JWS transactions, or an empty array if the customer has received no refunds in your app. The transactions are sorted in ascending order by their ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/revocationDate",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "signedTransactions",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/JWSTransaction",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:JWSTransaction",
              "text": "JWSTransaction"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
