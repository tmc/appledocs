    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the customer hasn’t received any refunds for in-app purchases in your app, the ",
              "type": "text"
            },
            {
              "code": "signedTransactions",
              "type": "codeVoice"
            },
            {
              "text": " array is empty. To read the transaction information, decode the payload for each ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/JWSTransaction",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object in the ",
              "type": "text"
            },
            {
              "code": "signedTransactions",
              "type": "codeVoice"
            },
            {
              "text": " array. Use a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/JWSTransactionDecodedPayload",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object to read the transaction information in the payload.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This response can contain a maximum of 50 transactions in the ",
              "type": "text"
            },
            {
              "code": "signedTransactions",
              "type": "codeVoice"
            },
            {
              "text": " array.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
