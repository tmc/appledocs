    {
      "kind": "possibleValues",
      "title": "Possible Values",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is active.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is expired.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is in a billing retry period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "3"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is in a Billing Grace Period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "4"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is revoked. The App Store refunded the transaction or revoked it from Family Sharing.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "5"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
