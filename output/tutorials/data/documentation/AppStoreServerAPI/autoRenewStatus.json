      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Automatic renewal is off. The customer has turned off automatic renewal for the subscription, and it won’t renew at the end of the current subscription period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "0"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Automatic renewal is on. The subscription renews at the end of the current subscription period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "1"
        }
      ]
    }
