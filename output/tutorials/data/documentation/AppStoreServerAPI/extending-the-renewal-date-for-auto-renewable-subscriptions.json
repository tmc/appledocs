{
  "abstract": [
    {
      "text": "Compensate eligible active subscribers for service interruptions by extending a subscription’s renewal date.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "data",
    "url": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/extending-the-renewal-date-for-auto-renewable-subscriptions"
  },
  "kind": "article",
  "legalNotices": {
