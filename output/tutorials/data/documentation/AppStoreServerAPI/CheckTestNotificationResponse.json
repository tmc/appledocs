    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of information the App Store server records for its attempts to send the ",
                  "type": "text"
                },
                {
                  "code": "TEST",
                  "type": "codeVoice"
                },
                {
                  "text": " notification to your server. The array may contain a maximum of six ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/sendAttemptItem",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " objects.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sendAttempts",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/sendAttemptItem",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:sendAttemptItem",
              "text": "sendAttemptItem"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The signed payload, in JWS format, that contains the ",
                  "type": "text"
                },
                {
                  "code": "TEST",
                  "type": "codeVoice"
                },
                {
                  "text": " notification that the App Store server sent to your server.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signedPayload",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/signedPayload",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:signedPayload",
              "text": "signedPayload"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The result of the App Store server’s first attempt to send the ",
                  "type": "text"
                },
                {
                  "code": "TEST",
                  "type": "codeVoice"
                },
                {
                  "text": " notification to your server.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Use the first ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/sendAttemptItem",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "code": "sendAttempts",
                  "type": "codeVoice"
                },
                {
                  "text": " array instead.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "name": "firstSendAttemptResult",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "kind": "mentions",
      "mentions": [
        "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/app-store-server-api-changelog"
