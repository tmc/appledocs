      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A UUID you create at the time of purchase that associates the transaction with a customer on your own service. This is the token that applies to the upcoming renewal transaction. If your app doesn’t provide an ",
                  "type": "text"
                },
                {
                  "code": "appAccountToken",
                  "type": "codeVoice"
                },
                {
                  "text": ", this field is omitted.  For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/StoreKit/Product/PurchaseOption/appAccountToken(_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "appAccountToken",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/appAccountToken",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:appAccountToken",
              "text": "appAccountToken"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier of the app download transaction.",
                  "type": "text"
                }
