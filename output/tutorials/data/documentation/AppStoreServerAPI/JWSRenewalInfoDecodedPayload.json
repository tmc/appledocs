          "name": "renewalPrice",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/renewalPrice",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_server_api:renewalPrice",
              "text": "renewalPrice"
