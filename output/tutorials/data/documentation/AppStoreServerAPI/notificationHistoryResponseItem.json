                {
                  "identifier": "doc://com.apple.appstoreserverapi/documentation/AppStoreServerAPI/sendAttemptItem",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "code": "sendAttempts",
                  "type": "codeVoice"
                },
                {
                  "text": " array instead.",
