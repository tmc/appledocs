          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of information the App Store server records for its attempts to send a notification to your server. The maximum number of entries in the array is six.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sendAttempts",
          "required": false,
          "type": [
            {
              "kind": "text",
