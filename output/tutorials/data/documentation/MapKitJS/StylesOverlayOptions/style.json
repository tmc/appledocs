    },
    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional object literal of style properties (defined below).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following table lists the available style properties:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "header": "row",
          "rows": [
            [
              [
                {
                  "inlineContent": [
                    {
                      "inlineContent": [
                        {
                          "text": "Property",
                          "type": "text"
                        }
                      ],
                      "type": "strong"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "inlineContent": [
                        {
                          "text": "Summary",
                          "type": "text"
                        }
                      ],
                      "type": "strong"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "strokeColor",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The stroke color of a line. This can be any valid CSS color value. The default value is ",
                      "type": "text"
                    },
                    {
                      "code": "rgb(0, 122, 255)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". Set this to ",
                      "type": "text"
                    },
                    {
                      "code": "null",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to have no line stroke.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "strokeOpacity",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The opacity to apply to strokes as a number between ",
                      "type": "text"
                    },
                    {
                      "code": "0",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "1",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". The default value is ",
                      "type": "text"
                    },
                    {
                      "code": "1",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "lineWidth",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The line width of the stroke line for overlays, in CSS pixels. The default value is ",
                      "type": "text"
                    },
                    {
                      "code": "1",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "lineCap",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The shape of the ends of the line. The three options for line endings are ",
                      "type": "text"
                    },
                    {
                      "code": "butt",
                      "type": "codeVoice"
                    },
                    {
                      "text": " (squared-off ends), ",
                      "type": "text"
                    },
                    {
                      "code": "round",
                      "type": "codeVoice"
                    },
                    {
                      "text": " (rounded ends), or ",
                      "type": "text"
                    },
                    {
                      "code": "square",
                      "type": "codeVoice"
                    },
                    {
                      "text": " (ends that have a half-square extension). The default value is ",
                      "type": "text"
                    },
                    {
                      "code": "round",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "lineJoin",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The style to use when drawing joins between line segments. The three options are ",
                      "type": "text"
                    },
                    {
                      "code": "miter",
                      "type": "codeVoice"
                    },
                    {
                      "text": " (the join is a sharp corner), ",
                      "type": "text"
                    },
                    {
                      "code": "round",
                      "type": "codeVoice"
                    },
                    {
                      "text": " (the join is a rounded corner), or ",
                      "type": "text"
                    },
                    {
                      "code": "bevel",
                      "type": "codeVoice"
                    },
                    {
                      "text": " (the join is a beveled corner). The default value is ",
                      "type": "text"
                    },
                    {
                      "code": "round",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "lineDash",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "An array to use as the line’s dash pattern where the numbers represent line and gap lengths in CSS pixels. For example, ",
                      "type": "text"
                    },
                    {
                      "code": "[10, 5]",
                      "type": "codeVoice"
                    },
                    {
                      "text": " means draw for 10 pixels, leave a 5-pixel gap, and repeat. MapKit JS copies and duplicates the array if there are an odd number of elements in it. Set this to an empty array (",
                      "type": "text"
                    },
                    {
                      "code": "[]",
                      "type": "codeVoice"
                    },
                    {
                      "text": ") to draw solid lines. The default value is ",
                      "type": "text"
                    },
                    {
                      "code": "[]",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "lineDashOffset",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The number of CSS pixels to offset drawing of the dash pattern. This has no effect if you set the ",
                      "type": "text"
                    },
                    {
                      "code": "lineDash",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to draw solid lines. The default value is ",
                      "type": "text"
                    },
                    {
                      "code": "0",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "fillColor",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The color to use for filling shapes. This can be any valid CSS color value. The default fill color is ",
                      "type": "text"
                    },
                    {
                      "code": "rgb(0, 122, 255)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". Set this to ",
                      "type": "text"
                    },
                    {
                      "code": "null",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to have no fill.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "fillOpacity",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The opacity to apply to fills as a number between ",
                      "type": "text"
                    },
                    {
                      "code": "0",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "1",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". The default opacity is ",
                      "type": "text"
                    },
                    {
                      "code": "0.1",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "code": "fillRule",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The rule to use for determining whether a point is inside or outside a polygon. This can be either the nonzero winding rule (",
                      "type": "text"
                    },
                    {
                      "code": "nonzero",
                      "type": "codeVoice"
                    },
                    {
                      "text": "), the default, or the even-odd rule (",
                      "type": "text"
                    },
                    {
                      "code": "evenodd",
                      "type": "codeVoice"
                    },
                    {
                      "text": ").",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ]
          ],
          "type": "table"
        }
      ],
      "kind": "content"
    }
  ],
