      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object literal with the keys defined in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.mapkitjs/documentation/MapKitJS/PaddingConstructorOptions",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", or a list of four numbers that represent inset margin values. The numbers represent the top, right, bottom, and left insets, respectively.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "top"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Create ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.mapkitjs/documentation/MapKitJS/mapkit.Padding/mapkit.Padding",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " by passing in four numbers that represent the inset values from the top, right, bottom, and left edges. Alternatively, use an object literal instance with the keys defined in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.mapkitjs/documentation/MapKitJS/PaddingConstructorOptions",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following examples show two ways to create padding:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// example 1: 4 numbers",
            "map.padding = new mapkit.Padding(",
            "    10, // top inset",
            "    10, // right inset",
            "    10, // bottom inset",
            "    10) // left inset",
            "",
            "// example 2: object literal conforming to PaddingConstructorOptions",
            "map.padding = new mapkit.Padding({top: 10, right: 10, bottom: 10, left:10})",
            ""
          ],
          "syntax": "javascript",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
