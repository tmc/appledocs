{
  "abstract": [
    {
      "text": "Link to the most recent autoupdating version of MapKit JS, or a version of your choice.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.mapkitjs/documentation/MapKitJS"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "data",
    "url": "doc://com.apple.mapkitjs/documentation/MapKitJS/loading-the-latest-version-of-mapkit-js"
  },
  "kind": "article",
  "legalNotices": {
    "copyright": "Copyright &copy; 2025 Apple Inc. All rights reserved.",
    "privacyPolicy": "https://www.apple.com/privacy/privacy-policy",
