        },
        {
          "inlineContent": [
            {
              "text": "The subtitle visibility controls the subtitle that renders below the balloon part of the marker. The default value is ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.mapkitjs/documentation/MapKitJS/mapkit.FeatureVisibility/Adaptive",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For adaptive visibility, the subtitle is always hidden in the normal state, by default. In the selected state, the subtitle follows the same rules as the title.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
