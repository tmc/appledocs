            {
              "inlineContent": [
                {
                  "text": "The payload in JSON Web Signature (JWS) format, signed by the App Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signedPayload",
          "required": false,
          "type": [
            {
