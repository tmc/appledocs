    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "responseBodyV2DecodedPayload",
              "type": "codeVoice"
            },
            {
              "text": " is the Base64URL-decoded notification information from the JWS payload portion of the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/signedPayload",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/notificationType",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/subtype",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to understand the event that led to this notification.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The payload can contain only one of the following three fields:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/data",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " object, which contains details including the environment, the app metadata, and the signed transaction and subscription renewal information.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/summary",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " object, which contains information only when the notification is a ",
                      "type": "text"
                    },
                    {
                      "code": "RENEWAL_EXTENSION",
                      "type": "codeVoice"
                    },
                    {
                      "text": " with a ",
                      "type": "text"
                    },
                    {
                      "code": "SUMMARY",
                      "type": "codeVoice"
                    },
                    {
                      "text": " subtype. For more information, see ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/AppStoreServerAPI/POST-v1-subscriptions-extend-mass-",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "An ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/externalPurchaseToken",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", which contains an external purchase token only when the notification is ",
                      "type": "text"
                    },
                    {
                      "code": "EXTERNAL_PURCHASE_TOKEN",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". For more information about this notification, see ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/externalPurchaseToken",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
