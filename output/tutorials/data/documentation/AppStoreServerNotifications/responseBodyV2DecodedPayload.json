        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The object that contains the app metadata and signed renewal and transaction information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "data",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "summary",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "externalPurchaseToken",
                  "type": "codeVoice"
                },
                {
                  "text": " fields are mutually exclusive. The payload contains only one of these fields.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/data",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_s2s:data",
              "text": "data"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The summary data that appears when the App Store server completes your request to extend a subscription renewal date for eligible subscribers. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/AppStoreServerAPI/POST-v1-subscriptions-extend-mass-",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "data",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "summary",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "externalPurchaseToken",
                  "type": "codeVoice"
                },
                {
                  "text": " fields are mutually exclusive. The payload contains only one of these fields.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "summary",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/summary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_s2s:summary",
              "text": "summary"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This field appears when the ",
                  "type": "text"
                },
                {
                  "code": "notificationType",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "EXTERNAL_PURCHASE_TOKEN",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "data",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "summary",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "externalPurchaseToken",
                  "type": "codeVoice"
                },
                {
                  "text": " fields are mutually exclusive. The payload contains only one of these fields.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "externalPurchaseToken",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/externalPurchaseToken",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:store_s2s:externalPurchaseToken",
              "text": "externalPurchaseToken"
            }
          ]
        },
