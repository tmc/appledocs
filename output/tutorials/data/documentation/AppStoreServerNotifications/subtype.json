      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Applies to the ",
                  "type": "text"
                },
                {
                  "code": "PRICE_INCREASE",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/notificationType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". A notification with this ",
                  "type": "text"
                },
                {
                  "code": "subtype",
                  "type": "codeVoice"
                },
                {
                  "text": " indicates that the customer consented to the subscription price increase if the price increase requires customer consent, or that the system notified them of a price increase if the price increase doesnâ€™t require customer consent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ACCEPTED"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Applies to the ",
                  "type": "text"
                },
                {
                  "code": "DID_CHANGE_RENEWAL_STATUS",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/notificationType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". A notification with this `subtype indicates that the user disabled subscription auto-renewal, or the App Store disabled subscription auto-renewal after the user requested a refund.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "AUTO_RENEW_DISABLED"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Applies to the ",
                  "type": "text"
                },
                {
                  "code": "DID_CHANGE_RENEWAL_STATUS",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/notificationType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". A notification with this ",
                  "type": "text"
                },
                {
                  "code": "subtype",
                  "type": "codeVoice"
                },
                {
                  "text": " indicates that the user enabled subscription auto-renewal.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "AUTO_RENEW_ENABLED"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Applies to the ",
                  "type": "text"
                },
                {
                  "code": "DID_RENEW",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreservernotifications/documentation/AppStoreServerNotifications/notificationType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". A notification with this subtype indicates that the expired subscription that previously failed to renew has successfully renewed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "BILLING_RECOVERY"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Applies to the EXPIRED notificationType. A notification with this subtype indicates that the subscription expired because the subscription failed to renew before the billing retry period ended.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "BILLING_RETRY"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Applies to the ",
                  "type": "text"
                },
                {
                  "code": "DID_CHANGE_RENEWAL_PREF",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "OFFER_REDEEMED",
                  "type": "codeVoice"
