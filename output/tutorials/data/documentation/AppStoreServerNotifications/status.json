          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is active.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is expired.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is in a billing retry period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "3"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is in a Billing Grace Period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "4"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The auto-renewable subscription is revoked.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "5"
        }
      ]
    },
