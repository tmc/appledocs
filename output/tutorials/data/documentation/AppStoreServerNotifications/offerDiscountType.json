        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A payment mode of a product discount that indicates a free trial.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "FREE_TRIAL"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A payment mode of a product discount that customers pay over a single or multiple billing periods.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "PAY_AS_YOU_GO"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A payment mode of a product discount that customers pay up front.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "PAY_UP_FRONT"
        }
      ]
    },
