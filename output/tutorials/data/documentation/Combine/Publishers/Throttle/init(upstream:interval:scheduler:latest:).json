      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The publisher from which this publisher receives elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "upstream"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The interval in which to find and emit the most recent element.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "interval"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scheduler on which to publish elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "scheduler"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value indicating whether to publish the most recent element. If ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", the publisher emits the first element received during the interval.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "latest"
        }
      ]
    }
