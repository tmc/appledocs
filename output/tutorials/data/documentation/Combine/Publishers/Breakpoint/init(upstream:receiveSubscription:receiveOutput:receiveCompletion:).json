          "content": [
            {
              "inlineContent": [
                {
                  "text": "The publisher from which this publisher receives elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "upstream"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that executes when the publisher receives a subscription, and can raise a debugger signal by returning a true Boolean value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "receiveSubscription"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that executes when the publisher receives output from the upstream publisher, and can raise a debugger signal by returning a true Boolean value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "receiveOutput"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that executes when the publisher receives completion, and can raise a debugger signal by returning a true Boolean value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "receiveCompletion"
        }
