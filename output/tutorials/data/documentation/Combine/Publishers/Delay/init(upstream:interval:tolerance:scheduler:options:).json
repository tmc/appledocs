      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The publisher from which this publisher receives its elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "upstream"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount of time to delay.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "interval"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The allowed tolerance in delivering delayed events. The ",
                  "type": "text"
                },
                {
                  "code": "Delay",
                  "type": "codeVoice"
                },
                {
                  "text": " publisher may deliver elements this much sooner or later than the interval specifies.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tolerance"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scheduler to deliver the delayed events.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "scheduler"
        },
        {
          "content": [
            {
              "inlineContent": [
