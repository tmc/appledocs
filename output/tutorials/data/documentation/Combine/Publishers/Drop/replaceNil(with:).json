          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Combine/documentation/Combine/Publisher/replaceNil(with:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " operator enables replacement of ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " values in a stream with a substitute value. In the example below, a collection publisher contains a nil value. The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Combine/documentation/Combine/Publisher/replaceNil(with:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " operator replaces this with ",
              "type": "text"
            },
            {
              "code": "0.0",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let numbers: [Double?] = [1.0, 2.0, nil, 3.0]",
            "numbers.publisher",
            "    .replaceNil(with: 0.0)",
            "    .sink { print(\"\\($0)\", terminator: \" \") }",
            "",
            "// Prints: \"Optional(1.0) Optional(2.0) Optional(0.0) Optional(3.0)\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
