    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that takes an element as its parameter and returns a Boolean value that indicates whether publishing should continue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "predicate"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A publisher that passes through elements until the predicate indicates publishing should finish.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Combine/documentation/Combine/Publisher/prefix(while:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to emit values while elements from the upstream publisher meet a condition you specify. The publisher finishes when the closure returns ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Combine/documentation/Combine/Publisher/prefix(while:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " operator emits values while the element it receives is less than five:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let numbers = (0...10)",
            "numbers.publisher",
            "    .prefix { $0 < 5 }",
            "    .sink { print(\"\\($0)\", terminator: \" \") }",
            "",
            "// Prints: \"0 1 2 3 4\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
