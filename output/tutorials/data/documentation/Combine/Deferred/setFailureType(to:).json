    "doc://com.apple.Combine/documentation/Combine/Deferred/setFailureType(to:)": {
      "abstract": [
        {
          "text": "Changes the failure type declared by the upstream publisher.",
          "type": "text"
        }
      ],
      "conformance": {
        "availabilityPrefix": [
          {
