          "content": [
            {
              "inlineContent": [
                {
                  "text": "The color to be applied to the text field’s text. Specifying ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " removes the color and returns the text to the color specified in the storyboard file. The default color is white.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "color"
        }
      ]
    },
    {
      "content": [
