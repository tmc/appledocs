          "inlineContent": [
            {
              "text": "Use this method to interrupt media playback and dismiss a modal media interface controller. When you dismiss the recording interface programmatically, the recording controller passes a value of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/false",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for the ",
              "type": "text"
            },
            {
              "code": "didSave",
              "type": "codeVoice"
            },
            {
              "text": " parameter of your completion block.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
