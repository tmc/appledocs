    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CoreTransferable/documentation/CoreTransferable/FileRepresentation",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for transferring types",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that involve a large amount of data.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example, if your app defines a ",
              "type": "text"
            },
            {
              "code": "Movie",
              "type": "codeVoice"
            },
            {
              "text": " type that could represent a lengthy video,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "use a ",
              "type": "text"
            },
            {
              "code": "FileRepresentation",
              "type": "codeVoice"
            },
            {
              "text": " instance",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to transfer the video data to another app or process.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Movie: Transferable {",
            "    let url: URL",
            "    static var transferRepresentation: some TransferRepresentation {",
            "        FileRepresentation(contentType: .mpeg4Movie) { movie in",
            "            SentTransferredFile($0.url)",
            "            } importing: { received in",
            "                let copy: URL = URL(fileURLWithPath: \"<#...#>\")",
            "                try FileManager.default.copyItem(at: received.file, to: copy)",
            "                return Self.init(url: copy) }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "It’s efficient to pass such data around as a file and the receiver",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "loads it into memory only if it’s required.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
