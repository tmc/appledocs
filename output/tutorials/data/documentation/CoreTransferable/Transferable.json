    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To conform to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CoreTransferable/documentation/CoreTransferable/Transferable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " protocol,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "implement the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CoreTransferable/documentation/CoreTransferable/Transferable/transferRepresentation",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example, an image editing app’s layer type might",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "conform to ",
              "type": "text"
            },
            {
              "code": "Transferable",
              "type": "codeVoice"
            },
            {
              "text": " to let people drag and drop image layers",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to reorder them within a document.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ImageDocumentLayer {",
            "    init(data: Data)",
            "    func data() -> Data",
            "    func pngData() -> Data",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following shows how you can extend ",
              "type": "text"
            },
            {
              "code": "ImageDocumentLayer",
              "type": "codeVoice"
            },
            {
              "text": "  to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "conform to ",
              "type": "text"
            },
            {
              "code": "Transferable",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension ImageDocumentLayer: Transferable {",
            "    static var transferRepresentation: some TransferRepresentation {",
            "        DataRepresentation(contentType: .layer) { layer in",
            "            layer.data()",
            "        } importing: { data in",
            "            ImageDocumentLayer(data: data)",
            "        }",
            "        DataRepresentation(exportedContentType: .png) { layer in",
            "            layer.pngData()",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When people drag and drop a layer within the app or onto another app",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that recognizes the custom ",
              "type": "text"
            },
            {
              "code": "layer",
              "type": "codeVoice"
            },
            {
              "text": " content type,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the app uses the first representation.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "When people drag and drop the layer onto a different image editor,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "it’s likely that the editor recognizes the PNG file type.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The second transfer representation adds support for PNG files.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following declares the custom ",
              "type": "text"
            },
            {
              "code": "layer",
              "type": "codeVoice"
            },
            {
              "text": " uniform type identifier:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension UTType {",
            "    static var layer: UTType { UTType(exportedAs: \"com.example.layer\") }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If your app declares custom uniform type identifiers,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "include corresponding entries in the app’s ",
                  "type": "text"
                },
                {
                  "code": "Info.plist",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/UniformTypeIdentifiers/defining-file-and-data-types-for-your-app",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "If one of your existing types conforms to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Swift/Codable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Transferable",
              "type": "codeVoice"
            },
            {
              "text": " automatically handles conversion to and from ",
              "type": "text"
            },
            {
              "code": "Data",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The following declares a simple ",
              "type": "text"
            },
            {
              "code": "Note",
              "type": "codeVoice"
            },
            {
              "text": " structure that’s ",
              "type": "text"
            },
            {
              "code": "Codable",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and an extension to make it ",
              "type": "text"
            },
            {
              "code": "Transferable",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Note: Codable {",
            "    let title: String",
            "    let body: String",
            "}",
            "",
            "extension Note: Transferable {",
            "    static var transferRepresentation: some TransferRepresentation {",
            "        CodableRepresentation(contentType: .note)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "To ensure compatibility with other apps that don’t know about",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the custom ",
              "type": "text"
            },
            {
              "code": "note",
              "type": "codeVoice"
            },
            {
              "text": " type identifier,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the following adds an additional transfer representation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that converts the note to text.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension Note: Transferable {",
            "    static var transferRepresentation: some TransferRepresentation {",
            "        CodableRepresentation(contentType: .note)",
            "        ProxyRepresentation(\\.title)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The order of the representations in the transfer representation matters;",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "place the representation that most accurately represents your type first,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "followed by a sequence of more compatible",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "but less preferable representations.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
