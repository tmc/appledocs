    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that determines whether the item is exportable.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Don’t perform long-running work in this closure.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "It shouldn’t contain network requests, file operations,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "or other potentially time-consuming tasks as they",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "can cause delays during operations with ",
                  "type": "text"
                },
                {
                  "code": "Transferable",
                  "type": "codeVoice"
                },
                {
                  "text": " items.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "condition"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Some instances of a model type may have state-dependent conditions that make them",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "unsuitable for export. For example, an ",
              "type": "text"
            },
            {
              "code": "Archive",
              "type": "codeVoice"
            },
            {
              "text": " structure that supports",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a comma-separated text representation only when it has compatible content:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Archive {",
            "    var supportsCSV: Bool",
            "    func csvData() -> Data",
            "    init(csvData: Data)",
            "}",
            "",
            "extension Archive: Transferable {",
            "    static var transferRepresentation: some TransferRepresentation {",
            "        DataRepresentation(contentType: .commaSeparatedText) { archive in",
            "            archive.csvData()",
            "        } importing: { data in Archive(csvData: data) }",
            "            .exportingCondition { archive in archive.supportsCSV }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
