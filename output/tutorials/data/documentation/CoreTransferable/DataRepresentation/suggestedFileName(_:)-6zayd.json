      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The optional closure that returns the suggested filename",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "including the filename extension.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If several suggested file names are specified on an item, only the last one will be used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fileName"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Any transfer representation can be written to disk.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Note: Transferable {",
            "    var title: String",
            "    var body: String",
            "    static var transferRepresentation: some TransferRepresentation {",
            "        ProxyRepresentation(exporting: \\.body)",
            "            .suggestedFileName { $0.title + \".txt\" }",
            "     }",
            " }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
