    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this transfer representation if your model is stored in memory.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example, a drawing app might have a notion of a ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "layer",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that can be converted to and from a custom binary data format and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "also converted to the PNG image type:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ImageDocumentLayer {",
            "    init(data: Data) throws",
            "    func data() -> Data",
            "    func pngData() -> Data",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can provide multiple transfer representations for a model type,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "even if the transfer representation types are the same.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The following shows the ",
              "type": "text"
            },
            {
              "code": "ImageDocumentLayer",
              "type": "codeVoice"
            },
            {
              "text": " structure",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "conforming to ",
              "type": "text"
            },
            {
              "code": "Transferable",
              "type": "codeVoice"
            },
            {
              "text": " with two ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.CoreTransferable/documentation/CoreTransferable/DataRepresentation",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instances",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "composed together:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension ImageDocumentLayer: Transferable {",
            "    static var transferRepresentation: some TransferRepresentation {",
            "        DataRepresentation(contentType: .layer) { layer in",
            "                layer.data()",
            "            } importing: { data in",
            "                try ImageDocumentLayer(data: data)",
            "            }",
            "        DataRepresentation(exportedContentType: .png) { layer in",
            "            layer.pngData()",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The example drawing app’s custom transfer representation comes first",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "so that apps that know about the custom transfer representation can use it.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The second transfer representation offers export compatibility with other apps",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that work with PNG images.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If a type conforms to ",
                  "type": "text"
                },
                {
                  "code": "Codable",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.CoreTransferable/documentation/CoreTransferable/CodableRepresentation",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " might be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "a more convenient choice than ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.CoreTransferable/documentation/CoreTransferable/DataRepresentation",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Tip",
          "style": "tip",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
