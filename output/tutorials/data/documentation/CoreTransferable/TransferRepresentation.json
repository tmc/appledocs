    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Combine multiple existing transfer representations",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to compose a single transfer representation that describes",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "how to transfer an item in multiple scenarios.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following shows a ",
              "type": "text"
            },
            {
              "code": "Greeting",
              "type": "codeVoice"
            },
            {
              "text": " type that transfers both as a ",
              "type": "text"
            },
            {
              "code": "Codable",
              "type": "codeVoice"
            },
            {
              "text": " type",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and by proxy through its ",
              "type": "text"
            },
            {
              "code": "message",
              "type": "codeVoice"
            },
            {
              "text": " string.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "import UniformTypeIdentifiers",
            "",
            "struct Greeting: Codable, Transferable {",
            "    let message: String",
            "    var displayInAllCaps: Bool = false",
            "",
            "    static var transferRepresentation: some TransferRepresentation {",
            "        CodableRepresentation(contentType: .greeting)",
            "        ProxyRepresentation(exporting: \\.message)",
            "    }",
            "}",
            "",
            "extension UTType {",
            "    static var greeting: UTType { .init(exportedAs: \"com.example.greeting\") }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
