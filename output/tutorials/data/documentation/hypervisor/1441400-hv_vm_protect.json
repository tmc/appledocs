          "content": [
            {
              "inlineContent": [
                {
                  "text": "The address in the intermediate physical address space. It must be page-aligned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Apple silicon only.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ipa"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The address in the guest physical address space. It must be page-aligned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Intel-based Mac only.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "gpa"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The size in bytes of the region to protect, in bytes. It must be a multiple of the page size.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "size"
        },
        {
          "content": [
            {
