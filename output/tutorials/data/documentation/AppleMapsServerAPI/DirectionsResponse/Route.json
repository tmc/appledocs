      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Total distance that the route covers, in meters.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "distanceMeters",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The estimated time to traverse this route in seconds. If you’ve specified a ",
                  "type": "text"
                },
                {
                  "code": "departureDate",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "arrivalDate",
                  "type": "codeVoice"
                },
                {
                  "text": ", then the estimated time includes traffic conditions assuming user departs or arrives at that time. If you set neither ",
                  "type": "text"
                },
                {
                  "code": "departureDate",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "arrivalDate",
                  "type": "codeVoice"
                },
                {
                  "text": ", then estimated time represents current traffic conditions assuming user departs “now” from the point of origin.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "durationSeconds",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "When ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", this route has tolls; if ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", this route has no tolls. If the value isn’t defined (“undefined”), the route may or may not have tolls.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "hasTolls",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The route name that you can use for display purposes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of integer values that you can use to determine the number steps along this route. Each value in the array corresponds to an index into the ",
                  "type": "text"
                },
                {
                  "code": "steps",
                  "type": "codeVoice"
                },
                {
                  "text": " array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stepIndexes",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[integer]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Automobile",
                "Walking"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that represents the mode of transportation the service used to estimate the arrival time. Same as the input query param ",
                  "type": "text"
                },
                {
                  "code": "transportType",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "Automobile",
                  "type": "codeVoice"
                },
                {
                  "text": " if the input query didn’t specify a transportation type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transportType",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
