      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Total distance covered by the step, in meters.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "distanceMeters",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The estimated time to traverse this step, in seconds.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "durationSeconds",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localized instruction string for this step that you can use for display purposes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You can specify the language to receive the response in using the ",
                  "type": "text"
                },
                {
                  "code": "lang",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "instructions",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to this step’s path. The pointer is in the form of an index into the ",
                  "type": "text"
                },
                {
                  "code": "stepPaths",
                  "type": "codeVoice"
                },
                {
                  "text": " array contained in a ",
                  "type": "text"
                },
                {
                  "code": "Route",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Step paths are self-contained which implies that the last point of a previous step path along a route is the same as the first point of the next step path. Clients are responsible for avoiding duplication when rendering the point.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stepPathIndex",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "Automobile",
                "Walking"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that indicates the transport type for this step if it’s different from the ",
                  "type": "text"
                },
                {
                  "code": "transportType",
                  "type": "codeVoice"
                },
                {
                  "text": " in the route.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transportType",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
