    "title": "Determine estimated arrival times and distances to one or more destinations"
  },
  "primaryContentSections": [
    {
      "kind": "restEndpoint",
      "title": "URL",
      "tokens": [
        {
          "kind": "method",
          "text": "GET"
        },
        {
          "kind": "text",
          "text": " "
        },
        {
          "kind": "baseURL",
          "text": "https://maps-api.apple.com/"
        },
        {
          "kind": "path",
          "text": "v1/etas"
        }
      ]
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The starting point for estimated arrival time requests, specified as a comma-separated string that contains the latitude and longitude. For example, ",
                  "type": "text"
                },
                {
                  "code": "origin=37.331423,-122.030503",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "origin",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Destination coordinates represented as pairs of latitude and longitude separated by a vertical bar character (”|”).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "For example, ",
                  "type": "text"
                },
                {
                  "code": "destinations=37.32556561130194,-121.94635203581443|37.44176585512703,-122.17259315798667",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The parameter must specify at least one destination coordinate, but no more than 10 destinations. Specify the location as a comma-separated string that contains the latitude and longitude.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destinations",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "Automobile"
            },
            {
              "kind": "allowedValues",
              "values": [
                "Automobile",
                "Transit",
                "Walking"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The mode of transportation to use when estimating arrival times.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transportType",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time of departure to use in an estimated arrival time request, in ISO 8601 format in UTC time.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "For example, ",
                  "type": "text"
                },
                {
                  "code": "departureDate=2020-09-15T16:42:00Z",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you don’t specify a departure date, the server uses the current date and time when you make the request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "departureDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The intended time of arrival in ISO 8601 format in UTC time.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "arrivalDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "restParameters",
      "source": "query",
      "title": "Query Parameters"
    },
    {
      "items": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/EtaResponse",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " objects that contain distance and time from the origin to each destination.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "mimeType": "application/json",
          "reason": "OK",
          "status": 200,
          "type": [
            {
              "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/EtaResponse",
              "kind": "typeIdentifier",
