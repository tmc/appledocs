    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The place to search for. For example, ",
                  "type": "text"
                },
                {
                  "code": "q=eiffel tower",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "q",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A comma-separated list of strings that describes the points of interest to exclude from the search results. For example, ",
                  "type": "text"
                },
                {
                  "code": "excludePoiCategories=Restaurant,Cafe",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/PoiCategory",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a complete list of possible values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "excludePoiCategories",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/PoiCategory",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:maps_web_api:PoiCategory",
              "text": "PoiCategory"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A comma-separated list of strings that describes the points of interest to include in the search results. For example, ",
                  "type": "text"
                },
                {
                  "code": "includePoiCategories=Restaurant,Cafe",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/PoiCategory",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a complete list of possible values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "includePoiCategories",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/PoiCategory",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:maps_web_api:PoiCategory",
              "text": "PoiCategory"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A comma-separated list of two-letter ISO 3166-1 codes of the countries to limit the results to. For example, ",
                  "type": "text"
                },
                {
                  "code": "limitToCountries=US,CA",
                  "type": "codeVoice"
                },
                {
                  "text": " limits the search to the United States and Canada.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you specify two or more countries, the results reflect the best available results for some or all of the countries rather than everything related to the query for those countries.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "limitToCountries",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A comma-separated list of strings that describes the kind of result types to include in the response. For example, ",
                  "type": "text"
                },
                {
                  "code": "resultTypeFilter=Poi",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resultTypeFilter",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/SearchResultType",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:maps_web_api:SearchResultType",
              "text": "SearchResultType"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "en-US"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The language the server should use when returning the response, specified using a BCP 47 language code. For example, for English use ",
                  "type": "text"
                },
                {
                  "code": "lang=en-US",
                  "type": "codeVoice"
                },
                {
                  "text": ". Defaults to ",
                  "type": "text"
                },
                {
                  "code": "en-US",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lang",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/Lang",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:maps_web_api:Lang",
              "text": "Lang"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A location defined by the application as a hint. Specify the location as a comma-separated string containing the latitude and longitude. For example, ",
                  "type": "text"
                },
                {
                  "code": "searchLocation=37.78,-122.42",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "searchLocation",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/SearchLocation",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:maps_web_api:SearchLocation",
              "text": "SearchLocation"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A region the app defines as a hint. Specify the region specified as a comma-separated string that describes the region in the form north-latitude,east-longitude,south-latitude,west-longitude. For example, ",
                  "type": "text"
                },
                {
                  "code": "searchRegion=38,-122.1,37.5,-122.5",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "searchRegion",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/SearchRegion",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:maps_web_api:SearchRegion",
              "text": "SearchRegion"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The location of the user, specified as a comma-separated string that contains the latitude and longitude. For example, ",
                  "type": "text"
                },
                {
                  "code": "userLocation=37.78,-122.42",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Search may opt to use the ",
                  "type": "text"
                },
                {
                  "code": "userLocation",
                  "type": "codeVoice"
                },
                {
                  "text": ", if specified, as a fallback for the ",
                  "type": "text"
                },
                {
                  "code": "searchLocation",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "userLocation",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/UserLocation",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:maps_web_api:UserLocation",
              "text": "UserLocation"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "default",
                "required"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that indicates the importance of the configured region.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "searchRegionPriority",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that tells the server that we expect paginated results.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "enablePagination",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that indicates which page of results to return.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pageToken",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A comma-separated list of strings that describes the addresses to include in the search results. For example, ",
                  "type": "text"
                },
                {
                  "code": "includeAddressCategories=SubLocality,PostalCode",
                  "type": "codeVoice"
                },
                {
                  "text": ". If you use this parameter, you must include ",
                  "type": "text"
                },
                {
                  "code": "address",
                  "type": "codeVoice"
                },
                {
                  "text": " in ",
                  "type": "text"
                },
                {
                  "code": "resultTypeFilter",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applemapsserverapi/documentation/AppleMapsServerAPI/AddressCategory",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a complete list of possible values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "includeAddressCategories",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
