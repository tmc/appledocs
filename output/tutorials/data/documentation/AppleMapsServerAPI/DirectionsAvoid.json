    {
      "content": [
        {
          "anchor": "Possible-Values",
          "level": 2,
          "text": "Possible Values",
          "type": "heading"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "When you set ",
                        "type": "text"
                      },
                      {
                        "code": "avoid=Tolls",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", routes without tolls are higher up in the list of returned routes. Note that even when you set ",
                        "type": "text"
                      },
                      {
                        "code": "avoid=Tolls",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", the routes the server returns may contain tolls (if no reasonable toll-free routes exist). Ensure you check the value of ",
                        "type": "text"
                      },
                      {
                        "code": "routes[i].hasTolls",
                        "type": "codeVoice"
                      },
                      {
                        "text": " in the response to verify toll assumptions.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "Tolls",
                    "type": "text"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        }
      ],
      "kind": "content"
