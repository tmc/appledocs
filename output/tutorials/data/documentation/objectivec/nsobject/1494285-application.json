{
  "abstract": [
    {
      "text": "Sent by Cocoa’s built-in scripting support during execution of ",
      "type": "text"
    },
    {
      "code": "get",
      "type": "codeVoice"
    },
    {
      "text": " or ",
      "type": "text"
    },
    {
      "code": "set",
      "type": "codeVoice"
    },
    {
      "text": " script commands to find out if the delegate can handle operations on the specified key-value key.",
      "type": "text"
    }
  ],
