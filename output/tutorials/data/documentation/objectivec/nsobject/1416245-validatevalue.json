                  "type": "codeVoice"
                },
                {
                  "text": " (with one or more relationships); for example ",
                  "type": "text"
                },
                {
                  "code": "department.name",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "department.manager.lastName",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
