            },
            {
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " to an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/objectivec/1418956-nsobject/1418795-isequal",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " request. See the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsdata",
              "isActive": true,
              "type": "reference"
            },
            {
