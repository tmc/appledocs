    "doc://com.apple.documentation/documentation/PDFKit/PDFAnnotationSubtype/widget": {
      "abstract": [
        {
          "text": "An annotation that displays interactive form elements, including text or signature fields, radio buttons, checkboxes, push buttons, pop-ups, and tables.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "keyword",
          "text": "static"
        },
        {
          "kind": "text",
