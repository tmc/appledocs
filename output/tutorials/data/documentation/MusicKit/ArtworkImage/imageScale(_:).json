    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The example below shows the relative scaling effect. The system renders",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the image at a relative size based on the available space and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "configuration options of the image it is scaling.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "VStack {",
            "    HStack {",
            "        Image(systemName: \"heart.fill\")",
            "            .imageScale(.small)",
            "        Text(\"Small\")",
            "    }",
            "    HStack {",
            "        Image(systemName: \"heart.fill\")",
            "            .imageScale(.medium)",
            "        Text(\"Medium\")",
            "    }",
            "",
            "    HStack {",
            "        Image(systemName: \"heart.fill\")",
            "            .imageScale(.large)",
            "        Text(\"Large\")",
            "    }",
            "}"
          ],
