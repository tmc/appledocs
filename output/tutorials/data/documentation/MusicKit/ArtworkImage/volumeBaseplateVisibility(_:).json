{
  "abstract": [
    {
      "text": "Sets the visibility of the baseplate of a volume, which appears",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "when a user looks towards the ‘floor’ of a volume and during resize.",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "Both ",
      "type": "text"
    },
    {
      "code": "automatic",
      "type": "codeVoice"
    },
    {
      "text": " and ",
      "type": "text"
    },
    {
      "code": "visible",
      "type": "codeVoice"
    },
    {
      "text": " will show the baseplate.",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "code": "hidden",
      "type": "codeVoice"
    },
    {
      "text": " will never show it.",
      "type": "text"
    }
  ],
