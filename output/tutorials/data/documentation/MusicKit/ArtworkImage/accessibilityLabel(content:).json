          "content": [
            {
              "inlineContent": [
                {
                  "text": "A view builder closure that takes a proxy",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "value representing the modified view. You can combine the modified",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "view with other content to create a new accessibility label for",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the original view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
