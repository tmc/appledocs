    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Which type of feedback to play.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "feedback"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value to monitor for changes to determine when to play.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trigger"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure to determine whether to play the feedback when",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "trigger",
                  "type": "codeVoice"
                },
                {
                  "text": " changes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "condition"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, you could play feedback for certain state transitions:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyView: View {",
            "    @State private var phase = Phase.inactive",
            "",
            "    var body: some View {",
            "        ContentView(phase: $phase)",
            "            .sensoryFeedback(.selection, trigger: phase) { old, new in",
            "                old == .inactive || new == .expanded",
            "            }",
            "    }",
            "",
            "    enum Phase {",
            "        case inactive",
            "        case preparing",
            "        case active",
            "        case expanded",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
