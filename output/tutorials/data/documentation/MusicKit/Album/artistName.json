        {
          "inlineContent": [
            {
              "text": "You can find more precise information about this album’s artists",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.MusicKit/documentation/MusicKit/Album/artists",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " relationship, which, unlike ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.MusicKit/documentation/MusicKit/Album/artistName",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "requires that you load it explicitly using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.MusicKit/documentation/MusicKit/MusicPropertyContainer/with(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method, as in the following example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "    let detailedAlbum = try await album.with([.artists])",
            "    let firstArtist = album.artists?.first"
          ],
          "syntax": "swift",
          "type": "codeListing"
