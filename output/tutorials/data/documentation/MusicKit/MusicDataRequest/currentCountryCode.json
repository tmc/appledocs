            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.MusicKit/documentation/MusicKit/MusicDataRequest",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " because a typical catalog endpoint for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Apple Music API requires the inclusion of a country code",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in the path of the corresponding URL.",
