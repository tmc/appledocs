        {
          "inlineContent": [
            {
              "text": "This property tells you how many views you’re responsible for filling with your content. For example, this function returns ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": " for a monoscopic display and ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": " for a stereoscopic display.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
