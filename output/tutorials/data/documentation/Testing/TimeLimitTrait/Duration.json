        },
        {
          "inlineContent": [
            {
              "text": "This type is intended for use specifically for specifying test timeouts",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with ",
              "type": "text"
            },
            {
              "identifier": "doc://org.swift.testing/documentation/Testing/TimeLimitTrait",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". It is used instead of Swiftâ€™s built-in ",
              "type": "text"
            },
            {
              "code": "Duration",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "type because test timeouts do not support high-precision, arbitrarily",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "short durations. The smallest allowed unit of time is minutes.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
