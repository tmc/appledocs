        {
          "inlineContent": [
            {
              "text": "The default type is ",
              "type": "text"
            },
            {
              "code": "Never",
              "type": "codeVoice"
            },
            {
              "text": ", which cannot be instantiated. The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "scopeProvider(for:testCase:)-cjmg",
              "type": "codeVoice"
            },
            {
              "text": " method for any trait with this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "default type must return ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", meaning that trait will not provide a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "custom scope for the tests it’s applied to.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
