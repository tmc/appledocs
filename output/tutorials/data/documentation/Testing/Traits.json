  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Pass built-in traits to test functions or suite types to comment, categorize,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "classify, and modify runtime behaviors. You can also use the ",
              "type": "text"
            },
            {
              "identifier": "doc://org.swift.testing/documentation/Testing/Trait",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://org.swift.testing/documentation/Testing/TestTrait",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and ",
              "type": "text"
            },
            {
              "identifier": "doc://org.swift.testing/documentation/Testing/SuiteTrait",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " protocols to create your own types that customize the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "behavior of test functions.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
