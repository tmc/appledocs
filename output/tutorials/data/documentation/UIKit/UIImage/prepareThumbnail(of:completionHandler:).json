    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The desired size of the thumbnail.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "size"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The completion handler to call when the thumbnail is ready. The handler executes on a background thread.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The completion handler takes the following parameters:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "A new thumbnail image. This parameter is ",
                            "type": "text"
                          },
                          {
                            "code": "nil",
                            "type": "codeVoice"
                          },
                          {
                            "text": " if the original image isnâ€™t backed by a ",
                            "type": "text"
                          },
                          {
                            "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgimage",
                            "isActive": true,
                            "type": "reference"
                          },
                          {
                            "text": " or if the image data is corrupt or malformed.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "thumbnail",
                        "type": "codeVoice"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            }
          ],
          "name": "completionHandler"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When displaying an image in a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UIImageView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", you can use the viewâ€™s ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UIView/contentMode-swift.property",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property to clip or scale the image automatically. But when the native image size is much larger than the bounds of the view, decoding the full size image creates unnecessary memory overhead. By creating a thumbnail image at a specified size with this method, you avoid the overhead of decoding the image at its full size.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This method asynchronously creates the thumbnail image on a background thread and calls the completion handler on that thread. If your app updates the UI in the completion handler, schedule the UI update on the main thread.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "tabs": [
            {
              "content": [
                {
                  "code": [
                    "func collectionView(_ collectionView: UICollectionView, cellForItemAt indexPath: IndexPath) -> UICollectionViewCell {",
                    "    guard let cell = collectionView.dequeueReusableCell(withReuseIdentifier: cellIdentifier, for: indexPath) as? ItemCell else {",
                    "        fatalError(\"Unexpected type for cell. Check configuration.\")",
                    "    }",
                    "        ",
                    "    let item = items[indexPath.item]",
                    "    cell.nameLabel?.text = item.name",
                    "    item.image.prepareThumbnail(of: thumbnailSize) { thumbnail in",
                    "        DispatchQueue.main.async {",
                    "            cell.thumbnailImageView?.image = thumbnail",
                    "        }",
                    "    }",
                    "    return cell",
                    "}"
                  ],
                  "syntax": "swift",
                  "type": "codeListing"
                }
              ],
              "title": "Swift"
            },
            {
              "content": [
                {
                  "code": [
                    "- (UICollectionViewCell *)collectionView:(UICollectionView *)collectionView cellForItemAtIndexPath:(NSIndexPath *)indexPath {",
                    "    UICollectionViewCell *cell = [collectionView dequeueReusableCellWithReuseIdentifier:self.cellIdentifier forIndexPath:indexPath];",
                    "    NSAssert([cell isKindOfClass:[ItemCell class]], @\"Unexpected type for cell. Check configuration.\\n\");",
                    "    ",
                    "    Item *item = self.items[indexPath.row];",
                    "    ItemCell *itemCell = (ItemCell *)cell;",
                    "    itemCell.nameLabel.text = item.name;",
                    "    [item.image prepareThumbnailOfSize:self.thumbnailSize completionHandler:^(UIImage *thumbnail) {",
                    "        dispatch_async(dispatch_get_main_queue(), ^{",
                    "            itemCell.thumbnailImageView.image = thumbnail;",
                    "        });",
                    "    }];",
                    "    return cell;",
                    "}"
                  ],
                  "syntax": "objc",
                  "type": "codeListing"
                }
              ],
              "title": "Objective-C"
            }
          ],
          "type": "tabNavigator"
        }
      ],
