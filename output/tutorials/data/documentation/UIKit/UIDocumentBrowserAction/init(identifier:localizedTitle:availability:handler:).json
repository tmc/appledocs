            }
          ],
          "name": "identifier"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The title that appears in the Edit Menu or navigation bar. This title should be a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/Swift/String",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " returned by  ",
                  "type": "text"
                },
                {
