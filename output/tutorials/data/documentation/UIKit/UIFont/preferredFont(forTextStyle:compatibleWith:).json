          "content": [
            {
              "inlineContent": [
                {
                  "text": "The text style for which to return a font. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/UIFont/TextStyle",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for recognized values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "style"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The traits to use when determining which font to return.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
