{
  "abstract": [
    {
      "text": "Modify the behavior of Writing Tools in standard iOS text views,",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "and adjust your app’s behavior while the feature is active.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
