        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier for the menu configuration object. If you want this method to generate a unique identifier for you, specify ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "identifier"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional block that returns the custom view controller that you use to preview content. If you specify ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", UIKit uses a default preview view controller.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "previewProvider"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional block that provides a contextual menu to display with the preview. If you specify ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", UIKit doesnâ€™t display a contextual menu with the previewed content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "actionProvider"
        }
      ]
    },
