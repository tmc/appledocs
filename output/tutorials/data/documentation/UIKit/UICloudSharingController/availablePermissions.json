face, and the ","type":"text"},{"identifier":"doc://com.apple.uikit/documentation/UIKit/UICloudSharingController/share","isActive":true,"type":"reference"},{"text":" permission setting defaults to the included ","type":"text"},{"identifier":"doc://com.apple.uikit/documentation/UIKit/UICloudSharingController/PermissionOptions","isActive":true,"type":"reference"},{"text":" option.","type":"text"}]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc://com.apple.documentation/documentation/technologies","doc://com.apple.uikit/documentation/UIKit","doc://com.apple.uikit/documentation/UIKit/UICloudSharingController"]]},"variants":[{"paths":["/documentation/uikit/uicloudsharingcontroller/availablepermissions"],"traits":[{"interfaceLanguage":"swift"}]},{"paths":["/documentation/uikit/uicloudsharingcontroller/availablepermissions"],"traits":[{"interfaceLanguage":"occ"}]}],"metadata":{"externalID":"c:objc(cs)UICloudSharingController(py)availablePermissions","navigatorTitle":[{"text":"availablePermissions","kind":"identifier"}],"role":"symbol","roleHeading":"Instance Property","symbolKind":"property","platforms":[{"introducedAt":"10.0","deprecated":false,"unavailable":false,"name":"iOS","beta":false},{"introducedAt":"10.0","deprecated":false,"unavailable":false,"name":"iPadOS","beta":false},{"introducedAt":"13.1","deprecated":false,"unavailable":false,"name":"Mac Catalyst","beta":false},{"introducedAt":"1.0","deprecated":false,"unavailable":false,"name":"visionOS","beta":false}],"title":"availablePermissions","modules":[{"name":"UIKit"}],"fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"availablePermissions"},{"kind":"text","text":": "},{"preciseIdentifier":"c:objc(cs)UICloudSharingController","kind":"typeIdentifier","text":"UICloudSharingController"},{"kind":"text","text":"."},{"text":"PermissionOptions","kind":"typeIdentifier","preciseIdentifier":"c:@E@UICloudSharingPermissionOptions"}]},"sections