      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The object to be registered with the restoration archive. The object must adopt the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/UIStateRestoring",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " protocol. This parameter must not be ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "object"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The restoration identifier for the object. UIKit uses this parameter to distinguish the object from other objects in the archive. This parameter must not be ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
