    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This option is ignored if ",
              "type": "text"
            },
            {
              "code": "NSStringDrawingUsesLineFragmentOrigin",
              "type": "codeVoice"
            },
            {
              "text": " is not also set. In addition, the line break mode must be either ",
              "type": "text"
            },
            {
              "code": "NSLineBreakByWordWrapping",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "NSLineBreakByCharWrapping",
              "type": "codeVoice"
            },
            {
              "text": " for this option to take effect. The line break mode can be specified in a paragraph style passed in the attributes dictionary argument of the drawing methods.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
