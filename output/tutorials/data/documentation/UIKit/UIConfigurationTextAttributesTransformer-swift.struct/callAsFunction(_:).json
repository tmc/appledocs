    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Using this syntax, you can call the text attributes transformer type as if it were a closure:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var container = AttributeContainer()",
            "container.backgroundColor = UIColor.blue",
            "let transformer = UIConfigurationTextAttributesTransformer { incoming in",
            "    var outgoing = incoming",
            "    outgoing.backgroundColor = incoming.backgroundColor?.withAlphaComponent(0.6)",
            "    return outgoing",
            "}",
            "let transformed = transformer.callAsFunction(container)",
            ""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
