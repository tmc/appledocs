{
  "abstract": [
    {
      "text": "For gestures that do not easily match a specific pattern, or when you want to use a gesture recognizer to gather touch input, create a continuous gesture recognizer.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
