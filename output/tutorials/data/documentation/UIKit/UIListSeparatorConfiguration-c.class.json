      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To specify list separator appearance for a section, set a default sectionwide ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UICollectionLayoutListConfiguration-c.class/separatorConfiguration",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " on your ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UICollectionLayoutListConfiguration-swift.struct",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " when you create your list.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var listConfig = UICollectionLayoutListConfiguration(appearance: .plain)",
            "listConfig.separatorConfiguration.color = .tertiarySystemFill",
            "let layout = UICollectionViewCompositionalLayout.list(using: listConfig)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "To override list separator appearance on a per-item basis, use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UICollectionLayoutListConfiguration-c.class/itemSeparatorHandler",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var listConfig = UICollectionLayoutListConfiguration(appearance: .plain)",
            "listConfig.separatorConfiguration.color = .tertiarySystemFill",
            "",
            "let indexPathToHide = IndexPath() ",
            " ",
            "listConfig.itemSeparatorHandler = { (indexPath, sectionSeparatorConfiguration) in    ",
            "    var configuration = sectionSeparatorConfiguration",
            "    if indexPath == indexPathToHide {",
            "        configuration.bottomSeparatorVisibility = .hidden    ",
            "    }    ",
            "    return configuration",
            "}",
            "",
            "let layout = UICollectionViewCompositionalLayout.list(using: listConfig)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
