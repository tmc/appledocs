    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The patterns to detect on the pasteboard.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "patterns"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An index set with each integer value identifying a pasteboard item positionally in the pasteboard. Pass ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to detect patterns in all pasteboard items.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "itemSet"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A block that the system invokes after detecting patterns on the pasteboard. The block receives a ",
                  "type": "text"
                },
                {
                  "code": "Result",
                  "type": "codeVoice"
                },
                {
                  "text": " instance that contains either an array with the patterns found on the pasteboard or an error if detection failed. If the ",
                  "type": "text"
                },
                {
                  "code": "Result",
                  "type": "codeVoice"
                },
                {
                  "text": " instance contains an array, the index of each element in the array corresponds to the pasteboard item index specified in ",
                  "type": "text"
                },
                {
                  "code": "itemSet",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "completionHandler"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Because this method only detects for the presence of patterns and does not read the contents of the pasteboard, the system doesnâ€™t notify the user about reading the contents of the pasteboard.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
