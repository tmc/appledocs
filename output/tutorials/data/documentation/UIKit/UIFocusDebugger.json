        {
          "inlineContent": [
            {
              "text": "You do not use this class or its methods directly from your code. During a debugging session, you can call the methods of this class from the ",
              "type": "text"
            },
            {
              "code": "lldb",
              "type": "codeVoice"
            },
            {
              "text": " debugger command line to obtain information about the current state of the focus system.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
