            {
              "inlineContent": [
                {
                  "text": "Return ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/swift/true",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to enable input clicks by way of the ",
                  "type": "text"
                },
                {
