      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An identifier for the detent. Specify a unique identifier for each custom detent for a sheet. If you don’t specify an identifier, the system generates a random identifier.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "identifier"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A block for resolving the detent value with an input of type ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/UISheetPresentationControllerDetentResolutionContext",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". The value you return from this block is a height within the safe area of the sheet. For example, return ",
                  "type": "text"
                },
                {
                  "code": "200",
                  "type": "codeVoice"
                },
                {
                  "text": " for a detent with a height of ",
                  "type": "text"
                },
                {
                  "code": "200",
                  "type": "codeVoice"
                },
                {
                  "text": " plus ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/UIView/safeAreaInsets",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/UIEdgeInsets/bottom",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " when the sheet is edge-attached, or ",
                  "type": "text"
                },
                {
                  "code": "200",
                  "type": "codeVoice"
                },
                {
                  "text": " when the sheet is floating. Return ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/UISheetPresentationControllerDetentInactive",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to specify that the detent is inactive according to the provided context.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If the block depends on any external inputs, call ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/UISheetPresentationController/invalidateDetents()",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " on the sheet when the external inputs change.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Don’t set any properties on ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/UISheetPresentationController",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " during the execution of this block.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resolver"
        }
      ]
    }
