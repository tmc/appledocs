        {
          "inlineContent": [
            {
              "text": "To customize the overall appearance of a navigation bar, use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UIAppearance",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " APIs instead of this method. If you use this initialization method to create a navigation bar that uses custom bar subclasses, you are responsible for pushing and setting view controllers before presenting the navigation controller onscreen.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
