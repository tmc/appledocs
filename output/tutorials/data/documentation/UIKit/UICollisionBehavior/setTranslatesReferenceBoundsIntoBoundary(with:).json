      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Insets to apply to the reference system’s bounds when defining the collision boundary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
