        {
          "inlineContent": [
            {
              "text": "UIKit populates this property with an appropriate role value based on the contents of your app’s ",
              "type": "text"
            },
            {
              "code": "Info.plist",
              "type": "codeVoice"
            },
            {
              "text": " file. You also specify this value when you create a new scene-configuration object.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
