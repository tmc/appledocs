        {
          "inlineContent": [
            {
              "text": "This property contains a description of the attribution source. For example, for an ad, ",
              "type": "text"
            },
            {
              "code": "sourceDescription",
              "type": "codeVoice"
            },
            {
              "text": " describes the content of the advertisement that the user tapped.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
