    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Always check the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UIMenuBuilder/system",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property to determine which menu system the builder is modifying before you add and remove menus. For example, when you want to modify the main menu bar, check ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UIMenuBuilder/system",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UIMenuSystem/main",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "override func buildMenu(with builder: UIMenuBuilder) {",
            "    super.buildMenu(with: builder)",
            "    ",
            "    // Ensure that the builder is modifying the menu bar system.",
            "    guard builder.system == UIMenuSystem.main else { return }",
            "    ",
            "    let refreshCommand = UICommand(title: \"Refresh\", action: #selector(refreshData(_:)))",
            "    let refreshMenu = UIMenu(title: \"\", options: .displayInline, children: [refreshCommand])",
            "",
            "    // Insert the menu into the File menu before the Close menu.",
            "    builder.insertSibling(refreshMenu, beforeMenu: .close)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
