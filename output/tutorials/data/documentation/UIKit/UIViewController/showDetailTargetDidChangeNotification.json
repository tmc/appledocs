        },
        {
          "inlineContent": [
            {
              "text": "When a view controller is using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UIViewController/show(_:sender:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.uikit/documentation/UIKit/UIViewController/showDetailViewController(_:sender:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", it may need to know when a split view controller higher in the view hierarchy has changed. This notification is sent when a split view controller expands or collapses. The object of this notification is the view controller that caused the change.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
