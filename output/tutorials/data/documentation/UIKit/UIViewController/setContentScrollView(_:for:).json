          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scroll view to observe. If ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", the view controller determines a scroll view automatically.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "scrollView"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The edge to observe for scroll view content alignment. Pass ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/NSDirectionalRectEdge/top",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/NSDirectionalRectEdge/bottom",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to set the scroll view for a specific edge, or pass ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.uikit/documentation/UIKit/NSDirectionalRectEdge/all",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to set the scroll view for all edges.",
                  "type": "text"
                }
              ],
