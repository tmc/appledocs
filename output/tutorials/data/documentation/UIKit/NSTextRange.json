    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "An ",
              "type": "text"
            },
            {
              "code": "NSTextRange",
              "type": "codeVoice"
            },
            {
              "text": " consists of the starting and terminating locations. There the two basic properties: ",
              "type": "text"
            },
            {
              "code": "location",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "endLocation",
              "type": "codeVoice"
            },
            {
              "text": ", respectively. The terminating ",
              "type": "text"
            },
            {
              "code": "location",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "endLocation",
              "type": "codeVoice"
            },
            {
              "text": ", is directly following the last location in the range. For example, a location contains a range if ",
              "type": "text"
            },
            {
              "code": "(range.location <= location) && (location < range.endLocation)",
              "type": "codeVoice"
            },
            {
              "text": " is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
