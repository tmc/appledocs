          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "AUD",
                "CAD",
                "EUR",
                "GBP",
                "JPY",
                "MXN",
                "NZD",
                "RUB",
                "USD"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The organization’s default currency that is set up in the ",
                  "type": "text"
                },
                {
                  "identifier": "https://searchads.apple.com/advanced",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "currency",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier of the organization that owns the campaign. Your ",
                  "type": "text"
                },
                {
                  "code": "orgId",
                  "type": "codeVoice"
                },
                {
                  "text": " is the same as your account in the ",
                  "type": "text"
                },
                {
                  "identifier": "https://searchads.apple.com/advanced",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orgId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of your organization.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orgName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Distinguishes the account from an ",
                  "type": "text"
                },
                {
                  "code": "orgId",
                  "type": "codeVoice"
                },
                {
                  "text": " belonging to a suborganization.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "parentOrgId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "LOC",
                "PAYG"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The payment model that you set through the ",
                  "type": "text"
                },
                {
                  "identifier": "https://searchads.apple.com/advanced",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". If you don’t set a payment model, campaigns can’t run.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "A pay-as-you-go payment model.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "PAYG",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "A line-of-credit payment model.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "LOC",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "There is no set payment method.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "<empty string>",
                        "type": "codeVoice"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            },
            {
              "inlineContent": [
                {
                  "text": "If you don’t select a payment model, you can still create campaigns. You must select a payment model before a campaign is eligible to run.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/Apple_Search_Ads/budget-orders",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for endpoints used to manage budget orders.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "paymentModel",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Each role governs what a user can see and do within the account:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "Manage all campaigns within an account with read-and-write capabilities.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "text": "API Account Manager",
                        "type": "text"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            },
            {
              "inlineContent": [
                {
                  "text": "View reporting across the organization.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Create and edit an API public key.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "View reporting across the account with read-only permission.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "text": "API Account Read Only",
                        "type": "text"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            },
            {
              "inlineContent": [
                {
                  "text": "Create and edit an API public key.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "API Read & Write:",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "View reporting.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "Manage all campaigns and settings within a campaign group with read-and-write capabilities.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "Create and edit an API public key.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "text": "Limited Access",
                        "type": "text"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "API Read Only:",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "View reporting across the organization.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "Create and edit an API public key.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "text": "Limited Access",
                        "type": "text"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            }
          ],
          "name": "roleNames",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time zone you set during account creation through the ",
                  "type": "text"
                },
                {
                  "identifier": "https://searchads.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". ",
                  "type": "text"
                },
                {
                  "code": "ORTZ",
                  "type": "codeVoice"
                },
                {
                  "text": ", the organization time zone, is the default.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timeZone",
          "required": false,
