  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Ads require approval by Apple. While in review, your ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Ad",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " is on hold. After approval, the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/AdServingStatus",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " changes to ",
              "type": "text"
            },
            {
              "code": "RUNNING",
              "type": "codeVoice"
            },
            {
              "text": " unless you schedule it to start on a specific date or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Update-an-Ad",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " status to ",
              "type": "text"
            },
            {
              "code": "PAUSED",
              "type": "codeVoice"
            },
            {
              "text": " while in review.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "In ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/Apple_Search_Ads/apple-search-ads-campaign-management-api-4#49",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " release, all previously rejected Today tab ad creatives were ",
                  "type": "text"
                },
                {
                  "code": "PAUSED",
                  "type": "codeVoice"
                },
                {
                  "text": " and resubmitted for re-review. You need to update the status to ",
                  "type": "text"
                },
                {
                  "code": "ENABLED",
                  "type": "codeVoice"
                },
                {
                  "text": " after the re-review. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Update-an-Ad",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Find-Ad-Creative-Rejection-Reasons",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Gets-a-product-page-reason",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to look up rejection reasons. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/ProductPageReason",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for rejection reason descriptions.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
