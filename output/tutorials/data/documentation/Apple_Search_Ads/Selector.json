              "inlineContent": [
                {
                  "text": "A list of condition objects that allow users to filter a list of records.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "conditions",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Condition",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:Condition",
              "text": "Condition"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of field names to return within each record.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fields",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of field names and grouping to sort the records by ",
                  "type": "text"
                },
                {
                  "code": "ASCENDING",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "DESCENDING",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orderBy",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Sorting",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:Sorting",
              "text": "Sorting"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A defined range and limit of the number of returned records.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pagination",
          "required": false,
          "type": [
            {
