    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this endpoint to obtain a ",
              "type": "text"
            },
            {
              "code": "reportId",
              "type": "codeVoice"
            },
            {
              "text": " to use in a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-a-Single-Impression-Share-Report",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " request. This endopoint supports selectors. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/CustomReportRequest",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for selector structure.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "You can generate up to 10 reports within 24 hours.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "You can create reports for a range of up to 30 days for any time period after ",
                      "type": "text"
                    },
                    {
                      "code": "2020-04-12",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "You can’t edit or remove report fields.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Impression Share reports with a ",
                      "type": "text"
                    },
                    {
                      "code": "WEEKLY",
                      "type": "codeVoice"
                    },
                    {
                      "text": " granularity value can’t have custom ",
                      "type": "text"
                    },
                    {
                      "code": "startTime",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "endTime",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in the request payload. Use ",
                      "type": "text"
                    },
                    {
                      "code": "dateRange",
                      "type": "codeVoice"
                    },
                    {
                      "text": " instead. See ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/CustomReportRequest",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Payload-example-Obtain-a-report-ID",
          "level": 3,
          "text": "Payload example: Obtain a report ID",
          "type": "heading"
        },
        {
          "tabs": [
            {
              "content": [
                {
                  "code": [
                    "POST https://api.searchads.apple.com/api/v5/custom-reports",
                    "",
                    "{",
                    "  \"name\": \"impression_share_API_report_example_1\",",
                    "  \"startTime\": \"2024-01-20\",",
                    "  \"endTime\": \"2024-01-29\",",
                    "  \"granularity\": \"DAILY\",",
                    "  \"selector\": {",
                    "    \"conditions\": [",
                    "      {",
                    "        \"field\": \"countryOrRegion\",",
                    "        \"operator\": \"IN\",",
                    "        \"values\": [",
                    "          \"US\",",
                    "          \"AU\"",
                    "        ]",
                    "      }",
                    "    ]",
                    "  }",
                    "}"
                  ],
                  "syntax": null,
                  "type": "codeListing"
                }
              ],
              "title": "Request"
            },
            {
              "content": [
                {
                  "code": [
                    "{",
                    "  \"data\": {",
                    "    \"id\": 986235,",
                    "    \"name\": \"impression_share_API_report_example_1\",",
                    "    \"startTime\": \"2024-01-20\",",
                    "    \"endTime\": \"2024-01-29\",",
                    "    \"granularity\": \"DAILY\",",
                    "    \"downloadUri\": \"https://blobstore.apple.com...\",",
                    "    \"dimensions\": [",
                    "      \"appName\",",
                    "      \"adamId\",",
                    "      \"countryOrRegion\",",
                    "      \"searchTerm\"",
                    "    ],",
                    "    “metrics”: [",
                    "      \"lowImpressionShare\",",
                    "      \"highImpressionShare\",",
                    "      \"rank\",",
                    "      \"searchPopularity\"",
                    "    ],",
                    "    \"selector\": {",
                    "      \"conditions\": [",
                    "        {",
                    "          \"field\": \"countryOrRegion\",",
                    "          \"operator\": \"IN\",",
                    "          \"values\": [",
                    "            \"US\",",
                    "            \"AU\"",
                    "          ]",
                    "        }",
                    "      ]",
                    "    },",
                    "    \"state\": \"QUEUED\",",
                    "    \"creationTime\": \"2024-01-12T04:47:27.782\",",
                    "    \"modificationTime\": \"2024-01-12T04:47:27.782\"",
                    "  },",
                    "  \"pagination\": null,",
                    "  \"error\": null",
                    "}"
                  ],
                  "syntax": "json",
                  "type": "codeListing"
                }
              ],
              "title": "Response"
            }
          ],
          "type": "tabNavigator"
        }
      ],
