    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The lifetime budget amount available to a campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Campaigns require a ",
                          "type": "text"
                        },
                        {
                          "code": "dailyBudgetAmount",
                          "type": "codeVoice"
                        },
                        {
                          "text": " or a ",
                          "type": "text"
                        },
                        {
                          "code": "budgetAmount",
                          "type": "codeVoice"
                        },
                        {
                          "text": ", or both.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "If you update a ",
                          "type": "text"
                        },
                        {
                          "code": "budgetAmount",
                          "type": "codeVoice"
                        },
                        {
                          "text": ", the updated amount must be greater than the previous ",
                          "type": "text"
                        },
                        {
                          "code": "budgetAmount",
                          "type": "codeVoice"
                        },
                        {
                          "text": " and must exceed the lifetime spend of the campaign.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "You can use the ",
                  "type": "text"
                },
                {
                  "code": "EQUALS",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "LESS_THAN",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "GREATER_THAN",
                  "type": "codeVoice"
                },
                {
                  "text": " selector ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Condition",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " operators with ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Find-Campaigns",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is updatable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "budgetAmount",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Money",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:Money",
              "text": "Money"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The budget orders that you assign to the campaign. This applies only to campaigns with a line-of-credit payment model. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/Apple_Search_Ads/budget-orders",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "budgetOrders",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[int64]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The App Store geoterritories where you’re promoting your app. The default value is ",
                  "type": "text"
                },
                {
                  "code": "US",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field requires an ISO country code value for the locations where you’re promoting.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "countriesOrRegions",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
