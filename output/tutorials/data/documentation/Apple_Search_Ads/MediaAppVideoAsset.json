    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier for an app preview or screenshot.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Your ",
                  "type": "text"
                },
                {
                  "code": "adamId",
                  "type": "codeVoice"
                },
                {
                  "text": " is the first numerical grouping in ",
                  "type": "text"
                },
                {
                  "code": "assetGenId",
                  "type": "codeVoice"
                },
                {
                  "text": ". For example, in ",
                  "type": "text"
                },
                {
                  "code": "1408851466;en-US;5;0;f8c9add6280c781e6f701c506be5a921",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "1408851466",
                  "type": "codeVoice"
                },
                {
                  "text": " is your ",
                  "type": "text"
                },
                {
                  "code": "adamId",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You can use the ",
                  "type": "text"
                },
                {
                  "code": "EQUALS",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "IN",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Selector",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Condition",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " operators with ",
                  "type": "text"
                },
                {
                  "code": "Find App Assets",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is sortable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "assetGenId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "APP_PREVIEW",
                "SCREENSHOT"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of creative asset.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "APP_PREVIEW",
                  "type": "codeVoice"
                },
                {
                  "text": " is a video still image of video assets that you upload to ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". Note, the playable URL isn’t in the API response.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "code": "SCREENSHOT",
                  "type": "codeVoice"
                },
                {
                  "text": " is a standard image  of the app that you upload to ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "overridingTitle": "App Store Connect",
                  "overridingTitleInlineContent": [
                    {
                      "text": "App Store Connect",
                      "type": "text"
                    }
                  ],
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "assetType",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The resolved URL for the screenshot. For a video asset, the image is the first frame.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "assetURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The fully resolved URL for the asset video. The field is non-null for preview assets; otherwise, it’s null.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "assetVideoURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "LANDSCAPE",
                "PORTRAIT",
                "UNKNOWN"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The orientation of the asset that you upload to ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orientation",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The position in the sort order to show the asset of app preview.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sortPosition",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The height of the asset that you upload to ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sourceHeight",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The width of the asset that you upload to ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sourceWidth",
          "required": false,
          "type": [
            {
              "kind": "text",
