      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this endpoint to create an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Ad",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object using your ",
              "type": "text"
            },
            {
              "code": "creativeId",
              "type": "codeVoice"
            },
            {
              "text": " in the request payload. To obtain a ",
              "type": "text"
            },
            {
              "code": "creativeId",
              "type": "codeVoice"
            },
            {
              "text": ", use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Create-a-Creative",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " endpoint. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Update-an-Ad",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to update or replace the ad group assignment of an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Ad",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In API version 5.2, this endpoint supports default product page ads. For more information, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/Apple_Search_Ads/apple-search-ads-campaign-management-api-5",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "Id",
              "type": "codeVoice"
            },
            {
              "text": " in the response is your ",
              "type": "text"
            },
            {
              "code": "adId",
              "type": "codeVoice"
            },
            {
              "text": ", representing the assignment of a creative to an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/AdGroup",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". Use your ",
              "type": "text"
            },
            {
              "code": "adId",
              "type": "codeVoice"
            },
            {
              "text": " in the resource path with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-an-Ad",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Update-an-Ad",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Delete-an-Ad",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", and in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-Ad-Level-Reports",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". Your ",
              "type": "text"
            },
            {
              "code": "adId",
              "type": "codeVoice"
            },
            {
              "text": " is also output in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/AdServices",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " attribution framework.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
