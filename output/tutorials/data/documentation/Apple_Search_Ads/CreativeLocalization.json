      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app name on ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "appName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "IPAD",
                "IPHONE"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The device classes assigned to a custom product page.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/DeviceClass",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for value descriptions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "deviceClasses",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The language associated with the ISO alpha-2 country code, such as ",
                  "type": "text"
                },
                {
                  "code": "US",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "language",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ISO 639-1 language code appended to the ISO alpha-2 country code, such as ",
                  "type": "text"
                },
                {
                  "code": "en-US",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "languageCode",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Text that appears at the top of the main description of a product page.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "promotionalText",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Concise, informative text to describe an app on a product page.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shortDescription",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A summary of an app that appears below the name of an app on a product page.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "subTitle",
          "required": false,
          "type": [
            {
              "kind": "text",
