        {
          "inlineContent": [
            {
              "text": "Use this endpoint to find all assigned Creative Sets for an ad group. Use the corresponding ",
              "type": "text"
            },
            {
              "code": "campaignId",
              "type": "codeVoice"
            },
            {
              "text": " of the ad group in the URI. Use the ",
              "type": "text"
            },
            {
              "code": "id",
              "type": "codeVoice"
            },
            {
              "text": " field with its corresponding ad group as a value in the request payload.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "3235201",
          "level": 3,
          "text": "Payload Example: Find Ad Group Creative Sets",
          "type": "heading"
        },
        {
          "request": {
            "content": [
              {
                "code": [
                  "POST https://api.searchads.apple.com/api/v4/campaigns/{campaignId}/adgroupcreativesets/find",
                  "",
                  "{",
                  "  \"selector\": {",
                  "    \"conditions\": [",
                  "      {",
                  "        \"field\": \"id\",",
                  "        \"operator\": \"EQUALS\",",
                  "        \"values\": [",
                  "          \"542317136\"",
                  "        ]",
                  "      }",
                  "    ]",
                  "  }",
                  "}"
                ],
                "collapsible": false
              }
            ]
          },
          "response": {
            "content": [
              {
                "code": [
                  "{",
                  "  \"data\": [",
                  "    {",
                  "      \"id\": 542317136,",
                  "      \"adGroupId\": 542317095,",
                  "      \"creativeSetId\": 542317136,",
                  "      \"campaignId\": 542370539,",
                  "      \"status\": \"ENABLED\",",
                  "      \"servingStatus\": \"RUNNING\",",
                  "      \"servingStatusReasons\": [],",
                  "      \"deleted\": false,",
                  "      \"modificationTime\": \"2020-04-08T22:12:18.270\"",
                  "    }",
                  "  ]",
                  "}"
                ],
                "collapsible": false
              }
            ]
