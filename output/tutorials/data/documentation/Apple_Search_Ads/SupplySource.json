    {
      "kind": "possibleValues",
      "title": "Possible Values",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Reaches users when they’re browsing apps across the App Store. Your ad appears at the top of the You Might Also Like list when users scroll to the bottom of relevant pages across the App Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "An optional ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/AppCategoryCriteria",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " targeting dimension is eligible to use with the ",
                  "type": "text"
                },
                {
                  "code": "APPSTORE_PRODUCT_PAGES_BROWSE",
                  "type": "codeVoice"
                },
                {
                  "text": " supply source.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See the Discussion section below for the combination of campaign and ad group values to use in campaign payloads with ",
                  "type": "text"
                },
                {
                  "code": "APPSTORE_PRODUCT_PAGES_BROWSE",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "APPSTORE_PRODUCT_PAGES_BROWSE"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The campaign runs in App Store search results.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See the Discussion section below for the combination of campaign and ad group values to use in campaign payloads with ",
                  "type": "text"
                },
                {
                  "code": "APPSTORE_SEARCH_RESULTS",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "APPSTORE_SEARCH_RESULTS"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The campaign runs on the App Store Search tab.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See the Discussion section below for the combination of campaign and ad group values to use in campaign payloads with ",
                  "type": "text"
                },
                {
                  "code": "APPSTORE_SEARCH_TAB",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Ad group validations include:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "You can’t use a ",
                          "type": "text"
                        },
                        {
                          "code": "cpaGoal",
                          "type": "codeVoice"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "You can’t use keywords.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "You can’t use the daypart targeting dimension.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Search Match (",
                          "type": "text"
                        },
                        {
                          "code": "automatedKeywordsOptIn",
                          "type": "codeVoice"
                        },
                        {
                          "text": ") can’t be ",
                          "type": "text"
                        },
                        {
                          "code": "true",
                          "type": "codeVoice"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "APPSTORE_SEARCH_TAB"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "See the Today tab compatibility subsection below.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "APPSTORE_TODAY_TAB"
        }
      ]
    },
