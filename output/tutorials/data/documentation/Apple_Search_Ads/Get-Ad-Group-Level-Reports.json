      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this endpoint to fetch reports for your ad groups in campaigns. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/ReportingAdGroup",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Condition",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " operators and field values to filter results with an object ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Selector",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". See the ",
              "type": "text"
            },
            {
              "code": "groupBy",
              "type": "codeVoice"
            },
            {
              "text": " parameter description in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/ReportingRequest",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for supported values per targeting dimension. The ",
              "type": "text"
            },
            {
              "code": "orderBy",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Selector",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " specifies fields to sort the records list by ",
              "type": "text"
            },
            {
              "code": "ASCENDING",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "DESCENDING",
              "type": "codeVoice"
            },
            {
              "text": ". All ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/ReportingAdGroup",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " fields are available to the ",
              "type": "text"
            },
            {
              "code": "orderBy",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Selector",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " except ",
              "type": "text"
            },
            {
              "code": "adGroupServingStateReasons",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
