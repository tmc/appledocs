        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dimension to target users by ",
                  "type": "text"
                },
                {
                  "code": "adminArea.",
                  "type": "codeVoice"
                },
                {
                  "text": " An ",
                  "type": "text"
                },
                {
                  "code": "adminArea",
                  "type": "codeVoice"
                },
                {
                  "text": " is a state or the equivalent according to its associated ",
                  "type": "text"
                },
                {
                  "code": "country",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Search-for-Geolocations",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " with ",
                  "type": "text"
                },
                {
                  "code": "entity",
                  "type": "codeVoice"
                },
                {
                  "text": " to search for and retrieve geolocations. Then use geotargeting dimensions ",
                  "type": "text"
                },
                {
                  "code": "country",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "adminArea",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "locality",
                  "type": "codeVoice"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "code": "TargetingDimensions",
                  "type": "codeVoice"
                },
                {
                  "text": " payload with the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Create-an-Ad-Group",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": "  and ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Update-an-Ad-Group",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " endpoints.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "adminArea",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/AdminAreaCriteria",
