        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dimension to target users by gender demographic. Specify the gender you want to show your ad to. To specify all genders, set the value to ",
                  "type": "text"
                },
                {
                  "code": "null",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
