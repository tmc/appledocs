      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The billing email.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is updatable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "billingEmail",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total budget amount available for the budget order.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is updatable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "budget",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Money",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:Money",
              "text": "Money"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The advertiser or product. This is a requirement for agency-type accounts.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is updatable only before the startDate of a campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clientName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scheduled end date and time for the budget order in the format of ",
                  "type": "text"
                },
                {
                  "code": "yyyy-mm-dd'T'HH:MM:SS.SSS",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is updatable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "endDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier for the budget order.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "When you create a budget order through the Apple Search Ads UI, the system returns a budget order ID (",
                  "type": "text"
                },
                {
                  "code": "boId",
                  "type": "codeVoice"
                },
                {
                  "text": ") that you can use with ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-a-Budget-Order",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to return details of a specific budget order for an organization or campaign group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the budget order, which is unique within an organization. This field is updatable only before the ",
                  "type": "text"
                },
                {
                  "code": "startDate",
                  "type": "codeVoice"
                },
                {
                  "text": " of a campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A purchase order number. This is a requirement for agency-type accounts.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is updatable only before the startDate of a campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orderNumber",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier of the organization that owns the budget order.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "parentOrgId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The primary buyerâ€™s email address.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is updatable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "primaryBuyerEmail",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The primary buyerâ€™s name.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is updatable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "primaryBuyerName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scheduled start date and time for the budget order in the format of ",
                  "type": "text"
                },
                {
                  "code": "yyyy-mm-dd'T'HH:MM:SS.SSS",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is updatable only before the ",
                  "type": "text"
                },
                {
                  "code": "startDate",
                  "type": "codeVoice"
                },
                {
                  "text": " of a campaign and is only editable if there are no campaigns assigned to the budget order.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "startDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "ACTIVE",
                "CANCELED",
                "COMPLETED",
                "EXHAUSTED",
                "INACTIVE"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The system-controlled status indicator for the budget order.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "This status occurs when the budget order reaches its start date.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "ACTIVE",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "This status occurs when you cancel the budget order.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "CANCELED",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "This status occurs when the budget order reaches its end date.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "COMPLETED",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "This status occurs when you exhaust the budget for the budget order before it reaches its end date.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "EXHAUSTED",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "This status occurs after you create the budget order and before it reaches its start date.",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "INACTIVE",
                        "type": "codeVoice"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            }
          ],
          "name": "status",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "APPSTORE_SEARCH_RESULTS",
                "APPSTORE_SEARCH_TAB"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The supply source of ads to use in a budget order and a campaign. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/SupplySource",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for enum descriptions and validations.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "supplySources",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        }
      ],
