    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The average CPM is the average amount you pay per one thousand ad impressions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "avgCPM",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Money",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:Money",
              "text": "Money"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The average cost-per-tap (CPT) is the ratio of spend over taps.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "avgCPT",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Money",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:Money",
              "text": "Money"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of times your ad appears in App Store search results within the reporting period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "impressions",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total spend of a campaign in the currency the organization uses.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "localSpend",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Money",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:Money",
              "text": "Money"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Prior to API 5, this was the ",
                  "type": "text"
                },
                {
                  "code": "avgCPA",
                  "type": "codeVoice"
                },
                {
                  "text": " field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The total campaign spend divided by the number of tap-through installs within the reporting period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tapInstallCPI",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Money",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:Money",
              "text": "Money"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Prior to API 5, this was the ",
                  "type": "text"
                },
                {
                  "code": "conversionRate",
                  "type": "codeVoice"
                },
                {
                  "text": " field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The total number of tap-through installs divided by the total number of taps within the reporting period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tapInstallRate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "double"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Prior to API 5, this was the ",
                  "type": "text"
                },
                {
                  "code": "installs",
                  "type": "codeVoice"
                },
                {
                  "text": " field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The total number of new downloads and redownloads from people who tapped your ad within a 30-day attribution window.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tapInstalls",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Prior to API 5, this was the ",
                  "type": "text"
                },
                {
                  "code": "newDownloads",
                  "type": "codeVoice"
                },
                {
                  "text": " field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "New downloads from people who tapped your ad within a 30-day attribution window.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The API counts new downloads when someone downloads your app to a device where your app hasn’t previously been installed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tapNewDownloads",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Prior to API 5, this was the ",
                  "type": "text"
                },
                {
                  "code": "redownloads",
                  "type": "codeVoice"
                },
                {
                  "text": " field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Redownloads from people who tapped your ad within a 30-day attribution window.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The API counts redownloads when someone downloads your app, deletes it, and downloads it again on the same device, or a different one, following an ad tap.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tapRedownloads",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of times people tap your ad within the reporting period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "taps",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total campaign spend divided by the total number of installs within the reporting period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalAvgCPI",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Money",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:Money",
              "text": "Money"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of installs divided by the total number of taps within the reporting period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalInstallRate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "double"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of tap-through and view-through new downloads and redownloads within the reporting period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalInstalls",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of tap-through and view-through new downloads within the reporting period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalNewDownloads",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of tap-through and view-through redownloads within the reporting period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalRedownloads",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tap-through rate (TTR) is the number of times people tap your ad divided by the total impressions your ad receives.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ttr",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "double"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of new downloads and redownloads from people who viewed your ad, but didn’t tap it, within a 1-day attribution window.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "viewInstalls",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "New downloads from people who viewed your ad, but didn’t tap it, within a 1-day attribution window. The API counts new downloads when someone downloads your app to a device where your app hasn’t previously been installed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "viewNewDownloads",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Redownloads from people who viewed your ad, but didn’t tap it, within a 1-day attribution window. The API counts redownloads when someone downloads your app, deletes it, and downloads it again on the same device, or a different one, following an ad view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "viewRedownloads",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        }
      ],
