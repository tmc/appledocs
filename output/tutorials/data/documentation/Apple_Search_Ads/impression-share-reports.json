  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Impression share reports provide insights into opportunities to scale keywords and optimize maximum CPT bids and budgets for your search results campaigns. The reports also show you how your app ranks in terms of impression share compared to other apps in the same countries and regions.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Impression-Share-Report",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " endpoint to obtain a report ",
              "type": "text"
            },
            {
              "code": "ID",
              "type": "codeVoice"
            },
            {
              "text": " to use with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-a-Single-Impression-Share-Report",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", or use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-All-Impression-Share-Reports",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " without a report ",
              "type": "text"
            },
            {
              "code": "ID",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/CustomReportRequest",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for formatting guidance on selector structure. For metrics descriptions, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/CustomReportResponse",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
