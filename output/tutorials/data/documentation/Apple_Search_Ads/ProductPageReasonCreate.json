    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Your unique App Store app identifier.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "adamId",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier for an app preview or screenshot.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "assetGenId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Custom comments from Apple about the rejection reason.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "comment",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The App Store geoterritories where you’re promoting your app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "countryOrRegion",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier for a creative.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id",
          "readOnly": true,
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ISO 639-1 language code appended to the ISO 3166-1 alpha-2 country code, such as ",
                  "type": "text"
                },
                {
                  "code": "en-US",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "languageCode",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The custom product page identifier associated with the ad creative rejection reason. This field is nullable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "productPageId",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Contains one of the ",
                  "type": "text"
                },
                {
                  "code": "RejectionReason",
                  "type": "codeVoice"
                },
                {
                  "text": " enumerations in the Discussion section below.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reasonCode",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The level at which the system applies the rejection reason. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/ReasonLevel",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for enumerations.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reasonLevel",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The reason type has a value of ",
                  "type": "text"
                },
                {
                  "code": "REJECTION_REASON",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reasonType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ad placement associated with the ad creative rejection reason.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "supplySource",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Descriptions of reason codes include the following:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Violent, offensive, sexually explicit, or otherwise inappropriate images aren’t allowed in the app icon.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "APP_ICON_GRAPHIC_OR_ADULT_THEMED_CONTENT",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The app icon doesn’t comply with Apple advertising guidelines.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "APP_ICON_NOT_ELIGIBLE",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The language in the app name needs to match the language selected in ",
                        "type": "text"
                      },
                      {
                        "identifier": "https://appstoreconnect.apple.com",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "APP_NAME_LANGUAGE_CONFLICT",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Violent, offensive, sexually explicit, or otherwise inappropriate wording isn’t allowed in the app name.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "APP_NAME_GRAPHIC_OR_ADULT_THEMED_CONTENT",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The app name doesn’t comply with Apple advertising guidelines.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "APP_NAME_NOT_ELIGIBLE",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The app isn’t eligible for promotion on the Today tab.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "APP_NOT_ELIGIBLE_AT_THIS_TIME",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The phrases ",
                        "type": "text"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "Game of the Day",
                            "type": "text"
                          }
                        ],
                        "type": "emphasis"
                      },
                      {
                        "text": " and ",
                        "type": "text"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "App of the Day",
                            "type": "text"
                          }
                        ],
                        "type": "emphasis"
                      },
                      {
                        "text": " aren’t allowed in the app icon, name, or subtitle.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "MIMICS_APP_STORE_TODAY_CARD",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The app icon from a product page optimization (PPO) experiment doesn’t comply with Apple Advertising guidelines.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "PRODUCT_PAGE_OPTIMIZATION_EXPERIMENT_APP_ICON_NOT_ELIGIBLE",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Violent, offensive, sexually explicit, or otherwise inappropriate wording isn’t allowed in the app subtitle.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "SUBTITLE_GRAPHIC_OR_ADULT_THEMED_CONTENT",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The language in the app subtitle needs to match the language of the primary locale in ",
                        "type": "text"
                      },
                      {
                        "identifier": "https://appstoreconnect.apple.com",
                        "isActive": true,
                        "overridingTitle": "App Store Connect",
                        "overridingTitleInlineContent": [
                          {
                            "text": "App Store Connect",
                            "type": "text"
                          }
                        ],
                        "type": "reference"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "SUBTITLE_LANGUAGE_CONFLICT",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The app’s subtitle doesn’t comply with Apple Search Ads advertising requirements.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "SUBTITLE_NOT_ELIGIBLE",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Pricing, offers, ranking, or other incentivized promotions aren’t allowed in the app subtitle.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "SUBTITLE_PRICING_OFFERS_OR_RANKING_CLAIMS",
                    "type": "codeVoice"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        }
      ],
      "kind": "content"
    }
  ],
