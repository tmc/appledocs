    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Your unique App Store app identifier. Use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-a-Campaign",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-all-Campaigns",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to obtain your ",
                  "type": "text"
                },
                {
                  "code": "adamId",
                  "type": "codeVoice"
                },
                {
                  "text": " used in your campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "adamId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date and time the object was created.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is not modifiable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "creationTime",
          "readOnly": true,
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The deep link set up in your custom product page metadata on ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Deep links are available on iOS 18 and later for Today tab and search results ad variations, and iPadOS 18 and later for search results ad variations.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Note that deep links are not available for ads with demographic targeting (age or gender).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is not modifiable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "deepLink",
          "readOnly": true,
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique string to identify a product page on ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". For example, ",
                  "type": "text"
                },
                {
                  "code": "45812c9b-c296-43d3-c6a0-c5a02f74bf6e",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date and time of the most recent modification of the object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is not modifiable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "modificationTime",
          "readOnly": true,
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of your custom product page on ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "HIDDEN",
                "VISIBLE"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The system state of the custom product page that indicates whether the page is visible or not.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "state",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
