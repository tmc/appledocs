    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this endpoint to fetch supported product page languages for countries or regions.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Payload-example-1-Get-supported-countries-or-regions",
          "level": 3,
          "text": "Payload example 1: Get supported countries or regions",
          "type": "heading"
        },
        {
          "tabs": [
            {
              "content": [
                {
                  "code": [
                    "GET https://api.searchads.apple.com/api/v5/countries-or-regions?countriesOrRegions=US,AU,GB,CA"
                  ],
                  "syntax": null,
                  "type": "codeListing"
                }
              ],
              "title": "Request"
            },
            {
              "content": [
                {
                  "code": [
                    "{",
                    "   \"data\": [",
                    "        {",
                    "            \"countryOrRegion\": “US”,",
                    "            \"supportedLanguages\": [",
                    "                {",
                    "                    \"language\": \"en\",",
                    "                    \"languageCode\": \"en-US\"",
                    "                },",
                    "                {",
                    "                    \"language\": “es”,",
                    "                    \"languageCode\": \"es-MX\"",
                    "                }",
                    "            ],",
                    "            \"defaultLanguage\": {",
                    "                \"language\": \"en\",",
                    "                \"languageCode\": \"en-US\"",
                    "            }",
                    "        },",
                    "        {",
                    "            \"countryOrRegion\": \"AU\",",
                    "            \"supportedLanguages\": [",
                    "                {",
                    "                    \"language\": \"en\",",
                    "                    \"languageCode\": \"en-AU\"",
                    "                },",
                    "                {",
                    "                    \"language\": \"en\",",
                    "                    \"languageCode\": \"en-GB\"",
                    "                }",
                    "            ],",
                    "            \"defaultLanguage\": {",
                    "                \"language\": \"en\",",
                    "                \"languageCode\": \"en-AU\"",
                    "            }",
                    "        },",
                    "        {",
                    "            \"countryOrRegion\": \"GB\",",
                    "            \"supportedLanguages\": [",
                    "                {",
                    "                    \"language\": \"en\",",
                    "                    \"languageCode\": \"en-GB\"",
                    "                }",
                    "            ],",
                    "            \"defaultLanguage\": {",
                    "                \"language\": \"en\",",
                    "                \"languageCode\": \"en-GB\"",
                    "            }",
                    "        },",
                    "        {",
                    "            \"countryOrRegion\": \"CA\",",
                    "            \"supportedLanguages\": [",
                    "                {",
                    "                    \"language\": \"en\",",
                    "                    \"languageCode\": \"en-CA\"",
                    "                },",
                    "                {",
                    "                    \"language”: \"fr\",",
                    "                    \"languageCode\": \"fr-CA\"",
                    "                }",
                    "            ],",
                    "            \"defaultLanguage\": {",
                    "                \"language\": \"en\",",
                    "                \"languageCode\": \"en-CA\"",
                    "            }",
                    "        }",
                    "    ]",
                    "}"
                  ],
                  "syntax": "json",
                  "type": "codeListing"
                }
              ],
              "title": "Response"
            }
          ],
          "type": "tabNavigator"
        },
        {
          "anchor": "Payload-example-2-Get-supported-countries-or-regions",
          "level": 3,
          "text": "Payload example 2: Get supported countries or regions",
          "type": "heading"
        },
        {
          "tabs": [
            {
              "content": [
                {
                  "code": [
                    "GET https://api.searchads.apple.com/api/v5/countries-or-regions?countriesOrRegions=CA"
                  ],
                  "syntax": null,
                  "type": "codeListing"
                }
              ],
              "title": "Request"
            },
            {
              "content": [
                {
                  "code": [
                    "{",
                    "  \"countryOrRegion\": \"CA\",",
                    "  \"supportedLocales\": [",
                    "    {",
                    "      \"language\": \"en\",",
                    "      \"languageCode\": \"en-CA\"",
                    "    },",
                    "    {",
                    "      \"language\": \"fr\",",
                    "      \"languageCode\": \"fr-CA\"",
                    "    }",
                    "  ],",
                    "  \"defaultLanguages\": [",
                    "    {",
                    "      \"language\": \"en\",",
                    "      \"languageCode\": \"en-CA\"",
                    "    },",
                    "    {",
                    "      \"language\": \"fr\",",
                    "      \"languageCode\": \"fr-CA\"",
                    "    }",
                    "  ]",
                    "}"
                  ],
                  "syntax": "json",
                  "type": "codeListing"
                }
              ],
              "title": "Response"
            }
          ],
          "type": "tabNavigator"
        }
      ],
      "kind": "content"
    }
