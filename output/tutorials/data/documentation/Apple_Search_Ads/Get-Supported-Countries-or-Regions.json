    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Filters by ISO alpha-2 country codes using one or more comma-separated values. For example, use ",
                  "type": "text"
                },
                {
                  "code": "https://api.searchads.apple.com/api/v5/countries-or-regions?countriesOrRegions=US",
                  "type": "codeVoice"
                },
                {
                  "text": " for a single country or region, or ",
                  "type": "text"
                },
                {
                  "code": "https://api.searchads.apple.com/api/v5/countries-or-regions?countriesOrRegions=US,",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "MX",
                  "type": "codeVoice"
                },
                {
                  "text": " for multiple countries or regions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "countriesOrRegions",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
