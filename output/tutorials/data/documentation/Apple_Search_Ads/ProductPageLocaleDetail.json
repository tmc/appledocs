    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Your unique App Store app identifier. Use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-a-Campaign",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-all-Campaigns",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to obtain your ",
                  "type": "text"
                },
                {
                  "code": "adamId",
                  "type": "codeVoice"
                },
                {
                  "text": " used in your campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "adamId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app name on ",
                  "type": "text"
                },
                {
                  "identifier": "https://appstoreconnect.apple.com",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "appName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A map between the device and available app preview details for that device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "appPreviewDeviceWithAssets",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/ProductPageLocaleDetail/AppPreviewDeviceWithAssets-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:search_ads:ProductPageLocaleDetail.AppPreviewDeviceWithAssets",
              "text": "ProductPageLocaleDetail.AppPreviewDeviceWithAssets"
            }
          ]
        },
        {
          "attributes": [
