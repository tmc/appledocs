    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of entries that return for this operation, which is the value of ",
                  "type": "text"
                },
                {
                  "code": "count",
                  "type": "codeVoice"
                },
                {
                  "text": " in the GET operation. The actual number of entries that return can be less than the maximum ",
                  "type": "text"
                },
                {
                  "code": "itemsPerPage",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "itemsPerPage",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The offset of the first entry that returns. Use ",
                  "type": "text"
                },
                {
                  "code": "offset",
                  "type": "codeVoice"
                },
                {
                  "text": " in the request query string or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Selector",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to override the default value of ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ". For example, if the request is ",
                  "type": "text"
                },
                {
                  "code": "offset=5",
                  "type": "codeVoice"
                },
                {
                  "text": ", the response ",
                  "type": "text"
                },
                {
                  "code": "startIndex",
                  "type": "codeVoice"
                },
                {
                  "text": " is also ",
                  "type": "text"
                },
                {
                  "code": "5",
                  "type": "codeVoice"
                },
                {
                  "text": ". A value of ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " retrieves the first ",
                  "type": "text"
                },
                {
                  "code": "itemsPerPage",
                  "type": "codeVoice"
                },
                {
                  "text": " results from the full result set.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "startIndex",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of entries that return for the operation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalResults",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int64"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "PageDetail",
              "type": "codeVoice"
            },
            {
              "text": " object contains the pagination response for returned multiple records.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "{",
            "   \"data\":[",
            "     { },",
            "     ...",
            "   ],",
            "   \"pagination\"{",
            "     \"totalResults\": 10,",
            "     \"startIndex\": 1,",
            "     \"itemsPerPage\": 10",
            "   },",
            "}",
            ""
          ],
          "syntax": "json",
          "type": "codeListing"
        }
      ],
      "kind": "content"
