      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Your unique App Store app identifier. Use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-a-Campaign",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-all-Campaigns",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to obtain your ",
                  "type": "text"
                },
                {
                  "code": "adamId",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You can use the ",
                  "type": "text"
                },
                {
                  "code": "EQUALS",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "IN",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Selector",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Condition",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " operators with ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Find-App-Assets",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This field is sortable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "adamId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicates the device model and corresponding display size. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/AppPreviewDevicesMappingResponse/Data-data.dictionary",
                  "isActive": true,
