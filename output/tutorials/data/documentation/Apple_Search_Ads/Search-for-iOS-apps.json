    {
      "items": [
        {
          "attributes": [
            {
              "kind": "default",
              "value": "20"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of items to return per request. The maximum is 1000.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "GET https://api.searchads.apple.com/api/v5/search/apps?limit=100"
              ],
              "syntax": "console",
              "type": "codeListing"
            }
          ],
          "name": "limit",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The offset pagination that limits the number of records returned. The start of each page is offset by the number specified. You can apply ",
                  "type": "text"
                },
                {
                  "code": "offset",
                  "type": "codeVoice"
                },
                {
                  "text": " to most API calls, but not all GET endpoints support it.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "GET https://api.searchads.apple.com/api/v5/search/apps?limit=<LIMIT>&offset=<OFFSET>"
              ],
              "syntax": "console",
              "type": "codeListing"
            }
          ],
          "name": "offset",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The query for a list of iOS apps using a matching prefix.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "GET https://api.searchads.apple.com/api/v5/search/apps?query=Run%20Ke"
              ],
              "syntax": "console",
              "type": "codeListing"
            },
            {
              "inlineContent": [
                {
                  "text": "The query search pattern uses a prefix-matching algorithm. You can use spaces in search patterns. Prefixes require a minimum of three characters. If you’re sending a quoted search string, use HTML encoding.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "query",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "false"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of apps belonging to your organization.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "GET https://api.searchads.apple.com/api/v5/search/apps?query=appexample&returnOwnedApps=true"
              ],
              "syntax": "console",
              "type": "codeListing"
            }
          ],
          "name": "returnOwnedApps",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        }
      ],
