    {
      "items": [
        {
          "attributes": [
            {
              "kind": "default",
              "value": "US"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The country or region to serve ads in. Campaigns that serve multiple countries or regions can’t use geotargeting.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The query uses a ",
                  "type": "text"
                },
                {
                  "code": "countrycode",
                  "type": "codeVoice"
                },
                {
                  "text": " value in an ISO alpha-2 country code format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "GET https://api.searchads.apple.com/api/v5/search/geo?countrycode=US"
              ],
              "syntax": "console",
              "type": "codeListing"
            }
          ],
          "name": "countrycode",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "country",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "AdminArea",
                  "type": "codeVoice"
                },
                {
                  "text": ", or L",
                  "type": "text"
                },
                {
                  "code": "ocality",
                  "type": "codeVoice"
                },
                {
                  "text": " locations available for targeting.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "code": "AdminArea",
                  "type": "codeVoice"
                },
                {
                  "text": " is the state or the equivalent according to its associated country. A ",
                  "type": "text"
                },
                {
                  "code": "Locality",
                  "type": "codeVoice"
                },
                {
                  "text": " is the city or the equivalent according to its associated ",
                  "type": "text"
                },
                {
                  "code": "AdminArea",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "countrycode",
                  "type": "codeVoice"
                },
                {
                  "text": " is a mandatory parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "GET https://api.searchads.apple.com/api/v5/search/geo?entity=AdminArea&countrycode=US"
              ],
              "syntax": "console",
              "type": "codeListing"
            },
            {
              "code": [
                "GET https://api.searchads.apple.com/api/v5/search/geo?entity=Locality&countrycode=US"
              ],
              "syntax": "console",
              "type": "codeListing"
            },
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "entity",
                  "type": "codeVoice"
                },
                {
                  "text": " query parameter searches the ",
                  "type": "text"
                },
                {
                  "code": "displayNames",
                  "type": "codeVoice"
                },
                {
                  "text": " for ",
                  "type": "text"
                },
                {
                  "code": "country",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "adminArea",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "Locality",
                  "type": "codeVoice"
                },
                {
                  "text": " in all languages.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Search results in the response payload are in the preferred language according to your organization.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you don’t input a query parameter, all applicable values return in the response payload as a default.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "entity",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "20"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The limit on the number of geolocations in the response.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "GET https://api.searchads.apple.com/api/v5/search/geo?limit=1000"
              ],
              "syntax": "console",
              "type": "codeListing"
            }
          ],
          "name": "limit",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "0"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The offset pagination that limits the number of returned records. The start of each page is offset by the specified number. You can apply ",
                  "type": "text"
                },
                {
                  "code": "offset",
                  "type": "codeVoice"
                },
                {
                  "text": " to most API calls, but not all GET endpoints support it.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offset",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "*:*"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "query",
                  "type": "codeVoice"
                },
                {
                  "text": " search pattern uses a prefix-matching algorithm. You can use spaces in search patterns. Prefixes require a minimum of three characters. If you’re sending a quoted search string, use HTML encoding.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "GET https://api.searchads.apple.com/api/v5/search/geo?query=%22New%20H%22"
              ],
              "syntax": "console",
              "type": "codeListing"
            }
          ],
          "name": "query",
          "required": false,
          "type": [
            {
              "kind": "text",
