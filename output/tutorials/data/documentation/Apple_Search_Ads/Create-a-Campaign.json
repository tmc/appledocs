                    "            \"US\": [",
                    "                \"APP_NOT_PUBLISHED_YET\"",
                    "            ]",
                    "        },",
                    "        \"supplySources\": [",
                    "            \"APPSTORE_TODAY_TAB\"",
                    "        ],",
                    "        \"adChannelType\": \"DISPLAY\",",
                    "        \"billingEvent\": \"TAPS\",",
                    "        \"displayStatus\": \"ON_HOLD\"",
                    "    },",
                    "    \"pagination\": null,",
                    "    \"error\": null",
                    "}"
                  ],
