    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of items to return per request. For most objects, the default is ",
                  "type": "text"
                },
                {
                  "code": "20",
                  "type": "codeVoice"
                },
                {
                  "text": " and the maximum is ",
                  "type": "text"
                },
                {
                  "code": "1000",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "limit",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The offset pagination that limits the number of returned records. The start of each page is offset by the specified number. You can apply ",
                  "type": "text"
                },
                {
                  "code": "offset",
                  "type": "codeVoice"
                },
                {
                  "text": " to most API calls, but not all GET endpoints support it. The default is ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offset",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "int32"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In the following example, the two optional parameters limit the number of campaigns that return. You specify ",
              "type": "text"
            },
            {
              "code": "offset",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "limit",
              "type": "codeVoice"
            },
            {
              "text": " as query string parameters.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "GET https://api.searchads.apple.com/api/v5/campaigns?limit=<LIMIT>&offset=<OFFSET>"
          ],
          "syntax": "console",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
