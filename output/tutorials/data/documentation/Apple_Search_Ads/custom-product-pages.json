  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In iOS 15 and later, you can use ",
              "type": "text"
            },
            {
              "identifier": "https://developer.apple.com/app-store/product-page/",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " that you build in ",
              "type": "text"
            },
            {
              "identifier": "https://appstoreconnect.apple.com",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to create ad variations in the Apple Search Ads Campaign Management API to promote your apps.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To create a custom product page ad, use the following workflow:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "media-3921834",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-Product-Pages",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to fetch your ",
                      "type": "text"
                    },
                    {
                      "code": "productPageId",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-Product-Page-Locales",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to fetch locale metadata and supported languages. Verify that your custom product page locale and asset data match the language and device types in your campaign ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/AdGroup",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Create-a-Creative",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " with your ",
                      "type": "text"
                    },
                    {
                      "code": "productPageId",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to obtain a ",
                      "type": "text"
                    },
                    {
                      "code": "creativeId",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Create-an-Ad",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " with a ",
                      "type": "text"
                    },
                    {
                      "code": "creativeId",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to assign a custom product page to an ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/AdGroup",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ". Each ad group can have one custom product page. For information about how to update a custom product page ad group assignment, see ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Update-an-Ad",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-Ad-Level-Reports",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to track the performance of ads in your campaigns. These details help you optimize your Apple Search Ads campaigns to deliver a contextualized customer experience.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "orderedList"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If youâ€™ve been using Creative Sets with the API, see section 4.1 in  ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/Apple_Search_Ads/apple-search-ads-campaign-management-api-4",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for Creative Sets deprecation details.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
