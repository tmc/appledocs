          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique name of the ad group. Responses don’t include deleted ad groups.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You can use the ",
                  "type": "text"
                },
                {
                  "code": "EQUALS",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "IN",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "STARTSWITH",
                  "type": "codeVoice"
                },
                {
                  "text": " selector ",
