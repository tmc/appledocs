              "values": [
                "DELETED",
                "ON_HOLD",
                "PAUSED",
                "RUNNING"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The status of the ad group. The status resolves according to ",
                  "type": "text"
                },
                {
                  "code": "servingStatus",
                  "type": "codeVoice"
                },
                {
                  "text": " and additional criteria.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "displayStatus",
          "readOnly": true,
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scheduled end time and date for the ad group, which the system determines from the ad group with the latest end time.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "The ",
                          "type": "text"
                        },
                        {
                          "code": "endTime",
                          "type": "codeVoice"
                        },
                        {
                          "text": " must be after the ",
                          "type": "text"
                        },
                        {
                          "code": "startTime",
                          "type": "codeVoice"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "The ",
                          "type": "text"
                        },
                        {
                          "code": "endTime",
                          "type": "codeVoice"
                        },
                        {
                          "text": " is updatable until you reach the designated time.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "The ",
                          "type": "text"
                        },
                        {
                          "code": "endTime",
                          "type": "codeVoice"
                        },
                        {
                          "text": " must be in UTC.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "endTime",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "minimum",
              "value": "0"
            }
          ],
          "content": [
            {
