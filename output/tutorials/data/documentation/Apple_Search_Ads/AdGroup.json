          ],
          "name": "pricingModel",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "ADGROUP_END_DATE_REACHED",
                "AD_GROUP_PAUSED_BY_USER",
                "APP_NOT_SUPPORT",
                "AUDIENCE_BELOW_THRESHOLD",
                "CAMPAIGN_END_DATE_REACHED",
                "CAMPAIGN_NOT_RUNNING",
                "CAMPAIGN_START_DATE_IN_FUTURE",
                "DELETED_BY_USER",
                "NO_AVAILABLE_ADS",
                "PENDING_AUDIENCE_VERIFICATION",
                "START_DATE_IN_THE_FUTURE",
                "TARGETED_DEVICE_CLASS_NOT_SUPPORTED_SUPPLY_SOURCE"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of reasons that displays when an ad group isn’t running.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "servingStateReasons",
          "readOnly": true,
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "RUNNING"
            },
            {
              "kind": "allowedValues",
              "values": [
                "NOT_RUNNING",
                "RUNNING"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The status of whether the ad group is serving.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "servingStatus",
          "readOnly": true,
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scheduled start date and time for the ad group with the earliest start time in the campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "The ",
                          "type": "text"
                        },
                        {
                          "code": "startTime",
                          "type": "codeVoice"
                        },
                        {
                          "text": " must be greater than the current time, and before the campaign ",
                          "type": "text"
                        },
                        {
                          "code": "endTime",
                          "type": "codeVoice"
                        },
                        {
                          "text": ", if you set it.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "If you don’t set a ",
                          "type": "text"
                        },
                        {
                          "code": "startTime",
                          "type": "codeVoice"
                        },
                        {
                          "text": ", the campaign defaults to the campaign request timestamp and the ",
                          "type": "text"
                        },
                        {
                          "code": "startTime",
                          "type": "codeVoice"
                        },
                        {
                          "text": " is updatable until you reach the designated time.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
