    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The parameter for enabling and disabling Search Match. If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system automatically adds optimized keywords in addition to those you explicitly add to the ad group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See the Enable and Disable Search Match section of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/Apple_Search_Ads/ad-groups",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You can use the ",
                  "type": "text"
                },
                {
                  "code": "EQUALS",
                  "type": "codeVoice"
                },
                {
                  "text": " selector ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Condition",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " operator with ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Find-Ad-Groups",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "automatedKeywordsOptIn",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique identifier for a campaign.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
