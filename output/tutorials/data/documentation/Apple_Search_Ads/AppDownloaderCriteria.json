    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dimension to limit viewing of your ad to users who have not downloaded your app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "{",
                "  \"appDownloaders\": {",
                "    \"excluded\": 654327167",
                "  }",
                "}"
              ],
              "syntax": "json",
              "type": "codeListing"
            }
          ],
          "name": "excluded",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dimension to limit viewing of your ad to users who have downloaded your app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "{",
                "  \"appDownloaders\": {",
                "    \"included\": 654327143",
                " ",
                "  }",
                "}"
              ],
              "syntax": "json",
              "type": "codeListing"
            }
          ],
          "name": "included",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To target all users, don’t include the ",
              "type": "text"
            },
            {
              "code": "AppDownloaderCriteria",
              "type": "codeVoice"
            },
            {
              "text": " dimension in the request payload.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "adamId",
              "type": "codeVoice"
            },
            {
              "text": " of the app you’re promoting in your campaign as an ",
              "type": "text"
            },
            {
              "code": "included",
              "type": "codeVoice"
            },
            {
              "text": "  or ",
              "type": "text"
            },
            {
              "code": "excluded",
              "type": "codeVoice"
            },
            {
              "text": " value. You can obtain your app ",
              "type": "text"
            },
            {
              "code": "adamId",
              "type": "codeVoice"
            },
            {
              "text": " through ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-a-Campaign",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Get-all-Campaigns",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Search-for-iOS-apps",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " using the ",
              "type": "text"
            },
            {
              "code": "returnOwnedApps",
              "type": "codeVoice"
            },
            {
              "text": " query.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
