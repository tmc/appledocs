    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this endpoint to determine whether an app is eligible to promote in a campaign. Use a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Selector",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/Condition",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to search for a specific country or region, ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/DeviceClass",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/AgeCriteria",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/SupplySource",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".  See the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applesearchads/documentation/apple_search_ads/EligibilityRecord",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object for parameter descriptions and selector condition operators.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Payload-example-Find-app-eligibility-records",
          "level": 3,
          "text": "Payload example: Find app eligibility records",
          "type": "heading"
        },
        {
          "tabs": [
            {
              "content": [
                {
                  "code": [
                    "POST https://api.searchads.apple.com/api/v5/apps/<adamId>/eligibilities/find",
                    "",
                    "{",
                    "    \"conditions\": [",
                    "        {",
                    "            \"field\": \"countryOrRegion\",",
                    "            \"operator\": \"IN\",",
                    "            \"values\": [\"US\", \"MX\"]",
                    "        },",
                    "        {",
                    "            \"field\": \"supplySource\",",
                    "            \"operator\": \"EQUALS\",",
                    "            \"values\": [\"APPSTORE_TODAY_TAB\"]",
                    "        }",
                    "    ],",
                    "    \"pagination\": {",
                    "        \"offset\": 0,",
                    "        \"limit\": 2",
                    "    }",
                    "}"
                  ],
                  "syntax": null,
                  "type": "codeListing"
                }
              ],
              "title": "Request"
            },
            {
              "content": [
                {
                  "code": [
                    "{",
                    "    \"data\": [",
                    "        {",
                    "            \"adamId\": 123456789,",
                    "            \"deviceClass\": \"IPHONE\",",
                    "            \"state\": \"ELIGIBLE\",",
                    "            \"minAge\": 18,",
                    "            \"countryOrRegion\": \"US\",",
                    "            \"supplySource\": \"APPSTORE_TODAY_TAB\"",
                    "        },",
                    "        {",
                    "            \"adamId\": 123456789,",
                    "            \"deviceClass\": \"IPAD\",",
                    "            \"state\": \"INELIGIBLE\",",
                    "            \"minAge\": 18,",
                    "            \"countryOrRegion\": \"US\",",
                    "            \"supplySource\": \"APPSTORE_TODAY_TAB\"",
                    "        }",
                    "    ],",
                    "    \"pagination\": {",
                    "        \"totalResults\": 4,",
                    "        \"startIndex\": 0,",
                    "        \"itemsPerPage\": 2",
                    "    }",
                    "}"
                  ],
                  "syntax": "json",
                  "type": "codeListing"
                }
              ],
              "title": "Response"
            }
          ],
          "type": "tabNavigator"
        }
      ],
      "kind": "content"
    }
