        {
          "inlineContent": [
            {
              "text": "This value is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " if there is no currently running activity.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If your deployment target is iOS 11.4 or later, you can have more than one activity running concurrently. When you do this, the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.classkit/documentation/ClassKit/CLSDataStore/runningActivity",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property holds the most recently started activity.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
