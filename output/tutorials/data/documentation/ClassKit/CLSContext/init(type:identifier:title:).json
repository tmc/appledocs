      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The context type, like a book chapter, or a game level. This value tells teachers what kind of app content this context represents.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that you create to uniquely identify the context among its siblings. The value must be unique among child contexts of a given parent, but not among contexts with different parents.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "identifier"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A human readable name for the new context. The system presents this title to teachers exactly as you store it, so consider localizing it if you distribute your app in more than one locale.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "title"
        }
      ]
