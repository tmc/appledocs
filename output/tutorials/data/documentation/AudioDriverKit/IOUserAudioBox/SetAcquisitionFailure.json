      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Set this code to provide the error to return from failed calls to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.audiodriverkit/documentation/AudioDriverKit/IOUserAudioBox/HandleChangeAcquireBox",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If successful, changing the acquisition failure code sends a notification to the host to update the object state.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This method synchronizes by using the work queue created by the object.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
