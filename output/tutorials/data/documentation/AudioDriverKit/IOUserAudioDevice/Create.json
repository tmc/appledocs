          "name": "in_manufacturer_uid"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/kernel/uint32_t",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " whose value indicates the number of sample frames the host can expect between successive timestamps returned from ",
                  "type": "text"
                },
                {
                  "code": "GetZeroTimeStamp()",
                  "type": "codeVoice"
                },
                {
                  "text": ". In other words, if ",
                  "type": "text"
                },
                {
                  "code": "GetZeroTimeStamp()",
                  "type": "codeVoice"
                },
                {
                  "text": " returns a sample time of ",
                  "type": "text"
                },
                {
                  "code": "x",
                  "type": "codeVoice"
                },
                {
                  "text": ", the host can expect that the next valid timestamp it receives will be ",
                  "type": "text"
                },
                {
                  "code": "x + in_zero_timestamp_period",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "in_zero_timestamp_period"
        }
