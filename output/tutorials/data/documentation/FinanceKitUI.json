  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "FinanceKitUI",
              "type": "codeVoice"
            },
            {
              "text": " framework contains a standardized UI that interacts securely",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/FinanceKit",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/FinanceKit/FinanceStore",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to support the addition",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of orders to a person’s Apple Wallet.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "code": "FinanceKitUI",
              "type": "codeVoice"
            },
            {
              "text": " provides an ",
              "type": "text"
            },
            {
              "code": "AddOrderToWalletButton",
              "type": "codeVoice"
            },
            {
              "text": " for SwiftUI. Add this button",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to your UI when you want to allow someone to add an order to their Apple Wallet.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The button’s style options are consistent with the standard Apple Pay and Wallet",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "design language, giving users a sense of familiarity and trust when they",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "interact with it.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
