{
  "abstract": [
    {
      "text": "Communicate about events and share data between the containing app and JavaScript by using native messaging and app groups.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.safariservices/documentation/SafariServices",
