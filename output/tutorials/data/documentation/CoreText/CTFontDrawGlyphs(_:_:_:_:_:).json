    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The font with glyphs to render. If the font has a size or matrix attribute, ",
                  "type": "text"
                },
                {
                  "code": "context",
                  "type": "codeVoice"
                },
                {
                  "text": " is set with these values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "font"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The glyphs to be rendered. The glyphs should be the result of proper Unicode text layout operations (such as with ",
                  "type": "text"
                },
                {
                  "code": "CTLine",
                  "type": "codeVoice"
                },
                {
                  "text": "). Functions such as ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.coretext/documentation/CoreText/CTFontGetGlyphsForCharacters(_:_:_:_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " do not perform any Unicode text layout.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "glyphs"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The positions (origins) for each glyph in ",
                  "type": "text"
                },
                {
                  "code": "glyphs",
                  "type": "codeVoice"
                },
                {
                  "text": ". The positions are in user space. The number of positions passed in must match the number of glyphs (in ",
                  "type": "text"
                },
                {
                  "code": "glyphs",
                  "type": "codeVoice"
                },
                {
                  "text": ").",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "positions"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of glyphs to be rendered from the ",
                  "type": "text"
                },
                {
                  "code": "glyphs",
                  "type": "codeVoice"
                },
                {
                  "text": " array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "count"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The graphics context used to render the glyphs.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "context"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function modifies graphics state including font, text size, and text matrix if these attributes are specified in ",
              "type": "text"
            },
            {
              "code": "font",
              "type": "codeVoice"
            },
            {
              "text": ". These attributes are not restored.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
