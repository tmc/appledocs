      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The line break can be triggered either by a hard-break character in the stream or by filling the specified width with characters. This function is equivalent to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.coretext/documentation/CoreText/CTTypesetterSuggestLineBreakWithOffset(_:_:_:_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with an offset of 0.0.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
