  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Regular expressions, also known as regexes,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "are a powerful tool for matching patterns in text.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Swift supports several ways to create a regular expression,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "including from a string, as a literal, and using this DSL.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let word = OneOrMore(.word)",
            "let emailPattern = Regex {",
            "    Capture {",
            "        ZeroOrMore {",
            "            word",
            "            \".\"",
            "        }",
            "        word",
            "    }",
            "    \"@\"",
            "    Capture {",
            "        word",
            "        OneOrMore {",
            "            \".\"",
            "            word",
            "        }",
            "    }",
            "}",
            "",
            "let text = \"My email is my.name@example.com.\"",
            "if let match = text.firstMatch(of: emailPattern) {",
            "    let (wholeMatch, name, domain) = match.output",
            "    // wholeMatch is \"my.name@example.com\"",
            "    // name is \"my.name\"",
            "    // domain is \"example.com\"",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
