  "primaryContentSections": [
    {
      "items": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional property that specifies the type of overlay shape to render.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Default: ",
                  "type": "text"
                },
                {
                  "code": "”polygon”",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Options: ",
                  "type": "text"
                },
                {
                  "code": "“circle”, ”polygon”, or “polyline”",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "title": "Possible Types",
              "values": [
                [
                  {
                    "kind": "text",
                    "text": "[string]"
                  }
                ],
                [
                  {
                    "kind": "text",
                    "text": "string"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of coordinates, specified as latitude and longitude coordinate pairs, or as a string encoded with the Encoded Polyline Algorithm Format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The array format specifies points for a polyline overlay. The following example specifies two points for a polyline overlay as an array of latitude and longitude coordinate pairs:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "\"points\": [\"37.779996,-122.51158\",\"37.78040,-122.51348\"]",
                ""
              ],
              "metadata": {
                "anchor": "3694078",
                "title": "Listing 1"
              },
              "syntax": "swift",
              "type": "codeListing"
            },
            {
              "inlineContent": [
                {
                  "text": "You can provide those same parameters in encoded polyline algorithm format:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "\"points\": \"w{qeFj`wjVwAzJ\""
              ],
              "metadata": {
                "anchor": "3694077",
                "title": "Listing 2"
              },
              "syntax": "other",
              "type": "codeListing"
            },
            {
              "inlineContent": [],
              "type": "paragraph"
            }
          ],
          "name": "points",
          "type": [
            {
              "kind": "text",
              "text": "*"
            }
          ],
          "typeDetails": [
            {
              "arrayMode": true,
              "baseType": "string"
            },
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The coordinate of the circle’s center.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "center",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The circle’s radius in meters.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "radius",
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The color of the line between each coordinate point. Supported values are HTML color names or hexadecimal color codes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "strokeColor",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The width of the line, in CSS pixels.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineWidth",
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array that defines a line’s dash pattern, where numbers represent line and gap lengths in CSS pixels. For example, ",
                  "type": "text"
                },
                {
                  "code": "[10,5]",
                  "type": "codeVoice"
                },
                {
                  "text": " means draw a line for 10 pixels, leave a 5-pixel gap, and repeat. An empty array draws a solid line, which is the default.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineDash",
          "type": [
            {
              "kind": "text",
              "text": "[integer]"
            }
          ],
          "typeDetails": [
            {
              "arrayMode": true,
              "baseType": "integer"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The object’s fill color.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillColor",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The opacity value used for filling the object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillOpacity",
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The rule used to determine the inside of the polygon.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillRule",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style used for line end points. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Default: ",
                  "type": "text"
                },
                {
                  "code": "”round”",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Options: ",
                  "type": "text"
                },
                {
                  "code": "”butt”, “round”, or “square”",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineCap",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The line dash offset.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineDashOffset",
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object that determines color stops for the gradient, positioned by offsets between 0 and 1.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "\"lineGradient\": {\"0\": \"green\", \"1\": \"blue\"}"
              ],
              "metadata": {
                "anchor": "4315594",
                "title": "Listing 3"
              },
              "syntax": "other",
              "type": "codeListing"
            },
            {
              "inlineContent": [],
              "type": "paragraph"
            }
          ],
          "name": "lineGradient",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/snapshots/overlay/linegradient",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:snapshots:Overlay.LineGradient",
              "text": "Overlay.LineGradient"
            }
          ],
          "typeDetails": [
            {
              "baseType": "dictionary"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object that determines where the color stops for the gradient. This property uses indexes of points on the polyline.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "\"lineGradientIndexes\": {\"1\": \"blue\"}"
              ],
              "metadata": {
                "anchor": "4315593",
                "title": "Listing 4"
              },
              "syntax": "other",
              "type": "codeListing"
            },
            {
              "inlineContent": [],
              "type": "paragraph"
            }
          ],
          "name": "lineGradientIndexes",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/snapshots/overlay/linegradientindexes",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:snapshots:Overlay.LineGradientIndexes",
              "text": "Overlay.LineGradientIndexes"
            }
          ],
          "typeDetails": [
            {
              "baseType": "dictionary"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style used for joins between line segments. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Default: ",
                  "type": "text"
                },
                {
                  "code": "“round”",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Options: ",
                  "type": "text"
                },
                {
                  "code": "“bevel”, “miter”, or “round”",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineJoin",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The opacity value for stroking. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "strokeOpacity",
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The index number used to reference an ",
                  "type": "text"
                },
                {
                  "code": "OverlayStyle",
                  "type": "codeVoice"
                },
                {
                  "text": " object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "styleIdx",
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
