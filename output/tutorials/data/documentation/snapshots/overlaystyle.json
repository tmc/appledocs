  "primaryContentSections": [
    {
      "items": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The color used to fill the object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillColor",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The opacity value used to fill the object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillOpacity",
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A rule used to determine the inside space of the polygon. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Default: ",
                  "type": "text"
                },
                {
                  "code": "“nonzero”",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Options: ",
                  "type": "text"
                },
                {
                  "code": "“nonzero” or “evenodd”",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillRule",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style used for line end points.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Default: ",
                  "type": "text"
                },
                {
                  "code": "“round”",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Options: ",
                  "type": "text"
                },
                {
                  "code": "”butt”, “round”, or “square”",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineCap",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of integers that specifies the line’s dash pattern.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineDash",
          "type": [
            {
              "kind": "text",
              "text": "[integer]"
            }
          ],
          "typeDetails": [
            {
              "arrayMode": true,
              "baseType": "integer"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The line dash offset.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineDashOffset",
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object that determines color stops for the gradient, positioned by offsets between 0 and 1.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "\"lineGradient\": {\"0\": \"green\", \"1\": \"blue\"}",
                ""
              ],
              "metadata": {
                "anchor": "4315597",
                "title": "Listing 1"
              },
              "syntax": "other",
              "type": "codeListing"
            },
            {
              "inlineContent": [],
              "type": "paragraph"
            }
          ],
          "name": "lineGradient",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/snapshots/overlaystyle/linegradient",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:snapshots:OverlayStyle.LineGradient",
              "text": "OverlayStyle.LineGradient"
            }
          ],
          "typeDetails": [
            {
              "baseType": "dictionary"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object that determines where the color stops for the gradient. This property uses indexes of points on the polyline.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "code": [
                "\"lineGradientIndexes\": {\"1\": \"blue\"}",
                ""
              ],
              "metadata": {
                "anchor": "4315596",
                "title": "Listing 2"
              },
              "syntax": "other",
              "type": "codeListing"
            },
            {
              "inlineContent": [],
              "type": "paragraph"
            }
          ],
          "name": "lineGradientIndexes",
          "type": [
            {
              "identifier": "doc://com.apple.documentation/documentation/snapshots/overlaystyle/linegradientindexes",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:snapshots:OverlayStyle.LineGradientIndexes",
              "text": "OverlayStyle.LineGradientIndexes"
            }
          ],
          "typeDetails": [
            {
              "baseType": "dictionary"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style used for joins between line segments. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Options: ",
                  "type": "text"
                },
                {
                  "code": "”miter”, “round”, or “bevel”",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineJoin",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An integer in CSS pixels that specifies the width of the line.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineWidth",
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ],
          "typeDetails": [
            {
              "baseType": "integer"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The color for stroking.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "strokeColor",
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The opacity value for stroking.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "strokeOpacity",
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You pass the ",
              "type": "text"
            },
            {
              "code": "OverlayStyle",
              "type": "codeVoice"
            },
            {
              "text": " object properties from the ",
              "type": "text"
            },
            {
              "code": "overlayStyles",
              "type": "codeVoice"
            },
            {
              "text": " query parameter. You can set the object properties and reuse the styles for multiple objects. Any changes made to an ",
              "type": "text"
            },
            {
              "code": "OverlayStyle",
              "type": "codeVoice"
            },
            {
              "text": " object property automatically changes the styles associated with that object, unless they’re overwritten using the ",
              "type": "text"
            },
            {
              "code": "Overlay",
              "type": "codeVoice"
            },
            {
              "text": " object. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows ",
              "type": "text"
            },
            {
              "code": "OverlayStyle",
              "type": "codeVoice"
            },
            {
              "text": " properties passed into the ",
              "type": "text"
            },
            {
              "code": "overlayStyles",
              "type": "codeVoice"
            },
            {
              "text": " query parameter:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "/snapshot?center=0,0&z=7&overlays=[{",
            "\"type\": \"polygon\",",
            "\"points\":[\"1,1\",\"1,-1\",\"-1,-1\",\"-1,1\"],",
            "\"styleIdx\": 2",
            "}]",
            "&overlayStyles=[{",
            "\"fillColor\": \"teal\",",
            "\"fillOpacity\": 0.7,",
            "\"strokeColor\": \"blue\",",
            "\"lineCap\": \"square\",",
            "\"lineWidth\": 11,",
            "\"strokeOpacity\": 0.7 }]"
          ],
          "metadata": {
            "anchor": "4315595",
            "title": "Listing 3"
          },
          "syntax": "other",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
