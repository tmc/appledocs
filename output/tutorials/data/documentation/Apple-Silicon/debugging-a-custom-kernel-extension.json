{
  "abstract": [
    {
      "text": "Configure your system to enable the debugging of custom kernel extensions from a",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "second Mac.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
