          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tint effect to use. Use ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to avoid  overriding",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the inherited tint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tint"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The containing list’s style applies the tint as appropriate. For",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example, watchOS uses the tint color for its background platter",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "appearance. Sidebars on iOS and macOS apply the tint color to their",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Label",
              "type": "codeVoice"
            },
            {
              "text": " icons, which otherwise use the accent color by default.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
