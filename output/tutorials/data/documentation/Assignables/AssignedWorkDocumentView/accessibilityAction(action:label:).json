          "inlineContent": [
            {
              "text": "For example, this is how a custom action to compose",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a new email could be added to a view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var body: some View {",
            "    ContentView()",
            "        .accessibilityAction {",
            "            // Handle action",
            "        } label: {",
            "            Label(\"New Message\", systemImage: \"plus\")",
            "        }",
            "}"
