    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A symbol effect to add to the view. Existing effects",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "added by ancestors of the view are preserved, but may be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "overridden by the new effect. Added effects will be applied",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to the ``SwiftUI/Image` views contained by the child view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "effect"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value to monitor for changes, the animation is",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "triggered each time the value changes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A copy of the view with a symbol effect added.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example adds a bounce effect to two symbol",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "images, the animation will play each time ",
              "type": "text"
            },
            {
              "code": "counter",
              "type": "codeVoice"
            },
            {
              "text": " changes:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "VStack {",
            "    Image(systemName: \"bolt.slash.fill\")",
            "    Image(systemName: \"folder.fill.badge.person.crop\")",
            "}",
            ".symbolEffect(.bounce, value: counter)"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
