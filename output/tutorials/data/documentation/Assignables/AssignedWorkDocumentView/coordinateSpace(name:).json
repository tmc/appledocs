      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A name used to identify this coordinate space.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "coordinateSpace(name:)",
              "type": "codeVoice"
            },
            {
              "text": " to allow another function to find and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "operate on a view and operate on dimensions relative to that view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below demonstrates how a nested view can find and operate on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "its enclosing view’s coordinate space:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    @State private var location = CGPoint.zero",
            "",
            "    var body: some View {",
            "        VStack {",
            "            Color.red.frame(width: 100, height: 100)",
            "                .overlay(circle)",
            "            Text(\"Location: \\(Int(location.x)), \\(Int(location.y))\")",
            "        }",
            "        .coordinateSpace(name: \"stack\")",
            "    }",
            "",
            "    var circle: some View {",
            "        Circle()",
            "            .frame(width: 25, height: 25)",
            "            .gesture(drag)",
            "            .padding(5)",
            "    }",
            "",
            "    var drag: some Gesture {",
            "        DragGesture(coordinateSpace: .named(\"stack\"))",
            "            .onChanged { info in location = info.location }",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Here, the ",
              "type": "text"
            },
            {
              "code": "VStack",
              "type": "codeVoice"
            },
            {
              "text": " in the ",
              "type": "text"
            },
            {
              "code": "ContentView",
              "type": "codeVoice"
            },
            {
              "text": " named “stack” is composed of a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "red frame with a custom ",
              "type": "text"
            },
            {
              "code": "Circle",
              "type": "codeVoice"
            },
            {
              "text": " view ",
              "type": "text"
            },
            {
              "code": "View/overlay(_:alignment:)",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "at its center.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "circle",
              "type": "codeVoice"
            },
            {
              "text": " view has an attached ",
              "type": "text"
            },
            {
              "code": "DragGesture",
              "type": "codeVoice"
            },
            {
              "text": " that targets the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "enclosing VStack’s coordinate space. As the gesture recognizer’s closure",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "registers events inside ",
              "type": "text"
            },
            {
              "code": "circle",
              "type": "codeVoice"
            },
            {
              "text": " it stores them in the shared ",
              "type": "text"
            },
            {
              "code": "location",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "state variable and the ",
              "type": "text"
            },
            {
              "code": "VStack",
              "type": "codeVoice"
            },
            {
              "text": " displays the coordinates in a ",
              "type": "text"
            },
            {
              "code": "Text",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
