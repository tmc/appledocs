          "content": [
            {
              "inlineContent": [
                {
                  "text": "The uniform type identifiers that describe the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "types of content this view can accept through drag and drop.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If the drag and drop operation doesn’t contain any of the supported",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "types, then this drop destination doesn’t activate and ",
                  "type": "text"
                },
                {
                  "code": "isTargeted",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "doesn’t update.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "supportedContentTypes"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A type that conforms to the ",
                  "type": "text"
                },
                {
                  "code": "DropDelegate",
                  "type": "codeVoice"
                },
                {
                  "text": " protocol. You",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "have comprehensive control over drop behavior when you use a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "delegate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "delegate"
        }
      ]
    },
