              "inlineContent": [
                {
                  "text": "A closure that returns the effect to be applied. The first",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "argument provided to the closure is a placeholder representing",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "this view. The second argument is a ",
                  "type": "text"
                },
                {
                  "code": "GeometryProxy3D",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
