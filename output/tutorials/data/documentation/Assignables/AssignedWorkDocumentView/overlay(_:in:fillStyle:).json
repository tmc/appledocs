    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "ShapeStyle",
                  "type": "codeVoice"
                },
                {
                  "text": " that SwiftUI uses to the fill the shape",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that you specify.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "style"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An instance of a type that conforms to ",
                  "type": "text"
                },
                {
                  "code": "Shape",
                  "type": "codeVoice"
                },
                {
                  "text": " that",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "SwiftUI draws in front of the view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shape"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "FillStyle",
                  "type": "codeVoice"
                },
                {
                  "text": " to use when drawing the shape.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The default style uses the nonzero winding number rule and",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "antialiasing.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillStyle"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view with the specified shape drawn in front of it.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to layer a type that conforms to the ",
              "type": "text"
            },
            {
              "code": "Shape",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "protocol — like a ",
              "type": "text"
            },
            {
              "code": "Rectangle",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "Circle",
              "type": "codeVoice"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "code": "Capsule",
              "type": "codeVoice"
            },
            {
              "text": " — in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "front of a view. Specify a ",
              "type": "text"
            },
            {
              "code": "ShapeStyle",
              "type": "codeVoice"
            },
            {
              "text": " that’s used to fill the shape.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example, you can overlay the outline of one rectangle in front of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "another:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Rectangle()",
            "    .frame(width: 200, height: 100)",
            "    .overlay(.teal, in: Rectangle().inset(by: 10).stroke(lineWidth: 5))"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The example above uses the ",
              "type": "text"
            },
            {
              "code": "InsettableShape/inset(by:)",
              "type": "codeVoice"
            },
            {
              "text": " method to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "slightly reduce the size of the overlaid rectangle, and the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Shape/stroke(lineWidth:)",
              "type": "codeVoice"
            },
            {
              "text": " method to fill only the shape’s outline.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This creates an inset border:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier is a convenience method for layering a shape over a view.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "To handle the more general case of overlaying a ",
              "type": "text"
            },
            {
              "code": "View",
              "type": "codeVoice"
            },
            {
              "text": " — or a stack",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of views — with control over the position, use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "View/overlay(alignment:content:)",
              "type": "codeVoice"
            },
            {
              "text": " instead. To cover a view with a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "ShapeStyle",
              "type": "codeVoice"
            },
            {
              "text": ", use ",
              "type": "text"
            },
            {
              "code": "View/overlay(_:ignoresSafeAreaEdges:)",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
