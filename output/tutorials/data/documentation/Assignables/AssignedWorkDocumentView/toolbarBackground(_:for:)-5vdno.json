    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The preferred visibility of the background of the bar.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "visibility"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bars to update the color scheme of or",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "ToolbarPlacement/automatic",
                  "type": "codeVoice"
                },
                {
                  "text": " if empty.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bars"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The preferred visibility flows up to the nearest container that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "renders a bar. This could be a ",
              "type": "text"
            },
            {
              "code": "NavigationView",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "TabView",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in iOS, or the root view of a ",
              "type": "text"
            },
            {
              "code": "WindowGroup",
              "type": "codeVoice"
            },
            {
              "text": " in macOS.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In iOS, a value of ",
              "type": "text"
            },
            {
              "code": "ToolbarPlacement/automatic",
              "type": "codeVoice"
            },
            {
              "text": " makes the visibility",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of a tab bar or navigation bar background depend on where a ",
              "type": "text"
            },
            {
              "code": "List",
              "type": "codeVoice"
            },
            {
              "text": " or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "ScrollView",
              "type": "codeVoice"
            },
            {
              "text": " settles. For example, when aligned to the bottom edge of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of a scroll view’s content, the background of a tab bar becomes",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "transparent.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Specify a value of ",
              "type": "text"
            },
            {
              "code": "Visibility/visible",
              "type": "codeVoice"
            },
            {
              "text": " to ensure that the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "background of a bar remains visible regardless of where any scroll",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view or list stops scrolling.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This example shows a view that prefers to always have the tab bar",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "visible when the middle tab is selected:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    FirstTab()",
            "    MiddleTab()",
            "        .toolbarBackground(.visible, for: .tabBar)",
            "    LastTab()",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can provide multiple placements to customize multiple bars",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "at once, as in the following example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    NavigationView {",
            "        ContentView()",
            "            .toolbarBackground(",
            "                .visible, for: .navigationBar, .tabBar)",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
