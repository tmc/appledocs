    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The property you want information about.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inPropertyID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The size of the buffer pointed to by the ",
                  "type": "text"
                },
                {
                  "code": "inSpecifier",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter. Pass ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " if no specifier buffer is required.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inSpecifierSize"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a specifier buffer, if such a buffer is required by the property about which you want information. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if no specifier is required.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inSpecifier"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On output, the size, in bytes, of the property value. To get the property value, you need a buffer of at least this size.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "outPropertyDataSize"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On output, ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " if the property is writable, or ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " if the property is read only.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "outWritable"
        }
      ]
