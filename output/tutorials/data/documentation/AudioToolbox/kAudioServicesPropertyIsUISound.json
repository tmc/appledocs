{
  "abstract": [
    {
      "text": "A ",
      "type": "text"
    },
    {
      "code": "UInt32",
      "type": "codeVoice"
    },
    {
      "text": " value, where ",
      "type": "text"
    },
    {
      "code": "1",
      "type": "codeVoice"
    },
    {
      "text": " means that, for the audio file specified by a system sound passed in the ",
      "type": "text"
    },
    {
      "code": "inSpecifier",
      "type": "codeVoice"
    },
    {
      "text": " parameter, the System Sound server respects the user setting in the Sound Effects preference and is silent when the user turns off sound effects.",
      "type": "text"
    }
  ],
