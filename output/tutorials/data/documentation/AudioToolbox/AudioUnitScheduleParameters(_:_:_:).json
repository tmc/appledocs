        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "One or more parameter events that you want to schedule.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inParameterEvent"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of audio unit parameter events represented in the ",
                  "type": "text"
                },
                {
                  "code": "inParameterEvent",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inNumParamEvents"
        }
      ]
    },
