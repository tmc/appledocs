    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The directory in which to create the new file.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inParentDir"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the new file.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inFileName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of file to create. This is a constant from AudioToolbox/AudioFile.h, e.g. kAudioFileAIFFType. Note that this is not an HFSTypeCode.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inFileType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The format of the audio data to be written to the file.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inStreamDesc"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The channel layout of the audio data. If non-null, this must be consistent with the number of channels specified by inStreamDesc.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inChannelLayout"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On output, a newly allocated extended audio file object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "outExtAudioFile"
        }
      ]
    },
    {
      "content": [
