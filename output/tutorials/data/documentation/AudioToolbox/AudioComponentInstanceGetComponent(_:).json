        {
          "inlineContent": [
            {
              "text": "A reference to the desired audio component. If the value provided in the ",
              "type": "text"
            },
            {
              "code": "inInstance",
              "type": "codeVoice"
            },
            {
              "text": " parameter is invalid, returns ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
