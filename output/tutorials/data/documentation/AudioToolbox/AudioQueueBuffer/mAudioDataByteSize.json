{
  "abstract": [
    {
      "text": "The number of bytes of valid audio data in the audio queue buffer’s ",
      "type": "text"
    },
    {
      "code": "mAudioData",
      "type": "codeVoice"
    },
    {
      "text": " field, initially set to ",
      "type": "text"
    },
    {
      "code": "0",
      "type": "codeVoice"
    },
    {
      "text": ". Your callback must set this value for a playback audio queue; for recording, the recording audio queue sets the value.",
