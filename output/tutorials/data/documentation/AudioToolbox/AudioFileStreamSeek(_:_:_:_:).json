              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The ID of the parser to which you wish to provide a byte offset. The parser ID is returned by the ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.audiotoolbox/documentation/AudioToolbox/AudioFileStreamOpen(_:_:_:_:_:)",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " function.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "inAudioFileStream"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The number of packets from the beginning of the file of the packet whose byte offset you wish to have returned.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "inPacketOffset"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "On output, the absolute byte offset of the packet whose offset you specify in the ",
                        "type": "text"
                      },
                      {
                        "code": "inPacketOffset",
                        "type": "codeVoice"
                      },
                      {
                        "text": " parameter. For audio file formats that do not contain packet tables, the returned offset may be an estimate.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "outDataByteOffset"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "On output, if the ",
                        "type": "text"
                      },
                      {
                        "code": "outDataByteOffset",
                        "type": "codeVoice"
                      },
                      {
                        "text": " parameter returns an estimate, this parameter returns the constant ",
                        "type": "text"
                      },
                      {
                        "code": "kAudioFileStreamSeekFlag_OffsetIsEstimated",
                        "type": "codeVoice"
                      },
                      {
                        "text": ". Currently, no input flags are defined for this call.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "ioFlags"
              }
            ]
          }
