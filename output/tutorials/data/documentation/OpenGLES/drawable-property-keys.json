    "doc://com.apple.opengles/documentation/OpenGLES/kEAGLDrawablePropertyColorFormat": {
      "abstract": [
        {
          "text": "The key specifying the internal color buffer format for the drawable surface. The value for this key is an ",
          "type": "text"
        },
        {
          "code": "NSString",
          "type": "codeVoice"
        },
        {
          "text": " object that specifies a specific color buffer format. This color buffer format is used by the ",
          "type": "text"
        },
        {
          "code": "EAGLContext",
          "type": "codeVoice"
        },
        {
          "text": " object to create the storage for a renderbuffer. The default value is ",
          "type": "text"
        },
        {
          "code": "kEAGLColorFormatRGBA8",
          "type": "codeVoice"
        },
        {
          "text": ".",
          "type": "text"
        }
      ],
      "deprecated": true,
      "fragments": [
