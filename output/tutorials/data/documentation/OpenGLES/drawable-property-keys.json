    "doc://com.apple.opengles/documentation/OpenGLES/kEAGLDrawablePropertyRetainedBacking": {
      "abstract": [
        {
          "text": "The key specifying whether the drawable surface retains its contents after displaying them.  The value for this key is an ",
          "type": "text"
        },
        {
          "code": "NSNumber",
          "type": "codeVoice"
        },
        {
          "text": " object containing a ",
          "type": "text"
        },
        {
          "code": "BOOL",
          "type": "codeVoice"
        },
        {
          "text": " data type. If ",
          "type": "text"
        },
        {
          "code": "false",
          "type": "codeVoice"
        },
        {
          "text": ", you may not rely on the contents being the same after the contents are displayed.  If ",
          "type": "text"
        },
        {
          "code": "true",
          "type": "codeVoice"
        },
        {
          "text": ", then the contents will not change after being displayed. Setting the value to ",
          "type": "text"
        },
        {
          "code": "true",
          "type": "codeVoice"
        },
        {
          "text": " is recommended only when you need the content to remain unchanged, as using it can result in both reduced performance and additional memory usage. The default value is ",
          "type": "text"
        },
        {
          "code": "false",
          "type": "codeVoice"
        },
        {
          "text": ".",
          "type": "text"
        }
      ],
      "deprecated": true,
      "fragments": [
        {
