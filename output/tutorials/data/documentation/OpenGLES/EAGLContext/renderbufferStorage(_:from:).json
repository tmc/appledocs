      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The OpenGL ES binding point for a currently bound renderbuffer. The value of this parameter must be ",
                  "type": "text"
                },
                {
                  "code": "GL_RENDERBUFFER",
                  "type": "codeVoice"
                },
                {
                  "text": " (or ",
                  "type": "text"
                },
                {
                  "code": "GL_RENDERBUFFER_OES",
                  "type": "codeVoice"
                },
                {
                  "text": " in an OpenGL ES 1.1 context).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "target"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object managing the data store for the renderbuffer. In iOS, the value of this parameter must be a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/QuartzCore/CAEAGLLayer",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "drawable"
        }
      ]
    },
    {
      "content": [
