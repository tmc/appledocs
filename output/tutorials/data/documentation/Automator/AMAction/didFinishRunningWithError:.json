              "inlineContent": [
                {
                  "text": "If an error occurred during asynchronous running of the action, upon return contains an instance of ",
                  "type": "text"
                },
                {
                  "code": "NSError",
                  "type": "codeVoice"
                },
                {
                  "text": " that describes the problem.",
                  "type": "text"
