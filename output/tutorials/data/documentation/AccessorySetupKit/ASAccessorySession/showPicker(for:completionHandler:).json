          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.AccessorySetupKit/documentation/AccessorySetupKit/ASPickerDisplayItem",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " instances describing accessories your app can set up. The picker displays only discovered accessories that match the properties of items in this array.",
