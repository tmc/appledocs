{
  "abstract": [
    {
      "text": "Test your notification interfaces on watchOS using a notification scheme and payload",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "file.",
      "type": "text"
    }
  ],
