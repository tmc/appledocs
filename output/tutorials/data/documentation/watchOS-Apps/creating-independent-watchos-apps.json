{
  "abstract": [
    {
      "text": "Set up a watchOS app that installs and runs without a companion iOS app.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
