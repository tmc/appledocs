          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ACL object that identifies the access control list entry for which you wish to set authorization tags.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "acl"
        },
