      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A CMSEncoder reference returned by the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/CMSEncoderCreate(_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " function.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cmsEncoder"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A timestamp policy. Specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " (or use the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/CMSEncoderCopySignerTimestamp(_:_:_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " function instead) to get the default, which is a policy using ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/kSecPolicyAppleTimeStamping",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/policies",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/certificate-key-and-trust-services",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for more about policies.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timeStampPolicy"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A number indicating which signer to examine. Signer index numbers start with 0. Use the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/CMSDecoderGetNumSigners(_:_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " function to determine the total number of signers for a message.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signerIndex"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The address of an absolute time value where the result should be stored.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timestamp"
        }
      ]
