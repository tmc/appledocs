      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A certificate object for the certificate from which to retrieve the data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "certificate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, points to the data for the certificate specified. You must allocate the space for a ",
                  "type": "text"
                },
                {
                  "code": "CSSM_DATA",
                  "type": "codeVoice"
                },
                {
                  "text": " structure before calling this function. This data pointer is only guaranteed to remain valid as long as the certificate remains unchanged and valid.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data"
        }
      ]
    },
    {
