{
  "abstract": [
    {
      "text": "Determines whether peer certificate chain validation is currently enabled.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "To check whether peer certificate chain validation is enabled in a context, call ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.security/documentation/Security/SSLGetSessionOption(_:_:_:)",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " to obtain the value of the ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.security/documentation/Security/SSLSessionOption/breakOnServerAuth",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " session option flag. If the value of this option flag is ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/swift/true",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": ", then verification is disabled.",
          "type": "text"
        }
      ],
      "type": "paragraph"
    }
  ],
