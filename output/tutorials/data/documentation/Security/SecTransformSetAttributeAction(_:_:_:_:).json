          "value": {
            "kind": "parameters",
            "parameters": [
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.security/documentation/Security/SecTransformImplementationRef",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " that is bound to an instance of a custom transform.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "ref"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The behavior to be set.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  },
                  {
                    "inlineContent": [
                      {
                        "text": "Use ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.security/documentation/Security/kSecTransformActionAttributeNotification",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " to add a block that is called when an attribute is set. If the name is ",
                        "type": "text"
                      },
                      {
                        "code": "NULL",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", then the supplied block is called for all set attributes except for ones that have a specific block as a handler.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  },
                  {
                    "inlineContent": [
                      {
                        "text": "Use ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.security/documentation/Security/kSecTransformActionAttributeValidation",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " to add a block that is called to validate the input to an attribute.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "action"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The name of the attribute that will be handled. An attribute reference may also be given here. A ",
                        "type": "text"
                      },
                      {
                        "code": "NULL",
                        "type": "codeVoice"
                      },
                      {
                        "text": " value indicates that the supplied action is for all attributes.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "attribute"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.security/documentation/Security/SecTransformAttributeActionBlock",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " which implements the behavior.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "newAction"
              }
            ]
          }
        },
