{
  "abstract": [
    {
      "text": "Establish Secure Sockets Layer (SSL) sessions to facilitate secure communication between client and server.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
