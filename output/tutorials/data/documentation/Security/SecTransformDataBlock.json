          "inlineContent": [
            {
              "text": "",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": " for the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.security/documentation/Security/kSecTransformActionInternalizeExtraData",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " action, the data to be passed to the output attribute for any other action, or a ",
              "type": "text"
            },
            {
