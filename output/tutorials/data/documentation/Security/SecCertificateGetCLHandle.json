    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The certificate object from which to obtain the certificate library handle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "certificate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, points to the certificate library handle of the specified certificate. This handle remains valid until the certificate object is released.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "clHandle"
        }
      ]
    },
    {
      "content": [
