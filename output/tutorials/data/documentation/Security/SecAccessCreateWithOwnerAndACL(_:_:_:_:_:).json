    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The user ID that owns this ACL.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "userId"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The group ID that owns this ACL.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "groupId"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Flags that control whether the specified user ID or group ID owns the resulting ACL. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/SecAccessOwnerType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for details.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ownerType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of ACL entries to associate with the access instance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "acls"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The address of an error instance. On error, the return value is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", and the variable referenced by this parameter is overwritten with a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cferror-ru8",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " instance that provides more information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "error"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The new access instance. In Objective-C, call the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1521153-cfrelease",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " function to release it when you are finished using it.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to create a customized access instance from ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.security/documentation/Security/SecACL",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instances that you’ve created with the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.security/documentation/Security/SecACLCreateWithSimpleContents(_:_:_:_:_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method. If you want a default access instance, use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.security/documentation/Security/SecAccessCreate(_:_:_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method instead.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
