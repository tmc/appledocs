    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An access instance from which to retrieve the owner and ACL entries.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "accessRef"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, the user ID that owns the access instance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "userId"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, the group ID that owns the access instance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "groupId"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, flags that indicate whether the specified user ID or group ID owns the resulting ACL entries. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/SecAccessOwnerType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for details.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ownerType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, an array of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/SecACL",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " instances associated with the access instance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "aclList"
        }
      ]
    },
