            },
            {
              "identifier": "doc://com.apple.documentation/documentation/security/authorizationengineref",
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@AuthorizationEngineRef",
              "text": "AuthorizationEngineRef"
            },
            {
              "kind": "text",
              "text": " inEngine, "
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/security/authorizationresult",
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@AuthorizationResult",
              "text": "AuthorizationResult"
