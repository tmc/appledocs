{
  "abstract": [
    {
      "text": "Sets the allowed Secure Sockets Layer (SSL) protocol versions.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "Use ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.security/documentation/Security/SSLSetProtocolVersionMin(_:_:)",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " and/or ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.security/documentation/Security/SSLSetProtocolVersionMax(_:_:)",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " to specify which protocols are enabled.",
          "type": "text"
        }
      ],
      "type": "paragraph"
    }
  ],
