        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Either a single certificate containing a public encryption key for one message recipient, specified as a certificate object (type ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/SecCertificate",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": "), or a set of certificates specified as a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/corefoundation/cfarray",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " of certificate objects.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "recipientOrArray"
