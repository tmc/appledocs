      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A CFType containing the message. This is where either intermediate or final results are returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "message"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If an error occurred, this will contain a CFErrorRef, otherwise this will be NULL. If not NULL the caller is responsible for releasing the CFErrorRef.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "error"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If set the message returned is the final result otherwise it is an intermediate result.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isFinal"
        }
      ]
    }
