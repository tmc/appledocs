    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A CMSDecoder reference returned by the ",
                  "type": "text"
                },
                {
                  "code": "CMSDecoderCreate",
                  "type": "codeVoice"
                },
                {
                  "text": " function.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cmsDecoder"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A number indicating which signer to examine. Signer index numbers start with 0. Use the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.security/documentation/Security/CMSDecoderGetNumSigners(_:_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " function to determine the total number of signers for a message.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signerIndex"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The address of an absolute time value where the result should be stored.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timestamp"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A result code. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.security/documentation/Security/security-framework-result-codes",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". Typically, this function returns ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.security/documentation/Security/errSecParam",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " if the CMS message was not signed or if ",
              "type": "text"
            },
            {
              "code": "signerIndex",
              "type": "codeVoice"
            },
            {
              "text": " is out of bounds.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This timestamp is an authenticated timestamp provided by a time stamping authority.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You must call ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.security/documentation/Security/CMSDecoderFinalizeMessage(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " before you call this function.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
