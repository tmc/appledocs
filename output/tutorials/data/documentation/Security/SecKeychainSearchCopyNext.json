{
  "abstract": [
    {
      "text": "Finds the next keychain item matching the given search criteria.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "Use ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.security/documentation/Security/SecItemCopyMatching(_:_:)",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " to conduct keychain item searches.",
          "type": "text"
        }
      ],
      "type": "paragraph"
    }
  ],
  "hierarchy": {
