    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of elements in column, excluding missing elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "someCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of missing elements in the column.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "noneCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The arithmetic mean of a column’s values, excluding missing elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mean"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The standard deviation of a column’s values, excluding missing elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "standardDeviation"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The smallest value, excluding missing elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "min"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The largest value, excluding missing elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "max"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The midpoint value that bisects the values of the non-missing elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "median"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value that’s above 25% of the values non-missing elements’ values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "firstQuartile"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value that’s above 75% of the non-missing elements’ values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "thirdQuartile"
        }
      ]
    }
