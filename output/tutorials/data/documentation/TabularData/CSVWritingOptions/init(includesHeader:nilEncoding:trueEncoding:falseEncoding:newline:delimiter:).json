          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether to write a header with the column names. Defaults to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "includesHeader"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The spelling for nil values. Defaults to an empty string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nilEncoding"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The spelling for true Boolean values. Defaults to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trueEncoding"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The spelling for false Boolean values. Defaults to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "falseEncoding"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The newline sequence. Defaults to a line feed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "newline"
        },
        {
          "content": [
            {
              "inlineContent": [
