          "content": [
            {
              "inlineContent": [
                {
                  "text": "A set of formatting options that affect the description string,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "including the maximum width of a column and the maximum number of rows.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "FormattingOptions.maximumLineWidth",
              "type": "codeVoice"
            },
            {
              "text": " needs to be wide enough to print at least the index column, the truncation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "column, and one data column (at least two characters, one for initial of the content, and one for “…”).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
