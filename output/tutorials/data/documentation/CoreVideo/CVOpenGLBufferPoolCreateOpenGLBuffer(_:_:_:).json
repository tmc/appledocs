    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The allocator to use for creating the buffer.  May be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " to specify the default allocator.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allocator"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The OpenGL buffer pool that should create the new OpenGL buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "openGLBufferPool"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On output, ",
                  "type": "text"
                },
                {
                  "code": "OpenGLBufferOut",
                  "type": "codeVoice"
                },
                {
                  "text": " points to the new OpenGL buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "openGLBufferOut"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A Core Video result code. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.corevideo/documentation/CoreVideo/core-video-constants",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for possible values.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The function creates a new OpenGL buffer using the OpenGL buffer attributes specified in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.corevideo/documentation/CoreVideo/CVOpenGLBufferPoolCreate(_:_:_:_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " call. This buffer has default attachments as specified in the ",
              "type": "text"
            },
            {
              "code": "openGLBufferAttributes",
              "type": "codeVoice"
            },
            {
              "text": " parameter of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.corevideo/documentation/CoreVideo/CVOpenGLBufferPoolCreate(_:_:_:_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " (using either the ",
              "type": "text"
            },
            {
              "code": "kCVBufferPropagatedAttachmentsKey",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "kCVBufferNonPropagatedAttachmentsKey",
              "type": "codeVoice"
            },
            {
              "text": " attributes).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
