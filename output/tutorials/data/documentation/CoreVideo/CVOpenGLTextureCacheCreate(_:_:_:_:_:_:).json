      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "CFAllocatorRef",
                  "type": "codeVoice"
                },
                {
                  "text": " to use for allocating the cache.  May be NULL.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allocator"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary specifying options for the cache’s behavior, or ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " to use default options. For applicable keys and values, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.corevideo/documentation/CoreVideo/CVOpenGLTextureCache-cache-attributes",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cacheAttributes"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The OpenGL context into which the texture objects will be created.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cglContext"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The OpenGL pixel format object used to create the passed in OpenGL context.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cglPixelFormat"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "CFDictionaryRef",
                  "type": "codeVoice"
                },
                {
                  "text": " containing the attributes to be used for creating the ",
                  "type": "text"
                },
                {
                  "code": "CVOpenGLTexture",
                  "type": "codeVoice"
                },
                {
                  "text": " objects.  May be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "textureAttributes"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Upon return, contains the newly created texture cache.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cacheOut"
        }
