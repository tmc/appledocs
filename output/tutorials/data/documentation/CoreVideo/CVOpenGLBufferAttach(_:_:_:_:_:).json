          "content": [
            {
              "inlineContent": [
                {
                  "text": "The buffer you want to attach an OpenGL context to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "openGLBuffer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The OpenGL context you want to attach.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cglContext"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The OpenGL face enumeration (",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " for non-cube maps.)",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "face"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The mipmap level for drawing in the OpenGL context. This value cannot exceed the maximum mipmap level for this buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "level"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The virtual screen number you want to use for this context.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "screen"
        }
      ]
