    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the data buffer to be sent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length of the buffer to be sent (in bytes).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "length"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A boolean if set to TRUE the call will wait until it is possible to send all the data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sleep"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "UInt32 pointer in which the caller received the nuber of bytes sent. If set to FALSE and it is not possible to send part of the data the method will return immediately.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "numBytesSent"
        }
      ]
    },
    {
      "content": [
