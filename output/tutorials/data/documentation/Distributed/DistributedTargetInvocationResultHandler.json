      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "An instance conforming to this type must be passed when invoking",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "executeDistributedTarget(on:target:invocationDecoder:handler:)",
              "type": "codeVoice"
            },
            {
              "text": " while handling an incoming distributed call.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The handler will then be invoked with the return value (or error) that the invoked target returned (or threw).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
