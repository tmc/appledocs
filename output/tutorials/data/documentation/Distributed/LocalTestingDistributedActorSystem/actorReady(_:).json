    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Reference to the (local) actor that was just fully initialized.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "actor"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The system is expected to store the reference to this actor, and maintain an ",
              "type": "text"
            },
            {
              "code": "ActorID: DistributedActor",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "mapping for the purpose of implementing the ",
              "type": "text"
            },
            {
              "code": "resolve(id:as:)",
              "type": "codeVoice"
            },
            {
              "text": " method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The system usually should NOT retain the passed reference, and it will be informed via",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Distributed/LocalTestingDistributedActorSystem/resignID(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " when the actor has been deallocated so it can remove the stale reference from its",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "internal ",
              "type": "text"
            },
            {
              "code": "ActorID: DistributedActor",
              "type": "codeVoice"
            },
            {
              "text": " mapping.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Distributed/DistributedActor/id",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " of the passed actor must be an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Distributed/LocalTestingDistributedActorSystem/ActorID",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " that this system previously has assigned.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If ",
              "type": "text"
            },
            {
              "code": "actorReady",
              "type": "codeVoice"
            },
            {
              "text": " gets called with some unknown ID, it should crash immediately as it signifies some",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "very unexpected use of the system.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
