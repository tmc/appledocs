    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "arguments",
              "type": "codeVoice"
            },
            {
              "text": " are the arguments container that was previously created",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by ",
              "type": "text"
            },
            {
              "code": "makeInvocationEncoder",
              "type": "codeVoice"
            },
            {
              "text": " and has been populated with all arguments.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This method should perform the actual remote function call, and await for its response.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Errors",
          "level": 2,
          "text": "Errors",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method is allowed to throw because of underlying transport or serialization errors,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as well as by re-throwing the error received from the remote callee (if able to).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
