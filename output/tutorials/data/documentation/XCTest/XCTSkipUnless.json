        },
        {
          "inlineContent": [
            {
              "text": "Call this macro to skip any remaining code and assertions in the test method when ",
              "type": "text"
            },
            {
              "code": "expression",
              "type": "codeVoice"
            },
            {
              "text": " evaluates to ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": ", and mark the test method as skipped. If a test failure occurs in the test method before the skip, the test runner marks the test method as failed.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
