    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this function to compare two non-optional values of the same type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Listing 1. Comparing Non-Optional Values Of The Same Type",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "class FrameSizeTests: XCTestCase {",
            "    func testInitialFrameSize() {",
            "        let view = CustomView()",
            "        let expectedSize = CGSize(width: 640.0, height: 480.0)",
            "        XCTAssertEqual(view.frame.size, expectedSize, \"Unexpected frame size.\")",
            "    }",
            "}"
          ],
          "syntax": "objc",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Listing 1 creates a new instance of a custom class called ",
              "type": "text"
            },
            {
              "code": "CustomView",
              "type": "codeVoice"
            },
            {
              "text": ", and checks that its size is equal to ",
              "type": "text"
            },
            {
              "code": "CGSize(width: 640.0, height: 480.0)",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
