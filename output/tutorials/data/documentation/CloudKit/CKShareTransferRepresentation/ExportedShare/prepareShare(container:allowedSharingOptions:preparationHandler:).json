        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkit/documentation/CloudKit/CKShareTransferRepresentation/ExportedShare",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with the new ",
              "type": "text"
            },
            {
