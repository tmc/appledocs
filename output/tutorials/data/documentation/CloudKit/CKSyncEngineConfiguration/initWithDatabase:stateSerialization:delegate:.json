      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The database to sync — either a person’s private database or their shared database.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "database"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If this is the first initialization of the associated sync engine, specify ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": "; otherwise, specify the state from the most recent ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.cloudkit/documentation/CloudKit/CKSyncEngineStateUpdateEvent",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that your delegate handled.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stateSerialization"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The object that provides the records to sync and handles any related events.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "delegate"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "An initialized configuration instance, or ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " if CloudKit can’t create one.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
