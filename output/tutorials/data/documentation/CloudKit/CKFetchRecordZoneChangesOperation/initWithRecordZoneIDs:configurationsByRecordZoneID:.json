              "inlineContent": [
                {
                  "text": "The IDs of the record zones that you want to query for changes. You can specify ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " for this parameter.",
                  "type": "text"
