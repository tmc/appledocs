        },
        {
          "inlineContent": [
            {
              "text": "Define the shape of the axes in the dimensions ",
              "type": "text"
            },
            {
              "code": "[w]",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "[h, w]",
              "type": "codeVoice"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "code": "[c, h, w]",
              "type": "codeVoice"
            },
            {
              "text": ", where ",
              "type": "text"
            },
            {
              "code": "w",
              "type": "codeVoice"
            },
            {
              "text": " is width, ",
              "type": "text"
            },
            {
              "code": "h",
              "type": "codeVoice"
            },
            {
              "text": " is height, and ",
              "type": "text"
            },
            {
              "code": "c",
              "type": "codeVoice"
            },
            {
              "text": " is channel count.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
