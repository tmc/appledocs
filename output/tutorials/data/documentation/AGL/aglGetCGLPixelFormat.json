        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, points to the CGL pixel format object associated with the specified AGL pixel format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cgl_pix"
        }
