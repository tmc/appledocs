  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Browse notable changes in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Vision",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "June-2024",
          "level": 2,
          "text": "June 2024",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use the new Swift-only API that follows best design practices in Swift and leverages modern language features like Swift concurrency for optimal performance.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Analyze an image for aesthetically pleasing attributes by using ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Vision/CalculateImageAestheticsScoresRequest",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " and provides a score through ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Vision/ImageAestheticsScoresObservation",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ". The score indicates whether an image contains memorable or exciting content.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Vision/DetectHumanBodyPoseRequest",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to detect hands along side the 2D body joint skeleton.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Vision/DetectHumanBodyPoseRequest/Revision-swift.enum/revision2",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to improve 2D human body pose detection.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
