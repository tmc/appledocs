            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Set average power goals for cycling and running with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WorkoutKit/PowerThresholdAlert",
