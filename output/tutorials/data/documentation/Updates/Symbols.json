  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Browse notable changes in ",
              "type": "text"
            },
            {
              "identifier": "https://developer.apple.com/documentation/symbols",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "June-2024",
          "level": 2,
          "text": "June 2024",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Make symbols even more expressive with new animations. Find new ways to respond to peopleâ€™s input, convey status changes, and signal ongoing activity with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Symbols/WiggleSymbolEffect",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Symbols/RotateSymbolEffect",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Symbols/BreatheSymbolEffect",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Allow symbols to more intelligently transition between related variants with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Symbols/ReplaceSymbolEffect/magic(fallback:)",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ". Slashes can now draw on and off, and badges can appear and disappear, or be replaced independently of the base symbol.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Symbols/SymbolEffectOptions/repeat(_:)-316cr",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " for new playback options for repeating animations. Apply a delay between repetitions, and use new, continuous repeat behavior with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Symbols/SymbolEffectOptions/RepeatBehavior/continuous",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to produce smoother animations when repeating indefinitely.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
