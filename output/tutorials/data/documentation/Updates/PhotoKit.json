  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Browse notable changes in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/PhotoKit",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "June-2024",
          "level": 2,
          "text": "June 2024",
          "type": "heading"
        },
        {
          "anchor": "Spatial-media",
          "level": 3,
          "text": "Spatial media",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Integrate spatial media from someone’s Photos library into your app using PhotoKit and PhotosUI:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Access a spatial media smart album by specifying the new ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/photokit/phassetcollectionsubtype/smartalbumspatial",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " value when fetching asset collections.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fetch and recognize spatial media assets by adding the new ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Photos/PHAssetMediaSubtype",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " option ",
                      "type": "text"
                    },
                    {
                      "code": "spatialMedia",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to the fetch options predicate.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Offer spatial media items in a ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/PhotosUI/PhotosPicker",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " by setting the new ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/PhotosUI/PHPickerFilter-swift.struct/spatialMedia",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " option on the picker configuration’s filter.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
