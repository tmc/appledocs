  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Browse notable changes in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "June-2024",
          "level": 2,
          "text": "June 2024",
          "type": "heading"
        },
        {
          "anchor": "Predicates",
          "level": 3,
          "text": "Predicates",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use ",
                      "type": "text"
                    },
                    {
                      "code": "Regex",
                      "type": "codeVoice"
                    },
                    {
                      "text": " regular expressions in predicates to perform pattern matching. You can use either the Swift regex domain specific language or traditional regex literals.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use the new ",
                      "type": "text"
                    },
                    {
                      "code": "#Expression",
                      "type": "codeVoice"
                    },
                    {
                      "text": " macro when you want behavior similar to ",
                      "type": "text"
                    },
                    {
                      "code": "#Predicate",
                      "type": "codeVoice"
                    },
                    {
                      "text": " but you need to return a type other than ",
                      "type": "text"
                    },
                    {
                      "code": "Bool",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Create compound predicates by writing a ",
                      "type": "text"
                    },
                    {
                      "code": "Predicate",
                      "type": "codeVoice"
                    },
                    {
                      "text": " that evaluates another ",
                      "type": "text"
                    },
                    {
                      "code": "Predicate",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Calendars",
          "level": 3,
          "text": "Calendars",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Perform ",
                      "type": "text"
                    },
                    {
                      "code": "Calendar",
                      "type": "codeVoice"
                    },
                    {
                      "text": " searches by calling ",
                      "type": "text"
                    },
                    {
                      "code": "dates(byMatching:startingAt:in:matchingPolicy:repeatedTimePolicy:direction:)",
                      "type": "codeVoice"
                    },
                    {
                      "text": " with ",
                      "type": "text"
                    },
                    {
                      "code": "DateComponent",
                      "type": "codeVoice"
                    },
                    {
                      "text": " values specifying what to search for, and receiving a ",
                      "type": "text"
                    },
                    {
                      "code": "Sequence",
                      "type": "codeVoice"
                    },
                    {
                      "text": " of matching dates. You can also repeatedly add ",
                      "type": "text"
                    },
                    {
                      "code": "DateComponent",
                      "type": "codeVoice"
                    },
                    {
                      "text": " values to a date with ",
                      "type": "text"
                    },
                    {
                      "code": "dates(byAdding:startingAt:in:wrappingComponents:)",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and receive a sequence. To add ",
                      "type": "text"
                    },
                    {
                      "code": "Calendar.Component",
                      "type": "codeVoice"
                    },
                    {
                      "text": " values instead, use ",
                      "type": "text"
                    },
                    {
                      "code": "dates(byAdding:value:startingAt:in:wrappingComponents:)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Format-styles",
          "level": 3,
          "text": "Format styles",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use the ",
                      "type": "text"
                    },
                    {
                      "code": "DiscreteFormatStyle",
                      "type": "codeVoice"
                    },
                    {
                      "text": " protocol to format values that constantly change, but donâ€™t necessarily change the formatted output on every update, such as time displays that truncate the seconds field. The following format style types conform to the new protocol: ",
                      "type": "text"
                    },
                    {
                      "code": "Duration.UnitsFormatStyle",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "Duration.TimeFormatStyle",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "Date.FormatStyle",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "Date.FormatStyle.Attributed",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "Date.VerbatimFormatStyle",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "Date.VerbatimFormatStyle.Attributed",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "Date.ISO8601FormatStyle",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "Duration.UnitsFormatStyle.Attributed",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "code": "Duration.TimeFormatStyle.Attributed",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Set new configuration options on ",
                      "type": "text"
                    },
                    {
                      "code": "FormatStyle",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to omit specific symbols from the output and suppress grouping of large time values (for example, in order to produce ",
                      "type": "text"
                    },
                    {
                      "code": "10000:00",
                      "type": "codeVoice"
                    },
                    {
                      "text": " rather than ",
                      "type": "text"
                    },
                    {
                      "code": "10,000:00",
                      "type": "codeVoice"
                    },
                    {
                      "text": ").",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use the ",
                      "type": "text"
                    },
                    {
                      "code": "notation",
                      "type": "codeVoice"
                    },
                    {
                      "text": " modifier on currency format styles to specify a notation such as ",
                      "type": "text"
                    },
                    {
                      "code": "compactName",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", similar to how ",
                      "type": "text"
                    },
                    {
                      "code": "notation",
                      "type": "codeVoice"
                    },
                    {
                      "text": " already works on numeric format styles.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Undo-manager",
          "level": 3,
          "text": "Undo manager",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Provide custom information for undo actions by setting user info on ",
                      "type": "text"
                    },
                    {
                      "code": "UndoManager",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". The info can include things like a timestamp of the actionâ€™s creation or an icon that complements the action name.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Reveal the size of an undo managerâ€™s current undo or redo stack with the properties ",
                      "type": "text"
                    },
                    {
                      "code": "undoCount",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "redoCount",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Key-Value-observing",
          "level": 3,
          "text": "Key-Value observing",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Add a collection of shared observers to ",
                      "type": "text"
                    },
                    {
                      "code": "Observable",
                      "type": "codeVoice"
                    },
                    {
                      "text": " objects that you can then add to multiple instance of the observable.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Observables now use ARC-style weak references to their observers, preventing crashes when observers deallocate without properly removing themselves.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Collections",
          "level": 3,
          "text": "Collections",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "You can initialize an ",
                      "type": "text"
                    },
                    {
                      "code": "IndexSet",
                      "type": "codeVoice"
                    },
                    {
                      "text": " from a Swift ",
                      "type": "text"
                    },
                    {
                      "code": "RangeSet",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", and vice versa.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
