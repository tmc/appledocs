  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Browse notable changes in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/WidgetKit",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "June-2024",
          "level": 2,
          "text": "June 2024",
          "type": "heading"
        },
        {
          "anchor": "General",
          "level": 3,
          "text": "General",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Controls allow people to customize and configure Control Center, their Lock Screen, and the Action button with actions from your apps. Take advantage of ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WidgetKit/ControlWidgetButton",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to execute an action and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WidgetKit/ControlWidgetToggle",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to toggle some state in your app.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Accent views of widgets on iOS and iPadOS using ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/View/widgetAccentable(_:)",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Provide context for when and where a widget is most relevant for surfacing in a Smart Stack with ",
                      "type": "text"
                    },
                    {
                      "text": "doc://com.apple.documentation/documentation/widgetkit/widgetrelevances",
                      "type": "text"
                    },
                    {
                      "text": ". Define context using a collection of ",
                      "type": "text"
                    },
                    {
                      "text": "doc://com.apple.documentation/documentation/widgetkit/widgetrelevanceentry",
                      "type": "text"
                    },
                    {
                      "text": " objects that work with existing ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WidgetKit/TimelineProvider",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WidgetKit/AppIntentTimelineProvider",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WidgetKit/IntentTimelineProvider",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " protocols.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Apple-Watch",
          "level": 3,
          "text": "Apple Watch",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Create interactive widgets on watchOS using a ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Button",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " or ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Toggle",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to perform actions without launching your app.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use an ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WidgetKit/AccessoryWidgetGroup",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " container to provide up to three content views, optionally interactive, in a watchOS widget. Style ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WidgetKit/AccessoryWidgetGroup",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " content views using ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WidgetKit/AccessoryWidgetGroupStyle/circular",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " or ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WidgetKit/AccessoryWidgetGroupStyle/roundedSquare",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
