      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Browse notable changes in ",
              "type": "text"
            },
            {
              "identifier": "https://developer.apple.com/documentation/weatherkit",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "June-2024",
          "level": 2,
          "text": "June 2024",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Benefit from efficient transfers with FlatBuffers.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Obtain hourly, daily, and monthly weather statistics for a particular location based on over 50 years of historical weather data.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Obtain a daily summary of high temperature, low temperature, precipitation amount, and snowfall amount for a specific day since August 2021.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Get a list of upcoming significant weather changes for the following elements: high temperature, low temperature, day precipitation amount, and night precipitation amount.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Access a new property for the current weather: ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/WeatherKit/CurrentWeather/cloudCoverByAltitude",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Access new elements for the current dayâ€™s weather:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "items": [
                    {
                      "content": [
                        {
                          "inlineContent": [
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/highTemperatureTime",
                              "isActive": true,
                              "type": "reference"
                            },
                            {
                              "text": " and ",
                              "type": "text"
                            },
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/lowTemperatureTime",
                              "isActive": true,
                              "type": "reference"
                            }
                          ],
                          "type": "paragraph"
                        }
                      ]
                    },
                    {
                      "content": [
                        {
                          "inlineContent": [
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/minimumHumidity",
                              "isActive": true,
                              "type": "reference"
                            },
                            {
                              "text": " and ",
                              "type": "text"
                            },
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/maximumHumidity",
                              "isActive": true,
                              "type": "reference"
                            }
                          ],
                          "type": "paragraph"
                        }
                      ]
                    },
                    {
                      "content": [
                        {
                          "inlineContent": [
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/minimumVisibility",
                              "isActive": true,
                              "type": "reference"
                            },
                            {
                              "text": " and ",
                              "type": "text"
                            },
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/maximumVisibility",
                              "isActive": true,
                              "type": "reference"
                            }
                          ],
                          "type": "paragraph"
                        }
                      ]
                    },
                    {
                      "content": [
                        {
                          "inlineContent": [
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/highWindSpeed",
                              "isActive": true,
                              "type": "reference"
                            }
                          ],
                          "type": "paragraph"
                        }
                      ]
                    },
                    {
                      "content": [
                        {
                          "inlineContent": [
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/precipitationAmountByType",
                              "isActive": true,
                              "type": "reference"
                            }
                          ],
                          "type": "paragraph"
                        }
                      ]
                    },
                    {
                      "content": [
                        {
                          "inlineContent": [
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/daytimeForecast",
                              "isActive": true,
                              "type": "reference"
                            },
                            {
                              "text": ", ",
                              "type": "text"
                            },
                            {
                              "identifier": "doc://com.apple.documentation/documentation/WeatherKit/DayWeather/overnightForecast",
                              "isActive": true,
                              "type": "reference"
                            },
                            {
                              "text": ", and ",
