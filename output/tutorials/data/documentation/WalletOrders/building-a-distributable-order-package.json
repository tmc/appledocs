{
  "abstract": [
    {
      "text": "Prepare an order for distribution by building, signing, and compressing the source files.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
