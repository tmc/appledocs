    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL where the customer can see the order return policy.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "returnPolicyURL",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "uri"
            }
          ],
          "typeDetails": [
            {
              "baseType": "uri"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether to display the return countdown until ",
                  "type": "text"
                },
                {
                  "code": "returnDeadline",
                  "type": "codeVoice"
                },
                {
                  "text": " in the user interface. Use ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " if all of the items in the order are returnable until the ",
                  "type": "text"
                },
                {
                  "code": "returnDeadline",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Default: ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "displayCountdown",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ],
          "typeDetails": [
            {
              "baseType": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date where the products can be partially or fully returned. The merchant can provide updates to an order that has a completed status until this date.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Use ",
                  "type": "text"
                },
                {
                  "code": "ISO 8601",
                  "type": "codeVoice"
                },
                {
                  "text": " format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "returnDeadline",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ],
          "typeDetails": [
            {
              "baseType": "date-time"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL where the customer can initiate a return.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "returnManagementURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "uri"
            }
          ],
          "typeDetails": [
            {
              "baseType": "uri"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A short description of the return policy. The merchant can include the common return window duration here.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "returnPolicyDescription",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
