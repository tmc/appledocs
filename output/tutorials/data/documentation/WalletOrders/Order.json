          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "open",
                "completed",
                "cancelled"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A high-level status of the order, used for display purposes. The system considers orders with status ",
                  "type": "text"
                },
                {
                  "code": "completed",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "cancelled",
                  "type": "codeVoice"
                },
                {
                  "text": " closed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "status",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The version of the schema used for the order. The current version is ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "schemaVersion",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "number"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date and time when the order was last updated, in RFC 3339 format. This should equal the ",
                  "type": "text"
                },
                {
                  "code": "createdAt",
                  "type": "codeVoice"
                },
                {
                  "text": " time, if the order hasn’t had any updates. Must be monotonically increasing. Consider using a hybrid logical clock if your web service can’t make that guarantee.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "updatedAt",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ],
          "typeDetails": [
            {
              "baseType": "date-time"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
