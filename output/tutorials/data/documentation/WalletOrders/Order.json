      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date and time when the customer created the order, in RFC 3339 format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "createdAt",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ],
          "typeDetails": [
            {
              "baseType": "date-time"
            }
          ]
