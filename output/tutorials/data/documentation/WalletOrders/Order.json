          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of order this bundle represents. Currently the only supported value is ",
                  "type": "text"
                },
                {
                  "code": "ecommerce",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orderType",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An identifier for the order type associated with the order. The value must correspond with your signing certificate and isn’t displayed to the user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orderTypeIdentifier",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
