          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of colors making up the transition points of the gradient.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "colors"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of unit distance values that correspond to the provided colors.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "locations"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The unit distance value of ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " represents the start of the polyline, and ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": " represents the end of the polyline. A gradient may have any number of steps along the length of the polyline.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To determine a location along the polyline, use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.mapkit/documentation/MapKit/MKMultiPoint/location(atPointIndex:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", or retrieve a set of locations using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.mapkit/documentation/MapKit/MKMultiPoint/locationsAtPointIndexes:",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
