                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "This property includes the full content of the message, and contains both the headers and the message body. If you only need the headers in the message, use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.mailkit/documentation/MailKit/MEMessage/headers",
                  "isActive": true,
                  "type": "reference"
                },
