          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL of the embeddable video to display (the Dailymotion, Vimeo, or YouTube embed link). The embed ",
                  "type": "text"
                },
                {
                  "code": "URL",
                  "type": "codeVoice"
                },
                {
                  "text": " is usually different from the standard video ",
                  "type": "text"
                },
                {
                  "code": "URL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "A Dailymotion embed ",
                  "type": "text"
                },
                {
                  "code": "URL",
                  "type": "codeVoice"
                },
                {
                  "text": " typically looks like this: ",
                  "type": "text"
                },
                {
                  "code": "https://geo.dailymotion.com/player.html?video=x84sh87",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "A Vimeo embed ",
                  "type": "text"
                },
                {
                  "code": "URL",
                  "type": "codeVoice"
                },
                {
                  "text": " typically looks like this: ",
                  "type": "text"
                },
                {
                  "code": "https://player.vimeo.com/video/121450839",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "A YouTube embed ",
                  "type": "text"
                },
                {
                  "code": "URL",
                  "type": "codeVoice"
                },
                {
                  "text": " typically looks like this: ",
                  "type": "text"
                },
                {
                  "code": "https://www.youtube.com/embed/0qwALOOvUik",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "URL",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "uri"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
