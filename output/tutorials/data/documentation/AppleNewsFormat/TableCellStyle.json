    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The background color for the cell.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you omit this property, the background is transparent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The cell background color is highest priority, followed by the column, and finally the row. Apple News applies all three colors, meaning that non-opaque values can cause combined colors. For example, using a red row together with a blue cell, both with 50% opacity, creates a purple cell.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "backgroundColor",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Color",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:Color",
              "text": "Color"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The border for the cell. Because the border is inside the cell, it affects the size of the content within the cell. The bigger the border, the less available space for content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "border",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TableBorder",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:TableBorder",
              "text": "TableBorder"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of styles you apply to cells that meet specified conditions. You can use this to change the appearance of specific table cells.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "conditional",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ConditionalTableCellStyle",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:ConditionalTableCellStyle",
              "text": "ConditionalTableCellStyle"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "values": [
                [
                  {
                    "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:news_format:SupportedUnits",
                    "text": "SupportedUnits"
                  }
                ],
                [
                  {
                    "kind": "text",
                    "text": "number"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The height of the cell and its row, as a number in points, or using one of the units of measure for components.See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNews/specifying-measurements-for-components",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "By default, the height of each row is determined by the height of the content in that row.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "height",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "("
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:SupportedUnits",
              "text": "SupportedUnits"
            },
            {
              "kind": "text",
              "text": " | number)"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "left"
            },
            {
              "kind": "allowedValues",
              "values": [
                "left",
                "center",
                "right"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Defines the horizontal alignment of content inside cells.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "horizontalAlignment",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "values": [
                [
                  {
                    "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:news_format:SupportedUnits",
                    "text": "SupportedUnits"
                  }
                ],
                [
                  {
                    "kind": "text",
                    "text": "number"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The minimum width of the cell and its column, as a number in points or in one of the available units of measure for components. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNews/specifying-measurements-for-components",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "minimumWidth",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "("
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:SupportedUnits",
              "text": "SupportedUnits"
            },
            {
              "kind": "text",
              "text": " | number)"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "values": [
                [
                  {
                    "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:news_format:SupportedUnits",
                    "text": "SupportedUnits"
                  }
                ],
                [
                  {
                    "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Padding",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:news_format:Padding",
                    "text": "Padding"
                  }
                ],
                [
                  {
                    "kind": "text",
                    "text": "number"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The space around the content in a table cell in points, supported units, or a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Padding",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object that specifies padding for each side separately.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "padding",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "("
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:SupportedUnits",
              "text": "SupportedUnits"
            },
            {
              "kind": "text",
              "text": " | "
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Padding",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:Padding",
              "text": "Padding"
            },
            {
              "kind": "text",
              "text": " | number)"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "values": [
                [
                  {
                    "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ComponentTextStyle",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:news_format:ComponentTextStyle",
                    "text": "ComponentTextStyle"
                  }
                ],
                [
                  {
                    "kind": "text",
                    "text": "string"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name string of one of your styles in the Article ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ArticleDocument/componentTextStyles-data.dictionary",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "textStyle",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "("
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ComponentTextStyle",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:ComponentTextStyle",
              "text": "ComponentTextStyle"
            },
            {
              "kind": "text",
              "text": " | string)"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "center"
            },
            {
              "kind": "allowedValues",
              "values": [
                "top",
                "center",
                "bottom"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Defines the vertical alignment of content inside cells.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "verticalAlignment",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The column width, as a percentage only. This property only indicates proportionate width; you can’t use it to control exact width. See ",
                  "type": "text"
                },
                {
                  "code": "minimumWidth",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "width",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can apply styles that affect the look of the cells in your tables, including the background color, the horizontal alignment of cell contents, and the cell height, width, and padding.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can also specify conditional styles for cells — styles applied to cells that meet certain criteria. For example, you might choose to change the background color for a cell at a specific location. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ConditionalTableCellStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for more information.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this object in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TableStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"components\": [",
            "    {",
            "      \"role\": \"datatable\",",
            "      \"style\": \"bookTableStyle\",",
            "      \"showDescriptorLabels\": true,",
            "      \"sortBy\": [",
            "        {",
            "          \"descriptor\": \"id-publication-date\",",
            "          \"direction\": \"ascending\"",
            "        }",
            "      ],",
            "      \"data\": {",
            "        \"descriptors\": [",
            "          {",
            "            \"identifier\": \"id-publication-date\",",
            "            \"key\": \"publicationDate\",",
            "            \"label\": \"Date\",",
            "            \"dataType\": \"string\"",
            "          },",
            "          {",
            "            \"identifier\": \"id-title\",",
            "            \"key\": \"title\",",
            "            \"label\": \"Title\",",
            "            \"dataType\": \"string\"",
            "          },",
            "          {",
            "            \"identifier\": \"id-publisher\",",
            "            \"key\": \"publisher\",",
            "            \"label\": \"Publisher\",",
            "            \"dataType\": \"string\"",
            "          }",
            "        ],",
            "        \"records\": [",
            "          {",
            "            \"title\": \"Mardi\",",
            "            \"publicationDate\": \"1849\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          },",
            "          {",
            "            \"title\": \"Typee\",",
            "            \"publicationDate\": \"1846\",",
            "            \"publisher\": \"Wiley and Putnam\"",
            "          },",
            "          {",
            "            \"title\": \"White-Jacket\",",
            "            \"publicationDate\": \"1850\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          },",
            "          {",
            "            \"title\": \"Omoo\",",
            "            \"publicationDate\": \"1847\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          },",
            "          {",
            "            \"title\": \"Redburn\",",
            "            \"publicationDate\": \"1849\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          },",
            "          {",
            "            \"title\": \"Moby-Dick\",",
            "            \"publicationDate\": \"1851\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          }",
            "        ]",
            "      }",
            "    }",
            "  ],",
            "  \"componentStyles\": {",
            "    \"bookTableStyle\": {",
            "      \"tableStyle\": {",
            "        \"columns\": {",
            "          \"backgroundColor\": \"#eeeeee\",",
            "          \"conditional\": [",
            "            {",
            "              \"selectors\": [",
            "                {",
            "                  \"columnIndex\": 0",
            "                }",
            "              ],",
            "              \"backgroundColor\": \"#dddddd\"",
            "            }",
            "          ]",
            "        },",
            "        \"headerCells\": {",
            "          \"padding\": 5,",
            "          \"textStyle\": {",
            "            \"fontWeight\": \"bold\"",
            "          }",
            "        },",
            "        \"cells\": {",
            "          \"padding\": 2",
            "        }",
            "      }",
            "    }",
            "  }",
            "}"
          ],
          "syntax": "json",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
