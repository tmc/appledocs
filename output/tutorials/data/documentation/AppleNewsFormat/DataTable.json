    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Add a ",
              "type": "text"
            },
            {
              "code": "DataTable",
              "type": "codeVoice"
            },
            {
              "text": " component that uses table data provided in a JSON format. Information about the table, including the data itself, is provided by the ",
              "type": "text"
            },
            {
              "code": "data",
              "type": "codeVoice"
            },
            {
              "text": " property, whose value is a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/RecordStore",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object that contains both the table data and information about the data, such as data types and header labels.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can apply styles to table rows, columns, and cells using a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ComponentStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object that has a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TableStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " defined.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"components\": [",
            "    {",
            "      \"role\": \"datatable\",",
            "      \"showDescriptorLabels\": true,",
            "      \"sortBy\": [",
            "        {",
            "          \"descriptor\": \"id-name\",",
            "          \"direction\": \"descending\"",
            "        }",
            "      ],",
            "      \"data\": {",
            "        \"descriptors\": [",
            "          {",
            "            \"identifier\": \"id-name\",",
            "            \"key\": \"name\",",
            "            \"label\": {",
            "              \"type\": \"formatted_text\",",
            "              \"text\": \"Name\",",
            "              \"textStyle\": {",
            "                \"textColor\": \"black\"",
            "              }",
            "            },",
            "            \"dataType\": \"string\"",
            "          },",
            "          {",
            "            \"identifier\": \"id-occupation\",",
            "            \"key\": \"occupation\",",
            "            \"label\": \"Occupation\",",
            "            \"dataType\": \"string\"",
            "          }",
            "        ],",
            "        \"records\": [",
            "          {",
            "            \"name\": \"Amelia Earhart\",",
            "            \"occupation\": \"Pilot\"",
            "          },",
            "          {",
            "            \"name\": \"Grace Hopper\",",
            "            \"occupation\": \"Computer Scientist\"",
            "          }",
            "        ]",
            "      },",
            "      \"style\": {",
            "        \"tableStyle\": {",
            "          \"rows\": {",
            "            \"backgroundColor\": \"#fff\",",
            "            \"divider\": {",
            "              \"width\": 1,",
            "              \"color\": \"#ddd\"",
            "            }",
            "          },",
            "          \"headerRows\": {",
            "            \"backgroundColor\": \"#ccc\",",
            "            \"divider\": {",
            "              \"width\": 2,",
            "              \"color\": \"#999\"",
            "            }",
            "          },",
            "          \"cells\": {",
            "            \"padding\": 6,",
            "            \"verticalAlignment\": \"top\"",
            "          }",
            "        }",
            "      },",
            "      \"layout\": {",
