          "content": [
            {
              "inlineContent": [
                {
                  "text": "An inline ",
                  "type": "text"
                },
                {
                  "code": "ComponentStyle",
                  "type": "codeVoice"
                },
                {
                  "text": " object that defines the appearance of this component or a string reference to a ",
