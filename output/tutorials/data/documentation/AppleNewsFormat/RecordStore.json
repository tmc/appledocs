  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: Apple News Format"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "RecordStore"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Provides information about the data that can be in each data record.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The order of the descriptors determines the order of the columns (or the rows if the tableâ€™s ",
                  "type": "text"
                },
                {
                  "code": "dataOrientation",
                  "type": "codeVoice"
                },
                {
                  "text": " is set to horizontal.)",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Version 1.5",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "descriptors",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/DataDescriptor",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:DataDescriptor",
              "text": "DataDescriptor"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Provides data records that fit within the structure defined by ",
                  "type": "text"
                },
                {
                  "code": "descriptors",
                  "type": "codeVoice"
                },
                {
                  "text": ". Each descriptor can be used only once per record.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You can choose not to include all values from a given record in a data table. Only data that corresponds to a data descriptor will be included in your data table.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "records",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/RecordStore/records-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:RecordStore.records",
              "text": "RecordStore.records"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "kind": "mentions",
      "mentions": [
        "doc://com.apple.applenews/documentation/AppleNews/defining-and-using-table-styles"
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "RecordStore",
              "type": "codeVoice"
            },
            {
              "text": " object uses two required properties to provide the ",
              "type": "text"
            },
            {
              "code": "data",
              "type": "codeVoice"
            },
            {
              "text": " for a table and to specify how that data will be used in the table.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Data ",
                      "type": "text"
                    },
                    {
                      "code": "descriptors",
                      "type": "codeVoice"
                    },
                    {
                      "text": " define how the data is interpreted and how it will be displayed in the table. For each set of data, data descriptors communicate the data type, unique keys, data formats, and header labels for the table.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Data ",
                      "type": "text"
                    },
                    {
                      "code": "records",
                      "type": "codeVoice"
                    },
                    {
                      "text": " provide the actual data as a set of key-value pairs that relate to the descriptors.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "For example, your data ",
              "type": "text"
            },
            {
              "code": "descriptors",
              "type": "codeVoice"
            },
            {
              "text": " might describe a table that contains stock symbols and prices; each data record could provide the symbol and price for a particular stock.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This object can be used in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/DataTable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"components\": [",
            "    {",
            "      \"role\": \"datatable\",",
            "      \"showDescriptorLabels\": true,",
            "      \"sortBy\": [",
            "        {",
            "          \"descriptor\": \"id-name\",",
            "          \"direction\": \"descending\"",
            "        }",
            "      ],",
            "      \"data\": {",
            "        \"descriptors\": [",
            "          {",
            "            \"identifier\": \"id-name\",",
            "            \"key\": \"name\",",
            "            \"label\": {",
            "              \"type\": \"formatted_text\",",
            "              \"text\": \"Name\"",
            "            },",
            "            \"dataType\": \"string\"",
            "          },",
            "          {",
            "            \"identifier\": \"id-occupation\",",
            "            \"key\": \"occupation\",",
            "            \"label\": \"Occupation\",",
            "            \"dataType\": \"string\"",
            "          }",
            "        ],",
            "        \"records\": [",
            "          {",
            "            \"name\": \"Amelia Earhart\",",
            "            \"occupation\": \"Pilot\"",
            "          },",
            "          {",
            "            \"name\": \"Grace Hopper\",",
            "            \"occupation\": \"Computer Scientist\"",
            "          }",
            "        ]",
            "      }",
            "    }",
            "  ]",
            "}"
          ],
          "syntax": "json",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
