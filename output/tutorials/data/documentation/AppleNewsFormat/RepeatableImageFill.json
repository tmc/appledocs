      "kind": "properties",
      "title": "Properties"
    },
    {
      "kind": "mentions",
      "mentions": [
        "doc://com.apple.applenews/documentation/AppleNews/applying-a-background-to-a-component",
        "doc://com.apple.applenews/documentation/AppleNews/preparing-image-video-audio-music-and-arkit-assets"
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "RepeatableImage",
              "type": "codeVoice"
            },
            {
              "text": " fill type object to add a repeatable image on components.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The recommended dimensions for a repeatable image are as follows:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Minimum dimensions: 3 x 6 or 6 x 3 pixels",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Maximum dimensions: 3000 x 3000 pixels",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"components\": [",
            "    {",
            "      \"role\": \"container\",",
            "      \"layout\": {",
            "        \"minimumHeight\": 300",
            "      },",
            "      \"style\": {",
            "        \"fill\": {",
            "          \"type\": \"repeatable_image\",",
            "          \"URL\": \"bundle://myimage.jpg\",",
            "          \"repeat\": \"x\",",
            "          \"width\": 30,",
            "          \"height\": 30,",
            "          \"verticalAlignment\": \"center\"",
            "        }",
            "      }",
            "    }",
            "  ]",
            "}"
          ],
          "syntax": "json",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
