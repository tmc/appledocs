            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "RepeatableImageFill"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "repeatable_image"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Always ",
                  "type": "text"
                },
                {
                  "code": "repeatable_image",
                  "type": "codeVoice"
                },
                {
                  "text": " for this object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL of the image file to use for filling the component.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Image URLs can begin with ",
                  "type": "text"
                },
                {
                  "code": "http://",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "https://",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "bundle://",
                  "type": "codeVoice"
                },
                {
                  "text": ". If the image URL begins with ",
                  "type": "text"
                },
                {
                  "code": "bundle://",
                  "type": "codeVoice"
                },
                {
                  "text": ", the referenced image file must be in the same directory as the document.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Encode image filenames as URLs.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNews/preparing-image-video-audio-music-and-arkit-assets",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "URL",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "uri"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "scroll"
            },
            {
              "kind": "allowedValues",
              "values": [
                "fixed",
                "scroll"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that indicates how the fill behaves when someone scrolls.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Valid values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "scroll",
                          "type": "codeVoice"
                        },
                        {
                          "text": " (default). The fill scrolls along with its component.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "fixed",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". The fill stays at a fixed position within the viewport.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "attachment",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "values": [
                [
                  {
                    "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:news_format:SupportedUnits",
                    "text": "SupportedUnits"
                  }
                ],
                [
                  {
                    "kind": "text",
                    "text": "number"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The height of the image as it’s repeated. When you omit the height, Apple News uses the width property  to determine the size based on the aspect ratio of the provided image.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "height",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "("
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:SupportedUnits",
              "text": "SupportedUnits"
            },
            {
              "kind": "text",
              "text": " | number)"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "center"
            },
            {
              "kind": "allowedValues",
              "values": [
                "left",
                "center",
                "right"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that sets the horizontal alignment of the image fill within its component.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Valid values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "left",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Aligns the left edge of the fill with the left edge of the component.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "center",
                          "type": "codeVoice"
                        },
                        {
                          "text": " (default). Aligns the horizontal center of the fill with the center of the component.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "right",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Aligns the right edge of the fill with the right edge of the component.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "horizontalAlignment",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "both"
            },
            {
              "kind": "allowedValues",
              "values": [
                "none",
                "x",
                "y",
                "both"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that defines the direction in which Apple News repeats the background image.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Valid values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "none",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Apple News doesn’t repeat the background image.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "x",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Apple News repeats the image horizontally based on the original size of the image.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "y",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Apple News repeats the image vertically based on the original size of the image.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "both",
                          "type": "codeVoice"
                        },
                        {
                          "text": " (default). The image is repeated horizontally and vertically based on the original size of the image.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "repeat",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "default",
              "value": "center"
            },
            {
              "kind": "allowedValues",
              "values": [
                "top",
                "center",
                "bottom"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The vertical alignment of the repeatable image fill within its component.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Valid values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "top",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Aligns the top of the fill with the top edge of the component.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "center",
                          "type": "codeVoice"
                        },
                        {
                          "text": " (default). Aligns the vertical center of the fill with the center of the component.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "bottom",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Aligns the bottom of the fill with the bottom edge of the component.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "This property has no effect when the ",
                  "type": "text"
                },
                {
                  "code": "repeat",
                  "type": "codeVoice"
                },
                {
                  "text": " property is set to ",
                  "type": "text"
                },
                {
                  "code": "both",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "y",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "verticalAlignment",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "values": [
                [
                  {
                    "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:news_format:SupportedUnits",
                    "text": "SupportedUnits"
                  }
                ],
                [
                  {
                    "kind": "text",
                    "text": "number"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The width of the image as it’s repeated. When you omit ",
                  "type": "text"
                },
                {
                  "code": "width",
                  "type": "codeVoice"
                },
                {
                  "text": ", Apple News uses the ",
                  "type": "text"
                },
                {
                  "code": "height",
                  "type": "codeVoice"
                },
                {
                  "text": " property to determine the size based on the aspect ratio of the provided image.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "width",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "("
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:SupportedUnits",
              "text": "SupportedUnits"
            },
            {
              "kind": "text",
              "text": " | number)"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        }
