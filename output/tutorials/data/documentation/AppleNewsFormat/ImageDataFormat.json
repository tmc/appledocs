          "content": [
            {
              "inlineContent": [
                {
                  "text": "The minimum width of an image in a cell as as a number in points or as one of the available units for components.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Default value: ",
                  "type": "text"
                },
                {
                  "code": "20cw",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "20%",
                  "type": "codeVoice"
                },
                {
                  "text": " of the component’s width. See  ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNews/specifying-measurements-for-components",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "minimumWidth",
