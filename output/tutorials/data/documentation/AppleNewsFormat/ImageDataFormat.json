    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When you specify ",
              "type": "text"
            },
            {
              "code": "image",
              "type": "codeVoice"
            },
            {
              "text": " as the data type for an item in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/DataDescriptor",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", you can also indicate a minimum and maximum height and width for the image in the table cell.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This object can be used in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/DataDescriptor",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "\"components\": [",
            " {",
            "      \"role\": \"datatable\",",
            "      \"data\": {",
            "        \"descriptors\": [",
            "          {",
            "            \"identifier\": \"id-image\",",
            "            \"dataType\": \"image\",",
            "            \"format\": {",
            "              \"type\": \"image\",",
            "              \"maximumHeight\": \"50pt\"",
            "            },",
            "            \"key\": \"image\",",
            "            \"label\": \"none\"",
            "          }",
            "        ],",
            "…]"
          ],
          "syntax": "json",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
