    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "Place",
              "type": "codeVoice"
            },
            {
              "text": " component to display a map with a single point of interest. The map automatically centers the point of interest based on the ",
              "type": "text"
            },
            {
              "code": "latitude",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "longitude",
              "type": "codeVoice"
            },
            {
              "text": " locations you provide. If you want to display multiple pins on a map, use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Map",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " component.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"components\": [",
            "    {",
            "      \"role\": \"title\",",
            "      \"text\": \"Maps\"",
            "    },",
            "    {",
            "      \"role\": \"body\",",
            "      \"text\": \"With turn-by-turn spoken directions, interactive 3D views, proactive suggestions, lane guidance, and more, Maps gets you where you need to go.\"",
            "    },",
            "    {",
            "      \"role\": \"heading2\",",
            "      \"text\": \"Place Heading\"",
            "    },",
            "    {",
            "      \"role\": \"place\",",
            "      \"mapType\": \"hybrid\",",
            "      \"latitude\": 35.065908,",
            "      \"longitude\": -109.781623",
            "    }",
            "  ]",
            "}"
          ],
