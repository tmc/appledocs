          "content": [
            {
              "inlineContent": [
                {
                  "text": "Defines the styling for the table header columns, which are present if ",
                  "type": "text"
                },
                {
                  "code": "dataOrientation",
                  "type": "codeVoice"
                },
                {
                  "text": " is  ",
                  "type": "text"
                },
                {
                  "code": "vertical",
                  "type": "codeVoice"
                },
                {
                  "text": ", which is the default.",
