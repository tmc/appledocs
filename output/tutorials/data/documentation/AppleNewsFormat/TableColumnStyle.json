      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The background color for the table column.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you omit this property, the background is transparent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The cell background color is highest priority, followed by the column, and finally the row. Apple News applies all three colors, meaning that non-opaque values can cause combined colors. For example, using a red row together with a blue column, both with 50% opacity, creates a purple cell.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "backgroundColor",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Color",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:Color",
              "text": "Color"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of styles you apply to columns that meet specified conditions. You can use this to create a table with alternating column background colors.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "conditional",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ConditionalTableColumnStyle",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:ConditionalTableColumnStyle",
              "text": "ConditionalTableColumnStyle"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The stroke style for the divider lines between columns.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "divider",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TableStrokeStyle",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:TableStrokeStyle",
              "text": "TableStrokeStyle"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "values": [
                [
                  {
                    "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:news_format:SupportedUnits",
                    "text": "SupportedUnits"
                  }
                ],
                [
                  {
                    "kind": "text",
                    "text": "number"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The minimum width of the columns, as a number in points or using the available units of measure for components. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNews/specifying-measurements-for-components",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "minimumWidth",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "("
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/SupportedUnits",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:SupportedUnits",
              "text": "SupportedUnits"
            },
            {
              "kind": "text",
              "text": " | number)"
            }
          ],
          "typeDetails": [
            {
              "baseType": "number"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The relative column width. This value influences the distribution of column width but doesn’t dictate any exact values. To set an exact minimum width, use ",
                  "type": "text"
                },
                {
                  "code": "minimumWidth",
                  "type": "codeVoice"
                },
                {
                  "text": " instead.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "It might be useful to think of the value of ",
                  "type": "text"
                },
                {
                  "code": "width",
                  "type": "codeVoice"
                },
                {
                  "text": " as a percentage of the component’s width. For example, if you want one column’s width to be about half that of the whole component, and another to be about a quarter of the component width, use values of ",
                  "type": "text"
                },
                {
                  "code": "50",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "25",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "width",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can apply styles that affect the look of the columns in your table, including a column background color, the color for divider lines between columns, and the minimum width for a column.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can also set up conditional column styles — styles applied to columns that meet certain criteria. For example, you could apply a background color to all odd-numbered columns. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ConditionalTableColumnStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this object in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TableStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"components\": [",
            "    {",
            "      \"role\": \"datatable\",",
            "      \"style\": \"bookTableStyle\",",
            "      \"showDescriptorLabels\": true,",
            "      \"sortBy\": [",
            "        {",
            "          \"descriptor\": \"id-publication-date\",",
            "          \"direction\": \"ascending\"",
            "        }",
            "      ],",
            "      \"data\": {",
            "        \"descriptors\": [",
            "          {",
            "            \"identifier\": \"id-publication-date\",",
            "            \"key\": \"publicationDate\",",
            "            \"label\": \"Date\",",
            "            \"dataType\": \"string\"",
            "          },",
            "          {",
            "            \"identifier\": \"id-title\",",
            "            \"key\": \"title\",",
            "            \"label\": \"Title\",",
            "            \"dataType\": \"string\"",
            "          },",
            "          {",
            "            \"identifier\": \"id-publisher\",",
            "            \"key\": \"publisher\",",
            "            \"label\": \"Publisher\",",
            "            \"dataType\": \"string\"",
            "          }",
            "        ],",
            "        \"records\": [",
            "          {",
            "            \"title\": \"Mardi\",",
            "            \"publicationDate\": \"1849\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          },",
            "          {",
            "            \"title\": \"Typee\",",
            "            \"publicationDate\": \"1846\",",
            "            \"publisher\": \"Wiley and Putnam\"",
            "          },",
            "          {",
            "            \"title\": \"White-Jacket\",",
            "            \"publicationDate\": \"1850\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          },",
            "          {",
            "            \"title\": \"Omoo\",",
            "            \"publicationDate\": \"1847\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          },",
            "          {",
            "            \"title\": \"Redburn\",",
            "            \"publicationDate\": \"1849\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          },",
            "          {",
            "            \"title\": \"Moby-Dick\",",
            "            \"publicationDate\": \"1851\",",
            "            \"publisher\": \"Harper & Brothers\"",
            "          }",
            "        ]",
            "      }",
            "    }",
            "  ],",
            "  \"componentStyles\": {",
            "    \"bookTableStyle\": {",
            "      \"tableStyle\": {",
            "        \"columns\": {",
            "          \"backgroundColor\": \"#eeeeee\"",
            "        },",
            "        \"headerCells\": {",
            "          \"padding\": 5,",
            "          \"textStyle\": {",
            "            \"fontWeight\": \"bold\"",
            "          }",
            "        },",
            "        \"cells\": {",
            "          \"padding\": 2",
            "        }",
            "      }",
            "    }",
            "  }",
            "}"
          ],
          "syntax": "json",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
