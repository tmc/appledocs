  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: Apple News Format"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "object "
            },
            {
              "kind": "identifier",
              "text": "Metadata"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The authors of this article. The value may or may not be the same string provided in the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Author",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Byline",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " component.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Note the following:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Include an author name or an array of author names formatted in title case. For example, Maria Ruiz.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Provide author names for your articles instead of team names. Only provide a team name, such as ",
                          "type": "text"
                        },
                        {
                          "code": "News",
                          "type": "codeVoice"
                        },
                        {
                          "text": " ",
                          "type": "text"
                        },
                        {
                          "code": "Staff",
                          "type": "codeVoice"
                        },
                        {
                          "text": ", for special cases.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Don’t include a website link, email ID, or Twitter handles, or use additional words such as ",
                          "type": "text"
                        },
                        {
                          "inlineContent": [
                            {
                              "text": "By",
                              "type": "text"
                            }
                          ],
                          "type": "emphasis"
                        },
                        {
                          "text": ", ",
                          "type": "text"
                        },
                        {
                          "inlineContent": [
                            {
                              "text": "and",
                              "type": "text"
                            }
                          ],
                          "type": "emphasis"
                        },
                        {
                          "text": ", ",
                          "type": "text"
                        },
                        {
                          "inlineContent": [
                            {
                              "text": "As told to",
                              "type": "text"
                            }
                          ],
                          "type": "emphasis"
                        },
                        {
                          "text": ", or ",
                          "type": "text"
                        },
                        {
                          "inlineContent": [
                            {
                              "text": "Illustrated by",
                              "type": "text"
                            }
                          ],
                          "type": "emphasis"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "Note that the ",
                  "type": "text"
                },
                {
                  "code": "byline",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "author",
                  "type": "codeVoice"
                },
                {
                  "text": " component in the article body don’t have these same requirements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This value appears in the article tile in channel feeds and section feeds.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "authors",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A set of key-value pairs, where the value is an array of at least one item that you can leverage to target your advertising campaigns to specific articles or groups of articles. See ",
                  "type": "text"
                },
                {
                  "identifier": "https://support.apple.com/guide/adguide/set-up-targeting-apd8725b40b2/1.0/icloud/1.0",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "identifier": "https://developer.apple.com/library/content/documentation/General/Conceptual/News_Advertising/index.html#//apple_ref/doc/uid/TP40016664-CH1-SW1",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "campaignData",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Metadata/campaignData-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:Metadata.campaignData",
              "text": "Metadata.campaignData"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The canonical URL of a web version of this article. If this Apple  News Format document corresponds to a web version of this article, set this property to the URL of the web article. You can use this property to point to one version of the article as well as to redirect devices that don’t support News content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you omit ",
                  "type": "text"
                },
                {
                  "code": "canonicalURL",
                  "type": "codeVoice"
                },
                {
                  "text": ", devices that don’t support News can’t display the article.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "canonicalURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "uri"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "AI"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional string value indicating the usage of artificial intelligence (AI) tools to generate content for this article.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Use the ",
                  "type": "text"
                },
                {
                  "code": "AI",
                  "type": "codeVoice"
                },
                {
                  "text": " value to indicate that the content is AI-generated.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contentGenerationType",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UTC date in ISO 8601 format (",
                  "type": "text"
                },
                {
                  "code": "YYYY-MM-DDTHH:mm:ss±ZZ:ZZ",
                  "type": "codeVoice"
                },
                {
                  "text": ") on which this article was created. This value may or may not be the same as ",
                  "type": "text"
                },
                {
                  "code": "datePublished",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dateCreated",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UTC date in ISO 8601 format (",
                  "type": "text"
                },
                {
                  "code": "YYYY-MM-DDTHH:mm:ss±ZZ:ZZ",
                  "type": "codeVoice"
                },
                {
                  "text": ") on which this article was last modified after it was published.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Apple News Format uses this date instead of ",
                  "type": "text"
                },
                {
                  "code": "datePublished",
                  "type": "codeVoice"
                },
                {
                  "text": " in the article tile if it’s later than ",
                  "type": "text"
                },
                {
                  "code": "datePublished",
                  "type": "codeVoice"
                },
                {
                  "text": " by less than 48 hours. ",
                  "type": "text"
                },
                {
                  "code": "dateModified",
                  "type": "codeVoice"
                },
                {
                  "text": " doesn’t affect the feed order. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNews/changing-the-appearance-of-your-article-tile-in-feeds",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dateModified",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The UTC date in ISO 8601 format (",
                  "type": "text"
                },
                {
                  "code": "YYYY-MM-DDTHH:mm:ss±ZZ:ZZ",
                  "type": "codeVoice"
                },
                {
                  "text": ") on which this article was first published. Apple News Format uses this date in the feed. Include this date when posting older content to make sure the articles don’t appear at the top of your feed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "datePublished",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A summary of your article. It can also be a subheadline or a quote.  Keep your excerpt within the recommended 80–300 character range.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This text may appear in the article tile in feeds. It can also appear when someone shares an article.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNews/changing-the-appearance-of-your-article-tile-in-feeds",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Don’t use HTML tags or Markdown syntax for this property.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Although this property is optional, it’s best to define it in all of your Apple News Format documents.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "excerpt",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier for the generator you use to create or provide this JSON document.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "generatorIdentifier",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the generator or system that you use to create the JSON document.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "generatorName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The version “number,” as a string, of the generator you use to create the JSON document.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "generatorVersion",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The object for defining information about an issue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "issue",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Issue",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:Issue",
              "text": "Issue"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The keywords that describe this article. You can define up to 50 keywords.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "keywords",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of links to other articles in Apple News.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "links",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/LinkedArticle",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:LinkedArticle",
              "text": "LinkedArticle"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL of an image that can represent this article in a News feed view (channel, topic, or For You). For best results, provide a high-resolution image. Apple News automatically scales the image down to the correct size.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Supported image types are JPEG, (",
                  "type": "text"
                },
                {
                  "code": ".jpeg",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": ".jpg",
                  "type": "codeVoice"
                },
                {
                  "text": ") WebP, GIF, or PNG.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Apple News converts WebP and GIF images you provide as ",
                  "type": "text"
                },
                {
                  "code": "thumbnailURL",
                  "type": "codeVoice"
                },
                {
                  "text": " to JPEG for use as an article thumbnail. Note that the system removes animations from WebP and GIF images.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The minimum size of the image must be 300 px wide x 300 px high.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The aspect ratio (width ÷ height) must be between 0.5 and 3.0.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "To improve the loading time of the article, use one of the images in the article as the thumbnail image. If you use the same images in both places and the image appears on the first screen of the article, the image moves with an animated effect from the feed to the article. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNews/preparing-image-video-audio-music-and-arkit-assets",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "thumbnailURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL for the video that represents this article. A glyph appears on the thumbnail of the article tile, allowing the video to be playable from For You, topic, and channel feeds.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Use the same URL for the  ",
                  "type": "text"
                },
                {
                  "code": "videoURL",
                  "type": "codeVoice"
                },
                {
                  "text": " that you use for one of the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Video",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " components in the article. For the best results or continuous playback for an opened article with a ",
                  "type": "text"
                },
                {
                  "code": "videoURL",
                  "type": "codeVoice"
                },
                {
                  "text": ", make sure that the ",
                  "type": "text"
                },
                {
                  "code": "thumbnailURL",
                  "type": "codeVoice"
                },
                {
                  "text": " property in metadata uses the same image file as the video component’s ",
                  "type": "text"
                },
                {
                  "code": "stillURL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Video URL must begin with ",
                  "type": "text"
                },
                {
                  "code": "http://",
                  "type": "codeVoice"
                },
                {
                  "text": " or preferably ",
                  "type": "text"
                },
                {
                  "code": "https://",
                  "type": "codeVoice"
                },
                {
                  "text": ". The video must be in one of the supported HTTP Live Streaming (HLS) formats. Streaming using the M3U playlist format is highly recommended. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNews/preparing-image-video-audio-music-and-arkit-assets",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "For more information about HLS, see ",
                  "type": "text"
                },
                {
                  "identifier": "https://developer.apple.com/streaming/",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "videoURL",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "uri"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "kind": "mentions",
      "mentions": [
        "doc://com.apple.applenews/documentation/AppleNews/changing-the-appearance-of-your-article-tile-in-feeds",
        "doc://com.apple.applenews/documentation/AppleNews/preparing-image-video-audio-music-and-arkit-assets",
        "doc://com.apple.applenews/documentation/AppleNews/apple-news-api-release-notes-for-ios-17-ipados-17-and-macos-14",
        "doc://com.apple.applenews/documentation/AppleNews/creating-an-article-main-steps",
        "doc://com.apple.applenews/documentation/AppleNews/apple-news-format-release-notes-for-ios-17-ipados-17-and-macos-14"
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For best results, include as many properties as you can even though they aren’t required.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"metadata\": {",
