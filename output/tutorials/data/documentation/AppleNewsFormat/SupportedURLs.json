  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "data"
          ],
          "platforms": [
            "Unsupported OS: Apple News Format"
          ],
          "tokens": [
            {
              "kind": "text",
              "text": "string "
            },
            {
              "kind": "identifier",
              "text": "SupportedURLs"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can define the following Apple domain links for the ",
              "type": "text"
            },
            {
              "code": "URL",
              "type": "codeVoice"
            },
            {
              "text": " property in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " components:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to an Apple News article. Begin each link with ",
                      "type": "text"
                    },
                    {
                      "code": "https://apple.news/",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". To link within the document, use an anchor link starting with ",
                      "type": "text"
                    },
                    {
                      "code": "#",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", followed by the component identifier to which you want to link. To link to a component in another article, you must include an Apple News URL with the ",
                      "type": "text"
                    },
                    {
                      "code": "#",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and component identifier after it; for example, ",
                      "type": "text"
                    },
                    {
                      "code": "https://apple.news/A5vHgPPmQSvuIxPjeXLTdGQ#TextComponent-1",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A canonical link element associated with an Apple News article by its ",
                      "type": "text"
                    },
                    {
                      "code": "canonicalURL",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Metadata",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to the iTunes Store, the App Store, Apple Books, Apple Music, or Apple Podcasts you create using a ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://tools.applemediaservices.com/app-store",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to a ticker symbol in the Stocks app, using the URL format ",
                      "type": "text"
                    },
                    {
                      "code": "stocks://?symbol=",
                      "type": "codeVoice"
                    },
                    {
                      "inlineContent": [
                        {
                          "text": "ticker symbol",
                          "type": "text"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "text": "; for example, ",
                      "type": "text"
                    },
                    {
                      "code": "stocks://?symbol=AAPL",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to Apple Music with the format ",
                      "type": "text"
                    },
                    {
                      "code": "music://",
                      "type": "codeVoice"
                    },
                    {
                      "text": " or ",
                      "type": "text"
                    },
                    {
                      "code": "musics://",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "You can define the following external links for the ",
              "type": "text"
            },
            {
              "code": "URL",
              "type": "codeVoice"
            },
            {
              "text": " property in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " components:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A universal link. See ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://developer.apple.com/ios/universal-links/",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to an external site.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to an email address that begins with ",
                      "type": "text"
                    },
                    {
                      "code": "mailto:",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to a hosted calendar that begins with ",
                      "type": "text"
                    },
                    {
                      "code": "webcal://</value>",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
