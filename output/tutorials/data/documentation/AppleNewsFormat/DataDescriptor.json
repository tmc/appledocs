                        {
                          "text": " data. ",
                          "type": "text"
                        },
                        {
                          "code": "text",
                          "type": "codeVoice"
                        },
                        {
                          "text": " supports data values that are provided as strings or as ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/FormattedText",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": ".",
                          "type": "text"
