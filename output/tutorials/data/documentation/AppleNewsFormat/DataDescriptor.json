                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "image",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Image URLs, starting with ",
                          "type": "text"
                        },
                        {
                          "code": "bundle://",
                          "type": "codeVoice"
                        },
                        {
                          "text": " for an image provided in the same location as ",
                          "type": "text"
                        },
                        {
                          "code": "article.json",
                          "type": "codeVoice"
                        },
                        {
                          "text": ", or starting with ",
                          "type": "text"
                        },
                        {
                          "code": "http://",
                          "type": "codeVoice"
                        },
                        {
                          "text": " or ",
                          "type": "text"
                        },
                        {
                          "code": "https://",
                          "type": "codeVoice"
                        },
                        {
                          "text": " for remote images. The images are displayed in the table. See ",
                          "type": "text"
                        },
