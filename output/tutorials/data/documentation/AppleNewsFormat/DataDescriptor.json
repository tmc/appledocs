                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "text",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". Long text values, as long as a typical sentence or longer. Column widths are calculated differently for ",
                          "type": "text"
                        },
                        {
                          "code": "string",
                          "type": "codeVoice"
                        },
                        {
                          "text": " and ",
