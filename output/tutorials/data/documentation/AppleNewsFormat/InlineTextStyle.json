    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length (in characters) of the portion of text to which Apple News applies alternative styling.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rangeLength",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The starting point of the text to which Apple News applies alternative styling. Note: the first available character is at ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ", not ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rangeStart",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "values": [
                [
                  {
                    "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TextStyle",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:news_format:TextStyle",
                    "text": "TextStyle"
                  }
                ],
                [
                  {
                    "kind": "text",
                    "text": "string"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Either a text style object or the name of a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TextStyle",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object defined in the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ArticleDocument/textStyles-data.dictionary",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "textStyle",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "("
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TextStyle",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:news_format:TextStyle",
              "text": "TextStyle"
            },
            {
              "kind": "text",
              "text": " | string)"
            }
          ],
          "typeDetails": [
            {
              "baseType": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "kind": "mentions",
      "mentions": [
        "doc://com.apple.applenews/documentation/AppleNews/enhancing-your-articles-with-styles",
        "doc://com.apple.applenews/documentation/AppleNews/defining-and-applying-text-styles"
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use an ",
              "type": "text"
            },
            {
              "code": "InlineTextStyle",
              "type": "codeVoice"
            },
            {
              "text": " object to apply text stylings (such as color, underline, font size, and font weight) to a specific range of text. The ",
              "type": "text"
            },
            {
              "code": "InlineTextStyle",
              "type": "codeVoice"
            },
            {
              "text": " object contains either a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TextStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object or a reference to a text style that’s been defined in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ArticleDocument/textStyles-data.dictionary",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object. Only properties that have values override the component text style and defaults.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this object in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"components\": [",
            "    {",
            "      \"role\": \"pullquote\",",
            "      \"text\": \"The text of the pullquote.\",",
            "      \"textStyle\": \"pullquote-medium\",",
            "      \"inlineTextStyles\": [",
            "        {",
            "          \"rangeStart\": 4,",
            "          \"rangeLength\": 4,",
            "          \"textStyle\": {",
            "            \"textColor\": \"#FF0000\",",
            "            \"backgroundColor\": \"#000\"",
            "          }",
            "        }",
            "      ]",
            "    }",
            "  ]",
            "}"
          ],
          "syntax": "json",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
