      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "ComponentStyle",
              "type": "codeVoice"
            },
            {
              "text": " object to define the visual appearance of a component, including its background color, fill, opacity, borders, and table style.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this object in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Component",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ArticleDocument/componentStyles-data.dictionary",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"components\": [",
            "    {",
            "      \"role\": \"container\",",
            "      \"style\": \"exampleComponentStyle\",",
            "      \"components\": [",
            "        {",
            "          \"role\": \"title\",",
            "          \"text\": \"Drought\"",
            "        }",
            "      ]",
            "    }",
            "  ],",
            "  \"componentStyles\": {",
            "    \"exampleComponentStyle\": {",
            "      \"backgroundColor\": \"#FFFFFF\",",
            "      \"opacity\": 1,",
            "      \"border\": {",
            "        \"all\": {",
            "          \"width\": 1,",
            "          \"color\": \"#333\"",
            "        },",
            "        \"left\": false,",
            "        \"right\": false",
            "      }",
            "    }",
            "  }",
            "}"
          ],
          "syntax": "json",
          "type": "codeListing"
        }
