    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "ListItemStyle",
              "type": "codeVoice"
            },
            {
              "text": " object to define text formatting for a bulleted (unordered) or numbered (ordered) list. Put ",
              "type": "text"
            },
            {
              "code": "listItemStyle",
              "type": "codeVoice"
            },
            {
              "text": " inside a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TextStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ComponentTextStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object and set the text component’s ",
              "type": "text"
            },
            {
              "code": "format",
              "type": "codeVoice"
            },
            {
              "text": " property to ",
              "type": "text"
            },
            {
              "code": "html",
              "type": "codeVoice"
            },
            {
              "text": ". See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNews/using-html-with-apple-news-format",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this object in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/TextStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/ComponentTextStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
