        }
      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "bullet",
                "decimal",
                "lower_roman",
                "upper_roman",
                "lower_alphabetical",
                "upper_alphabetical",
                "character",
                "none"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of list item indicator to use.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Allowed options are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "bullet",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Prepends a bullet before each list item. This is the default for unordered lists.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "decimal",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Prepends each list item with an incremental number. This is the default for ordered lists.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "lower_alphabetical",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Prepends list items with a lowercase letter.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "upper_alphabetical",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Prepends list items with an uppercase letter.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "lower_roman",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Prepends list items with a lowercase Roman numeral.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "upper_roman",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Prepends list items with an uppercase Roman numeral.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "character",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Prepends list items with a custom character. Use the ",
                          "type": "text"
                        },
                        {
                          "code": "character",
                          "type": "codeVoice"
                        },
                        {
                          "text": " property to specify a character.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "none",
                          "type": "codeVoice"
                        },
                        {
                          "text": ": Doesn’t prepend any indicator for list items. Indenting is still in effect.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            }
          ],
          "name": "type",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "maximumLength",
              "value": "1"
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "type",
                  "type": "codeVoice"
                },
                {
                  "text": " is set to ",
                  "type": "text"
                },
                {
                  "code": "character",
                  "type": "codeVoice"
                },
                {
                  "text": ", provide the character to use as the list item indicator. Only a single character is supported.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "character",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
