    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can define the following Apple domain links for the ",
              "type": "text"
            },
            {
              "code": "URL",
              "type": "codeVoice"
            },
            {
              "text": " property in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Aside",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Chapter",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Container",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Logo",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Image",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Section",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " objects:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to an Apple News article. Begin each link with ",
                      "type": "text"
                    },
                    {
                      "code": "https://apple.news/",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". To link within the document, use an anchor link starting with ",
                      "type": "text"
                    },
                    {
                      "code": "#",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", followed by the component identifier to which you want to link. To link to a component in another article, you must include an Apple News URL with the ",
                      "type": "text"
                    },
                    {
                      "code": "#",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and component identifier after it; for example, ",
                      "type": "text"
                    },
                    {
                      "code": "https://apple.news/A5vHgPPmQSvuIxPjeXLTdGQ#TextComponent-1",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A canonical link element associated with an Apple News article by its ",
                      "type": "text"
                    },
                    {
                      "code": "canonicalURL",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Metadata",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to the iTunes Store, the App Store, Apple Books, Apple Music, or Apple Podcasts you create using a ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://tools.applemediaservices.com/app-store",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to a ticker symbol in the Stocks app, using the URL format ",
                      "type": "text"
                    },
                    {
                      "code": "stocks://?symbol=",
                      "type": "codeVoice"
                    },
                    {
                      "inlineContent": [
                        {
                          "text": "ticker symbol",
                          "type": "text"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "text": "; ",
                      "type": "text"
                    },
                    {
                      "code": "f",
                      "type": "codeVoice"
                    },
                    {
                      "text": "or example, ",
                      "type": "text"
                    },
                    {
                      "code": "stocks://?symbol=AAPL",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A link to a hosted calendar, using ",
                      "type": "text"
                    },
                    {
                      "code": "webcal://</value>",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
