      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "Gallery",
              "type": "codeVoice"
            },
            {
              "text": " component for sequences of images where the ordering of the items is important to the story. To display images in no specific order, use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNewsFormat/Mosaic",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " component. People can swipe to view the images in a gallery as shown in this example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "media-3591432",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Example",
          "level": 3,
          "text": "Example",
          "type": "heading"
        },
        {
          "code": [
            "{",
            "  \"components\": [",
            "    {",
            "      \"role\": \"gallery\",",
            "      \"items\": [",
            "        {",
            "          \"URL\": \"bundle://gallery-01.jpg\",",
            "          \"caption\": \"Thanks to the record drought, mountain lions have begun to descend from the peaks, sometimes into urban settings.\"",
            "        },",
            "        {",
            "          \"URL\": \"bundle://gallery-02.jpg\",",
            "          \"caption\": \"Coyotes are also seen in cities more often.\"",
            "        },",
            "        {",
            "          \"URL\": \"bundle://gallery-03.jpg\",",
            "          \"explicitContent\": true",
            "        }",
            "      ]",
            "    }",
            "  ]",
            "}"
          ],
          "syntax": "json",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
