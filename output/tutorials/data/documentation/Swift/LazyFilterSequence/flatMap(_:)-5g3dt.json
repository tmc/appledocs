        {
          "inlineContent": [
            {
              "text": "Use this method to receive a single-level sequence when your",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "transformation produces a sequence or collection for each element.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Calling ",
              "type": "text"
            },
            {
              "code": "flatMap(_:)",
              "type": "codeVoice"
            },
            {
              "text": " on a sequence ",
              "type": "text"
            },
            {
              "code": "s",
              "type": "codeVoice"
            },
            {
              "text": " is equivalent to calling",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "s.map(transform).joined()",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
