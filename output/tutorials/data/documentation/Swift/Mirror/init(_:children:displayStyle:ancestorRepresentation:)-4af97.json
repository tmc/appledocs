    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The instance to represent in the new mirror.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "subject"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The structure to use for the mirror. The collection",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "traversal modeled by ",
                  "type": "text"
                },
                {
                  "code": "children",
                  "type": "codeVoice"
                },
                {
                  "text": " is captured so that the resulting",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "mirror’s children may be upgraded to a bidirectional or random",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "access collection later. See the ",
                  "type": "text"
                },
                {
                  "code": "children",
                  "type": "codeVoice"
                },
                {
                  "text": " property for details.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "children"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The preferred display style for the mirror when",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "presented in the debugger or in a playground. The default is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "displayStyle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The means of generating the subject’s",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "ancestor representation. ",
                  "type": "text"
                },
                {
                  "code": "ancestorRepresentation",
                  "type": "codeVoice"
                },
                {
                  "text": " is ignored if",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "subject",
                  "type": "codeVoice"
                },
                {
                  "text": " is not a class instance. The default is ",
                  "type": "text"
                },
                {
                  "code": ".generated",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ancestorRepresentation"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You use this initializer from within your type’s ",
              "type": "text"
            },
            {
              "code": "customMirror",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "implementation to create a customized mirror.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If ",
              "type": "text"
            },
            {
              "code": "subject",
              "type": "codeVoice"
            },
            {
              "text": " is a class instance, ",
              "type": "text"
            },
            {
              "code": "ancestorRepresentation",
              "type": "codeVoice"
            },
            {
              "text": " determines",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "whether ancestor classes will be represented and whether their",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "customMirror",
              "type": "codeVoice"
            },
            {
              "text": " implementations will be used. By default, the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "customMirror",
              "type": "codeVoice"
            },
            {
              "text": " implementation of any ancestors is ignored. To prevent",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "bypassing customized ancestors, pass",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": ".customized({ super.customMirror })",
              "type": "codeVoice"
            },
            {
              "text": " as the ",
              "type": "text"
            },
            {
              "code": "ancestorRepresentation",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "parameter when implementing your type’s ",
              "type": "text"
            },
            {
              "code": "customMirror",
              "type": "codeVoice"
            },
            {
              "text": " property.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
