    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the dynamic type of ",
              "type": "text"
            },
            {
              "code": "subject",
              "type": "codeVoice"
            },
            {
              "text": " conforms to ",
              "type": "text"
            },
            {
              "code": "CustomReflectable",
              "type": "codeVoice"
            },
            {
              "text": ", the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "resulting mirror is determined by its ",
              "type": "text"
            },
            {
              "code": "customMirror",
              "type": "codeVoice"
            },
            {
              "text": " property.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Otherwise, the result is generated by the language.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If the dynamic type of ",
              "type": "text"
            },
            {
              "code": "subject",
              "type": "codeVoice"
            },
            {
              "text": " has value semantics, subsequent",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "mutations of ",
              "type": "text"
            },
            {
              "code": "subject",
              "type": "codeVoice"
            },
            {
              "text": " will not observable in ",
              "type": "text"
            },
            {
              "code": "Mirror",
              "type": "codeVoice"
            },
            {
              "text": ".  In general,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "though, the observability of mutations is unspecified.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
