              "inlineContent": [
                {
                  "text": "A range of integers. The bounds of the range must be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "valid indices of the array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
