    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example uses this initializer to create a new ",
              "type": "text"
            },
            {
              "code": "Double",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "instance. ",
              "type": "text"
            },
            {
              "code": "Double",
              "type": "codeVoice"
            },
            {
              "text": " is a binary floating-point type that has a radix of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let x = Double(sign: .plus, exponent: -2, significand: 1.5)",
            "// x == 0.375"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "This initializer is equivalent to the following calculation, where ",
              "type": "text"
            },
            {
              "code": "**",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is exponentiation, computed as if by a single, correctly rounded,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "floating-point operation:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let sign: FloatingPointSign = .plus",
            "let exponent = -2",
            "let significand = 1.5",
            "let y = (sign == .minus ? -1 : 1) * significand * Double.radix ** exponent",
            "// y == 0.375"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "As with any basic operation, if this value is outside the representable",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "range of the type, overflow or underflow occurs, and zero, a subnormal",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value, or infinity may result. In addition, there are two other edge",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "cases:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the value you pass to ",
                      "type": "text"
                    },
                    {
                      "code": "significand",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is zero or infinite, the result",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "is zero or infinite, regardless of the value of ",
                      "type": "text"
                    },
                    {
                      "code": "exponent",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the value you pass to ",
                      "type": "text"
                    },
                    {
                      "code": "significand",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is NaN, the result is NaN.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "For any floating-point value ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": " of type ",
              "type": "text"
            },
            {
              "code": "F",
              "type": "codeVoice"
            },
            {
              "text": ", the result of the following",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is equal to ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": ", with the distinction that the result is canonicalized",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "if ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": " is in a noncanonical encoding:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let x0 = F(sign: x.sign, exponent: x.exponent, significand: x.significand)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "This initializer implements the ",
              "type": "text"
            },
