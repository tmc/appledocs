              "inlineContent": [
                {
                  "text": "The index at which to start the resulting subsequence.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "start",
                  "type": "codeVoice"
                },
