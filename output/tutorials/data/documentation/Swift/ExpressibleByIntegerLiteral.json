    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The standard library integer and floating-point types, such as ",
              "type": "text"
            },
            {
              "code": "Int",
              "type": "codeVoice"
            },
            {
              "text": " and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Double",
              "type": "codeVoice"
            },
            {
              "text": ", conform to the ",
              "type": "text"
            },
            {
              "code": "ExpressibleByIntegerLiteral",
              "type": "codeVoice"
            },
            {
              "text": " protocol. You can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "initialize a variable or constant of any of these types by assigning an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "integer literal.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Type inferred as 'Int'",
            "let cookieCount = 12",
            "",
            "// An array of 'Int'",
            "let chipsPerCookie = [21, 22, 25, 23, 24, 19]",
            "",
            "// A floating-point value initialized using an integer literal",
            "let redPercentage: Double = 1",
            "// redPercentage == 1.0"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "anchor": "Conforming-to-ExpressibleByIntegerLiteral",
          "level": 1,
          "text": "Conforming to ExpressibleByIntegerLiteral",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To add ",
              "type": "text"
            },
            {
              "code": "ExpressibleByIntegerLiteral",
              "type": "codeVoice"
            },
            {
              "text": " conformance to your custom type,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "implement the required initializer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
