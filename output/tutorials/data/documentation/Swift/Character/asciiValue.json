    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "code": [
            "let chars: [Character] = [\"a\", \" \", \"™\"]",
            "for ch in chars {",
            "    print(ch, \"-->\", ch.asciiValue)",
            "}",
            "// Prints:",
            "// a --> Optional(97)",
            "//   --> Optional(32)",
            "// ™ --> nil"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "A character with the value “\\r\\n” (CR-LF) is normalized to “\\n” (LF) and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "has an ",
              "type": "text"
            },
            {
              "code": "asciiValue",
              "type": "codeVoice"
            },
            {
              "text": " property equal to 10.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let cr = \"\\r\" as Character",
            "// cr.asciiValue == 13",
            "let lf = \"\\n\" as Character",
            "// lf.asciiValue == 10",
            "let crlf = \"\\r\\n\" as Character",
            "// crlf.asciiValue == 10"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
