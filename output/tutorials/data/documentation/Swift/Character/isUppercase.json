      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Uppercase characters vary under case-conversion to lowercase, but not when",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "converted to uppercase. The following characters are all uppercase:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "“É” (U+0045 LATIN CAPITAL LETTER E, U+0301 COMBINING ACUTE ACCENT)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "“И” (U+0418 CYRILLIC CAPITAL LETTER I)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "“Π” (U+03A0 GREEK CAPITAL LETTER PI)",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
