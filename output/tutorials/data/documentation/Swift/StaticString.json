    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Instances of the ",
              "type": "text"
            },
            {
              "code": "StaticString",
              "type": "codeVoice"
            },
            {
              "text": " type are immutable.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "code": "StaticString",
              "type": "codeVoice"
            },
            {
              "text": " provides only low-level access to its contents, unlike",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Swift’s more commonly used ",
              "type": "text"
            },
            {
              "code": "String",
              "type": "codeVoice"
            },
            {
              "text": " type. A static string can use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "either of the following as its storage:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "a pointer to a null-terminated sequence of UTF-8 code units:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "code": [
                    "let emoji: StaticString = \"\\u{1F600}\"",
                    "emoji.hasPointerRepresentation  //-> true",
                    "emoji.isASCII                   //-> false",
                    "emoji.unicodeScalar             //-> Fatal error!",
                    "emoji.utf8CodeUnitCount         //-> 4",
                    "emoji.utf8Start[0]              //-> 0xF0",
                    "emoji.utf8Start[1]              //-> 0x9F",
                    "emoji.utf8Start[2]              //-> 0x98",
                    "emoji.utf8Start[3]              //-> 0x80",
                    "emoji.utf8Start[4]              //-> 0x00"
                  ],
                  "syntax": "swift",
                  "type": "codeListing"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "a single Unicode scalar value, under very limited circumstances:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "code": [
                    "struct MyStaticScalar: ExpressibleByUnicodeScalarLiteral {",
                    "    typealias UnicodeScalarLiteralType = StaticString",
                    "    let value: StaticString",
                    "    init(unicodeScalarLiteral value: StaticString) {",
                    "        self.value = value",
                    "    }",
                    "}",
                    "",
                    "let emoji: StaticString = MyStaticScalar(\"\\u{1F600}\").value",
                    "emoji.hasPointerRepresentation  //-> false",
                    "emoji.isASCII                   //-> false",
                    "emoji.unicodeScalar.value       //-> 0x1F600",
                    "emoji.utf8CodeUnitCount         //-> Fatal error!",
                    "emoji.utf8Start                 //-> Fatal error!"
                  ],
                  "syntax": "swift",
                  "type": "codeListing"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "You can use the ",
              "type": "text"
            },
            {
              "code": "withUTF8Buffer(_:)",
              "type": "codeVoice"
            },
            {
              "text": " method to access a static string’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "contents, regardless of which representation the static string uses.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "emoji.withUTF8Buffer { utf8 in",
            "    utf8.count  //-> 4",
            "    utf8[0]     //-> 0xF0",
            "    utf8[1]     //-> 0x9F",
            "    utf8[2]     //-> 0x98",
            "    utf8[3]     //-> 0x80",
            "    utf8[4]     //-> Fatal error!",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
