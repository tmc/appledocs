    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This is the unit of the least significant digit in this valueâ€™s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "significand. For most numbers ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": ", this is the difference between ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and the next greater (in magnitude) representable number. There are some",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "edge cases to be aware of:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "x",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is not a finite number, then ",
                      "type": "text"
                    },
                    {
                      "code": "x.ulp",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is NaN.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "x",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is very small in magnitude, then ",
                      "type": "text"
                    },
                    {
                      "code": "x.ulp",
                      "type": "codeVoice"
                    },
                    {
                      "text": " may be a subnormal",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "number. If a type does not support subnormals, ",
                      "type": "text"
                    },
                    {
                      "code": "x.ulp",
                      "type": "codeVoice"
                    },
                    {
                      "text": " may be rounded",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "to zero.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "greatestFiniteMagnitude.ulp",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is a finite number, even though the next",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "greater representable value is ",
                      "type": "text"
                    },
                    {
                      "code": "infinity",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "See also the ",
              "type": "text"
            },
            {
              "code": "ulpOfOne",
              "type": "codeVoice"
            },
            {
              "text": " static property.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
