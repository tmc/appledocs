        },
        {
          "inlineContent": [
            {
              "text": "Locale-independent case-insensitive operation, and other needs, can be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "achieved by calling ",
              "type": "text"
            },
            {
              "code": "range(of:options:range:locale:)",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Equivalent to:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "range(of: other, options: .caseInsensitiveSearch,",
            "      locale: Locale.current) != nil"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
