    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "An array containing substrings that have been divided from the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "string using ",
              "type": "text"
            },
            {
              "code": "separator",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The substrings in the resulting array appear in the same order as the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "original string. Adjacent occurrences of the separator string produce",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "empty strings in the result. Similarly, if the string begins or ends",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with the separator, the first or last substring, respectively, is empty.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The following example shows this behavior:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let list1 = \"Karin, Carrie, David\"",
            "let items1 = list1.components(separatedBy: \", \")",
            "// [\"Karin\", \"Carrie\", \"David\"]",
            "",
            "// Beginning with the separator:",
            "let list2 = \", Norman, Stanley, Fletcher\"",
            "let items2 = list2.components(separatedBy: \", \")",
            "// [\"\", \"Norman\", \"Stanley\", \"Fletcher\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If the list has no separators, the array contains only the original",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "string itself.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let name = \"Karin\"",
            "let list = name.components(separatedBy: \", \")",
            "// [\"Karin\"]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
