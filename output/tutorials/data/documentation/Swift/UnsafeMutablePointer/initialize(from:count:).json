          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the values to copy. The memory region",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "source..<(source + count)",
                  "type": "codeVoice"
                },
                {
                  "text": " must be initialized. The memory regions",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "referenced by ",
                  "type": "text"
                },
                {
                  "code": "source",
                  "type": "codeVoice"
                },
                {
                  "text": " and this pointer must not overlap.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of instances to move from ",
                  "type": "text"
                },
                {
                  "code": "source",
                  "type": "codeVoice"
                },
                {
                  "text": " to this",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "pointer’s memory. ",
                  "type": "text"
                },
                {
                  "code": "count",
                  "type": "codeVoice"
                },
                {
                  "text": " must not be negative.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "count"
        }
      ]
    },
    {
