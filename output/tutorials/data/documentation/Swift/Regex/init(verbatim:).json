        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This example creates a regular expression that matches the string",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "\"(adj)\"",
              "type": "codeVoice"
            },
            {
              "text": ", including the parentheses. Although parentheses are regular",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "expression metacharacters, they do not need escaping in the string passed",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as ",
              "type": "text"
            },
            {
              "code": "verbatimString",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let adjectiveDesignator = Regex<Substring>(verbatim: \"(adj.)\")",
            "",
            "print(\"awesome (adj.)\".contains(adjectiveDesignator))",
            "// Prints \"true\"",
            "print(\"apple (n.)\".contains(adjectiveDesignator))",
            "// Prints \"false\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
