    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of element the ",
                  "type": "text"
                },
                {
                  "code": "AsyncStream",
                  "type": "codeVoice"
                },
                {
                  "text": " produces.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "elementType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "Continuation.BufferingPolicy",
                  "type": "codeVoice"
                },
                {
                  "text": " value to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "set the streamâ€™s buffering behavior. By default, the stream buffers an",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "unlimited number of elements. You can also set the policy to buffer a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "specified number of oldest or newest elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "limit"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A custom closure that yields values to the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "AsyncStream",
                  "type": "codeVoice"
                },
                {
                  "text": ". This closure receives an ",
                  "type": "text"
                },
                {
                  "code": "AsyncStream.Continuation",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "instance that it uses to provide elements to the stream and terminate the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "stream when finished.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "build"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "AsyncStream.Continuation",
              "type": "codeVoice"
            },
            {
              "text": " received by the ",
              "type": "text"
            },
            {
              "code": "build",
              "type": "codeVoice"
            },
            {
              "text": " closure is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "appropriate for use in concurrent contexts. It is thread safe to send and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "finish; all calls to the continuation are serialized. However, calling",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "this from multiple concurrent contexts could result in out-of-order",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "delivery.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows an ",
              "type": "text"
            },
            {
              "code": "AsyncStream",
              "type": "codeVoice"
            },
            {
              "text": " created with this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "initializer that produces 100 random numbers on a one-second interval,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "calling ",
              "type": "text"
            },
            {
              "code": "yield(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to deliver each element to the awaiting call point.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "When the ",
              "type": "text"
            },
            {
              "code": "for",
              "type": "codeVoice"
            },
            {
              "text": " loop exits, the stream finishes by calling the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "continuationâ€™s ",
              "type": "text"
            },
            {
              "code": "finish()",
              "type": "codeVoice"
            },
            {
              "text": " method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let stream = AsyncStream<Int>(Int.self,",
            "                              bufferingPolicy: .bufferingNewest(5)) { continuation in",
            "    Task.detached {",
            "        for _ in 0..<100 {",
            "            await Task.sleep(1 * 1_000_000_000)",
            "            continuation.yield(Int.random(in: 1...10))",
            "        }",
            "        continuation.finish()",
            "    }",
            "}",
            "",
            "// Call point:",
            "for await random in stream {",
            "    print(random)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
