      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When ",
              "type": "text"
            },
            {
              "code": "next()",
              "type": "codeVoice"
            },
            {
              "text": " returns ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", this signifies the end of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "AsyncStream",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "It is a programmer error to invoke ",
              "type": "text"
            },
            {
              "code": "next()",
              "type": "codeVoice"
            },
            {
              "text": " from a concurrent",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "context that contends with another such call, which results in a call to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "fatalError()",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If you cancel the task this iterator is running in while ",
              "type": "text"
            },
            {
              "code": "next()",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is awaiting a value, the ",
              "type": "text"
            },
            {
              "code": "AsyncStream",
              "type": "codeVoice"
            },
            {
              "text": " terminates. In this case,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "next()",
              "type": "codeVoice"
            },
            {
              "text": " might return ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " immediately, or return ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "subsequent calls.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
