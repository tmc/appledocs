      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If this value is a finite nonzero number, ",
              "type": "text"
            },
            {
              "code": "significandWidth",
              "type": "codeVoice"
            },
            {
              "text": " is the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "number of fractional bits required to represent the value of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "significand",
              "type": "codeVoice"
            },
            {
              "text": "; otherwise, ",
              "type": "text"
            },
            {
              "code": "significandWidth",
              "type": "codeVoice"
            },
            {
              "text": " is -1. The value of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "significandWidth",
              "type": "codeVoice"
            },
            {
              "text": " is always -1 or between zero and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "significandBitCount",
              "type": "codeVoice"
            },
            {
              "text": ". For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "For any representable power of two, ",
                      "type": "text"
                    },
                    {
                      "code": "significandWidth",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is zero, because",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "code": "significand",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is ",
                      "type": "text"
                    },
                    {
                      "code": "1.0",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "x",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is 10, ",
                      "type": "text"
                    },
                    {
                      "code": "x.significand",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is ",
                      "type": "text"
                    },
                    {
                      "code": "1.01",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in binary, so",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "code": "x.significandWidth",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is 2.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "x",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is Float.pi, ",
                      "type": "text"
                    },
                    {
                      "code": "x.significand",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is ",
                      "type": "text"
                    },
                    {
                      "code": "1.10010010000111111011011",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "binary, and ",
                      "type": "text"
                    },
                    {
                      "code": "x.significandWidth",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is 23.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
