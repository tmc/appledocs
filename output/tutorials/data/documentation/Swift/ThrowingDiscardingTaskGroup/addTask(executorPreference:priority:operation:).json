      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The task executor that the child task should be started on and keep using.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " is passed explicitly, that parent task’s executor preference (if any),",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "will be ignored. In order to inherit the parent task’s executor preference",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "invoke ",
                  "type": "text"
                },
                {
                  "code": "addTask()",
                  "type": "codeVoice"
                },
                {
                  "text": " without passing a value to the ",
                  "type": "text"
                },
                {
                  "code": "taskExecutor",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "and it will be inherited automatically.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "taskExecutor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The priority of the operation task.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Omit this parameter or pass ",
                  "type": "text"
                },
                {
                  "code": ".unspecified",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to set the child task’s priority to the priority of the group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "priority"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The operation to execute as part of the task group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "operation"
        }
      ]
    }
