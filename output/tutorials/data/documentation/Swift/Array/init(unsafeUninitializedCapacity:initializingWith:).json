      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of elements to allocate",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "space for in the new array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "unsafeUninitializedCapacity"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that initializes elements and sets the count",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of the new array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "Parameters:",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    },
                    {
                      "items": [
                        {
                          "content": [
                            {
                              "inlineContent": [
                                {
                                  "text": "buffer: A buffer covering uninitialized memory with room for the",
                                  "type": "text"
                                },
                                {
                                  "text": " ",
                                  "type": "text"
                                },
                                {
                                  "text": "specified number of elements.",
                                  "type": "text"
                                }
                              ],
                              "type": "paragraph"
                            }
                          ]
                        },
                        {
                          "content": [
                            {
                              "inlineContent": [
                                {
                                  "text": "initializedCount: The count of initialized elements in the array,",
                                  "type": "text"
                                },
                                {
                                  "text": " ",
                                  "type": "text"
                                },
                                {
                                  "text": "which begins as zero. Set ",
                                  "type": "text"
                                },
                                {
                                  "code": "initializedCount",
                                  "type": "codeVoice"
                                },
                                {
                                  "text": " to the number of",
                                  "type": "text"
                                },
                                {
                                  "text": " ",
                                  "type": "text"
                                },
                                {
                                  "text": "elements you initialize.",
                                  "type": "text"
                                }
                              ],
                              "type": "paragraph"
                            }
                          ]
                        }
                      ],
                      "type": "unorderedList"
                    }
