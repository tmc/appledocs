    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An iterator of code units to be decoded. ",
                  "type": "text"
                },
                {
                  "code": "input",
                  "type": "codeVoice"
                },
                {
                  "text": " must be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the same iterator instance in repeated calls to this method. Do not",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "advance the iterator or any copies of the iterator outside this",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "method.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "input"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "UnicodeDecodingResult",
              "type": "codeVoice"
            },
            {
              "text": " instance, representing the next",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Unicode scalar, an indication of an error, or an indication that the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "UTF sequence has been fully decoded.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To decode a code unit sequence completely, call this method repeatedly",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "until it returns ",
              "type": "text"
            },
            {
              "code": "UnicodeDecodingResult.emptyInput",
              "type": "codeVoice"
            },
            {
              "text": ". Checking that the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "iterator was exhausted is not sufficient, because the decoder can store",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "buffered data from the input iterator.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Because of buffering, it is impossible to find the corresponding position",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in the iterator for a given returned ",
              "type": "text"
            },
            {
              "code": "Unicode.Scalar",
              "type": "codeVoice"
            },
            {
              "text": " or an error.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example decodes the UTF-8 encoded bytes of a string into an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "array of ",
              "type": "text"
            },
            {
              "code": "Unicode.Scalar",
              "type": "codeVoice"
            },
            {
              "text": " instances:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let str = \"✨Unicode✨\"",
            "print(Array(str.utf8))",
            "// Prints \"[226, 156, 168, 85, 110, 105, 99, 111, 100, 101, 226, 156, 168]\"",
            "",
            "var bytesIterator = str.utf8.makeIterator()",
            "var scalars: [Unicode.Scalar] = []",
            "var utf8Decoder = UTF8()",
            "Decode: while true {",
            "    switch utf8Decoder.decode(&bytesIterator) {",
            "    case .scalarValue(let v): scalars.append(v)",
            "    case .emptyInput: break Decode",
            "    case .error:",
            "        print(\"Decoding error\")",
            "        break Decode",
            "    }",
            "}",
            "print(scalars)",
            "// Prints \"[\"\\u{2728}\", \"U\", \"n\", \"i\", \"c\", \"o\", \"d\", \"e\", \"\\u{2728}\"]\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
