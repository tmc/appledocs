            {
              "text": "The remainder of this value divided by ",
              "type": "text"
            },
            {
              "code": "other",
              "type": "codeVoice"
            },
            {
              "text": " using",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "truncating division.",
              "type": "text"
            }
          ],
