    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Unicode code point to use for the scalar. ",
                  "type": "text"
                },
                {
                  "code": "v",
                  "type": "codeVoice"
                },
                {
                  "text": " must be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "a valid Unicode scalar value, in the ranges ",
                  "type": "text"
                },
                {
                  "code": "0...0xD7FF",
                  "type": "codeVoice"
                },
                {
                  "text": " or",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "0xE000...0x10FFFF",
                  "type": "codeVoice"
                },
                {
                  "text": ". In case of an invalid unicode scalar value, nil is",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "v"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code sample creates a ",
              "type": "text"
            },
            {
              "code": "Unicode.Scalar",
              "type": "codeVoice"
            },
            {
              "text": " instance",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with a value of an emoji character:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let codepoint = 127881",
            "let emoji = Unicode.Scalar(codepoint)!",
            "print(emoji)",
            "// Prints \"ðŸŽ‰\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In case of an invalid input value, nil is returned.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let codepoint: UInt32 = extValue // This might be an invalid value.",
            "if let emoji = Unicode.Scalar(codepoint) {",
            "  print(emoji)",
            "} else {",
            "  // Do something else",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
