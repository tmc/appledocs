    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Scalar values representing characters that are normally unprintable or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that otherwise require escaping are escaped with a backslash.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let tab = Unicode.Scalar(9)!",
            "print(tab)",
            "// Prints \" \"",
            "print(tab.escaped(asASCII: false))",
            "// Prints \"\\t\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When the ",
              "type": "text"
            },
            {
              "code": "forceASCII",
              "type": "codeVoice"
            },
            {
              "text": " parameter is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ", a ",
              "type": "text"
            },
            {
              "code": "Unicode.Scalar",
              "type": "codeVoice"
            },
            {
              "text": " instance",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with a value greater than 127 is represented using an escaped numeric",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value; otherwise, non-ASCII characters are represented using their",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "typical string value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let bap = Unicode.Scalar(48165)!",
            "print(bap.escaped(asASCII: false))",
            "// Prints \"ë°¥\"",
            "print(bap.escaped(asASCII: true))",
            "// Prints \"\\u{BC25}\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
