      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An instance of ",
                  "type": "text"
                },
                {
                  "code": "UnsafeContinuation",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that hasn’t yet been resumed.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "After passing the unsafe continuation to this initializer,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "don’t use it outside of this object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "continuation"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string identifying the declaration that is the notional",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "source for the continuation, used to identify the continuation in",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "runtime diagnostics related to misuse of this continuation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "function"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Instead of calling this initializer,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "most code calls the ",
              "type": "text"
            },
            {
              "code": "withCheckedContinuation(function:_:)",
              "type": "codeVoice"
            },
            {
              "text": " or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "withCheckedThrowingContinuation(function:_:)",
              "type": "codeVoice"
            },
            {
              "text": " function instead.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "You only need to initialize",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "your own ",
              "type": "text"
            },
            {
              "code": "CheckedContinuation<T, E>",
              "type": "codeVoice"
            },
            {
              "text": " if you already have an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "UnsafeContinuation",
              "type": "codeVoice"
            },
            {
              "text": " you want to impose checking on.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
