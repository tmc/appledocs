    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This macro can only be used to define a macro;",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using it in any other context is an error.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The specified type must conform to the protocols",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that correspond to the roles of the macro being declared.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "macro stringify(_ value: T) -> (T, String) =",
            "    #externalMacro(module: \"ExampleMacros\", type: \"StringifyMacro\")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
