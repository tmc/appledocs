        {
          "inlineContent": [
            {
              "text": "If the task is canceled before the time ends, this function throws",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "CancellationError",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This function doesn’t block the underlying thread.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "  try await Task.sleep(until: .now + .seconds(3))"
          ],
