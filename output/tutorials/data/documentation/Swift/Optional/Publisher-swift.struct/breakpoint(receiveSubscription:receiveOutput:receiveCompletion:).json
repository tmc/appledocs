      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that executes when the publisher receives a subscription. Return ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " from this closure to raise ",
                  "type": "text"
                },
                {
                  "code": "SIGTRAP",
                  "type": "codeVoice"
                },
                {
                  "text": ", or false to continue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "receiveSubscription"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that executes when the publisher receives a value. Return ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " from this closure to raise ",
                  "type": "text"
                },
                {
                  "code": "SIGTRAP",
                  "type": "codeVoice"
                },
                {
                  "text": ", or false to continue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "receiveOutput"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that executes when the publisher receives a completion. Return ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " from this closure to raise ",
                  "type": "text"
                },
                {
                  "code": "SIGTRAP",
                  "type": "codeVoice"
                },
                {
                  "text": ", or false to continue.",
                  "type": "text"
                }
              ],
