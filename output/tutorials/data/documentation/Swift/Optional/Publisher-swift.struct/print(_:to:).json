          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string —- which defaults to empty -— with which to prefix all log messages.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "prefix"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A stream for text output that receives messages, and which directs output to the console by default.  A custom stream can be used to log messages to other destinations.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stream"
        }
      ]
    },
