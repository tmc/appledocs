    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In the following example, the publisher for an ",
              "type": "text"
            },
            {
              "code": "Int?",
              "type": "codeVoice"
            },
            {
              "text": " optional publishes its value once, then finishes normally:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let optional1: Int? = 1",
            "    optional1.publisher",
            "        .sink(receiveCompletion: { print(\"optional1 completed.\") },",
            "              receiveValue: { print(\"optional1 = \\($0)\") }",
            "        )",
            "",
            "// Prints:",
            "// optional1 = 1.",
            "// optional1 completed."
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In contrast with the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Combine/Just",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " publisher, which always publishes a single value, this publisher might not send any values and instead finish normally if the optional’s ",
              "type": "text"
            },
            {
              "code": "output",
              "type": "codeVoice"
            },
            {
              "text": " is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ". In the next example, an ",
              "type": "text"
            },
            {
              "code": "Int?",
              "type": "codeVoice"
            },
            {
              "text": " optional that’s ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " immediately sends the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Combine/Subscribers/Completion/finished",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " completion, without producing any values.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let optional2: Int? = nil",
            "optional2.publisher",
            "        .sink(receiveCompletion: { print(\"optional2 completed.\") },",
            "              receiveValue: { print(\"optional2 = \\($0)\") }",
            "        )",
            "",
            "// Prints:",
            "// optional2 completed."
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
