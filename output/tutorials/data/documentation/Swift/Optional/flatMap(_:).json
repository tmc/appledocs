    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "code": "flatMap",
              "type": "codeVoice"
            },
            {
              "text": " method with a closure that returns an optional value.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This example performs an arithmetic operation with an optional result on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "an optional integer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let possibleNumber: Int? = Int(\"42\")",
            "let nonOverflowingSquare = possibleNumber.flatMap { x -> Int? in",
            "    let (result, overflowed) = x.multipliedReportingOverflow(by: x)",
            "    return overflowed ? nil : result",
            "}",
            "print(nonOverflowingSquare)",
            "// Prints \"Optional(1764)\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
