    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In this example, ",
              "type": "text"
            },
            {
              "code": "filter(_:)",
              "type": "codeVoice"
            },
            {
              "text": " is used to include only names shorter than",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "five characters.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let cast = [\"Vivien\", \"Marlon\", \"Kim\", \"Karl\"]",
            "let shortNames = cast.filter { $0.count < 5 }",
            "print(shortNames)",
            "// Prints \"[\"Kim\", \"Karl\"]\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
