        {
          "inlineContent": [
            {
              "text": "Use this method to generate an integer within a specific range. This",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example creates three new values in the range ",
              "type": "text"
            },
            {
              "code": "1...100",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "for _ in 1...3 {",
            "    print(Int.random(in: 1...100))",
            "}",
            "// Prints \"53\"",
            "// Prints \"64\"",
            "// Prints \"5\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
