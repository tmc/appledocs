      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The units that the formatted string may include.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allowedUnits"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The width of the unit and the spacing between the value and the unit.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "width"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of duration units to include in the output string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maximumUnitCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The strategy for handling leading units with zero values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "zeroValueUnits"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The padding or truncating behavior of the unit value, as an ",
                  "type": "text"
                },
                {
                  "code": "Int",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "valueLength"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The strategy for displaying a duration if a formatted string can’t represent it exactly with the allowed units.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fractionalPart"
        }
      ]
    },
