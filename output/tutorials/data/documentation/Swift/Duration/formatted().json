    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A localized formatted string that describes the duration, such as ",
              "type": "text"
            },
            {
              "code": "1:30:56",
              "type": "codeVoice"
            },
            {
              "text": " for a duration of 1 hour, 30 minutes, and 56 seconds in the U.S. English locale. In the Finnish locale, this returns ",
              "type": "text"
            },
            {
              "code": "1.30.56",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows the effect of applying the default formatting to a duration of two seconds:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let duration = Duration.seconds(2)",
            "let formattedDuration = duration.formatted() // \"0:00:02\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "This method uses a default ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/Duration/TimeFormatStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". To modify the formatting, customize a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/Duration/TimeFormatStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/Duration/UnitsFormatStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", then call ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/Duration/formatted(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " on the duration, passing in the style. You can also call ",
              "type": "text"
            },
            {
              "code": "format(_:)",
              "type": "codeVoice"
            },
            {
              "text": " on the style, passing in a duration.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
