    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The starting value to use for the sequence. If the sequence",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "contains any values, the first one is ",
                  "type": "text"
                },
                {
                  "code": "start",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "start"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An end value to limit the sequence. ",
                  "type": "text"
                },
                {
                  "code": "end",
                  "type": "codeVoice"
                },
                {
                  "text": " is never an element of",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the resulting sequence.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "end"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount to step by with each iteration. A positive ",
                  "type": "text"
                },
                {
                  "code": "stride",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "iterates upward; a negative ",
                  "type": "text"
                },
                {
                  "code": "stride",
                  "type": "codeVoice"
                },
                {
                  "text": " iterates downward.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stride"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A sequence from ",
              "type": "text"
            },
            {
              "code": "start",
              "type": "codeVoice"
            },
            {
              "text": " toward, but not including, ",
              "type": "text"
            },
            {
              "code": "end",
              "type": "codeVoice"
            },
            {
              "text": ". Each",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value in the sequence steps by ",
              "type": "text"
            },
            {
              "code": "stride",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this function to stride over values of any type that conforms",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to the ",
              "type": "text"
            },
            {
              "code": "Strideable",
              "type": "codeVoice"
            },
            {
              "text": " protocol, such as integers or floating-point types.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Starting with ",
              "type": "text"
            },
            {
              "code": "start",
              "type": "codeVoice"
            },
            {
              "text": ", each successive value of the sequence adds ",
              "type": "text"
            },
            {
              "code": "stride",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "until the next value would be equal to or beyond ",
              "type": "text"
            },
            {
              "code": "end",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "for radians in stride(from: 0.0, to: .pi * 2, by: .pi / 2) {",
            "    let degrees = Int(radians * 180 / .pi)",
            "    print(\"Degrees: \\(degrees), radians: \\(radians)\")",
            "}",
            "// Degrees: 0, radians: 0.0",
            "// Degrees: 90, radians: 1.5707963267949",
            "// Degrees: 180, radians: 3.14159265358979",
            "// Degrees: 270, radians: 4.71238898038469"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can use ",
              "type": "text"
            },
            {
              "code": "stride(from:to:by:)",
              "type": "codeVoice"
            },
            {
              "text": " to create a sequence that strides upward",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or downward. Pass a negative value as ",
              "type": "text"
            },
            {
              "code": "stride",
              "type": "codeVoice"
            },
            {
              "text": " to create a sequence from a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "higher start to a lower end:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "for countdown in stride(from: 3, to: 0, by: -1) {",
            "    print(\"\\(countdown)...\")",
            "}",
            "// 3...",
            "// 2...",
            "// 1..."
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If you pass a value as ",
              "type": "text"
            },
            {
              "code": "stride",
              "type": "codeVoice"
            },
            {
              "text": " that moves away from ",
              "type": "text"
            },
            {
              "code": "end",
              "type": "codeVoice"
            },
            {
              "text": ", the sequence",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "contains no values.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "for x in stride(from: 0, to: 10, by: -1) {",
            "    print(x)",
            "}",
            "// Nothing is printed."
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
