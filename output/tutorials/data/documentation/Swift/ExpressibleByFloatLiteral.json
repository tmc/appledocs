    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The standard library floating-point types—",
              "type": "text"
            },
            {
              "code": "Float",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "Double",
              "type": "codeVoice"
            },
            {
              "text": ", and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Float80",
              "type": "codeVoice"
            },
            {
              "text": " where available—all conform to the ",
              "type": "text"
            },
            {
              "code": "ExpressibleByFloatLiteral",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "protocol. You can initialize a variable or constant of any of these types",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by assigning a floating-point literal.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Type inferred as 'Double'",
            "let threshold = 6.0",
            "",
            "// An array of 'Double'",
            "let measurements = [2.2, 4.1, 3.65, 4.2, 9.1]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "anchor": "Conforming-to-ExpressibleByFloatLiteral",
          "level": 1,
          "text": "Conforming to ExpressibleByFloatLiteral",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To add ",
              "type": "text"
            },
            {
              "code": "ExpressibleByFloatLiteral",
              "type": "codeVoice"
            },
            {
              "text": " conformance to your custom type,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "implement the required initializer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
