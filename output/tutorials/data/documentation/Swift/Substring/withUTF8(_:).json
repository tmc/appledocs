      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Note that it is unsafe to escape the pointer provided to ",
              "type": "text"
            },
            {
              "code": "body",
              "type": "codeVoice"
            },
            {
              "text": ". For",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example, strings of up to 15 UTF-8 code units in length may be represented",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in a small-string representation, and thus will be spilled into",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "temporary stack space which is invalid after ",
              "type": "text"
            },
            {
              "code": "withUTF8",
              "type": "codeVoice"
            },
            {
              "text": " finishes",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "execution.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Complexity: O(n) if non-contiguous, O(1) if already contiguous",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
