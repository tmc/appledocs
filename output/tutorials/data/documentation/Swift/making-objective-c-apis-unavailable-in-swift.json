{
  "abstract": [
    {
      "text": "Use the ",
      "type": "text"
    },
    {
      "code": "NS_SWIFT_UNAVAILABLE",
      "type": "codeVoice"
    },
    {
      "text": " macro to prevent an API from being used in Swift.",
      "type": "text"
    }
  ],
