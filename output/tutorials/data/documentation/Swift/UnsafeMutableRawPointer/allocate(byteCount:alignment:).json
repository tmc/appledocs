    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of bytes to allocate. ",
                  "type": "text"
                },
                {
                  "code": "byteCount",
                  "type": "codeVoice"
                },
                {
                  "text": " must not be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "negative.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "byteCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The alignment of the new region of allocated memory, in",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "bytes. ",
                  "type": "text"
                },
                {
                  "code": "alignment",
                  "type": "codeVoice"
                },
                {
                  "text": " must be a whole power of 2.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alignment"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A pointer to a newly allocated region of memory. The memory is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "allocated, but not initialized.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You are in charge of managing the allocated memory. Be sure to deallocate",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "any memory that you manually allocate.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The allocated memory is not bound to any specific type and must be bound",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "before performing any typed operations. If you are using the memory for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a specific type, allocate memory using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "UnsafeMutablePointer.allocate(capacity:)",
              "type": "codeVoice"
            },
            {
              "text": " static method instead.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
