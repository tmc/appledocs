        {
          "inlineContent": [
            {
              "text": "A typed pointer to the memory referenced by this raw pointer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The memory referenced by this pointer must be uninitialized or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "initialized to a trivial type, and must be properly aligned for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "accessing ",
              "type": "text"
            },
            {
              "code": "T",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example allocates raw memory for one instance of ",
              "type": "text"
            },
            {
              "code": "UInt",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and then uses the ",
              "type": "text"
            },
            {
              "code": "initializeMemory(as:to:)",
              "type": "codeVoice"
            },
            {
              "text": " method",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to initialize the allocated memory.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let bytePointer = UnsafeMutableRawPointer.allocate(",
            "        byteCount: MemoryLayout<UInt>.stride,",
            "        alignment: MemoryLayout<UInt>.alignment)",
            "let int8Pointer = bytePointer.initializeMemory(as: UInt.self, to: 0)",
            "",
            "// After using 'int8Pointer':",
            "int8Pointer.deallocate()"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "After calling this method on a raw pointer ",
              "type": "text"
            },
            {
              "code": "p",
              "type": "codeVoice"
            },
            {
              "text": ", the region starting at",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "self",
              "type": "codeVoice"
            },
            {
              "text": " and continuing up to ",
              "type": "text"
            },
            {
              "code": "p + MemoryLayout<T>.stride",
              "type": "codeVoice"
            },
            {
              "text": " is bound",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to type ",
              "type": "text"
            },
            {
              "code": "T",
              "type": "codeVoice"
            },
            {
              "text": " and initialized. If ",
              "type": "text"
            },
            {
              "code": "T",
              "type": "codeVoice"
            },
            {
              "text": " is a nontrivial type, you must",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "eventually deinitialize the memory in this region to avoid memory leaks.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
