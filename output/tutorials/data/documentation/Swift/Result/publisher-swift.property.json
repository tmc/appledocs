    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In the following example, ",
              "type": "text"
            },
            {
              "code": "goodResult",
              "type": "codeVoice"
            },
            {
              "text": " provides a successful result with the integer value ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ". A sink subscriber connected to the result’s publisher receives the output ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ", followed by a normal completion (",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Combine/Subscribers/Completion/finished",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ").",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " let goodResult: Result<Int, MyError> = .success(1)",
            " goodResult.publisher",
            "     .sink(receiveCompletion: { print(\"goodResult done: \\($0)\")},",
            "           receiveValue: { print(\"goodResult value: \\($0)\")} )",
            " // Prints:",
            " // goodResult value: 1",
            " // goodResult done: finished"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In contrast with the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Combine/Just",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " publisher, which always publishes a single value, this publisher might not send any values and instead terminate with an error, if the result is ",
              "type": "text"
            },
            {
              "code": "/Swift/Result/failure",
              "type": "codeVoice"
            },
            {
              "text": ". In the next example, ",
              "type": "text"
            },
            {
              "code": "badResult",
              "type": "codeVoice"
            },
            {
              "text": " is a failure result that wraps a custom error. A sink subscriber connected to this result’s publisher immediately receives a termination (",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Combine/Subscribers/Completion/failure(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ").",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " struct MyError: Error, CustomDebugStringConvertible {",
            "     var debugDescription: String = \"MyError\"",
            " }",
            " let badResult: Result<Int, MyError> = .failure(MyError())",
            " badResult.publisher",
            "     .sink(receiveCompletion: { print(\"badResult done: \\($0)\")},",
            "           receiveValue: { print(\"badResult value: \\($0)\")} )",
            " // Prints:",
            " // badResult done: failure(MyError)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
