    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to convert an instance of any type to its preferred",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "representation as a ",
              "type": "text"
            },
            {
              "code": "String",
              "type": "codeVoice"
            },
            {
              "text": " instance. The initializer creates the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "string representation of ",
              "type": "text"
            },
            {
              "code": "instance",
              "type": "codeVoice"
            },
            {
              "text": " in one of the following ways,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "depending on its protocol conformance:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "instance",
                      "type": "codeVoice"
                    },
                    {
                      "text": " conforms to the ",
                      "type": "text"
                    },
                    {
                      "code": "TextOutputStreamable",
                      "type": "codeVoice"
                    },
                    {
                      "text": " protocol, the",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "result is obtained by calling ",
                      "type": "text"
                    },
                    {
                      "code": "instance.write(to: s)",
                      "type": "codeVoice"
                    },
                    {
                      "text": " on an empty",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "string ",
                      "type": "text"
                    },
                    {
                      "code": "s",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "instance",
                      "type": "codeVoice"
                    },
                    {
                      "text": " conforms to the ",
                      "type": "text"
                    },
                    {
                      "code": "CustomStringConvertible",
                      "type": "codeVoice"
                    },
                    {
                      "text": " protocol, the",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "result is ",
                      "type": "text"
                    },
                    {
                      "code": "instance.description",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "instance",
                      "type": "codeVoice"
                    },
                    {
                      "text": " conforms to the ",
                      "type": "text"
                    },
                    {
                      "code": "CustomDebugStringConvertible",
                      "type": "codeVoice"
                    },
                    {
                      "text": " protocol,",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "the result is ",
                      "type": "text"
                    },
                    {
                      "code": "instance.debugDescription",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "An unspecified result is supplied automatically by the Swift standard",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "library.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "For example, this custom ",
              "type": "text"
            },
            {
              "code": "Point",
              "type": "codeVoice"
            },
            {
              "text": " struct uses the default representation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "supplied by the standard library.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Point {",
            "    let x: Int, y: Int",
            "}",
            "",
            "let p = Point(x: 21, y: 30)",
            "print(String(describing: p))",
            "// Prints \"Point(x: 21, y: 30)\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "After adding ",
              "type": "text"
            },
            {
              "code": "CustomStringConvertible",
              "type": "codeVoice"
            },
            {
              "text": " conformance by implementing the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "description",
              "type": "codeVoice"
            },
            {
              "text": " property, ",
              "type": "text"
            },
            {
              "code": "Point",
              "type": "codeVoice"
            },
            {
              "text": " provides its own custom representation.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension Point: CustomStringConvertible {",
            "    var description: String {",
            "        return \"(\\(x), \\(y))\"",
            "    }",
            "}",
            "",
            "print(String(describing: p))",
            "// Prints \"(21, 30)\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
