    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to remove every element in a collection that meets",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "particular criteria. The order of the remaining elements is preserved.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This example removes all the vowels from a string:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var phrase = \"The rain in Spain stays mainly in the plain.\"",
            "",
            "let vowels: Set<Character> = [\"a\", \"e\", \"i\", \"o\", \"u\"]",
            "phrase.removeAll(where: { vowels.contains($0) })",
            "// phrase == \"Th rn n Spn stys mnly n th pln.\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "O(",
                  "type": "text"
                },
                {
