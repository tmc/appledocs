            {
              "inlineContent": [
                {
                  "text": "A valid index of the character view. ",
                  "type": "text"
                },
                {
                  "code": "position",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "must be less than the view’s end index.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "position"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example searches a string’s Unicode scalars view for a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "capital letter and then prints the character and Unicode scalar value",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "at the found index:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let greeting = \"Hello, friend!\"",
            "if let i = greeting.unicodeScalars.firstIndex(where: { \"A\"...\"Z\" ~= $0 }) {",
            "    print(\"First capital letter: \\(greeting.unicodeScalars[i])\")",
            "    print(\"Unicode scalar value: \\(greeting.unicodeScalars[i].value)\")",
            "}",
            "// Prints \"First capital letter: H\"",
            "// Prints \"Unicode scalar value: 72\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
