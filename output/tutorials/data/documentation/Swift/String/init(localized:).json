    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "LocalizedStringResource",
                  "type": "codeVoice"
                },
                {
                  "text": " that provides the localization key, table, bundle, and locale.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resource"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Call this initializer to look up the localization that ",
              "type": "text"
            },
            {
              "code": "resource",
              "type": "codeVoice"
            },
            {
              "text": " indicates from a string catalog or ",
              "type": "text"
            },
            {
              "code": "strings",
              "type": "codeVoice"
            },
            {
              "text": " file in your app bundle. Alter the resource’s ",
              "type": "text"
            },
            {
              "code": "locale",
              "type": "codeVoice"
            },
            {
              "text": " prior to calling this method if you want to localize this string in a different locale than the process that creates the ",
              "type": "text"
            },
            {
              "code": "LocalizedStringResource",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Assume the string catalog contains \"Hello, world!\" for English",
            "// and \"Bonjour, le monde!\" for French.",
            "var localizable = LocalizedStringResource(\"Hello, world!\")",
            "// Potentially using localizable in another process…",
            "localizable.locale = Locale(identifier: \"en\")",
            "let enLocalized = String(localized: localizable)",
            "localizable.locale = Locale(identifier: \"fr\")",
            "let frLocalized = String(localized: localizable)",
            "// enLocalized == \"Hello, world!\"",
            "// frLocalized == \"Bonjour, le monde!\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If you don’t need the deferred-loading behavior of ",
              "type": "text"
            },
            {
              "code": "LocalizedStringResource",
              "type": "codeVoice"
            },
            {
              "text": ", use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/String/init(localized:table:bundle:locale:comment:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instead. If you prefer to use an arbitrary localization key rather than the localized string in the development language, use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/String/init(localized:defaultValue:table:bundle:locale:comment:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
