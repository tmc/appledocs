      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a null-terminated sequence of",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "code units encoded in ",
                  "type": "text"
                },
                {
                  "code": "encoding",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cString"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Unicode encoding of the data referenced by ",
                  "type": "text"
                },
                {
                  "code": "cString",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "encoding"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Pass ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " to create a new string, even when the data",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "referenced by ",
                  "type": "text"
                },
                {
                  "code": "cString",
                  "type": "codeVoice"
                },
                {
                  "text": " contains ill-formed sequences. Ill-formed",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "sequences are replaced with the Unicode replacement character",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "(",
                  "type": "text"
                },
                {
                  "code": "\"\\u{FFFD}\"",
                  "type": "codeVoice"
                },
                {
                  "text": "). Pass ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " to interrupt the creation of the new",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "string if an ill-formed sequence is detected.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isRepairing"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A tuple with the new string and a Boolean value that indicates",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "whether any repairs were made. If ",
              "type": "text"
            },
            {
              "code": "isRepairing",
              "type": "codeVoice"
            },
            {
              "text": " is ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": " and an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "ill-formed sequence is detected, this method returns ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When you pass ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " as ",
              "type": "text"
            },
            {
              "code": "isRepairing",
              "type": "codeVoice"
            },
            {
              "text": ", this method replaces ill-formed",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "sequences with the Unicode replacement character (",
              "type": "text"
            },
            {
              "code": "\"\\u{FFFD}\"",
              "type": "codeVoice"
            },
            {
              "text": ");",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "otherwise, an ill-formed sequence causes this method to stop decoding",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and return ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example calls this method with pointers to the contents of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "two different ",
              "type": "text"
            },
            {
              "code": "CChar",
              "type": "codeVoice"
            },
            {
              "text": " arrays—the first with well-formed UTF-8 code",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "unit sequences and the second with an ill-formed sequence at the end.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let validUTF8: [UInt8] = [67, 97, 102, 195, 169, 0]",
            "validUTF8.withUnsafeBufferPointer { ptr in",
            "    let s = String.decodeCString(ptr.baseAddress,",
            "                                 as: UTF8.self,",
            "                                 repairingInvalidCodeUnits: true)",
            "    print(s)",
            "}",
            "// Prints \"Optional((result: \"Café\", repairsMade: false))\"",
            "",
            "let invalidUTF8: [UInt8] = [67, 97, 102, 195, 0]",
            "invalidUTF8.withUnsafeBufferPointer { ptr in",
            "    let s = String.decodeCString(ptr.baseAddress,",
            "                                 as: UTF8.self,",
            "                                 repairingInvalidCodeUnits: true)",
            "    print(s)",
            "}",
            "// Prints \"Optional((result: \"Caf�\", repairsMade: true))\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
