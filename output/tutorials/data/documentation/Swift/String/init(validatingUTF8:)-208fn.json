    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This initializer does not try to repair ill-formed UTF-8 code unit",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "sequences. If any are found, the result of the initializer is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example calls this initializer with pointers to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "contents of two different ",
              "type": "text"
            },
            {
              "code": "CChar",
              "type": "codeVoice"
            },
            {
              "text": " arrays—the first with well-formed",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "UTF-8 code unit sequences and the second with an ill-formed sequence at",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the end.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let validUTF8: [CChar] = [67, 97, 102, -61, -87, 0]",
            "validUTF8.withUnsafeBufferPointer { ptr in",
            "    let s = String(validatingUTF8: ptr.baseAddress!)",
            "    print(s)",
            "}",
            "// Prints \"Optional(\"Café\")\"",
            "",
            "let invalidUTF8: [CChar] = [67, 97, 102, -61, 0]",
            "invalidUTF8.withUnsafeBufferPointer { ptr in",
            "    let s = String(validatingUTF8: ptr.baseAddress!)",
            "    print(s)",
            "}",
            "// Prints \"nil\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This initializer has been renamed. Use",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "String.init?(validatingCString:)",
                  "type": "codeVoice"
                },
                {
                  "text": " instead.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
