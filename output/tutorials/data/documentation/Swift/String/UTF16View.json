    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can access a string’s view of UTF-16 code units by using its ",
              "type": "text"
            },
            {
              "code": "utf16",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "property. A string’s UTF-16 view encodes the string’s Unicode scalar",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "values as 16-bit integers.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let flowers = \"Flowers 💐\"",
            "for v in flowers.utf16 {",
            "    print(v)",
            "}",
            "// 70",
            "// 108",
            "// 111",
            "// 119",
            "// 101",
            "// 114",
            "// 115",
            "// 32",
            "// 55357",
            "// 56464"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Unicode scalar values that make up a string’s contents can be up to 21",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "bits long. The longer scalar values may need two ",
              "type": "text"
            },
            {
              "code": "UInt16",
              "type": "codeVoice"
            },
            {
              "text": " values for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "storage. Those “pairs” of code units are called ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "surrogate pairs",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let flowermoji = \"💐\"",
            "for v in flowermoji.unicodeScalars {",
            "    print(v, v.value)",
            "}",
            "// 💐 128144",
            "",
            "for v in flowermoji.utf16 {",
            "    print(v)",
            "}",
            "// 55357",
            "// 56464"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "To convert a ",
              "type": "text"
            },
            {
              "code": "String.UTF16View",
              "type": "codeVoice"
            },
            {
              "text": " instance back into a string, use the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "String",
              "type": "codeVoice"
            },
            {
              "text": " type’s ",
              "type": "text"
            },
            {
              "code": "init(_:)",
              "type": "codeVoice"
            },
            {
              "text": " initializer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let favemoji = \"My favorite emoji is 🎉\"",
            "if let i = favemoji.utf16.firstIndex(where: { $0 >= 128 }) {",
            "    let asciiPrefix = String(favemoji.utf16[..<i])!",
            "    print(asciiPrefix)",
            "}",
            "// Prints \"My favorite emoji is \""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "anchor": "UTF16View-Elements-Match-NSString-Characters",
          "level": 1,
          "text": "UTF16View Elements Match NSString Characters",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The UTF-16 code units of a string’s ",
              "type": "text"
            },
            {
              "code": "utf16",
              "type": "codeVoice"
            },
            {
              "text": " view match the elements",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "accessed through indexed ",
              "type": "text"
            },
            {
              "code": "NSString",
              "type": "codeVoice"
            },
            {
              "text": " APIs.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "print(flowers.utf16.count)",
            "// Prints \"10\"",
            "",
            "let nsflowers = flowers as NSString",
            "print(nsflowers.length)",
            "// Prints \"10\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Unlike ",
              "type": "text"
            },
            {
              "code": "NSString",
              "type": "codeVoice"
            },
            {
              "text": ", however, ",
              "type": "text"
            },
            {
              "code": "String.UTF16View",
              "type": "codeVoice"
            },
            {
              "text": " does not use integer",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "indices. If you need to access a specific position in a UTF-16 view, use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Swift’s index manipulation methods. The following example accesses the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "fourth code unit in both the ",
              "type": "text"
            },
            {
              "code": "flowers",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "nsflowers",
              "type": "codeVoice"
            },
            {
              "text": " strings:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "print(nsflowers.character(at: 3))",
            "// Prints \"119\"",
            "",
            "let i = flowers.utf16.index(flowers.utf16.startIndex, offsetBy: 3)",
            "print(flowers.utf16[i])",
            "// Prints \"119\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Although the Swift overlay updates many Objective-C methods to return",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "native Swift indices and index ranges, some still return instances of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "NSRange",
              "type": "codeVoice"
            },
            {
              "text": ". To convert an ",
              "type": "text"
            },
            {
              "code": "NSRange",
              "type": "codeVoice"
            },
            {
              "text": " instance to a range of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "String.Index",
              "type": "codeVoice"
            },
            {
              "text": ", use the ",
              "type": "text"
            },
            {
              "code": "Range(_:in:)",
              "type": "codeVoice"
            },
            {
              "text": " initializer, which takes an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "NSRange",
              "type": "codeVoice"
            },
            {
              "text": " and a string as arguments.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let snowy = \"❄️ Let it snow! ☃️\"",
            "let nsrange = NSRange(location: 3, length: 12)",
            "if let range = Range(nsrange, in: snowy) {",
            "    print(snowy[range])",
            "}",
            "// Prints \"Let it snow!\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
