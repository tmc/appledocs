    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value to convert to a string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The base to use for the string representation. ",
                  "type": "text"
                },
                {
                  "code": "radix",
                  "type": "codeVoice"
                },
                {
                  "text": " must be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "at least 2 and at most 36. The default is 10.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "radix"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Pass ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " to use uppercase letters to represent numerals",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "greater than 9, or ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " to use lowercase letters. The default is",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "uppercase"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example converts the maximal ",
              "type": "text"
            },
            {
              "code": "Int",
              "type": "codeVoice"
            },
            {
              "text": " value to a string and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "prints its length:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let max = String(Int.max)",
            "print(\"\\(max) has \\(max.count) digits.\")",
            "// Prints \"9223372036854775807 has 19 digits.\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Numerals greater than 9 are represented as Roman letters. These letters",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "start with ",
              "type": "text"
            },
            {
              "code": "\"A\"",
              "type": "codeVoice"
            },
            {
              "text": " if ",
              "type": "text"
            },
            {
              "code": "uppercase",
              "type": "codeVoice"
            },
            {
              "text": " is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": "; otherwise, with ",
              "type": "text"
            },
            {
              "code": "\"a\"",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let v = 999_999",
            "print(String(v, radix: 2))",
            "// Prints \"11110100001000111111\"",
            "",
            "print(String(v, radix: 16))",
            "// Prints \"f423f\"",
            "print(String(v, radix: 16, uppercase: true))",
            "// Prints \"F423F\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
