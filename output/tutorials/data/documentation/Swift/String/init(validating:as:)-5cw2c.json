    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This initializer does not try to repair ill-formed code unit sequences.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "If any are found, the result of the initializer is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example calls this initializer with the contents of two",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "different arrays—first with a well-formed UTF-8 code unit sequence and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "then with an ill-formed ASCII code unit sequence.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let validUTF8: [Int8] = [67, 97, 0, 102, -61, -87]",
            "let valid = String(validating: validUTF8, as: UTF8.self)",
            "print(valid ?? \"nil\")",
            "// Prints \"Café\"",
            "",
            "let invalidASCII: [Int8] = [67, 97, -5]",
            "let invalid = String(validating: invalidASCII, as: Unicode.ASCII.self)",
            "print(invalid ?? \"nil\")",
            "// Prints \"nil\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
