    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this initializer to create a new string from the result of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "one or more collection operations on a string’s characters. For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let str = \"The rain in Spain stays mainly in the plain.\"",
            "",
            "let vowels: Set<Character> = [\"a\", \"e\", \"i\", \"o\", \"u\"]",
            "let disemvoweled = String(str.lazy.filter { !vowels.contains($0) })",
            "",
            "print(disemvoweled)",
            "// Prints \"Th rn n Spn stys mnly n th pln.\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
