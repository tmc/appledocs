    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example finds the position of a space in a string‚Äôs ",
              "type": "text"
            },
            {
              "code": "utf16",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view and then converts that position to an index in the string‚Äôs",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "utf8",
              "type": "codeVoice"
            },
            {
              "text": " view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let cafe = \"Caf√© üçµ\"",
            "",
            "let utf16Index = cafe.utf16.firstIndex(of: 32)!",
            "let utf8Index = String.UTF8View.Index(utf16Index, within: cafe.utf8)!",
            "",
            "print(Array(cafe.utf8[..<utf8Index]))",
            "// Prints \"[67, 97, 102, 195, 169]\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If the position passed in ",
              "type": "text"
            },
            {
              "code": "utf16Index",
              "type": "codeVoice"
            },
            {
              "text": " doesn‚Äôt have an exact",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "corresponding position in ",
              "type": "text"
            },
            {
              "code": "utf8",
              "type": "codeVoice"
            },
            {
              "text": ", the result of the initializer is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ". For example, because UTF-8 and UTF-16 represent high Unicode code",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "points differently, an attempt to convert the position of the trailing",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "surrogate of a UTF-16 surrogate pair fails.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The next example attempts to convert the indices of the two UTF-16 code",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "points that represent the teacup emoji (",
              "type": "text"
            },
            {
              "code": "\"üçµ\"",
              "type": "codeVoice"
            },
            {
              "text": "). The index of the lead",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "surrogate is successfully converted to a position in ",
              "type": "text"
            },
            {
              "code": "utf8",
              "type": "codeVoice"
            },
            {
              "text": ", but the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "index of the trailing surrogate is not.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let emojiHigh = cafe.utf16.index(after: utf16Index)",
            "print(String.UTF8View.Index(emojiHigh, within: cafe.utf8))",
            "// Prints \"Optional(String.Index(...))\"",
            "",
            "let emojiLow = cafe.utf16.index(after: emojiHigh)",
            "print(String.UTF8View.Index(emojiLow, within: cafe.utf8))",
            "// Prints \"nil\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
