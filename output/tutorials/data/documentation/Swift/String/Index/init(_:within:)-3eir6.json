    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A position in a view of the ",
                  "type": "text"
                },
                {
                  "code": "target",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "sourcePosition",
                  "type": "codeVoice"
                },
                {
                  "text": " must be a valid index of at least one of the views",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of ",
                  "type": "text"
                },
                {
                  "code": "target",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sourcePosition"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The string referenced by the resulting index.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "target"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the index passed as ",
              "type": "text"
            },
            {
              "code": "sourcePosition",
              "type": "codeVoice"
            },
            {
              "text": " represents the start of an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "extended grapheme cluster—the element type of a string—then the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "initializer succeeds.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example converts the position of the Unicode scalar ",
              "type": "text"
            },
            {
              "code": "\"e\"",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "into its corresponding position in the string. The character at that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "position is the composed ",
              "type": "text"
            },
            {
              "code": "\"é\"",
              "type": "codeVoice"
            },
            {
              "text": " character.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let cafe = \"Cafe\\u{0301}\"",
            "print(cafe)",
            "// Prints \"Café\"",
            "",
            "let scalarsIndex = cafe.unicodeScalars.firstIndex(of: \"e\")!",
            "let stringIndex = String.Index(scalarsIndex, within: cafe)!",
            "",
            "print(cafe[...stringIndex])",
            "// Prints \"Café\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If the index passed as ",
              "type": "text"
            },
            {
              "code": "sourcePosition",
              "type": "codeVoice"
            },
            {
              "text": " doesn’t have an exact",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "corresponding position in ",
              "type": "text"
            },
            {
              "code": "target",
              "type": "codeVoice"
            },
            {
              "text": ", the result of the initializer is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ". For example, an attempt to convert the position of the combining",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "acute accent (",
              "type": "text"
            },
            {
              "code": "\"\\u{0301}\"",
              "type": "codeVoice"
            },
            {
              "text": ") fails. Combining Unicode scalars do not have",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "their own position in a string.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let nextScalarsIndex = cafe.unicodeScalars.index(after: scalarsIndex)",
            "let nextStringIndex = String.Index(nextScalarsIndex, within: cafe)",
            "",
            "print(nextStringIndex)",
            "// Prints \"nil\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
