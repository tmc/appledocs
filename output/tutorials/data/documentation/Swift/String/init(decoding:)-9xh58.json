      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The path component to be interpreted as",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "CInterop.PlatformUnicodeEncoding",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "component"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the content of the path component isn’t a well-formed Unicode string,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "this initializer replaces invalid bytes with U+FFFD.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This means that, depending on the semantics of the specific file system,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "conversion to a string and back to a path component",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "might result in a value that’s different from the original path component.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
