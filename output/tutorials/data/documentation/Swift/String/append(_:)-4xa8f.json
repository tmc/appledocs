          "inlineContent": [
            {
              "text": "The following example builds a customized greeting by using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "append(_:)",
              "type": "codeVoice"
            },
            {
              "text": " method:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var greeting = \"Hello, \"",
            "if let name = getUserName() {",
            "    greeting.append(name)",
            "} else {",
            "    greeting.append(\"friend\")",
            "}",
            "print(greeting)",
            "// Prints \"Hello, friend\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
