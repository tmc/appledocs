        {
          "inlineContent": [
            {
              "text": "All the elements following ",
              "type": "text"
            },
            {
              "code": "i",
              "type": "codeVoice"
            },
            {
              "text": " are moved to close the gap. This example",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "removes the hyphen from the middle of a string.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var nonempty = \"non-empty\"",
            "if let i = nonempty.firstIndex(of: \"-\") {",
            "    nonempty.remove(at: i)",
            "}",
            "print(nonempty)",
            "// Prints \"nonempty\""
          ],
