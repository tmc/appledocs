    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If ",
              "type": "text"
            },
            {
              "code": "cString",
              "type": "codeVoice"
            },
            {
              "text": " contains ill-formed UTF-8 code unit sequences, this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "initializer replaces them with the Unicode replacement character",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "(",
              "type": "text"
            },
            {
              "code": "\"\\u{FFFD}\"",
              "type": "codeVoice"
            },
            {
              "text": ").",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example calls this initializer with pointers to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "contents of two different ",
              "type": "text"
            },
            {
              "code": "CChar",
              "type": "codeVoice"
            },
            {
              "text": " arrays—the first with well-formed",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "UTF-8 code unit sequences and the second with an ill-formed sequence at",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the end.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let validUTF8: [CChar] = [67, 97, 102, -61, -87, 0]",
            "validUTF8.withUnsafeBufferPointer { ptr in",
            "    let s = String(cString: ptr.baseAddress!)",
            "    print(s)",
            "}",
            "// Prints \"Café\"",
            "",
            "let invalidUTF8: [CChar] = [67, 97, 102, -61, 0]",
            "invalidUTF8.withUnsafeBufferPointer { ptr in",
            "    let s = String(cString: ptr.baseAddress!)",
            "    print(s)",
            "}",
            "// Prints \"Caf�\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
