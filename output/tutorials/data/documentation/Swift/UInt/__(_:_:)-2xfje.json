      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value to shift.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lhs"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of bits to shift ",
                  "type": "text"
                },
                {
                  "code": "lhs",
                  "type": "codeVoice"
                },
                {
                  "text": " to the left.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rhs"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "<<",
              "type": "codeVoice"
            },
            {
              "text": " operator performs a ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "smart shift",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": ", which defines a result for a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "shift of any value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Using a negative value for ",
                      "type": "text"
                    },
                    {
                      "code": "rhs",
                      "type": "codeVoice"
                    },
                    {
                      "text": " performs a right shift using",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "code": "abs(rhs)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Using a value for ",
                      "type": "text"
                    },
                    {
                      "code": "rhs",
                      "type": "codeVoice"
                    },
                    {
                      "text": " that is greater than or equal to the bit width",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "of ",
                      "type": "text"
                    },
                    {
                      "code": "lhs",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is an ",
                      "type": "text"
                    },
                    {
                      "inlineContent": [
                        {
                          "text": "overshift",
                          "type": "text"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "text": ", resulting in zero.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Using any other value for ",
                      "type": "text"
                    },
                    {
                      "code": "rhs",
                      "type": "codeVoice"
                    },
                    {
                      "text": " performs a left shift on ",
                      "type": "text"
                    },
                    {
                      "code": "lhs",
                      "type": "codeVoice"
                    },
                    {
                      "text": " by that",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "amount.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "The following example defines ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": " as an instance of ",
              "type": "text"
            },
            {
              "code": "UInt8",
              "type": "codeVoice"
            },
            {
              "text": ", an 8-bit,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "unsigned integer type. If you use ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": " as the right-hand-side value in an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "operation on ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": ", the value is shifted left by two bits.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let x: UInt8 = 30                 // 0b00011110",
            "let y = x << 2",
            "// y == 120                       // 0b01111000"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If you use ",
              "type": "text"
            },
            {
              "code": "11",
              "type": "codeVoice"
            },
            {
              "text": " as ",
              "type": "text"
            },
            {
              "code": "rhs",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": " is overshifted such that all of its bits",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "are set to zero.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let z = x << 11",
            "// z == 0                         // 0b00000000"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Using a negative value as ",
              "type": "text"
            },
            {
              "code": "rhs",
              "type": "codeVoice"
            },
            {
              "text": " is the same as performing a right shift",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with ",
              "type": "text"
            },
            {
              "code": "abs(rhs)",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let a = x << -3",
            "// a == 3                         // 0b00000011",
            "let b = x >> 3",
            "// b == 3                         // 0b00000011"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
