        {
          "inlineContent": [
            {
              "text": "The following example reverses the elements of an array of characters:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var characters: [Character] = [\"C\", \"a\", \"f\", \"é\"]",
            "characters.reverse()",
            "print(characters)",
            "// Prints \"[\"é\", \"f\", \"a\", \"C\"]\""
          ],
