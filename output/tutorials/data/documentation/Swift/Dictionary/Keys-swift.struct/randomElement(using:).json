        },
        {
          "inlineContent": [
            {
              "text": "Call ",
              "type": "text"
            },
            {
              "code": "randomElement(using:)",
              "type": "codeVoice"
            },
            {
              "text": " to select a random element from an array or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "another collection when you are using a custom random number generator.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This example picks a name at random from an array:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let names = [\"Zoey\", \"Chloe\", \"Amani\", \"Amaia\"]",
            "let randomName = names.randomElement(using: &myGenerator)!",
            "// randomName == \"Amani\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "O(1) if the collection conforms to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "RandomAccessCollection",
                  "type": "codeVoice"
                },
                {
                  "text": "; otherwise, O(",
                  "type": "text"
                },
