      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key-value pairs that will make up the new",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "dictionary. Each key in ",
                  "type": "text"
                },
                {
                  "code": "elements",
                  "type": "codeVoice"
                },
                {
                  "text": " must be unique.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "elements"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Do not call this initializer directly. It is called by the compiler to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "handle dictionary literals. To use a dictionary literal as the initial",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value of a dictionary, enclose a comma-separated list of key-value pairs",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in square brackets.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the code sample below creates a dictionary with string keys",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and values.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let countryCodes = [\"BR\": \"Brazil\", \"GH\": \"Ghana\", \"JP\": \"Japan\"]",
            "print(countryCodes)",
            "// Prints \"[\"BR\": \"Brazil\", \"JP\": \"Japan\", \"GH\": \"Ghana\"]\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
