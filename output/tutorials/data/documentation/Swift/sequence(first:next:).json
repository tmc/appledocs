      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The first element to be returned from the sequence.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "first"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that accepts the previous sequence element and",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "returns the next element.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "next"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A sequence that starts with ",
              "type": "text"
            },
            {
              "code": "first",
              "type": "codeVoice"
            },
            {
              "text": " and continues with every",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value returned by passing the previous element to ",
              "type": "text"
            },
            {
              "code": "next",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The first element in the sequence is always ",
              "type": "text"
            },
            {
              "code": "first",
              "type": "codeVoice"
            },
            {
              "text": ", and each successive",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "element is the result of invoking ",
              "type": "text"
            },
            {
              "code": "next",
              "type": "codeVoice"
            },
            {
              "text": " with the previous element. The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "sequence ends when ",
              "type": "text"
            },
            {
              "code": "next",
              "type": "codeVoice"
            },
            {
              "text": " returns ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ". If ",
              "type": "text"
            },
            {
              "code": "next",
              "type": "codeVoice"
            },
            {
              "text": " never returns ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "sequence is infinite.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This function can be used to replace many cases that were previously handled",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using C-style ",
              "type": "text"
            },
            {
              "code": "for",
              "type": "codeVoice"
            },
            {
              "text": " loops.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Walk the elements of a tree from a node up to the root",
            "for node in sequence(first: leaf, next: { $0.parent }) {",
            "  // node is leaf, then leaf.parent, then leaf.parent.parent, etc.",
            "}",
            "",
            "// Iterate over all powers of two (ignoring overflow)",
            "for value in sequence(first: 1, next: { $0 * 2 }) {",
            "  // value is 1, then 2, then 4, then 8, etc.",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
