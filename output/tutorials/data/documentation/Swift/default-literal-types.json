  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This example declares the ",
              "type": "text"
            },
            {
              "code": "numberOfCookies",
              "type": "codeVoice"
            },
            {
              "text": " constant, using an integer literal to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "express its value:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let numberOfCookies = 5",
            "// type(of: numberOfCookies) == Int.self"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When a literal expression is written with no type information, Swift uses these type",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "aliases to determine what type to use for the expression. In this case, the ",
              "type": "text"
            },
            {
              "code": "numberOfCookies",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "constant has the default type for an integer literal, ",
              "type": "text"
            },
            {
              "code": "Int",
              "type": "codeVoice"
            },
            {
              "text": ", as designated by the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "IntegerLiteralType",
              "type": "codeVoice"
            },
            {
              "text": " type alias.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
