      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You create an instance of the ",
              "type": "text"
            },
            {
              "code": "Repeated",
              "type": "codeVoice"
            },
            {
              "text": " collection by calling the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "repeatElement(_:count:)",
              "type": "codeVoice"
            },
            {
              "text": " function. The following example creates a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "collection containing the name “Humperdinck” repeated five times:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let repeatedName = repeatElement(\"Humperdinck\", count: 5)",
            "for name in repeatedName {",
            "    print(name)",
            "}",
            "// \"Humperdinck\"",
            "// \"Humperdinck\"",
            "// \"Humperdinck\"",
            "// \"Humperdinck\"",
            "// \"Humperdinck\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
