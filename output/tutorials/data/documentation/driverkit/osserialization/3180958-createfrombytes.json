      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The serialized data from a previous call to OSSerialization::finalizeBuffer().",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bytes"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length of the serialized data from a previous call to OSSerialization::finalizeBuffer().",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "length"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A required block to be called when the OSSerialization is freed and the serialized data will no longer be accessed. Note that unserialized objects may retain the OSSerialization they were created from, so the OSSerialization will retain the data until they have been freed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "freeBuffer"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "NULL on failure, otherwise the allocated OSSerialization with reference count 1 to be released by the caller.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
