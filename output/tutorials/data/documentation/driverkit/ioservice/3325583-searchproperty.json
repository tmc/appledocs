    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the property as a c-string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the registry plane to search. The method uses this value only when you include ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/driverkit/3325572-search_options/kioservicesearchpropertyparents",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "code": "options",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter. If you don't include that option, you may specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " for this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "plane"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Options to use during the search. Specify ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/driverkit/3325572-search_options/kioservicesearchpropertyparents",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to expand the search to include all parent services. If you don't specify ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/driverkit/3325572-search_options/kioservicesearchpropertyparents",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": ", this method looks for the property only in the current service.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A variable in which to store the value of the property. It is a programmer error to specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " for this parameter. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If the specified property is found, this method puts the value in the provided variable; you are responsible for releasing that value. If the specified property isn't found, this method sets the value of your variable to ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "property"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "identifier": "doc://com.apple.documentation/documentation/driverkit/kioreturnsuccess",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " on success, or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/driverkit/kioreturnnotfound",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " if the property was not found.  ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method searches for the property in the current service first, followed by any parent services when applicable. The method returns the first instance of the property it finds. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
