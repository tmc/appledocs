        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether to enable the dispatch source. Specify ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " to enable the dispatch source, or ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " to disable it. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "enable"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional block to execute when disabling the dispatch source. The dispatch source executes it after all pending interrupts finish executing. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "handler"
        }
      ]
    },
