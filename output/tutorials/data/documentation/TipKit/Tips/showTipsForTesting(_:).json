          "code": [
            "import SwiftUI",
            "import TipKit",
            "",
            "@main",
            "struct LandmarkTips: App {",
            "    var body: some Scene {",
            "        WindowGroup {",
            "            ContentView()",
            "        }",
            "    }",
            "",
            "    init() {",
            "        setupTips()",
            "    }",
            "",
            "    // Configure tips in the app.",
            "    func setupTips() {",
            "        do {",
            "            #if DEBUG",
            "            Tips.showTipsForTesting([FindTrailheadTip.self, SlopeProfileTip.self])",
            "            #endif",
            "",
            "            try Tips.configure()",
            "        }",
            "        catch {",
            "            print(\"Error initializing TipKit \\(error.localizedDescription)\")",
            "        }",
            "    }",
            "}"
