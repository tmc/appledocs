        {
          "inlineContent": [
            {
              "text": "Use this option to to reset your parameter to a default value between app launches.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct LandmarksUser {",
            "    // Define the user interaction you want to use for a display rule.",
            "    @Parameter(.transient)",
            "    static var lastViewedLandmark: Landmark? = nil",
            "}"
          ],
