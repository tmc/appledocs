      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this option to automatically invalidate a tip after it has appeared a specified number of times.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "By default tips have no maximum display count.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct FavoriteBackyardTip: Tip {",
            "    var options: [any Option] {",
            "        // Tip will only appear 3 times before it is automatically invalidated.",
            "        MaxDisplayCount(3)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
