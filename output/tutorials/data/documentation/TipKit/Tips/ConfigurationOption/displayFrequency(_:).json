    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "displayFrequency",
              "type": "codeVoice"
            },
            {
              "text": " to control how often new tips are displayed.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example, if display frequency is set to ",
              "type": "text"
            },
            {
              "code": ".daily",
              "type": "codeVoice"
            },
            {
              "text": " and your ",
              "type": "text"
            },
            {
              "code": "FavoriteLandmarkTip",
              "type": "codeVoice"
            },
            {
              "text": " is displayed, no new tips",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "will be shown for at least 24 hours.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Display frequency only applies to tips that have not appeared.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Previously displayed tips will still appear if their display ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.TipKit/documentation/TipKit/Tip/rules",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " are satisfied.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Individual tips can override this behavior by specifying ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.TipKit/documentation/TipKit/Tips/IgnoresDisplayFrequency",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " in their ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.TipKit/documentation/TipKit/Tip/options",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The default value for this option is ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.TipKit/documentation/TipKit/Tips/ConfigurationOption/DisplayFrequency/immediate",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Display frequency can be set using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.TipKit/documentation/TipKit/Tips/configure(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@main",
            "struct SampleApp: App {",
            "    init() {",
            "        do {",
            "            // Configure your tips with a daily display frequency.",
            "            try Tips.configure([",
            "                .displayFrequency(.daily)",
            "            ])",
            "        }",
            "        catch {",
            "            // Handle TipKit errors",
            "            print(\"Error initializing TipKit \\(error.localizedDescription)\")",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "anchor": "Display-Frequency-Values",
          "level": 3,
          "text": "Display Frequency Values",
          "type": "heading"
        },
        {
