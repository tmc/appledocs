    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "datastoreLocation",
              "type": "codeVoice"
            },
            {
              "text": " to change the on-disk location of your tips persistent storage.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "By default ",
              "type": "text"
            },
            {
              "code": "URL.applicationSupportDirectory",
              "type": "codeVoice"
            },
            {
              "text": " is used on macOS, iOS, watchOS, and visionOS.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "On tvOS, ",
              "type": "text"
            },
            {
              "code": "URL.cachesDirectory",
              "type": "codeVoice"
            },
            {
              "text": " is used by default in conjunction with ",
              "type": "text"
            },
            {
              "code": "UserDefaults",
              "type": "codeVoice"
            },
            {
              "text": " to manage tip statuses.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@main",
            "struct SampleApp: App {",
            "    init() {",
            "        do {",
            "            // Save the tips datastore in a group container.",
            "            try Tips.configure([",
            "                .datastoreLocation(.groupContainer(identifier: \"group.com.apple.TipKitTrails\"))",
            "            ])",
            "        }",
            "        catch {",
            "            // Handle TipKit errors",
            "            print(\"Error initializing TipKit \\(error.localizedDescription)\")",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
