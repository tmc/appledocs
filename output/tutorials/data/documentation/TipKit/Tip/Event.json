    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use an event when you want to track an action that can occur one or more times in your app (such as a user logging in).",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Then use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.TipKit/documentation/TipKit/Tips/Event/donate()",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to donate to the event when the action occurs, increasing the event count by one.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "In order to remain performant, by default events only query their most recent 1000 donations.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "anchor": "Creating-an-event-with-no-associated-donation-value",
          "level": 3,
          "text": "Creating an event with no associated donation value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The example below creates a ",
              "type": "text"
            },
            {
              "code": "landmarksAppDidOpen",
              "type": "codeVoice"
            },
            {
              "text": " event with no associated donation value and donates it anytime ",
              "type": "text"
            },
            {
              "code": "ContentView",
              "type": "codeVoice"
            },
            {
              "text": " appears:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct LandmarkTips: App {",
            "    static let landmarksAppDidOpen = Tips.Event(id: \"landmarksAppDidOpen\")",
            "",
            "    var body: some Scene {",
            "        WindowGroup {",
            "            ContentView()",
            "                .onAppear { Self.landmarksAppDidOpen.sendDonation() }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The example below creates a display rule for ",
              "type": "text"
            },
            {
              "code": "LandmarkFeatureTip",
              "type": "codeVoice"
            },
            {
              "text": " based on the ",
              "type": "text"
            },
            {
              "code": "landmarksAppDidOpen",
              "type": "codeVoice"
            },
            {
              "text": " event.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct LandmarkFeatureTip: Tip {",
            "    var rules: [Rule] {",
            "        // Tip will only display when the landmarksAppDidOpen event has been donated 3 or more times in the last week.",
            "        #Rule(LandmarkTips.landmarksAppDidOpen) {",
            "            $0.donations.donatedWithin(.week).count >= 3",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "anchor": "Creating-an-event-with-an-associated-donation-value",
          "level": 3,
          "text": "Creating an event with an associated donation value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The example below creates a ",
              "type": "text"
            },
            {
              "code": "didViewLandmarkDetail",
              "type": "codeVoice"
            },
            {
              "text": " event with an associated donation value and donates it anytime the ",
              "type": "text"
            },
            {
              "code": "LandmarkDetail",
              "type": "codeVoice"
            },
            {
              "text": " appears:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct LandmarkDetail: View {",
            "    static let didViewLandmarkDetail = Tips.Event<DidViewLandmark>(id: \"didViewLandmarkDetail\")",
            "",
            "    struct DidViewLandmark: Codable, Sendable {",
            "        let landmarkID: Int",
            "        let landmarkName: String",
            "    }",
            "",
            "    var landmark: Landmark",
            "",
            "    var body: some View {",
            "        ScrollView {",
            "            MapView(coordinate: landmark.locationCoordinate)",
            "        }",
            "        .onAppear {",
            "            Self.didViewLandmarkDetail.sendDonation(.init(landmarkID: landmark.id, landmarkName: landmark.name))",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The example below creates a display rule for ",
              "type": "text"
            },
            {
              "code": "LandmarkDetailTip",
              "type": "codeVoice"
            },
            {
              "text": " based on the ",
              "type": "text"
            },
            {
              "code": "didViewLandmarkDetail",
              "type": "codeVoice"
            },
            {
              "text": " event.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct LandmarkDetailTip: Tip {",
            "    var rules: [Rule] {",
            "        // Tip will only display when the didViewLandmarkDetail has been donated 3 or more times for landmarks not named \"Wilbere Bowl\".",
            "        #Rule(LandmarkDetail.didViewLandmarkDetail) {",
            "            $0.donations.filter({ $0.landmarkName != \"Wilbere Bowl\" }).count > 3",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
