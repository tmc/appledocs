    "title": "Hiding Code from a Playground Page"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can hide code that’s unrelated to the main content of a playground page, like",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "calls to page setup functions, code for assessing the learner’s progress, and functions",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in editable examples. Hidden code is still executed when the learner runs the playground.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Place code you want to hide between the ",
              "type": "text"
            },
            {
              "code": "hidden-code",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "end-hidden-code",
              "type": "codeVoice"
            },
            {
              "text": " delimiters.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "hiding-code-from-a-playground-page-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following shows the raw syntax for a playground page with hidden code.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "//#-hidden-code",
            "import UIKit",
            "import PlaygroundSupport",
            "let viewRect = CGRect(x: 0, y: 0, width: 100 , height: 400)",
            "let theView = CustomView(frame: viewRect)",
            "PlaygroundPage.current.liveView = theView",
            "//#-end-hidden-code",
            "//#-editable-code",
            "theView.markColor = UIColor.darkGray",
            "theView.isChecked = true",
            "//#-end-editable-code"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following shows how the previous code appears when it’s rendered in the playground",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "page source editor.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "theView.markColor = UIColor.darkGray",
            "theView.isChecked = true"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
