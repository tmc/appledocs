{
  "abstract": [
    {
      "text": "Extend the functionality of your app playground by finding and adding a publicly available Swift package.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
