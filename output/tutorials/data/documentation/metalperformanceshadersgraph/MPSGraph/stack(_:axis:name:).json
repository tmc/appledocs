      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Stacks all input tensors along ",
              "type": "text"
            },
            {
              "code": "axis",
              "type": "codeVoice"
            },
            {
              "text": " into a result tensor of ",
              "type": "text"
            },
            {
              "code": "rank + 1",
              "type": "codeVoice"
            },
            {
              "text": ". Tensors must be broadcast",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "compatible along all dimensions except ",
              "type": "text"
            },
            {
              "code": "axis",
              "type": "codeVoice"
            },
            {
              "text": ", and have the same type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
