        },
        {
          "inlineContent": [
            {
              "text": "A valid ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.metalperformanceshadersgraph/documentation/MetalPerformanceShadersGraph/MPSGraphTensor",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object",
              "type": "text"
            }
          ],
