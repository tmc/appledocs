            {
              "inlineContent": [
                {
                  "text": "A sparseDescriptor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sparseDescriptor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of input tensors as [sparseVals, indexTensor0, indexTensor1].",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inputTensorArray"
        },
        {
