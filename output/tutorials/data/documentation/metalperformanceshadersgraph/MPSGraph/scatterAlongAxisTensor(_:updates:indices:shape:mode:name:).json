    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Scatter values from ",
              "type": "text"
            },
            {
              "code": "updatesTensor",
              "type": "codeVoice"
            },
            {
              "text": " along the specified ",
              "type": "text"
            },
            {
              "code": "axis",
              "type": "codeVoice"
            },
            {
              "text": " at indices in ",
              "type": "text"
            },
            {
              "code": "indicesTensor",
              "type": "codeVoice"
            },
            {
              "text": " into a result tensor.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Values are updated following ",
              "type": "text"
            },
            {
              "code": "mode",
              "type": "codeVoice"
            },
            {
              "text": ". See MPSGraphScatterMode.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The shape of ",
              "type": "text"
            },
            {
              "code": "updatesTensor",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "indicesTensor",
              "type": "codeVoice"
            },
            {
              "text": " must match. ",
              "type": "text"
            },
            {
              "code": "shape",
              "type": "codeVoice"
            },
            {
              "text": " must match except at ",
              "type": "text"
            },
            {
              "code": "axis",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The shape of the result tensor is equal to ",
              "type": "text"
            },
            {
              "code": "shape",
              "type": "codeVoice"
            },
            {
              "text": " and initialized with an initial value corresponding to ",
              "type": "text"
            },
            {
              "code": "mode",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "If an index is out of bounds of ",
              "type": "text"
            },
            {
              "code": "shape",
              "type": "codeVoice"
            },
            {
              "text": " along ",
              "type": "text"
            },
            {
              "code": "axis",
              "type": "codeVoice"
            },
            {
              "text": " the update value is skipped.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
