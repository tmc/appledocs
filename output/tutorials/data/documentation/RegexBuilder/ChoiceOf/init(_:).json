          "content": [
            {
              "inlineContent": [
                {
                  "text": "A builder closure that declares a list of regex",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "components, each of which can be exclusively matched.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "builder"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In this example, ",
              "type": "text"
            },
            {
              "code": "regex",
              "type": "codeVoice"
            },
            {
              "text": " successfully matches either a ",
              "type": "text"
            },
            {
              "code": "\"CREDIT\"",
              "type": "codeVoice"
            },
            {
              "text": " or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "\"DEBIT\"",
              "type": "codeVoice"
            },
            {
              "text": " substring:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let regex = Regex {",
            "    ChoiceOf {",
            "        \"CREDIT\"",
            "        \"DEBIT\"",
            "    }",
            "}",
            "let match = try regex.prefixMatch(in: \"DEBIT    04032020    Payroll $69.73\")",
            "print(match?.0 as Any)",
            "// Prints \"DEBIT\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
