        {
          "inlineContent": [
            {
              "text": "For example, you can use the ",
              "type": "text"
            },
            {
              "code": "inverted",
              "type": "codeVoice"
            },
            {
              "text": " property to create a character",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "class that excludes a specific group of characters:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let validCharacters = CharacterClass(\"a\"...\"z\", .anyOf(\"-_\"))",
            "let invalidCharacters = validCharacters.inverted",
            "",
            "let username = \"user123\"",
            "if username.contains(invalidCharacters) {",
            "    print(\"Invalid username: '\\(username)'\")",
            "}",
            "// Prints \"Invalid username: 'user123'\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
