      "kind": "declarations"
    },
    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The point in 2D view space to project onto a plane.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The coordinate space for this point has its origin is in the upper left corner and a size matching the ",
                  "type": "text"
                },
                {
                  "code": "viewportSize",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "point"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A transform matrix specifying the position and orientation of a plane (with infinite extent) in 3D world space. The plane is the xz-plane of the local coordinate space this transform defines.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "planeTransform"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The orientation in which the camera image is to be presented.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orientation"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The size, in points, of the view in which the camera image is to be presented.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "viewportSize"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The 3D point in world space where a ray projected from the specified 2D point intersects the specified plane, or ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " if the ray does not intersect the plane.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If you display AR content with SceneKit, the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.arkit/documentation/ARKit/ARSCNView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " class provides an otherwise equivalent ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.arkit/documentation/ARKit/ARSCNView/unprojectPoint(_:ontoPlane:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method that requires fewer parameters (because the view can infer its orientation and size).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
