      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object that defines motion and scene tracking behaviors for the session.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "configuration"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Options affecting how existing session state (if any) transitions to the new configuration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If the session is running for the first time, this parameter has no effect.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        }
      ]
    },
    {
