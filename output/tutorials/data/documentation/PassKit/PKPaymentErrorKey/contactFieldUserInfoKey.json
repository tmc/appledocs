    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.passkit/documentation/PassKit/PKContactField",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for the values to use with this key.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use this key with the error code ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.passkit/documentation/PassKit/PKPaymentError/Code/shippingContactInvalidError",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to indicate an error in the name, email address, phone number, or the shipping address as a whole.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use this key with the error code ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.passkit/documentation/PassKit/PKPaymentError/billingContactInvalidError",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": "  to indicate an error with the billing address as a whole, or billing name.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example code in ",
              "type": "text"
            },
            {
              "identifier": "https://developer.apple.com/library/archive/qa/qa1639/_index.html#//apple_ref/doc/uid/DTS40008751-CH1-SOURCECODE2",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " shows the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.passkit/documentation/PassKit/PKPaymentErrorKey/contactFieldUserInfoKey",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " used to indicate a phone number error.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows an error indicating a problem with the shipping contactâ€™s phone number.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let phoneError = NSError.init(domain: PKPaymentErrorDomain,",
            "                               code: PKPaymentError.shippingContactInvalidError.rawValue,",
            "                               userInfo: [NSLocalizedDescriptionKey:\"Phone number is invalid\",",
            "                               PKPaymentErrorKey.contactFieldUserInfoKey:PKContactField.phoneNumber])"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
