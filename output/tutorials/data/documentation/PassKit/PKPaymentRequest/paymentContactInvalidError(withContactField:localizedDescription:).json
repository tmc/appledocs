      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value from ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.passkit/documentation/PassKit/PKContactField",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that indicates which part of the contact information has an error.",
                  "type": "text"
                }
              ],
