  "primaryContentSections": [
    {
      "details": {
        "ideTitle": "Collection Purposes",
        "name": "NSPrivacyCollectedDataTypePurposes",
        "platforms": [],
        "titleStyle": "symbol",
        "value": [
          {
            "arrayMode": true,
            "baseType": "string "
          }
        ]
      },
      "kind": "details",
      "title": "Details"
    },
    {
      "kind": "possibleValues",
      "title": "Possible Values",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Third-party advertising; such as displaying third-party ads in your app, or sharing data with entities who display third-party ads.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "NSPrivacyCollectedDataTypePurposeThirdPartyAdvertising"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Developer’s advertising or marketing; such as displaying first-party ads in your app, sending marketing communications directly to your users, or sharing data with entities who will display your ads.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "NSPrivacyCollectedDataTypePurposeDeveloperAdvertising"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Analytics — using data to evaluate user behavior, including to understand the effectiveness of existing product features, plan new features, or measure audience size or characteristics.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "NSPrivacyCollectedDataTypePurposeAnalytics"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Product personalization — customizing what the user sees, such as a list of recommended products, posts, or suggestions.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "NSPrivacyCollectedDataTypePurposeProductPersonalization"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "App functionality; such as to authenticate the user, enable features, prevent fraud, implement security measures, ensure server up-time, minimize app crashes, improve scalability and performance, or perform customer support.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "NSPrivacyCollectedDataTypePurposeAppFunctionality"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Any other purposes not listed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "NSPrivacyCollectedDataTypePurposeOther"
        }
      ]
    },
    {
