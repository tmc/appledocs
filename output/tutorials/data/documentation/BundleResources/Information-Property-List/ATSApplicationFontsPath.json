      "details": {
        "ideTitle": "Application fonts resource path",
        "name": "ATSApplicationFontsPath",
        "platforms": [],
        "titleStyle": "symbol",
        "value": [
          {
            "arrayMode": false,
            "baseType": "string"
          }
        ]
      },
      "kind": "details",
      "title": "Details"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If you set this key, the system allows the app in the bundle to use the fonts at the specified path. Set this key to the path relative to the bundle’s ",
              "type": "text"
            },
            {
              "code": "Resources",
              "type": "codeVoice"
            },
            {
              "text": " folder. For example, if the fonts are in ",
              "type": "text"
            },
            {
              "code": ".../Resources/MyFonts",
              "type": "codeVoice"
            },
            {
              "text": ", set this key to ",
              "type": "text"
            },
            {
              "code": "MyFonts/",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
