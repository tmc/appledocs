    {
      "kind": "possibleValues",
      "title": "Possible Values",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Use version 1 of the API.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Use version 2 of the API.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "2"
        }
      ]
    },
    {
      "attributes": [
        {
          "kind": "default",
          "value": "1"
        }
      ],
      "kind": "attributes",
      "title": "Attributes"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "This type is available in iOS 12.5, and in iOS 13.7 and later.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "iOS 13.7 introduces a new method to calculate the user’s Exposure Risk Value, described in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/ExposureNotification/ENExposureConfiguration",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". Set this value to ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": " to use this new version and its calculation method, or set this value to ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": " to use the earlier API and its calculation method. If you don’t explicitly set this value, the default is ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
