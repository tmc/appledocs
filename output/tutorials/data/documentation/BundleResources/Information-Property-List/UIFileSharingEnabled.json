  "primaryContentSections": [
    {
      "details": {
        "ideTitle": "Application supports iTunes file sharing",
        "name": "UIFileSharingEnabled",
        "platforms": [],
        "titleStyle": "symbol",
        "value": [
          {
            "arrayMode": false,
            "baseType": "boolean"
          }
        ]
      },
      "kind": "details",
      "title": "Details"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If you set this key to ",
              "type": "text"
            },
            {
              "code": "YES",
              "type": "codeVoice"
            },
            {
              "text": ", your app can share files with the user. Place the files in a ",
              "type": "text"
            },
            {
              "code": "Documents",
              "type": "codeVoice"
            },
            {
              "text": " folder located in the app’s home directiory. The default value is ",
              "type": "text"
            },
            {
              "code": "NO",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
