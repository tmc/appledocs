    "doc://com.apple.bundleresources/documentation/BundleResources/Information-Property-List/NSUpdateSecurityPolicy": {
      "abstract": [
        {
          "text": "A dictionary that identifies which apps or installer packages the operating system allows to write to the app’s bundle.",
          "type": "text"
