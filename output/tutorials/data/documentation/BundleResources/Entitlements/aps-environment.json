  "primaryContentSections": [
    {
      "details": {
        "ideTitle": "APS Environment Entitlement",
        "name": "aps-environment",
        "platforms": [],
        "titleStyle": "title",
        "value": [
          {
            "arrayMode": false,
            "baseType": "string"
          }
        ]
      },
      "kind": "details",
      "title": "Details"
    },
    {
      "kind": "possibleValues",
      "title": "Possible Values",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The APNs development environment.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "development"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The APNs production environment.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "production"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This key specifies whether to use the development or production Apple Push Notification service (APNs) environment when registering for push notifications.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Xcode sets the value of the entitlement based on your app’s current provisioning profile. For example, if you’re using a development provisioning profile, Xcode sets the value to ",
              "type": "text"
            },
            {
              "code": "development",
              "type": "codeVoice"
            },
            {
              "text": ". Production provisioning profile and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/AppStoreConnectAPI/prerelease-versions-and-beta-testers",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " use ",
              "type": "text"
            },
            {
              "code": "production",
              "type": "codeVoice"
            },
            {
              "text": ". These default settings can be modified. The ",
              "type": "text"
            },
            {
              "code": "development",
              "type": "codeVoice"
            },
            {
              "text": " environment is also referred to as the ",
              "type": "text"
            },
            {
              "code": "sandbox",
              "type": "codeVoice"
            },
            {
              "text": " environment.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use this entitlement for both the UserNotifications and PushKit frameworks.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To add this entitlement to your app, enable the Push Notifications capability in Xcode.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
