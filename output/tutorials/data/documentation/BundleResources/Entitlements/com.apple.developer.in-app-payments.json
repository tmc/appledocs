  "primaryContentSections": [
    {
      "details": {
        "ideTitle": "Merchant IDs Entitlement",
        "name": "com.apple.developer.in-app-payments",
        "platforms": [],
        "titleStyle": "title",
        "value": [
          {
            "arrayMode": true,
            "baseType": "string "
          }
        ]
      },
      "kind": "details",
      "title": "Details"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The value for this key is an array of strings containing the merchant IDs—typically in reverse domain name notation, starting with the string ‘",
              "type": "text"
            },
            {
              "code": "merchant",
              "type": "codeVoice"
            },
            {
              "text": "’.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To add this entitlement, enable the Apple Pay capability in Xcode and select the merchant IDs you want to use in your app. Alternatively, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/PassKit/setting-up-apple-pay",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for how to create merchant IDs in your developer account.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
