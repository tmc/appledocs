  "primaryContentSections": [
    {
      "details": {
        "ideTitle": "ClassKit Environment Entitlement",
        "name": "com.apple.developer.ClassKit-environment",
        "platforms": [],
        "titleStyle": "title",
        "value": [
          {
            "arrayMode": false,
            "baseType": "string"
          }
        ]
      },
      "kind": "details",
      "title": "Details"
    },
    {
      "kind": "possibleValues",
      "title": "Possible Values",
      "values": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The environment used to develop and test your app locally, without requiring a Managed Apple ID issued by an educational institution.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "development"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The environment used by customers of your app who have a Managed Apple ID. This enviroment enables teachers and students to share data through iCloud.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "production"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This key specifies the ClassKit environment your app uses to share data with Appleâ€™s Schoolwork app.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To support testing locally, Xcode sets the value to ",
              "type": "text"
            },
            {
              "code": "development",
              "type": "codeVoice"
            },
            {
              "text": " by default. When you upload your app to the App Store, Xcode changes the value to ",
              "type": "text"
            },
            {
              "code": "production",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To add this entitlement to your app, enable the ClassKit capability in Xcode.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
