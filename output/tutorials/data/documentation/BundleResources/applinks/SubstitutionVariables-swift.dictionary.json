            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "code": "$(xdigit)",
              "type": "codeVoice"
            },
            {
              "text": " — All hexadecimal digits: ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": ", …, ",
              "type": "text"
            },
            {
              "code": "F",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "f",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "code": "$(region)",
              "type": "codeVoice"
            },
            {
              "text": " — All ISO regions in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/locale/2293271-isoregioncodes",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", such as ",
              "type": "text"
            },
            {
              "code": "CA",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "UK",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "US",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "code": "$(lang)",
              "type": "codeVoice"
            },
            {
              "text": " — All ISO languages in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/locale/2293744-isolanguagecodes",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", such as ",
              "type": "text"
            },
            {
              "code": "ar",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "en",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "zh",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This example code shows how to use universal links substitution variables in an association file:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "{",
            "  \"applinks\": {",
            "    \"substitutionVariables\": {",
            "      \"food\": [ \"burrito\", \"pizza\", \"sushi\", \"samosa\" ]",
            "    },",
            "    \"details\": [{",
            "      \"appIDs\": [ ... ],",
            "      \"components\": [",
            "        { \"/\" : \"/$(lang)_$(region)/$(food)/\" }",
            "      ]",
            "    }]",
            "  }",
            "}"
          ],
          "syntax": "javascript",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
