          "content": [
            {
              "inlineContent": [
                {
                  "text": "The default arrangement for the tab view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "defaultPlacement"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier is only effective on iPadOS in the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "TabViewStyle/sidebarAdaptable",
              "type": "codeVoice"
            },
            {
              "text": " style. In any other configuration,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the system ignores it.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows a ",
              "type": "text"
            },
            {
              "code": "TabView",
              "type": "codeVoice"
            },
            {
              "text": " with three tabs, where",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the tab view displays the sidebar representation when the app",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "initially launches.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView(selection: $selection) {",
            "    Tab(\"Home\", systemImage: \"house\", value: MyTab.home) {",
            "        MyHomeView()",
            "    }",
            "",
            "    Tab(\"Downloads\", systemImage: \"square.and.arrow.down.fill\",",
            "        value: MyTab.downloads",
            "    ) {",
            "        MyDownloadsView()",
            "    }",
            "",
            "    Tab(\"Browse\", systemImage: \"list.bullet\", value: MyTab.browse) {",
            "        MyBrowseView()",
            "    }",
            "}",
            ".tabViewStyle(.sidebarAdaptable)",
            ".defaultAdaptableTabBarPlacement(.sidebar)"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
