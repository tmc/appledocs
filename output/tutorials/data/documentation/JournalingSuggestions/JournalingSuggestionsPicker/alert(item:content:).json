    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding to an optional source of truth for the alert.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "if ",
                  "type": "text"
                },
                {
                  "code": "item",
                  "type": "codeVoice"
                },
                {
                  "text": " is non-",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system passes the contents to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the modifierâ€™s closure. You use this content to populate the fields",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of an alert that you create that the system displays to the user.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "item",
                  "type": "codeVoice"
                },
                {
                  "text": " changes, the system dismisses the currently displayed",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "alert and replaces it with a new one using the same process.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "item"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure returning the alert to present.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method when you need to show an alert that contains",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "information from a binding to an optional data source that you provide.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The example below shows a custom data source ",
              "type": "text"
            },
            {
              "code": "FileInfo",
              "type": "codeVoice"
            },
            {
              "text": " whose",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "properties configure the alertâ€™s ",
              "type": "text"
            },
            {
              "code": "message",
              "type": "codeVoice"
            },
            {
              "text": " field:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct FileInfo: Identifiable {",
            "    var id: String { name }",
            "    let name: String",
            "    let fileType: UTType",
            "}",
            "",
            "struct ConfirmImportAlert: View {",
            "    @State private var alertDetails: FileInfo?",
            "    var body: some View {",
            "        Button(\"Show Alert\") {",
            "            alertDetails = FileInfo(name: \"MyImageFile.png\",",
            "                                    fileType: .png)",
            "        }",
            "        .alert(item: $alertDetails) { details in",
            "            Alert(title: Text(\"Import Complete\"),",
            "                  message: Text(\"\"\"",
            "                    Imported \\(details.name) \\n File",
            "                    type: \\(details.fileType.description).",
            "                    \"\"\"),",
            "                  dismissButton: .default(Text(\"Dismiss\")))",
            "        }",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
