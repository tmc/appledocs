{
  "abstract": [
    {
      "text": "Make your CarPlay-enabled apps compatible with older system versions, such as iOS 13 and earlier.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
