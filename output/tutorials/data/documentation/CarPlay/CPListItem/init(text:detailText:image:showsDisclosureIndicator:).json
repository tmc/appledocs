      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The primary text to show in the list item cell.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "text"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Additional text to display below the primary text in the list item cell.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "detailText"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The image to display on the leading edge of the list item cell. If the image is larger than ",
                  "type": "text"
                },
                {
                  "code": "CPMaximumListItemImageSize",
                  "type": "codeVoice"
                },
                {
                  "text": ", the list item scales down the image to maximum size. If you provide an animated image, the list item uses the first image in the animation sequence.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "image"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the list item cell displays a disclosure indicator. Set to ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/swift/true",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to display the indicator on the trailing edge of the list item cell.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "showsDisclosureIndicator"
        }
      ]
    },
