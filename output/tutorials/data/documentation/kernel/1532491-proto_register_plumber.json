      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The protocol family these plumbing functions will handle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "proto_fam"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The interface family these plumbing functions will handle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "if_fam"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The function to call to attach the protocol to an interface.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "plumb"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The function to call to detach the protocol to an interface, may be NULL in which case ifnet_detach_protocol will be used to detach the protocol.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "unplumb"
        }
      ]
    },
    {
      "content": [
