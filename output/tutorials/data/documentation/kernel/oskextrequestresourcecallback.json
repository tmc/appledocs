      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tag of the request that the callback pertains to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "requestTag"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The result of the request: ",
                  "type": "text"
                },
                {
                  "code": "kOSReturnSuccess",
                  "type": "codeVoice"
                },
                {
                  "text": " if the request was fulfilled; ",
                  "type": "text"
                },
                {
                  "code": "kOSKextReturnTimeout",
                  "type": "codeVoice"
                },
                {
                  "text": " if the request has timed out; ",
                  "type": "text"
                },
                {
                  "code": "kOSKextReturnStopping",
                  "type": "codeVoice"
                },
                {
                  "text": " if the kext containing the callback address for the kext is being unloaded; or other values on error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "result"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the requested resource data. Owned by the system; the kext should make a copy if it needs to keep the data past the callback.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resourceData"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length of ",
                  "type": "text"
                },
                {
                  "code": "resourceData",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resourceDataLength"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The context pointer originally passed to ",
                  "type": "text"
                },
                {
