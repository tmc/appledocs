          "platforms": [],
          "tokens": [
            {
              "kind": "text",
              "text": "static IOAudioPort *withAttributes("
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " UInt32 portType,"
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " const char *portName = 0,"
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " UInt32 subType = 0,"
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " OSDictionary *properties = 0); "
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A readable string representing the type of port. Common port types are defined in IOAudioTypes.h and are prefixed with 'kIOAudioPortType'. Please provide feedback if there are other common port types that should be included.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "portType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A readable string representing the name of the port. For example: 'Internal Speaker', 'Line Out'. This field is optional, but useful for providing information to the application/user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "portName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Developer defined readable string representing a subtype for the port. (optional)",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "subType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Standard property list passed to the init of any new IOService. This dictionary gets stored in the registry for this instance. (optional)",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "properties"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Returns the newly allocated and initialized IOAudioPort instance.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This static method allocates a new IOAudioPort and calls initWithAttributes() on it with the parameters passed in to it.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
