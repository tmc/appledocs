        {
          "inlineContent": [
            {
              "text": "If ",
              "type": "text"
            },
            {
              "code": "Conjugate",
              "type": "codeVoice"
            },
            {
              "text": " = 1, this function multiplies the first ",
              "type": "text"
            },
            {
              "code": "N",
              "type": "codeVoice"
            },
            {
              "text": " complex elements of ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " by corresponding complex elements of ",
              "type": "text"
            },
            {
              "code": "B",
              "type": "codeVoice"
            },
            {
              "text": ". If ",
              "type": "text"
            },
            {
              "code": "Conjugate",
              "type": "codeVoice"
            },
            {
              "text": " = -1, it multiplies the complex conjugates of the first ",
              "type": "text"
            },
            {
              "code": "N",
              "type": "codeVoice"
            },
            {
              "text": " complex elements of ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " by corresponding complex elements of ",
              "type": "text"
            },
            {
              "code": "B",
              "type": "codeVoice"
            },
            {
              "text": ". In either case, the complex results are left in ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
