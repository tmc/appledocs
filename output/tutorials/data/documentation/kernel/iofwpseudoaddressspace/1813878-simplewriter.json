      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Client's callback object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "refcon"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "FireWire Write to nodeID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nodeID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "at this 'speed'.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "speed"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "with FireWire address 'addr'.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "addr"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "write 'len' bytes to nodeID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "len"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "obtain bytes from location given by 'buf'.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "buf"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Can be queried for extra info about the request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reqrefcon"
        }
