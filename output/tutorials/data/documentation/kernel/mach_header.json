    "doc://com.apple.documentation/documentation/kernel/mach_header/1525759-magic": {
      "abstract": [
        {
          "text": "An integer containing a value identifying this file as a 32-bit Mach-O file. Use the constant ",
          "type": "text"
        },
        {
          "code": "MH_MAGIC",
          "type": "codeVoice"
        },
        {
          "text": " if the file is intended for use on a CPU with the same endianness as the computer on which the compiler is running. The constant ",
          "type": "text"
        },
        {
          "code": "MH_CIGAM",
          "type": "codeVoice"
        },
        {
          "text": " can be used when the byte ordering scheme of the target machine is the reverse of the host CPU.",
          "type": "text"
        }
      ],
