    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The mbuf to attach this tag to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mbuf"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A module identifier returned by mbuf_tag_id_find.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "module_id"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A 16 bit type value. For a given module_id, you can use a number of different tag types.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length, in bytes, to allocate for storage that will be associated with this tag on this mbuf.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "length"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Indicate whether you want to block and wait for memory if memory is not immediately available.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "how"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Upon successful return, *data_p will point to the buffer allocated for the mtag.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data_p"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "0 upon success otherwise the errno error.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Allocate an mbuf tag. Mbuf tags allow various portions of the stack to tag mbufs with data that will travel with the mbuf through the stack.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Tags may only be added to mbufs with packet headers (MBUF_PKTHDR flag is set). Mbuf tags are freed when the mbuf is freed or when mbuf_tag_free is called.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
