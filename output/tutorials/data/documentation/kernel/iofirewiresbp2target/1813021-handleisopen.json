          "content": [
            {
              "inlineContent": [
                {
                  "text": "If non-zero, isOpen returns the open state for that client. If zero is passed, isOpen returns the open state for all clients.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "forClient"
        }
