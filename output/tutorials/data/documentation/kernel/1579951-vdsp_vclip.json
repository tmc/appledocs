    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This performs the following operation:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "for (n = 0; n < N; ++n) {",
            "    if (A[n*IA] < *B)",
            "        D[n*ID] = *B;",
            "    else if (A[n*IA] > *C)",
            "        D[n*ID] = *C;",
            "    else",
            "        D[n*ID] = A[n*IA];",
            "}"
          ],
          "metadata": {
            "anchor": "3921244",
            "title": "Listing 1"
          },
          "syntax": "occ",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Elements of ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " are copied to ",
              "type": "text"
            },
            {
              "code": "D",
              "type": "codeVoice"
            },
            {
              "text": ", while clipping elements that are outside the interval [",
              "type": "text"
            },
            {
              "code": "*B",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "*C",
              "type": "codeVoice"
            },
            {
              "text": "] to the endpoints of the interval.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
