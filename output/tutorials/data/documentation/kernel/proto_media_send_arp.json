    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The interface the arp packet should be sent on.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ifp"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The protocol family of the addresses (PF_INET).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "protocol_family"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The arp operation (usually ARPOP_REQUEST or ARPOP_REPLY).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "arpop"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value to use for the sender hardware address field. If this is NULL, use the hardware address of the interface.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sender_hw"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value to use for the sender protocol address field. This will not be NULL.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sender_proto"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value to use for the target hardware address. If this is NULL, the target hardware address in the ARP packet should be NULL and the link-layer destination for the back should be a broadcast. If this is not NULL, this value should be used for both the link-layer destination and the target hardware address.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "target_hw"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The target protocol address. This will not be NULL.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "target_proto"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Return zero on success or an errno error value on failure.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "proto_media_send_arp is called by the stack to generate an ARP packet. This field is currently only used with IP. This function should inspect the parameters and transmit an arp packet using the information passed in.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
