  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "occ"
          ],
          "platforms": [],
          "tokens": [
            {
              "kind": "text",
              "text": "virtual IOReturn createAGPSpace("
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " IOOptionBitsoptions, "
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " IOPhysicalAddress *address, "
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " IOPhysicalLength *length ); "
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "No options are currently defined, pass zero.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The physical range allocated for the AGP space is passed back to the caller.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "address"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An in/out parameter - the caller sets the devices maximum AGP addressing and the actual size created is passed back.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "length"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Returns an IOReturn code indicating success or failure.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method should be called by the driver for the AGP primary device to set the size of the space and enable AGP transactions. It will destroy any AGP space currently allocated.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
