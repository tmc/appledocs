      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "physio() allows I/O directly from a device to user-space memory. It waits for all I/O to complete before returning.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
