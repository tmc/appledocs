    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function calculates the mean of the signed squares of the first ",
              "type": "text"
            },
            {
              "code": "N",
              "type": "codeVoice"
            },
            {
              "text": " elements of ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " and writes the result to ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "media-3921242",
              "metadata": {
                "anchor": "3921242",
                "title": "Figure 1"
              },
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The operation is: ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "C[0] = sum(A[n] * |A[n]|, 0 <= n < N);"
          ],
          "metadata": {
            "anchor": "3921231",
            "title": "Listing 1"
          },
          "syntax": "other",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following code shows an example of using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1450006-vdsp_svs",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "let a: [Float] = [-1.5, 2.25, 3.6,",
            "                  0.2, -0.1, -4.3]",
            "let n = vDSP_Length(a.count)",
            "",
            "var c: Float = .nan",
            "",
            "vDSP_svs(a,",
            "         stride,",
            "         &c,",
            "         n)",
            "",
            "// Prints \"sum of signed squares -2.6875\".",
            "print(String(format: \"sum of signed squares %.4f\", c))"
          ],
          "metadata": {
            "anchor": "3921232",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
