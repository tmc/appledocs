      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The data structure that stores the biquadratic filter state and coefficients.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Parameter 0"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the single-precision interleaved stereo input data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Parameter 1"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the single-precision interleaved stereo output data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Parameter 2"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of elements per channel.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Parameter 3"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this function to apply a stereo biquadratic IIR filter to the input and write the filtered result to the output. The input and output are interleaved so that even elements correspond to the left channel and odd elements correspond to the right channel.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
