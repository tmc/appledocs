          "content": [
            {
              "inlineContent": [
                {
                  "text": "The load tag of the kext to be retained. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/kernel/1508336-oskextgetcurrentloadtag",
                  "isActive": true,
                  "type": "reference"
