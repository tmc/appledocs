      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A variable for storing the properties. On return, the variable contains a dictionary with the properties, or ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if the service has no registered properties. If this method returns a valid dictionary, you are responsible for releasing that dictionary when you are finished with it. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "It is a programmer error to specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " or an invalid pointer for this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "properties"
        }
      ]
    },
