    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function calculates the maximum value of the first ",
              "type": "text"
            },
            {
              "code": "N",
              "type": "codeVoice"
            },
            {
              "text": " elements of ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " and writes the result to ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "media-3921224",
              "metadata": {
                "anchor": "3921224",
                "title": "Figure 1"
              },
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The operation is: ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "*C = -INFINITY;",
            "for (n = 0; n < N; ++n)",
            "    if (*C < A[n*I])",
            "        *C = A[n*I];"
          ],
          "metadata": {
            "anchor": "3921210",
            "title": "Listing 1"
          },
          "syntax": "other",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following code shows an example of using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1450621-vdsp_maxv",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "let a: [Float] = [-1.5, 2.25, 3.6,",
            "                  0.2, -0.1, -4.3]",
            "let n = vDSP_Length(a.count)",
            "",
            "var c: Float = .nan",
            "",
            "vDSP_maxv(a,",
            "          stride,",
            "          &c,",
            "          n)",
            "",
            "print(\"max\", c) // Prints \"max 3.6\""
          ],
          "metadata": {
            "anchor": "3921211",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
