    "doc://com.apple.documentation/documentation/kernel/kern_ctl_reg/1809212-ctl_name": {
      "abstract": [
        {
          "text": "A Bundle ID string of up to MAX_KCTL_NAME bytes (including the ending zero). This string should not be empty.",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/kernel/kern_ctl_reg/1809212-ctl_name",
      "kind": "symbol",
