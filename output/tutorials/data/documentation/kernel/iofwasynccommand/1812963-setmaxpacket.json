          "content": [
            {
              "inlineContent": [
                {
                  "text": "Maximum packet size in bytes. If the maxsize is 4 then quadlet transfers will be used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxBytes"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Sets the maximum size for block transfers used by the command. The command is initialized to use the maximum packet size calculated from the device's PHY speed, bus info block and the bus topology. Call this method before calling submit().",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
