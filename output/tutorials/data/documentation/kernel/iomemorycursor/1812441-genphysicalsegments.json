          "tokens": [
            {
              "kind": "text",
              "text": "virtual UInt32 genPhysicalSegments( "
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " IOMemoryDescriptor *descriptor, "
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " IOByteCount fromPosition, "
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " void *segments, "
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " UInt32 maxSegments, "
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " UInt32 maxTransferSize = 0, "
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": " IOByteCount *transferSize = 0); "
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "IOMemoryDescriptor that describes the data associated with an I/O request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "descriptor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Starting location of the I/O within a memory descriptor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fromPosition"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Void pointer to base of output physical scatter/gather list. Always passed directly onto the SegmentFunction without interpretation by the cursor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "segments"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Maximum number of segments that can be written to segments array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxSegments"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Maximum transfer size is limited to that many bytes, otherwise it defaults to the maximum transfer size specified when the memory cursor was initialized.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxTransferSize"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Pointer to an IOByteCount variable that can contain the total size of the transfer being described. Defaults to 0 indicating that no transfer size need be returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transferSize"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the descriptor is exhausted of memory, a zero is returned, otherwise the number of segments that were filled in is returned.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Generates a list of physical segments from the given memory descriptor, relative to the current position of the descriptor.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
