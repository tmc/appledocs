    "doc://com.apple.documentation/documentation/kernel/vnop_searchfs_args/1807800-pl_offset": {
      "abstract": [
        {
          "text": "Offset in UPL from which to start paging out data. Under the new VFC_VFSVNOP_PAGEOUTV2 semantics, this is the offset in the range specified that must be paged out if the associated page is dirty.",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/kernel/vnop_searchfs_args/1807800-pl_offset",
      "kind": "symbol",
      "role": "pseudoSymbol",
