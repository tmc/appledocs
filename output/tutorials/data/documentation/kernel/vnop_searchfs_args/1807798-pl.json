    },
    "doc://com.apple.documentation/documentation/kernel/vnop_searchfs_args/1807802-f_offset": {
      "abstract": [
        {
          "text": "Offset in file of data needing to be paged out. Under the new VFC_VFSVNOP_PAGEOUTV2 semantics, this represents the offset in the file where we should start looking for dirty pages.",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/kernel/vnop_searchfs_args/1807802-f_offset",
