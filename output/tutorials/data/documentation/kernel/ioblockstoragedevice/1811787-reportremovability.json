      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Pointer to returned result. True indicates that the media is removable; False indicates the media is not removable.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isRemovable"
        }
      ]
    },
    {
      "content": [
