    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function calculates the scalar value ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": " divied by the first ",
              "type": "text"
            },
            {
              "code": "N",
              "type": "codeVoice"
            },
            {
              "text": " elements of ",
              "type": "text"
            },
            {
              "code": "B",
              "type": "codeVoice"
            },
            {
              "text": ", writing the result to ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "media-3921195",
              "metadata": {
                "anchor": "3921195",
                "title": "Figure 1"
              },
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The operation is:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " for (n = 0; n < N; ++n)",
            "    C[n] = A / B[n];"
          ],
          "metadata": {
            "anchor": "3921184",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following code shows an example of using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/accelerate/1450680-vdsp_vsdiv",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let stride = vDSP_Stride(1)",
            "",
            "var a: Float = 2",
            "let b: [Float] = [1, 2, 4, 5]",
            "",
            "",
            "let n = vDSP_Length(b.count)",
            "",
            "var c = [Float](repeating: 0,",
            "                count: b.count)",
            "",
            "vDSP_svdiv(&a,",
            "           b, stride,",
            "           &c, stride,",
            "           n)",
            "",
            "// Prints \"[2, 1, 0.5, 0.4]\"",
            "print(c)"
          ],
          "metadata": {
            "anchor": "3921199",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
