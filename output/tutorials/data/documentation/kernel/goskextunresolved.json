        {
          "inlineContent": [
            {
              "text": "A kext must test a weak symbol before using it. A weak symbol is only safe to use if it is not equal to ",
              "type": "text"
            },
            {
              "code": "gOSKextUnresolved",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Example for a weak symbol named ",
              "type": "text"
            },
            {
              "code": "foo",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " ",
            " ",
            "      if (&foo != gOSKextUnresolved) {",
            "          foo();",
            "      } else {",
            "          printf(\"foo() is not supported\\n\");",
            "      }",
            " ",
            " "
          ],
