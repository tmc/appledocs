    "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.Notification": {
      "abstract": [
        {
          "text": "A ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.Notification",
          "isActive": true,
          "type": "reference"
        },
        {
