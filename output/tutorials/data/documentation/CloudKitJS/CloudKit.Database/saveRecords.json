            {
              "inlineContent": [
                {
                  "text": "A dictionary containing options to use when saving records. Possible dictionary keys are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "header": "row",
              "rows": [
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "Key",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "Description",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ],
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "code": "zoneID",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "A ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.ZoneID",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " or zone name (",
                          "type": "text"
                        },
                        {
                          "code": "String",
                          "type": "codeVoice"
                        },
                        {
                          "text": ") that identifies the record zone in the database where you want to perform the operation. The default is the database default zone.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ],
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "code": "desiredKeys",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "An array of strings containing record field names that limits the amount of data returned in this operation. Only the fields specified in the array are returned. The default is ",
                          "type": "text"
                        },
                        {
                          "code": "null",
                          "type": "codeVoice"
                        },
                        {
                          "text": " which fetches all record fields.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ],
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "code": "numbersAsStrings",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "A Boolean value indicating whether number fields should be represented by strings. The default value is ",
                          "type": "text"
                        },
                        {
                          "code": "false",
                          "type": "codeVoice"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
