          "content": [
            {
              "inlineContent": [
                {
                  "text": "Either a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.Query",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " dictionary or a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.QueryResponse",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object describing the matching criteria.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "query"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary containing options to use when fetching records. Possible dictionary keys are:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "header": "row",
              "rows": [
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "Key",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "Description",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ],
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "code": "zoneID",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "A ",
                          "type": "text"
                        },
                        {
                          "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.ZoneID",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": " or zone name (",
                          "type": "text"
                        },
                        {
                          "code": "String",
                          "type": "codeVoice"
                        },
                        {
                          "text": ") that identifies the record zone in the database where you want to perform the operation. The default is the database default zone.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ],
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "code": "resultsLimit",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "The maximum number of records to fetch. The default is the maximum number of records in a response that is allowed, described in ",
                          "type": "text"
                        },
                        {
                          "identifier": "https://developer.apple.com/library/archive/documentation/DataManagement/Conceptual/CloudKitWebServicesReference/PropertyMetrics.html#//apple_ref/doc/uid/TP40015240-CH23",
                          "isActive": true,
                          "type": "reference"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ],
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "code": "continuationMarker",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "Marks the location of the last batch of results. Use this key when the results of a previous fetch exceed the maximum. The default value is ",
                          "type": "text"
                        },
                        {
                          "code": "null",
                          "type": "codeVoice"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ],
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "code": "desiredKeys",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "An array of strings containing record field names that limits the amount of data returned in this operation. Only the fields specified in the array are returned. The default is ",
                          "type": "text"
                        },
                        {
                          "code": "null",
                          "type": "codeVoice"
                        },
                        {
                          "text": ", which fetches all record fields.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ],
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "code": "zoneWide",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "Boolean value determining whether all zones should be searched. This key is ignored if ",
                          "type": "text"
                        },
                        {
                          "code": "zoneID",
                          "type": "codeVoice"
                        },
                        {
                          "text": " is non-",
                          "type": "text"
                        },
                        {
                          "code": "null",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". To search all zones, set to ",
                          "type": "text"
                        },
                        {
                          "code": "true",
                          "type": "codeVoice"
                        },
                        {
                          "text": ". To search the default zone only, set to ",
                          "type": "text"
                        },
                        {
                          "code": "false",
                          "type": "codeVoice"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ],
                [
                  [
                    {
                      "inlineContent": [
                        {
                          "code": "numbersAsStrings",
                          "type": "codeVoice"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  [
                    {
                      "inlineContent": [
                        {
                          "text": "A Boolean value indicating whether number fields should be represented by strings. The default value is ",
                          "type": "text"
                        },
                        {
                          "code": "false",
                          "type": "codeVoice"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                ]
              ],
              "type": "table"
            }
          ],
          "name": "options"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "Promise",
              "type": "codeVoice"
            },
            {
              "text": " object that resolves to a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.QueryResponse",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object if the operation succeeds; otherwise, a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit/CKError",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To fetch records that match some criteria, create a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.Query",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " dictionary.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var query = {",
            "    recordType: 'Artwork',",
            "    filterBy: [{",
            "        comparator: 'EQUALS',",
            "        fieldName: 'address',",
            "        fieldValue: { value: 'Fort Bragg, CA' }",
            "    }]",
            "}"
          ],
          "syntax": "javascript",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Execute the query using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.Database/performQuery",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method. To get the records that were fetched, use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.RecordsResponse/records",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.QueryResponse",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " class. Otherwise, handle any errors appropriately.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "database.performQuery(query).then(function(response) {",
            "    if (response.hasErrors) {",
            "        // Insert error handling",
            "        throw response.errors[0];",
            "    } else {",
            "        // Insert successfully fetched record code",
            "    }",
            "});"
          ],
          "syntax": "javascript",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Creating Queries That Search by Location",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can fetch records whose ",
              "type": "text"
            },
            {
              "code": "Location",
              "type": "codeVoice"
            },
            {
              "text": " field lies within a circular region by specifying the center and radius of a circle in a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.Query",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " dictionary. The ",
              "type": "text"
            },
            {
              "code": "Location",
              "type": "codeVoice"
            },
            {
              "text": " field specified in the query must be indexed for the fetch to succeed.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var query = {",
            "    recordType: 'Artwork',",
            "    filterBy: [{",
            "        fieldName: 'location',",
            "        distance: 100000,",
            "        fieldValue: {",
            "           latitude: 37.7749300,",
            "           longitude: -122.4194200",
            "       }",
            "    }]",
            "};"
          ],
          "syntax": "javascript",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Execute this query using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.Database/performQuery",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Handling Large Numbers of Results",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If the results of a fetch exceeds the maximum number of records allowed in a response, perform the fetch again until all the records are fetched. You can also specify the results limit when performing a query.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To set a results limit, pass a dictionary as the ",
              "type": "text"
            },
            {
              "code": "options",
              "type": "codeVoice"
            },
            {
              "text": " parameter to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.Database/performQuery",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method. In the dictionary, set the ",
              "type": "text"
            },
            {
              "code": "resultsLimit",
              "type": "codeVoice"
            },
            {
              "text": " key to the number of records you want to fetch for each operation. If the ",
              "type": "text"
            },
            {
              "code": "moreComing",
              "type": "codeVoice"
            },
            {
              "text": " property of the response object is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ", the results exceeded the limit and you should perform the fetch operation again until ",
              "type": "text"
            },
            {
              "code": "moreComing",
              "type": "codeVoice"
            },
            {
              "text": " is false. The next time you perform the operation, pass the response object to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.cloudkitjs/documentation/CloudKitJS/CloudKit.Database/performQuery",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method. The response object contains the fetch options you passed the first time you called the method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, perform a query limiting the results to 10 records until all records matching the query are fetched.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "database.performQuery(query, { resultsLimit: 10 }).then(function(response) {",
            "    if (response.hasErrors) {",
            "        // Insert error handling",
            "        throw response.errors[0];",
            "    } else {",
            "        // Insert successfully fetched record code",
            "        if(response.moreComing) {",
            "            return database.performQuery(response);",
            "        }",
            "    }",
            "});"
          ],
          "syntax": "javascript",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "For the maximum number of records in a response and other data limits, see ",
              "type": "text"
            },
            {
              "identifier": "https://developer.apple.com/library/archive/documentation/DataManagement/Conceptual/CloudKitWebServicesReference/PropertyMetrics.html#//apple_ref/doc/uid/TP40015240-CH23",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " in ",
              "type": "text"
            },
            {
              "identifier": "https://developer.apple.com/library/archive/documentation/DataManagement/Conceptual/CloudKitWebServicesReference/index.html#//apple_ref/doc/uid/TP40015240",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
