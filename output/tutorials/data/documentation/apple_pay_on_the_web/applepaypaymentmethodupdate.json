      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If a user changes their payment method on the payment sheet, you respond by updating the payment request details in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/apple_pay_on_the_web/applepaypaymentmethodupdate",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You must provide an updated total for the transaction in the ",
              "type": "text"
            },
            {
              "code": "newTotal",
              "type": "codeVoice"
            },
            {
              "text": " line item. You can optionally provide updated line items if the change to the payment method changes the line items.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
