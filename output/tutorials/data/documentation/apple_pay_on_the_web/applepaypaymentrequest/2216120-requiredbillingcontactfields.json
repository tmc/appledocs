      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "requiredBillingContactFields",
              "type": "codeVoice"
            },
            {
              "text": " to request the user's billing address that is associated with their payment method. The field you can request for a billing contact is shown in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/apple_pay_on_the_web/applepaypaymentrequest/2216120-requiredbillingcontactfields#2793371",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "\"requiredBillingContactFields\": [",
            "    \"postalAddress\"",
            "]"
          ],
          "metadata": {
            "anchor": "2793371",
            "title": "Listing 1"
          },
          "syntax": "javascript",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You will receive the postal address as well as the user's name after the user authorizes the transaction.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The source of the information may be the user's Me contact card, the back of credit card in Wallet, or may be entered by the user directly into the payment sheet.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
