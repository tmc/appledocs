    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Line items appear in the payment sheet.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following code is an example of a line item for a $20 charge that recurs every six months starting on 01/01/2022 and ending on 01/01/2024:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "{",
            "    \"label\": \"Subscription\",",
            "    \"amount\": \"20.00\",",
            "    \"type\": \"final\",",
            "    \"paymentTiming\": \"recurring\",",
            "    \"recurringPaymentStartDate\": new Date(\"2022-01-01T00:00:00\"),",
            "    \"recurringPaymentIntervalUnit\": \"month\",",
            "    \"recurringPaymentIntervalCount\": 6,",
            "    \"recurringPaymentEndDate\": new Date(\"2024-01-01T00:00:00\"),",
            "}"
          ],
          "metadata": {
            "anchor": "2852006",
            "title": "Listing 1"
          },
          "syntax": "javascript",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The following code is an example of a line item for a deferred charge of $50 on 07/01/2023:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "{",
            "    \"label\": \"After Trial Period\",",
            "    \"amount\": \"50.00\",",
            "    \"type\": \"final\",",
            "    \"paymentTiming\": \"deferred\",",
            "    \"deferredPaymentDate\": new Date(\"2023-07-01T00:00:00\"),",
            "}"
          ],
          "metadata": {
            "anchor": "3950364",
            "title": "Listing 2"
          },
          "syntax": "javascript",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "For examples of line items within an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/apple_pay_on_the_web/applepaypaymentrequest",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/apple_pay_on_the_web/applepaypaymentrequest/1916120-lineitems",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/apple_pay_on_the_web/applepaypaymentrequest/1916119-total",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
