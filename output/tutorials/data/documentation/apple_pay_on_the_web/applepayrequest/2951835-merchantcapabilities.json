    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The supported values for ",
              "type": "text"
            },
            {
              "code": "merchantCapabilities",
              "type": "codeVoice"
            },
            {
              "text": " are:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "supports3DS",
                      "type": "codeVoice"
                    },
                    {
                      "text": " - Required. This value must be supplied.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "supportsCredit",
                      "type": "codeVoice"
                    },
                    {
                      "text": " - Optional. If present, only transactions that are categorized as credit cards are allowed. ",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "supportsDebit",
                      "type": "codeVoice"
                    },
                    {
                      "text": " - Optional. If present, only transactions that are categorized as debit cards are allowed.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "supportsEMV",
                      "type": "codeVoice"
                    },
                    {
                      "text": " - Include this value only if you support China Union Pay transactions.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "If both or neither ",
              "type": "text"
            },
            {
              "code": "supportsCredit",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "supportsDebit",
              "type": "codeVoice"
            },
            {
              "text": " values are supplied, the transaction allows both credit and debit cards.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
