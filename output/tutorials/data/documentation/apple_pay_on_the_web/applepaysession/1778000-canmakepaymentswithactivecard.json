{
  "abstract": [
    {
      "text": "Indicates whether the device supports Apple Pay and whether the user has an active card in Wallet.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "Use the ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/apple_pay_on_the_web/applepaysession/4440085-applepaycapabilities",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " method instead. The ",
          "type": "text"
        },
        {
          "code": "canMakePaymentsWithActiveCard",
          "type": "codeVoice"
        },
        {
          "text": " method will continue to work on Safari browsers and always return ",
          "type": "text"
        },
        {
          "code": "true",
          "type": "codeVoice"
        },
        {
          "text": " on third-party browsers.",
          "type": "text"
        }
      ],
      "type": "paragraph"
    }
  ],
