      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a converter object reference. On return, the reference points to a newly created text converter object. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "newEncodingConverter"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ordered array of text encoding specifications, beginning with the source encoding specification and ending with the destination encoding specification. Each adjacent pair of text encodings must represent a conversion that is supported by the Text Encoding Converter. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inPath"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of text encoding specifications in the ",
                  "type": "text"
                },
                {
                  "code": "inPath",
                  "type": "codeVoice"
                },
                {
                  "text": " array. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inEncodings"
        }
      ]
    },
