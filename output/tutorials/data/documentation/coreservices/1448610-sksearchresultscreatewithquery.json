        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The search group to query.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inSearchGroup"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The query string to search for.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inQuery"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The category of search to perform. See the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/sksearchtype",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " enumeration for options.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inSearchType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of found items to return. Your application must pass in a positive integer value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inMaxFoundDocuments"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An application-specified context for use by the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/sksearchresultsfiltercallback",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". Can be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", but if you want to use the callback you must supply a context.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inContext"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A callback function for hit testing during searching. Can be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", in which case your application receives the returned results directly and without any custom postprocessing. If non-",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", you must supply a context. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/sksearchresultsfiltercallback",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inFilterCallBack"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A search results object.",
