          "content": [
            {
              "inlineContent": [
                {
                  "text": "The component instance from which you want error information. Your application obtains the component instance from the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1516523-opendefaultcomponent",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " function or the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1516607-opencomponent",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " function. You can use a component identifier here, but you must coerce the data type appropriately. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
