    "doc://com.apple.documentation/documentation/coreservices/componentparameters/1516330-paramsize": {
      "abstract": [
        {
          "text": "Specifies the number of bytes of parameter data for this request. The actual parameters are stored in the ",
          "type": "text"
        },
        {
          "code": "params",
          "type": "codeVoice"
        },
        {
          "text": " field.",
          "type": "text"
        }
