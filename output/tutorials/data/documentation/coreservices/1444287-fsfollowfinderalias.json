          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the file to use for a first attempt at a relative resolution; pass a pointer to the alias file's ",
                  "type": "text"
                },
                {
                  "code": "FSRef",
                  "type": "codeVoice"
                },
                {
                  "text": " for this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fromFile"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A handle to the alias record taken from the alias file's resources.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alias"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the Alias Manager attempts to mount a volume if necessary to complete the resolution of the alias.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "logon"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to an ",
                  "type": "text"
                },
                {
                  "code": "FSRef",
                  "type": "codeVoice"
                },
                {
                  "text": " structure. On return, this ",
                  "type": "text"
                },
                {
                  "code": "FSRef",
                  "type": "codeVoice"
                },
                {
                  "text": " refers to the target found by the resolution.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "target"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a Boolean value. ",
                  "type": "text"
                },
                {
                  "code": "FSFollowFinderAlias",
                  "type": "codeVoice"
                },
                {
                  "text": " sets this value to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " if it has updated the alias record.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "wasChanged"
        }
