      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A handle to the alias record from which to get information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inAlias"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a string that, on return, contains the name of the target item. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if you do not want this information returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "targetName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a string that, on return, contains the name of the volume the target resides on. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if you do not want this information returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "volumeName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer a CFString that, on return, contains the POSIX path to the target. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if you do not want this information returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pathString"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a variable of type ",
                  "type": "text"
                },
                {
                  "code": "FSAliasInfoBitmap",
                  "type": "codeVoice"
                },
                {
                  "text": ". On return, this field indicates which fields in the alias information block, specified in the ",
                  "type": "text"
                },
                {
                  "code": "info",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter, contain valid data. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/fsaliasinfobitmap",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a description of the values that may be returned here. This parameter may be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "whichInfo"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a structure of type ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/fsaliasinfo",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". On return, this structure contains information about the alias. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if you do not want this information returned.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "info"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A result code.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This function returns the requested information from the alias handle passed in the ",
              "type": "text"
            },
            {
              "code": "inAlias",
              "type": "codeVoice"
            },
            {
              "text": " parameter. The information is gathered only from the alias record, so it may not match what is on disk. No disk input/output is performed.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "FSCopyAliasInfo",
              "type": "codeVoice"
            },
            {
              "text": " function adds support for unicode filenames and filenames longer than 32 bytes. It replaces the ",
              "type": "text"
            },
            {
              "code": "GetAliasInfo",
              "type": "codeVoice"
            },
            {
              "text": " function.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
