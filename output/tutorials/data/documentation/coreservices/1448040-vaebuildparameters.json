          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Apple event to which you are adding parameters. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/appleevent",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "event"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to an ",
                  "type": "text"
                },
                {
                  "code": "AEBuildError",
                  "type": "codeVoice"
                },
                {
                  "text": " structure where additional information about any errors that occur will be saved. This is an optional parameter and you can pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if this information is not required. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aebuilderror",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "error"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "code": "AEBuild",
                  "type": "codeVoice"
                },
                {
                  "text": " format string describing the ",
                  "type": "text"
                },
                {
                  "code": "AEDesc",
                  "type": "codeVoice"
                },
                {
                  "text": " parameters to be created.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "format"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A reference to a previously defined, variable argument parameter list to use with the descriptor-string. The file ",
                  "type": "text"
                },
                {
                  "code": "<stdarg.h>",
                  "type": "codeVoice"
                },
                {
                  "text": " defines macros for declaring and using the ",
                  "type": "text"
                },
                {
                  "code": "va_list",
                  "type": "codeVoice"
                },
                {
                  "text": " data type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "args"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A result code. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/applicationservices/apple_event_manager#1656145",
