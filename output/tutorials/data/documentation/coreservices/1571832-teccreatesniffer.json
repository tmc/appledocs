      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a sniffer object reference, which is of type ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/tecsnifferobjectref",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". On return, the reference pertains to the newly created sniffer object. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "encodingSniffer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of text encoding specifications supplied by the caller; ",
                  "type": "text"
                },
                {
                  "code": "TECCreateSniffer",
                  "type": "codeVoice"
                },
                {
                  "text": " creates a sniffer that can detect each of these encodings. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "testEncodings"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of text encoding specifications in the ",
                  "type": "text"
                },
                {
                  "code": "testEncodings[] ",
                  "type": "codeVoice"
                },
                {
                  "text": "array. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "numTextEncodings"
        }
      ]
    },
