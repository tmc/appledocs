    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a file-system reference designating the item whose preferred application is requested; see the ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "File Manager Reference",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " in the Carbon File Management Documentation for a description of the ",
                  "type": "text"
                },
                {
                  "code": "FSRef",
                  "type": "codeVoice"
                },
                {
                  "text": " data type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inItemRef"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A bit mask specifying the application’s desired role or roles with respect to the designated item; see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/lsrolesmask",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a description of this mask. If the role is unimportant, pass ",
                  "type": "text"
                },
                {
                  "code": "kLSRolesAll",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inRolesMask"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a file-system reference that, on return, will identify the item’s preferred application; see the ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "File Manager Reference",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " in the Carbon File Management Documentation for a description of the ",
                  "type": "text"
                },
                {
                  "code": "FSRef",
                  "type": "codeVoice"
                },
                {
                  "text": " data type. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if you are not interested in identifying the preferred application in this form; however, this parameter and ",
                  "type": "text"
                },
                {
                  "code": "outAppURL",
                  "type": "codeVoice"
                },
                {
                  "text": " cannot both be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "outAppRef"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a Core Foundation URL reference that, on return, will identify the item’s preferred application; see the ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "CFURL Reference",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " in the Core Foundation Reference Documentation for a description of the ",
                  "type": "text"
                },
                {
                  "code": "CFURLRef",
                  "type": "codeVoice"
                },
                {
                  "text": " data type. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if you are not interested in identifying the preferred application in this form; however, this parameter and ",
                  "type": "text"
                },
                {
                  "code": "outAppRef",
                  "type": "codeVoice"
                },
                {
                  "text": " cannot both be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Despite the absence of the word ",
                  "type": "text"
                },
                {
                  "code": "Copy",
                  "type": "codeVoice"
                },
                {
                  "text": " in its name, this function retains the URL reference object on your behalf; you are responsible for releasing this object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "outAppURL"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A result code; see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/launch_services",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". If no application suitable for opening the item is found in the Launch Services database, the function will return the result code ",
              "type": "text"
            },
            {
              "code": "kLSApplicationNotFoundErr",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Thread-safe since Mac OS version 10.2.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
