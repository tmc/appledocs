      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The keyword for the special handler to get. You can specify any of the constants described in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/apple_events/1572726-special_handler_callback_constan",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aekeyword",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "functionClass"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A universal procedure pointer. On return, a pointer to the specified special handler, if one exists that matches the value supplied in the ",
                  "type": "text"
                },
                {
                  "code": "functionClass",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aeeventhandlerupp",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "handler"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies the special handler dispatch table to get the handler from. Pass ",
                  "type": "text"
                },
                {
                  "code": "TRUE",
                  "type": "codeVoice"
                },
                {
                  "text": " to get the handler from the system special handler dispatch table or ",
                  "type": "text"
                },
                {
                  "code": "FALSE",
                  "type": "codeVoice"
                },
                {
                  "text": " to get the handler from your applicationâ€™s special handler dispatch table. Use of the system special handler dispatch table is not recommended.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isSysHandler"
        }
      ]
    },
