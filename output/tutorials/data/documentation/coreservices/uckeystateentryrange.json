{
  "abstract": [
    {
      "text": "Maps from a dead-key state to either the resultant Unicode character(s) or the new dead key state produced when the current state is terminated by a given character key for a ",
      "type": "text"
    },
    {
      "code": "'uchr'",
      "type": "codeVoice"
    },
    {
      "text": " resource.",
      "type": "text"
    }
  ],
