      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A Search Kit index object contains the textual contents of one or more documents, as well as document URL objects (SKDocumentRefs) representing those documentsâ€™ locations.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To create a new disk-based Search Kit index object, use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/1446111-skindexcreatewithurl",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". To create a memory-based index, use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/1447500-skindexcreatewithmutabledata",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". For other operations on indexes, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/search_kit#1654608",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/search_kit#1654733",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". Also see",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/search_kit#1655469",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "1681550",
          "level": 3,
          "text": "Special Considerations",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You cannot use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/corefoundation/1521163-cfmakecollectable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with SKIndex objects. In a garbage-collected environment, you must use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/1442401-skindexclose",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to dispose of an SKIndex object.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
