        },
        {
          "inlineContent": [
            {
              "text": "Default search options include:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Relevance scores will be computed",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Spaces in a query are interpreted as Boolean ",
                      "type": "text"
                    },
                    {
                      "code": "AND",
                      "type": "codeVoice"
                    },
                    {
                      "text": " operators.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Do not use similarity searching.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
