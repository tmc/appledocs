              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aeremoteprocessresolverref",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to query. Acquired from a previous call to ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1445692-aecreateremoteprocessresolver",
                  "isActive": true,
                  "type": "reference"
