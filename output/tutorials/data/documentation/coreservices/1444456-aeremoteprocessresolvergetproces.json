            }
          ],
          "name": "ref"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If the function result is ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "outError",
                  "type": "codeVoice"
                },
                {
                  "text": " contains information about the failure. See the Core Foundation Reference Documentation for a description of the ",
                  "type": "text"
                },
                {
                  "code": "CFStreamError",
                  "type": "codeVoice"
                },
                {
                  "text": " data type.",
                  "type": "text"
