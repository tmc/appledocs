            {
              "text": "Returns ",
              "type": "text"
            },
            {
              "code": "TRUE",
              "type": "codeVoice"
            },
            {
              "text": " if the query was started, ",
              "type": "text"
            },
            {
              "code": "FALSE",
              "type": "codeVoice"
            },
            {
              "text": " otherwise. Queries cannot be executed more than once.",
              "type": "text"
            }
          ],
          "type": "paragraph"
