      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that determines the kind of information the function supplies in the ",
                  "type": "text"
                },
                {
                  "code": "result",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Pass the value ",
                  "type": "text"
                },
                {
                  "code": "keyAERecorderCount",
                  "type": "codeVoice"
                },
                {
                  "text": " to obtain the number of processes that are currently recording Apple events.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Pass the value ",
                  "type": "text"
                },
                {
                  "code": "keyAEVersion",
                  "type": "codeVoice"
                },
                {
                  "text": " to obtain version information for the Apple Event Manager, in ",
                  "type": "text"
                },
                {
                  "code": "NumVersion",
                  "type": "codeVoice"
                },
                {
                  "text": " format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Some keyword constants are defined in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/apple_events/1527206-keyword_parameter_constants",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aekeyword",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "keyWord"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a long value. On return, provides information that depends on what you pass in the ",
                  "type": "text"
                },
                {
                  "code": "keyword",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you pass ",
                  "type": "text"
                },
                {
                  "code": "keyAERecorderCount",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "result",
                  "type": "codeVoice"
                },
                {
                  "text": " specifies the number of processes that are currently recording Apple events.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you pass ",
                  "type": "text"
                },
                {
                  "code": "keyAEVersion",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "result",
                  "type": "codeVoice"
                },
                {
                  "text": " supplies version information for the Apple Event Manager, in a format that matches the ",
                  "type": "text"
                },
                {
                  "code": "'vers'",
                  "type": "codeVoice"
                },
                {
                  "text": " resource.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "result"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A result code. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/applicationservices/apple_event_manager#1656145",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For recordable applications, the information provided by ",
              "type": "text"
            },
            {
              "code": "AEManagerInfo",
              "type": "codeVoice"
            },
            {
              "text": " may be useful when the application is responding to Apple events that it sends to itself.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For information on determining whether the Apple Event Manager is available, see the Apple Event Manager Gestalt Selector, described in ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "Inside macOS: Gestalt Manager Reference",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": ". ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "1819457",
          "level": 3,
          "text": "Version-Notes",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Thread safe starting in OS X v10.2.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "AEManagerInfo",
              "type": "codeVoice"
            },
            {
              "text": " function is available only in version 1.01 and later of the Apple Event Manager.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
