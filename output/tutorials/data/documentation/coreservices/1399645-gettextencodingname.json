    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A text encoding specification whose name you want to obtain. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "iEncoding"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The portion of the encoding name you want to obtain. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/textencodingnameselector",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a list of possible values. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "iNamePartSelector"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The preferred region to use for the name. You can specify a Mac OS region code (which also implies a language)for this parameter. If the function cannot return the name for the preferred region, it returns the name using a region code with the same language or in a default language (for example, English). ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "iPreferredRegion"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The preferred encoding to use for the name. For example, ASCII, Mac OS Roman, or Shift-JIS. If the function cannot return the name using the preferred encoding, it returns the name using another encoding, such as Unicode or ASCII.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "iPreferredEncoding"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length in bytes of the output buffer that your application provides for the returned encoding name. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "iOutputBufLen"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a value of type ",
                  "type": "text"
                },
                {
                  "code": "ByteCount",
                  "type": "codeVoice"
                },
                {
                  "text": ". On return, this parameter holds the actual length, in bytes, of the text encoding name. The value represents the full length of the name, which might be greater than the size of the output buffer, specified by the ",
                  "type": "text"
                },
                {
                  "code": "iOutputBufLen",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter. The length of the portion of the name actually contained in the output buffer is the smaller of ",
                  "type": "text"
                },
                {
                  "code": "oNameLength",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "iOutputBufLen",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "oNameLength"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a value of type ",
                  "type": "text"
                },
                {
                  "code": "RegionCode",
                  "type": "codeVoice"
                },
                {
                  "text": ". On return, this parameter holds the actual region associated with the returned encoding name.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "oActualRegion"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a value of type ",
                  "type": "text"
                },
                {
                  "code": "TextEncoding",
                  "type": "codeVoice"
                },
                {
                  "text": ". On return, this parameter holds the actual encoding associated with the returned encoding name.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "oActualEncoding"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": " A pointer to a buffer you provide. On return, this parameter holds the text encoding name. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "oEncodingName"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A resultcode. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/carbon_core/text_encoding_conversion_manager#1664324",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Names returned by ",
              "type": "text"
            },
            {
              "code": "GetTextEncodingName ",
              "type": "codeVoice"
            },
            {
              "text": " (in the buffer referred to by ",
              "type": "text"
            },
            {
              "code": "oEncodingName",
              "type": "codeVoice"
            },
            {
              "text": ")can contain parentheses and other menu item meta characters, andso cannot be used with ",
              "type": "text"
            },
            {
              "code": "AppendMenu",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "InsertMenuItem",
              "type": "codeVoice"
            },
            {
              "text": ".You can use them with ",
              "type": "text"
            },
            {
              "code": "SetMenuItemText",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This function can return resources and memory errors, andthe following result codes:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "kTextUnsupportedEncodingErr",
                      "type": "codeVoice"
                    },
                    {
                      "text": ",which indicates that the encoding whose name you want to obtainis not supported. ",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "kTECMissingTableErr",
                      "type": "codeVoice"
                    },
                    {
                      "text": ",which indicates the name resource associated with the encoding ismissing.  ",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "kTECTableFormatErr",
                      "type": "codeVoice"
                    },
                    {
                      "text": " or ",
                      "type": "text"
                    },
                    {
                      "code": "kTECTableCheckSumErr",
                      "type": "codeVoice"
                    },
                    {
                      "text": ",which indicates that the name resource associated with that encodingis invalid.  ",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
