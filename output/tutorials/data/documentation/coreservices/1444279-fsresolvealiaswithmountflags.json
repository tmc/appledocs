    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the starting point for a relative search. If you pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " in this parameter, ",
                  "type": "text"
                },
                {
                  "code": "FSResolveAliasWithMountFlags",
                  "type": "codeVoice"
                },
                {
                  "text": " performs an absolute search. If you pass a pointer to a valid ",
                  "type": "text"
                },
                {
                  "code": "FSRef",
                  "type": "codeVoice"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "code": "fromFile",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter, ",
                  "type": "text"
                },
                {
                  "code": "FSResolveAliasWithMountFlags",
                  "type": "codeVoice"
                },
                {
                  "text": " performs a relative search for the target, followed by an absolute search only if the relative search fails. If you want to perform an absolute search followed by a relative search, you should use the function ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1444389-fsmatchaliasbulk",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fromFile"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A handle to the alias record to be resolved and, if necessary, updated.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inAlias"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to an ",
                  "type": "text"
                },
                {
                  "code": "FSRef",
                  "type": "codeVoice"
                },
                {
                  "text": " structure. On successful return, this ",
                  "type": "text"
                },
                {
                  "code": "FSRef",
                  "type": "codeVoice"
                },
                {
                  "text": " refers to the target of the alias record. This parameter must point to a valid ",
                  "type": "text"
                },
                {
                  "code": "FSRef",
                  "type": "codeVoice"
                },
                {
                  "text": " structure.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "target"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a Boolean value indicating, on return, whether the alias record to be resolved was updated because it contained some outdated information about the target. If it updates the alias record, ",
                  "type": "text"
                },
                {
                  "code": "FSResolveAliasWithMountFlags",
                  "type": "codeVoice"
                },
                {
                  "text": " sets the ",
                  "type": "text"
                },
                {
                  "code": "wasChanged",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ". Otherwise, it sets it to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ". (",
                  "type": "text"
                },
                {
                  "code": "FSResolveAliasWithMountFlags",
                  "type": "codeVoice"
                },
                {
                  "text": " never updates a minimal alias, so it never sets ",
                  "type": "text"
                },
                {
                  "code": "wasChanged",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " when resolving a minimal alias.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "wasChanged"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Options controlling how the alias is resolved. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1444436-volume_mount_options",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a description of the values you can use here. Set this parameter to ",
                  "type": "text"
                },
                {
                  "code": "kResolveAliasFileNoUI",
                  "type": "codeVoice"
                },
                {
                  "text": " to prevent any user interaction while the alias is being resolved.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mountFlags"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A result code.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The function ",
              "type": "text"
            },
            {
              "code": "FSResolveAliasWithMountFlags",
              "type": "codeVoice"
            },
            {
              "text": " is identical to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/1444297-fsresolvealias",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with the exception that it provides the ",
              "type": "text"
            },
            {
              "code": "mountFlags",
              "type": "codeVoice"
            },
            {
              "text": " parameter, allowing callers additional control over how the alias is resolved.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
