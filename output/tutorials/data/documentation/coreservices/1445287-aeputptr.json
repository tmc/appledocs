      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the descriptor list to add a descriptor to. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aedesclist",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "theAEDescList"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A one-based positive integer indicating the position to insert the descriptor at. If there is already a descriptor in the specified position, it is replaced.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "You can pass a value of zero or count + 1 to add the descriptor at the end of the list. ",
                  "type": "text"
                },
                {
                  "code": "AEPutPtr",
                  "type": "codeVoice"
                },
                {
                  "text": " returns an error (",
                  "type": "text"
                },
                {
                  "code": "AEIllegalIndex",
                  "type": "codeVoice"
                },
                {
                  "text": ") if you pass a negative number or a value that is out of range.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "index"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The descriptor type for the descriptor to be put into the list. For a list of AppleScriptâ€™s predefined descriptor types, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/apple_events/1542788-descriptor_type_constants",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/desctype",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "typeCode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the data for the descriptor to add.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dataPtr"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length, in bytes, of the data for the descriptor to add.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dataSize"
        }
      ]
    },
    {
