      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "TECDisposeConverter",
              "type": "codeVoice"
            },
            {
              "text": " functionautomatically disposes of the pointer for you. This means you shouldnot reference the pointer after you have disposed of the converter object. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Plug-ins that perform one-to-many conversions use the ",
              "type": "text"
            },
            {
              "code": "TECGetEncodingList",
              "type": "codeVoice"
            },
            {
              "text": " functionto get the output encoding list from the converter object reference. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
