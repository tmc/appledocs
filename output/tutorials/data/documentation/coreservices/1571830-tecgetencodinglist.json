          "content": [
            {
              "inlineContent": [
                {
                  "text": "A reference to the text encoding conversion object returned by the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/1571794-teccreateonetomanyconverter",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " function. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "encodingConverter"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, a pointer to the number of encodings specified by the ",
                  "type": "text"
                },
                {
                  "code": "encodingList",
                  "type": "codeVoice"
                },
                {
                  "text": " handle. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "numEncodings"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A handle to an array of text encoding specifications. On return, it contains an array of text encoding specifications to which the converter object can convert. The memory for the array is allocated automatically by the Text Encoding Converter. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "encodingList"
        }
