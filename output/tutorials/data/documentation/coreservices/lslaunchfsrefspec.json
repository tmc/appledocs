      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This data type defines a file-based launch specification designating,by file-system reference, an application to launch, items to open,or both. To request that items be opened in a particular application,set ",
              "type": "text"
            },
            {
              "code": "appRef",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "numDocs",
              "type": "codeVoice"
            },
            {
              "text": ",and ",
              "type": "text"
            },
            {
              "code": "itemRefs",
              "type": "codeVoice"
            },
            {
              "text": " accordingly.To request that each designated item be opened in its own preferredapplication, set ",
              "type": "text"
            },
            {
              "code": "appRef",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": ". Torequest that a particular application be launched without openingany documents, set ",
              "type": "text"
            },
            {
              "code": "appRef",
              "type": "codeVoice"
            },
            {
              "text": " accordinglyand set ",
              "type": "text"
            },
            {
              "code": "numDocs",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
