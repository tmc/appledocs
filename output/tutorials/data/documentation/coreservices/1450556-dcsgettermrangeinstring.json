              "inlineContent": [
                {
                  "text": "This parameter is reserved for future use, so pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ". Dictionary Services searches in all active dictionaries.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dictionary"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Text that contains the word or phrase to look up.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "textString"
        },
        {
          "content": [
            {
