{
  "abstract": [
    {
      "text": "Specify the types of the four keyword-specified descriptors that make up the data in an object specifier, as well as constants for position, logical operations, and comparison operations.",
      "type": "text"
    }
  ],
  "documentVersion": 0,
  "hierarchy": {
