          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the descriptor list containing the descriptor. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/aedesclist",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "theAEDescList"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A one-based positive integer indicating the position of the descriptor to get the data size for. ",
                  "type": "text"
                },
                {
                  "code": "AESizeOfNthItem",
                  "type": "codeVoice"
                },
                {
                  "text": " returns an error if you pass zero, a negative number, or a value that is out of range.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "index"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a descriptor type. On return, specifies the descriptor type of the descriptor. For a list of AppleScriptâ€™s predefined descriptor types, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/apple_events/1542788-descriptor_type_constants",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coreservices/desctype",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "typeCode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a size variable. On return, the length (in bytes) of the data in the descriptor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dataSize"
        }
      ]
