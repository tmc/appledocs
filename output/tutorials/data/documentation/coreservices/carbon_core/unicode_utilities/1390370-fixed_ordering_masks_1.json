  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can use these constants to set or obtain a value that specifies a fixed ordering scheme. For a description of the available types of fixed ordering schemes, see  ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/carbon_core/unicode_utilities/1390361-fixed_ordering_scheme",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, to specify ",
              "type": "text"
            },
            {
              "code": "kUCCollateTypeHFSExtended",
              "type": "codeVoice"
            },
            {
              "text": " in the ",
              "type": "text"
            },
            {
              "code": "options",
              "type": "codeVoice"
            },
            {
              "text": " parameter of the function  ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/1390513-uccomparetextnolocale",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " , the ",
              "type": "text"
            },
            {
              "code": "kUCCollateTypeHFSExtended",
              "type": "codeVoice"
            },
            {
              "text": " value must be shifted by ",
              "type": "text"
            },
            {
              "code": "kUCCollateTypeShiftBits ",
              "type": "codeVoice"
            },
            {
              "text": " :",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "code": "options = kUCCollateTypeHFSExtended     kUCCollateTypeShiftBits; ",
              "type": "codeVoice"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You would obtain the ordering scheme value from the ",
              "type": "text"
            },
            {
              "code": "options",
              "type": "codeVoice"
            },
            {
              "text": " parameter as follows:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "fixedOrderType = ((options > > kUCCollateTypeShiftBits) &  kUCCollateTypeSourceMask);"
          ],
          "metadata": {
            "anchor": "2557463",
            "title": "Listing 1"
          },
          "syntax": "occ",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "See also ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coreservices/carbon_core/unicode_utilities/1390573-fixed_ordering_masks_2",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
