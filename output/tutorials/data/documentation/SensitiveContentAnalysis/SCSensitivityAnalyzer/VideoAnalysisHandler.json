    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Checking video for sensitive content can take longer than checking images. This class",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "enables an app to stay informed on the frameworkâ€™s progress while it checks a video",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for sensitive content. The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SensitiveContentAnalysis/documentation/SensitiveContentAnalysis/SCSensitivityAnalyzer",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SensitiveContentAnalysis/documentation/SensitiveContentAnalysis/SCSensitivityAnalyzer/videoAnalysis(forFileAt:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " returns",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "an instance of this class.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To analyze a video file for sensitive content, pass a video URL into the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SensitiveContentAnalysis/documentation/SensitiveContentAnalysis/SCSensitivityAnalyzer/videoAnalysis(forFileAt:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "function. While waiting on the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SensitiveContentAnalysis/documentation/SensitiveContentAnalysis/SCSensitivityAnalyzer/VideoAnalysisHandler/hasSensitiveContent()",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "method, check ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SensitiveContentAnalysis/documentation/SensitiveContentAnalysis/SCSensitivityAnalyzer/VideoAnalysisHandler/progress",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for the completion status:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let handler = analyzer.videoAnalysis(forFileAt: videoFileUrl)",
            "",
            "let progress = handler.progress",
            "// Do something with the `progress` property, such as dispatch a UI update thread.",
            "",
            "// Wait here until the video check for sensitive content completes.",
            "let response = try await handler.hasSensitiveContent()"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
