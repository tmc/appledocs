{
  "abstract": [
    {
      "text": "Trigger your app’s intervention flow by using a special QR code and profile that",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "Apple provides for testing.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.SensitiveContentAnalysis/documentation/SensitiveContentAnalysis"
      ]
    ]
  },
