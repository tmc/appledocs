    "doc://com.apple.photokit/documentation/Photos/PHPhotoLibraryChangeObserver/photoLibraryDidChange(_:)": {
      "abstract": [
        {
          "text": "Tells your observer that a set of changes has occurred in the Photos library.",
          "type": "text"
        }
      ],
      "fragments": [
        {
