    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The size of the received TXT Record",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "txtLen"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Pointer to the received TXT Record bytes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "txtRecord"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A null-terminated ASCII string containing the key name.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "key"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On output, will be set to the size of the “value” data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "valueLen"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Returns NULL if the key does not exist in this TXT record, or exists with no value (to differentiate between these two cases use TXTRecordContainsKey()). Returns pointer to location within TXT Record bytes if the key exists with empty or non-empty value. For empty value, valueLen will be zero. For non-empty value, valueLen will be length of value data.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
