            "parameters": [
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A pointer to a buffer that where the resulting full domain name is to be written. The buffer must be ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.dnssd/documentation/dnssd/kDNSServiceMaxDomainName",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " (1009) bytes in length to accommodate the longest legal domain name without buffer overrun.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "fullName"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The service name - any dots or backslashes must NOT be escaped. May be NULL (to construct a PTR record name, e.g. “_ftp._tcp.apple.com.”).",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "service"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The service type followed by the protocol, separated by a dot (e.g. “_ftp._tcp”).",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "regtype"
              },
              {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The domain name, e.g. “apple.com.”. Literal dots or backslashes, if any, must be escaped, e.g. “1st. Floor.apple.com.”",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ],
                "name": "domain"
              }
            ]
          }
