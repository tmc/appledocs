    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to annotate your List views in your app with entities defined",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in your app to become discoverable by the system the moment your views",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "appear on screen.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example annotates a custom List view with an entity.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct BookListView: View {",
            "   let books: [Book]",
            "   @State private var selection = Set<Book.ID>()",
            "   var body: some View {",
            "       List(selection: $selection) {",
            "           ForEach(books) { book in",
            "               BookView(book.name)",
            "           }",
            "       }",
            "       .appEntityIdentifier(forSelectionType: Book.ID.self) { bookId in",
            "           EntityIdentifier(for: Book.self, identifier: bookId)",
            "       }",
            "   }",
            " }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
