          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of columns that the view should consume",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "when placed in a grid row.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
