    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The app entity instance to associate with the view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "identifier"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier allows your app entity to become discoverable by the system the moment your view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "appears on screen. When multiple of these modifiers are used on the same view, the innermost",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier takes precedence and that entity is associated with the view by its identifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct FooView {",
            "    let entityA: EntityA",
            "    let entityB: EntityB",
            "    let entityC: EntityC",
            "    var body: some View {",
            "    BarView {",
            "        Rectangle()",
            "            .appEntityIdentifier(entityA.fullyQualifiedIdentifier)",
            "    }",
            "        .appEntityIdentifier(entityB.fullyQualifiedIdentifier)",
            "        .appEntityIdentifier(entityC.fullyQualifiedIdentifier)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "This results in the ",
              "type": "text"
            },
            {
              "code": "Rectangle",
              "type": "codeVoice"
            },
            {
              "text": " being annotated with ",
              "type": "text"
            },
            {
              "code": "entityA",
              "type": "codeVoice"
            },
            {
              "text": ", and BarView being",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "annotated with ",
              "type": "text"
            },
            {
              "code": "entityB",
              "type": "codeVoice"
            },
            {
              "text": " while the ",
              "type": "text"
            },
            {
              "code": "entityC",
              "type": "codeVoice"
            },
            {
              "text": " annotation is discarded.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Providing a ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " value resets the association.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
