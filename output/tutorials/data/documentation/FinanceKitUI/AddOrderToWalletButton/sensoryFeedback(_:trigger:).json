              "inlineContent": [
                {
                  "text": "Which type of feedback to play.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "feedback"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value to monitor for changes to determine when to play.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trigger"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, you could play feedback when a state value changes:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyView: View {",
            "    @State private var showAccessory = false",
            "",
            "    var body: some View {",
            "        ContentView()",
            "            .sensoryFeedback(.selection, trigger: showAccessory)",
            "            .onLongPressGesture {",
            "                showAccessory.toggle()",
            "            }",
            "",
            "        if showAccessory {",
            "            AccessoryView()",
            "        }",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
