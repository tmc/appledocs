        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "​A string representing the issue’s category; for example, the name of the build phase where the issue occurred.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "category",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The file and line number where Xcode Cloud encountered an issue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fileSource",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/FileLocation",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:FileLocation",
              "text": "FileLocation"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "ANALYZER_WARNING",
                "ERROR",
                "TEST_FAILURE",
                "WARNING"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that indicates what kind of issue Xcode Cloud encountered.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "issueType",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Information about the issue that occurred.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "message",
          "required": false,
          "type": [
            {
              "kind": "text",
