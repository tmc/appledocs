      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of the action.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "actionType",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/CiActionType",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:CiActionType",
              "text": "CiActionType"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A type that indicates whether a build’s artifact is eligible for release on the App Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "buildDistributionAudience",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/BuildAudienceType",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:BuildAudienceType",
              "text": "BuildAudienceType"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "ANY_IOS_DEVICE",
                "ANY_IOS_SIMULATOR",
                "ANY_TVOS_DEVICE",
                "ANY_TVOS_SIMULATOR",
                "ANY_WATCHOS_DEVICE",
                "ANY_WATCHOS_SIMULATOR",
                "ANY_MAC",
                "ANY_MAC_CATALYST",
                "ANY_VISIONOS_DEVICE",
                "ANY_VISIONOS_SIMULATOR"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that describes the destination Xcode Cloud uses for an action.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destination",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the action must succeed in order for a build to succeed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isRequiredToPass",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the action; for example, archive or test.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "MACOS",
                "IOS",
                "TVOS",
                "WATCHOS",
                "VISIONOS"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The platform Xcode Cloud uses for the action.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "platform",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the scheme that Xcode Cloud uses to perform the action.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "scheme",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
