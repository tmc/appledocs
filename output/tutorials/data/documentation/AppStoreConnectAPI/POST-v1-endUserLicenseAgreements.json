              "content": [
                {
                  "code": [
                    "{",
                    "  \"data\" : {",
                    "    \"type\" : \"endUserLicenseAgreements\",",
                    "    \"id\" : \"b25d1669-d6b1-4e9b-8679-02863557222a\",",
                    "    \"attributes\" : {",
                    "      \"agreementText\" : \"...\"",
                    "    },",
                    "    \"relationships\" : {",
                    "      \"app\" : {",
