  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "scmGitReferences",
              "type": "codeVoice"
            },
            {
              "text": " resource represents the canonical Git reference for the Git commit that Xcode Cloud used to perform a build. The resource includes the following information:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The referenceâ€™s name and canonical name",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Whether the reference has been deleted",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The type of the reference",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "For example, if Xcode Cloud uses the ",
              "type": "text"
            },
            {
              "code": "bug-fix",
              "type": "codeVoice"
            },
            {
              "text": " branch for a build, the canonical Git reference is ",
              "type": "text"
            },
            {
              "code": "refs/heads/bug-fix",
              "type": "codeVoice"
            },
            {
              "text": " and the name is ",
              "type": "text"
            },
            {
              "code": "bug-fix",
              "type": "codeVoice"
            },
            {
              "text": ". If Xcode Cloud starts a build for the ",
              "type": "text"
            },
            {
              "code": "release-1.0",
              "type": "codeVoice"
            },
            {
              "text": " tag, the canonical name is ",
              "type": "text"
            },
            {
              "code": "refs/tags/release-1.0",
              "type": "codeVoice"
            },
            {
              "text": " and the name is ",
              "type": "text"
            },
            {
              "code": "release-1.0",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "This resource supports JSON web tokens with a lifetime of up to six months. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/generating-tokens-for-api-requests#Determine-the-Appropriate-Token-Lifetime",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
