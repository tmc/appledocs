          "inlineContent": [
            {
              "text": "App Store Connect API version 2.1 provides resources that enable you to automate actions you take in App Store Connect.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "New-features",
          "level": 3,
          "text": "New features",
          "type": "heading"
        },
