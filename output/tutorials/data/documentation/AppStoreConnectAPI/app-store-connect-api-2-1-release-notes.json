              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/ScreenshotDisplayType",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/PreviewType",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " objects add values that enable you to submit screenshots and app previews for iPhone 14, iPhone 14 Plus, iPhone 14 Pro, iPhone 14 Pro Max, Apple Watch Series 8, and Apple Watch Ultra.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
