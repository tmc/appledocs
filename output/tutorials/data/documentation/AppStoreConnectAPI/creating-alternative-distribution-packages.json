{
  "abstract": [
    {
      "text": "Create distribution packages for your apps that you distribute on alternative marketplaces or on the web.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI",
