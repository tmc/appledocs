      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the group is internal. Only existing users of App Store Connect may be added for internal beta testing.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isInternalGroup",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name for the beta group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL of the public link provided to your appâ€™s beta testers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "publicLink",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether a public link is enabled. Enable a link to invite anyone outside of your team to beta test your app. When you share this link, testers can install the beta version of your app on their devices in TestFlight and share the link with others.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "publicLinkEnabled",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ID as part of the URL of the public link.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "publicLinkId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of testers that can join this beta group using the public link. Values must be between 1 and 10,000.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "publicLinkLimit",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that limits the number of testers who can join the beta group using the public link.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "publicLinkLimitEnabled",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The creation date of the beta group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "createdDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date-time"
            }
          ]
        },
        {
