      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "You can download consolidated or separate financial reports per territory. For a complete list of possible values, see ",
                  "type": "text"
                },
                {
                  "identifier": "https://developer.apple.com/help/app-store-connect/reference/financial-report-regions-and-currencies",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "filter[regionCode]",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The fiscal month of the report you wish to download based on the ",
                  "type": "text"
                },
                {
                  "identifier": "https://itunesconnect.apple.com/WebObjects/iTunesConnect.woa/wa/jumpTo?page=fiscalcalendar",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". The fiscal month is specified in the ",
                  "type": "text"
                },
                {
                  "code": "YYYY-MM",
                  "type": "codeVoice"
                },
                {
                  "text": " format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "filter[reportDate]",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "FINANCIAL",
                "FINANCE_DETAIL"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This value is always ",
                  "type": "text"
                },
                {
                  "code": "FINANCIAL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "filter[reportType]",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "You can find your vendor number in ",
                  "type": "text"
                },
                {
                  "identifier": "https://developer.apple.com/help/app-store-connect/getting-paid/view-payments-and-proceeds",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "filter[vendorNumber]",
          "required": true,
          "type": [
            {
              "kind": "text",
