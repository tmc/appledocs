          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "App Store Connect API version 1.3 provides resources that enable you to automate actions you take in App Store Connect.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "New-features",
          "level": 3,
          "text": "New features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-infos",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource adds an ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/age-rating-declarations",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " relationship.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AgeRatingDeclaration/Attributes-data.dictionary",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " now separates the ",
                      "type": "text"
                    },
                    {
                      "code": "gamblingAndContests",
                      "type": "codeVoice"
                    },
                    {
                      "text": " entry into two entries: ",
                      "type": "text"
                    },
                    {
                      "code": "contests",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "gambling.",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Deprecations",
          "level": 3,
          "text": "Deprecations",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/GET-v1-appStoreVersions-_id_-ageRatingDeclaration",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " endpoint is now deprecated.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "code": "ageRatingDeclaration",
                      "type": "codeVoice"
                    },
                    {
                      "text": " field within the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/GET-v1-apps-_id_-appStoreVersions",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " endpoint is now deprecated.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "code": "gamblingAndContests",
                      "type": "codeVoice"
                    },
                    {
                      "text": " entry of ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AgeRatingDeclaration/Attributes-data.dictionary",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " is now deprecated.",
                      "type": "text"
                    }
