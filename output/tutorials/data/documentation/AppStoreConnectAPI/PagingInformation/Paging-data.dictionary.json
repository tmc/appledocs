      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of resources matching your request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "total",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum number of resources to return per page, from 0 to 200.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "limit",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Adjust the number of resources returned per page by using the ",
              "type": "text"
            },
            {
              "code": "limit",
              "type": "codeVoice"
            },
            {
              "text": " query parameter in your request. For example, the following request  returns the first 10 testers:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "GET /v1/betaTesters?limit=10"
          ],
          "syntax": "javascript",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
