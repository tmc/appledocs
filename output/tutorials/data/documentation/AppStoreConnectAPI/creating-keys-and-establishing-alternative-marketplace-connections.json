{
  "abstract": [
    {
      "text": "Manage keys you use to sign JSON web tokens and connect marketplaces with apps.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
