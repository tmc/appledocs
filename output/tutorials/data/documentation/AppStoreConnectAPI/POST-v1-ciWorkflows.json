                    "“data”: {",
                    "        “type”: “ciWorkflows”,",
                    "        “attributes”: {",
                    "            “name”: “A new workflow”,",
                    "            “description”: “A new workflow that verifies my changes.”,",
                    "            “branchStartCondition”: {",
                    "                “source”: {",
                    "                    “isAllMatch”: false,",
                    "                    “patterns”: [",
