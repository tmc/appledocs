      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates the intent to distribute your app on the French App Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "availableOnFrenchStore",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier that can be added to your app to associate it with a given declaration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "codeValue",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates your app implements any proprietary encryption algorithms.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "containsProprietaryCryptography",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates your app implements any standard encryption algorithms instead of, or in addition to, using or accessing the encryption in Apple’s operating systems.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "containsThirdPartyCryptography",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The document name of your submitted export compliance documentation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "deprecated": true,
          "introducedVersion": "3.6.0",
          "name": "documentName",
          "required": false,
          "type": [
            {
              "kind": "text",
