{
  "abstract": [
    {
      "text": "Configure win-back offers for your auto-renewable subscriptions with the App Store Connect API.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
