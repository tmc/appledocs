      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The source code management provider’s display name; for example, ",
                  "type": "text"
                },
                {
                  "code": "Bitbucket Server",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "displayName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether it’s a self-hosted source code management provider.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isOnPremise",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "BITBUCKET_CLOUD",
                "BITBUCKET_SERVER",
                "GITHUB_CLOUD",
                "GITHUB_ENTERPRISE",
                "GITLAB_CLOUD",
                "GITLAB_SELF_MANAGED"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that represents the kind of a Providers resource.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "kind",
          "required": false,
          "type": [
            {
              "kind": "text",
