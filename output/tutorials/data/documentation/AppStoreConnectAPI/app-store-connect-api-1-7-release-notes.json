  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "App Store Connect API version 1.7 provides resources that enable you to automate actions you take in App Store Connect.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "New-features",
          "level": 3,
          "text": "New features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The new resources in ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-events-and-metadata",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " support scheduling and managing in-app events, including ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-events",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-event-video-clips",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-event-screenshots",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-event-localizations",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "New resources support creating and managing your app’s custom product pages, including ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-custom-product-pages",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-custom-product-page-versions",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-custom-product-page-localizations",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-store-version-experiments",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource now supports creating and managing product page optimization treatments.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/review-submissions",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/review-submission-items",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resources support the updated App Store submission process, which lets you submit multiple items, submit without a new app version, and retrieve the submission statuses.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Deprecations",
          "level": 3,
          "text": "Deprecations",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/app-store-version-submissions",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource and all its related endpoints and objects are now deprecated, including: ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/POST-v1-appStoreVersionSubmissions",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/DELETE-v1-appStoreVersionSubmissions-_id_",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppStoreVersionSubmission",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppStoreVersionSubmissionCreateRequest",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppStoreVersionSubmissionResponse",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
