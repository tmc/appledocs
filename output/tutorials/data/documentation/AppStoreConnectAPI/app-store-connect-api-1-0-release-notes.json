  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This REST API provides resources that enable you to automate actions you take in App Store Connect.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "New-features",
          "level": 3,
          "text": "New features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Manage beta builds of your app, beta testers, and groups for Testflight with resources in ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/prerelease-versions-and-beta-testers",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Manage your users and their access with the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/users",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resources. Invite users to join your team with the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/user-invitations",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Download sales and financial reports in ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/sales-and-finance",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
