  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "App Store Connect API version 3.8 provides resources that enable you to automate actions you take in App Store Connect.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "New-Features",
          "level": 3,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/merchantids",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource is now available to automate tasks related to Apple Pay and Apple Wallet. Apple Pay certificates now have enum values on the ",
                      "type": "text"
                    },
                    {
                      "code": "certificates",
                      "type": "codeVoice"
                    },
                    {
                      "text": " resource.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/PATCH-v1-certificates-_id_",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " is now available. Use this endpoint to modify the activation status of your Payment Processing certificate. To learn more, see ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/managing-payment-processing-certificates",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Create public links that accept testers with specific device and OS combinations using the ",
                      "type": "text"
                    },
                    {
                      "code": "betaRecruitmentCriteria",
                      "type": "codeVoice"
                    },
                    {
                      "text": " resource. To learn more, see ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/beta-recruitment-criteria",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use the new ",
                      "type": "text"
                    },
                    {
                      "code": "nominations",
                      "type": "codeVoice"
                    },
                    {
                      "text": " resource to share your new content, app enhancement, or app launch with Apple. To learn more see, ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://developer.apple.com/app-store/getting-featured/",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Three new content statuses, ",
                      "type": "text"
                    },
                    {
                      "code": "TRADER_STATUS_NOT_PROVIDED",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "TRADER_STATUS_VERIFICATION_FAILED",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "code": "TRADER_STATUS_VERIFICATION_STATUS_MISSING",
                      "type": "codeVoice"
                    },
                    {
                      "text": " are available for ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/TerritoryAvailability/Attributes-data.dictionary",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ". To learn more, see ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://developer.apple.com/help/app-store-connect/manage-compliance-information/manage-european-union-digital-services-act-trader-requirements",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Improvements",
          "level": 3,
          "text": "Improvements",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The field ",
                      "type": "text"
                    },
                    {
                      "code": "iosBuildsAvailableForAppleVision",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is now in the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/beta-groups",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Deprecations",
          "level": 3,
          "text": "Deprecations",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The attribute ",
                      "type": "text"
                    },
                    {
                      "code": "brazilAgeRatings",
                      "type": "codeVoice"
                    },
                    {
                      "text": " for ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppInfo/Attributes-data.dictionary",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " is deprecated. Use ",
                      "type": "text"
                    },
                    {
                      "code": "brazilAgeRatingV2",
                      "type": "codeVoice"
                    },
                    {
                      "text": " instead.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Removals",
          "level": 3,
          "text": "Removals",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "code": "PromotedPurchaseImages",
                      "type": "codeVoice"
                    },
                    {
                      "text": " resources is now removed and replaced with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/subscription-images",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/in-app-purchase-images",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The enum ",
                      "type": "text"
                    },
                    {
                      "code": "ELAPSED_TIME_MILLISECOND",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is now removed from ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/GameCenterLeaderboardFormatter",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
