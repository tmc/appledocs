        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An email address to which beta testers can send feedback. Also appears as the reply-to address for TestFlight invitation emails.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "feedbackEmail",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
