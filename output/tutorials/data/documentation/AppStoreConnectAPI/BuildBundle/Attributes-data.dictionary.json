      ],
      "kind": "declarations"
    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle ID of the build bundle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundleId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "APP",
                "APP_CLIP"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of the build bundle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundleType",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The protocols that the app uses to communicate with external accessory hardware. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/BundleResources/Information-Property-List/UISupportedExternalAccessoryProtocols",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "deviceProtocols",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL to the symbolication file for the app or App Clip.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dSYMUrl",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "uri"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Entitlement information for your app or App Clip.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "entitlements",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/BuildBundle/Attributes-data.dictionary/Entitlements-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:BuildBundle.Attributes.Entitlements",
              "text": "BuildBundle.Attributes.Entitlements"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the build bundle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fileName",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the build bundle contains on-demand resources.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "hasOnDemandResources",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the build bundle contains a pre-rendered app icon.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "hasPrerenderedIcon",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the build bundle contains an app or App Clip that allows users to interact with it through voice, intelligent suggestions, and personalized workflows.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "hasSirikit",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the build bundle includes symbol information for debugging and crash reports.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "includesSymbols",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether an iOS app included in the build bundle is included on the Mac App Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isIosBuildMacAppStoreCompatible",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The specified locale. Refer to ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/BetaAppLocalizationCreateRequest/Data-data.dictionary/Attributes-data.dictionary",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for possible values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "locales",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The build number of the OS you used to build the app or App Clip.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "platformBuild",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of capabilities that your app or App Clip requires.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "requiredCapabilities",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that identifies the SDK you used to build your app or App Clip.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sdkBuild",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of supported CPU architectures that your app or App Clip supports.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "supportedArchitectures",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the app or App Clip included in the build bundle uses location services.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "usesLocationServices",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
