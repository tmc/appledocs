    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The call-to-action verb that appears on the App Clip card.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppClipAction",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:AppClipAction",
              "text": "AppClipAction"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "AUTOMOTIVE",
                "BEAUTY",
                "BIKES",
                "BOOKS",
                "CASINO",
                "EDUCATION",
                "EDUCATION_JAPAN",
                "ENTERTAINMENT",
                "EV_CHARGER",
                "FINANCIAL_USD",
                "FINANCIAL_CNY",
                "FINANCIAL_GBP",
                "FINANCIAL_JPY",
                "FINANCIAL_EUR",
                "FITNESS",
                "FOOD_AND_DRINK",
                "GAS",
                "GROCERY",
                "HEALTH_AND_MEDICAL",
                "HOTEL_AND_TRAVEL",
                "MUSIC",
                "PARKING",
                "PET_SERVICES",
                "PROFESSIONAL_SERVICES",
                "SHOPPING",
                "TICKETING",
                "TRANSIT"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The business category of an advanced App Clip experience; for example, ",
                  "type": "text"
                },
                {
                  "code": "PARKING",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "businessCategory",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The default language for the advanced App Clip experience.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "defaultLanguage",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppClipAdvancedExperienceLanguage",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:AppClipAdvancedExperienceLanguage",
              "text": "AppClipAdvancedExperienceLanguage"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the advanced App Clip experience was submitted by a platform provider that serves multiple businesses.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isPoweredBy",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The physical location you associate with the advanced App Clip experience. If you associate an advanced App Clip experience with a place, users can launch your App Clip from location-based suggestions from Siri Suggestions and the Maps app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "place",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppClipAdvancedExperienceUpdateRequest/Data-data.dictionary/Attributes-data.dictionary/Place-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:AppClipAdvancedExperienceUpdateRequest.Data.Attributes.Place",
              "text": "AppClipAdvancedExperienceUpdateRequest.Data.Attributes.Place"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether you want to delete an advanced App Clip experience. To delete the advanced App Clip experience, set it to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "removed",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    }
  ],
