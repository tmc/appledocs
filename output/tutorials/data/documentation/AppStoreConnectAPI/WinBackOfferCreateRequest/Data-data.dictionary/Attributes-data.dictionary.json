    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "How long a customer was a subscriber.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Possible values:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "1 to 24 months, as integers",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "1-24",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "3 years",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "36",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "4 years",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "48",
                        "type": "codeVoice"
                      }
                    ]
                  }
                },
                {
                  "definition": {
                    "content": [
                      {
                        "inlineContent": [
                          {
                            "text": "5 years",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ]
                  },
                  "term": {
                    "inlineContent": [
                      {
                        "code": "60",
                        "type": "codeVoice"
                      }
                    ]
                  }
                }
              ],
              "type": "termList"
            }
          ],
          "name": "customerEligibilityPaidSubscriptionDurationInMonths",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "How long since the subscribe last had an active subscription.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "customerEligibilityTimeSinceLastSubscribedInMonths",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/IntegerRange",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:IntegerRange",
              "text": "IntegerRange"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "How long since the subscribe last had an active subscription.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "customerEligibilityWaitBetweenOffersInMonths",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length of time for the offer period.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "duration",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/SubscriptionOfferDuration",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:SubscriptionOfferDuration",
              "text": "SubscriptionOfferDuration"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Minimum length for a win-back offer is 3 days.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "endDate",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "date"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "(Required) string of alphanumeric characters, periods, and underscores, up to 100 characters. Use a unique value that you have not used for a promotional offer, offer code, or introductory",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "offer, when you create your win-back offer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offerId",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "(Required) Describes how payment is configured for a win-back offer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offerMode",
          "required": true,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/SubscriptionOfferMode",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:SubscriptionOfferMode",
              "text": "SubscriptionOfferMode"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "(Required) The number of subscription duration intervals.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "periodCount",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "integer"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "HIGH",
                "NORMAL"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Select how this offer ranks among your other offers and in-app events. ",
                  "type": "text"
                },
                {
                  "code": "HIGH",
                  "type": "codeVoice"
                },
                {
                  "text": " priority offers will appear above other offers and events on your app ºs product page.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "priority",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "NOT_PROMOTED",
                "USE_AUTO_GENERATED_ASSETS"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "You can promote this offer on your App Store product page. Promoted offers can also display in search results and may be featured on the Today, Games, and Apps tabs.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If your win-back offer is live and ",
                  "type": "text"
                },
                {
                  "code": "promotionIntent",
                  "type": "codeVoice"
                },
                {
                  "text": " is set to ",
                  "type": "text"
                },
                {
                  "code": "USE_AUTO_GENERATED_ASSETS",
                  "type": "codeVoice"
                },
                {
                  "text": " you need to delete the win-back offer in order to remove it from promotion.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "promotionIntent",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string of alphanumeric characters, spaces, periods, and underscores, up to 65 characters.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Use a unique value that you have not used for a promotional offer, offer code, or introductory offer, when you create your win-back offer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "referenceName",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "First available date is today + 1 day.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "startDate",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "date"
            }
          ]
        }
      ],
