        {
          "inlineContent": [
            {
              "code": "endUserLicenseAgreements",
              "type": "codeVoice"
            },
            {
              "text": " represents the custom End User License Agreement (EULA) that you provide. Apple provides a standard EULA that applies in all territories. If you need to provide a custom EULA, use this resource to provide your agreement text. Relate it to the app and territories for which it applies using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/territories",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " resource.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For more information, see App Store Connect Help: ",
              "type": "text"
            },
            {
              "identifier": "https://developer.apple.com/help/app-store-connect/update-your-app/create-a-new-version",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
