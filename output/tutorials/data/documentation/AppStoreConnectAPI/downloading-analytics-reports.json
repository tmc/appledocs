{
  "abstract": [
    {
      "text": "Learn how to request and review data about your apps, their usage, engagement, and performance.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
