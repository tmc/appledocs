{
  "abstract": [
    {
      "text": "Learn how to prepare and submit your subscriptions, subscription groups, and subscription screenshots for the App Review process with the App Store Connect API.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
