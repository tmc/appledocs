    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A machine-readable code indicating the type of error. The code is a hierarchical value with levels of specificity separated by the ‘",
                  "type": "text"
                },
                {
                  "code": ".",
                  "type": "codeVoice"
                },
                {
                  "text": "’ character. This value is parseable for programmatic error handling in code.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "code",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The HTTP status code of the error. This status code usually matches the response’s status code; however, if the request produces multiple errors, these two codes may differ.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "status",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique ID of a specific instance of an error, request, and response. Use this ID when providing feedback to or debugging issues with Apple.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A summary of the error. Do not use this field for programmatic error handling.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "title",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A detailed explanation of the error. Do not use this field for programmatic error handling.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "detail",
          "required": true,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedTypes",
              "values": [
                [
                  {
                    "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/JsonPointer",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:app_store_connect_api:JsonPointer",
                    "text": "JsonPointer"
                  }
                ],
                [
                  {
                    "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/Parameter",
                    "kind": "typeIdentifier",
                    "preciseIdentifier": "data:app_store_connect_api:Parameter",
                    "text": "Parameter"
                  }
                ]
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "One of two possible types of values: ",
                  "type": "text"
                },
                {
                  "code": "source.Parameter",
                  "type": "codeVoice"
                },
                {
                  "text": ", provided when a query parameter produced the error, or ",
                  "type": "text"
                },
                {
                  "code": "source.JsonPointer",
                  "type": "codeVoice"
                },
                {
                  "text": ", provided when a problem with the entity produced the error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "source",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "*"
            }
          ],
          "typeDetails": []
        },
