              "inlineContent": [
                {
                  "text": "​A Boolean value that indicates whether a start condition’s settings apply to all tags. If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the ",
                  "type": "text"
                },
                {
                  "code": "patterns",
                  "type": "codeVoice"
                },
                {
                  "text": " attribute isn’t expected. If ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", the ",
                  "type": "text"
                },
                {
                  "code": "patterns",
                  "type": "codeVoice"
                },
                {
                  "text": " attribute is required.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isAllMatch",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "boolean"
            }
          ]
        },
        {
