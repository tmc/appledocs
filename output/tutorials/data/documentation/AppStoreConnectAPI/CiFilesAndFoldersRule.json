      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Directory and file information Xcode Cloud uses to determine if a change to a file or directory matches a custom start condition.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "matchers",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "["
            },
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/CiStartConditionFileMatcher",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:CiStartConditionFileMatcher",
              "text": "CiStartConditionFileMatcher"
            },
            {
              "kind": "text",
              "text": "]"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "START_IF_ANY_FILE_MATCHES",
                "DO_NOT_START_IF_ALL_FILES_MATCH"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that indicates whether a workflow’s start condition’s Files and Folders setting should start a new build or not for a change.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mode",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
