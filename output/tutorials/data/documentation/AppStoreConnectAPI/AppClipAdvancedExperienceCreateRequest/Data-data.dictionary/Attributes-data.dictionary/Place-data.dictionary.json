    },
    {
      "items": [
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of categories for a place in Apple Maps you associate with the Advanced App Clip experience.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "categories",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Coordinates of a place in Apple Maps you associate with an advanced App Clip experience.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "displayPoint",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppClipAdvancedExperienceCreateRequest/Data-data.dictionary/Attributes-data.dictionary/Place-data.dictionary/DisplayPoint-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:AppClipAdvancedExperienceCreateRequest.Data.Attributes.Place.DisplayPoint",
              "text": "AppClipAdvancedExperienceCreateRequest.Data.Attributes.Place.DisplayPoint"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The optional website URL for a place.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "homePage",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The main address of a place in Apple Maps. This value is required if you don’t provide coordinates for a place.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mainAddress",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppClipAdvancedExperienceCreateRequest/Data-data.dictionary/Attributes-data.dictionary/Place-data.dictionary/MainAddress-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:AppClipAdvancedExperienceCreateRequest.Data.Attributes.Place.MainAddress",
              "text": "AppClipAdvancedExperienceCreateRequest.Data.Attributes.Place.MainAddress"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "BUY_TICKETS",
                "VIEW_AVAILABILITY",
                "VIEW_PRICING",
                "HOTEL_BOOK_ROOM",
                "PARKING_RESERVE_PARKING",
                "RESTAURANT_JOIN_WAITLIST",
                "RESTAURANT_ORDER_DELIVERY",
                "RESTAURANT_ORDER_FOOD",
                "RESTAURANT_ORDER_TAKEOUT",
                "RESTAURANT_RESERVATION",
                "SCHEDULE_APPOINTMENT",
                "RESTAURANT_VIEW_MENU",
                "THEATER_NOW_PLAYING"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that describes the intent behind an App Clip invocation from location-based suggestions from Siri Suggestions and the Maps app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mapAction",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of names as strings for a place in Apple Maps.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "names",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "[string]"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The phone number that’s associated with a place in Apple Maps as a string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "phoneNumber",
          "required": false,
          "type": [
            {
              "identifier": "doc://com.apple.appstoreconnectapi/documentation/AppStoreConnectAPI/AppClipAdvancedExperienceCreateRequest/Data-data.dictionary/Attributes-data.dictionary/Place-data.dictionary/PhoneNumber-data.dictionary",
              "kind": "typeIdentifier",
              "preciseIdentifier": "data:app_store_connect_api:AppClipAdvancedExperienceCreateRequest.Data.Attributes.Place.PhoneNumber",
              "text": "AppClipAdvancedExperienceCreateRequest.Data.Attributes.Place.PhoneNumber"
            }
          ]
        },
        {
          "attributes": [],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An opaque ID that uniquely identifies a place. If you previously created a place in Apple Maps for the place you associate with your advanced App Clip experience, use its ID instead of creating a new ID.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "placeId",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        },
        {
          "attributes": [
            {
              "kind": "allowedValues",
              "values": [
                "OWNER",
                "AUTHORIZED",
                "OTHER"
              ]
            }
          ],
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A navigational link to related data and included resource types and IDs.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "relationship",
          "required": false,
          "type": [
            {
              "kind": "text",
              "text": "string"
            }
          ]
        }
      ],
      "kind": "properties",
      "title": "Properties"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Apple Maps uses any location data that you provide solely for matching an App Clip experience to an existing location. If it can’t find a match, Apple Maps doesn’t use the provided location data.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
