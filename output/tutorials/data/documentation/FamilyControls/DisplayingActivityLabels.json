  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To display an application, category, or web domain, choose an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/ManagedSettings/ApplicationToken",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/ManagedSettings/ActivityCategoryToken",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/ManagedSettings/WebDomainToken",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that represents an item selected by the user.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example, you can access these tokens from the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FamilyControls/documentation/FamilyControls/FamilyActivitySelection",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " bound",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FamilyControls/documentation/FamilyControls/FamilyActivityPicker",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use one of these tokens to create a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Label",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "instance by passing the token to its initializer.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Display the activity item like any SwiftUI view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ExampleView: View {",
            "    @State var selection = FamilyActivitySelection()",
            "",
            "    var body: some View {",
            "        VStack {",
            "            FamilyActivityPicker(selection: $selection)",
            "            if let applicationToken = selection.applicationTokens.first {",
            "                Label(applicationToken)",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
