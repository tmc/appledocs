    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To protect the user’s privacy, ",
              "type": "text"
            },
            {
              "code": "FamilyActivitySelection",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "holds opaque values that represent categories, applications, and web domains",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "selected by the user.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can then pass these opaque values to instances and methods",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "from the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/ManagedSettings",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/DeviceActivity",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " frameworks",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to set up and manage parental controls.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If a user, parent, or guardian revokes authorization of your app,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "any tokens that ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.FamilyControls/documentation/FamilyControls/FamilyActivitySelection",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " provided while your app",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "was authorized are voided.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "For more information on prompting the user to select items, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FamilyControls/documentation/FamilyControls/FamilyActivityPicker",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
