    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To prompt the user for their selection,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "create a binding to a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FamilyControls/documentation/FamilyControls/FamilyActivitySelection",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instance,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and use the binding to create a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FamilyControls/documentation/FamilyControls/FamilyActivityPicker",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instance.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "You can then display the picker like any SwiftUI view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ExampleView: View {",
            "    @State var selection = FamilyActivitySelection()",
            "",
            "    var body: some View {",
            "        VStack {",
            "            Image(systemName: \"eye\")",
            "                .font(.system(size: 76.0))",
            "                .padding()",
            "",
            "            FamilyActivityPicker(selection: $selection)",
            "",
            "            Image(systemName: \"hourglass\")",
            "                .font(.system(size: 76.0))",
            "                .padding()",
            "        }",
            "        .onChange(of: selection) { newSelection in",
            "            let applications = selection.applications",
            "            let categories = selection.categories",
            "            let webDomains = selection.webDomains",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "FamilyControls/FamilyActivityPicker",
                  "type": "codeVoice"
                },
                {
                  "text": " shown on a parent device only displays",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "applications and websites from authorized child devices within the Family Sharing Group.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "FamilyControls/FamilyActivityPicker",
                  "type": "codeVoice"
                },
                {
                  "text": " shown on an individually authorized device",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "includes applications and websites from that same device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "To streamline this process,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "call  the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FamilyControls/documentation/FamilyControls/FamilyActivityPicker/familyActivityPicker(isPresented:selection:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier on a view in your user interface.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This modifier displays the picker view as a sheet over your",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "user interface when the ",
              "type": "text"
            },
            {
              "code": "isPresented",
              "type": "codeVoice"
            },
            {
              "text": " binding is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ExampleView: View {",
            "    @State var selection = FamilyActivitySelection()",
            "    @State var isPresented = false",
            "",
            "   var body: some View {",
            "       Button(\"Present FamilyActivityPicker\") { isPresented = true }",
            "       .familyActivityPicker(isPresented: $isPresented,",
            "                             selection: $selection)",
            "       .onChange(of: selection) { newSelection in",
            "           let applications = selection.applications",
            "           let categories = selection.categories",
            "           let webDomains = selection.webDomains",
            "       }",
            "   }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When you present the ",
              "type": "text"
            },
            {
              "code": "FamilyActivityPicker",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the system displays a view where the user can select categories, applications, and web domains.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "As soon as the user confirms their selection,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the system updates the ",
              "type": "text"
            },
            {
              "code": "FamilyActivitySelection",
              "type": "codeVoice"
            },
            {
              "text": " binding with the user’s selections.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "To protect the user’s privacy,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the system uses opaque values to represent the selections.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Your app passes the selected values to the appropriate instances and methods",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "from the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/ManagedSettings",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/DeviceActivity",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " frameworks.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
