    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value to monitor for changes to determine when to play.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trigger"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure to determine whether to play the feedback and",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "what type of feedback to play when ",
                  "type": "text"
                },
                {
                  "code": "trigger",
                  "type": "codeVoice"
                },
                {
                  "text": " changes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "feedback"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, you could play different feedback for different state",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "transitions:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyView: View {",
            "    @State private var phase = Phase.inactive",
            "",
            "    var body: some View {",
            "        ContentView(phase: $phase)",
            "            .sensoryFeedback(trigger: phase) { old, new in",
            "                switch (old, new) {",
            "                    case (.inactive, _): return .success",
            "                    case (_, .expanded): return .impact",
            "                    default: return nil",
            "                }",
            "            }",
            "    }",
            "",
            "    enum Phase {",
            "        case inactive",
            "        case preparing",
            "        case active",
            "        case expanded",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
