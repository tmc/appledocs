              "inlineContent": [
                {
                  "text": "A binding to ",
                  "type": "text"
                },
                {
                  "code": "Bool",
                  "type": "codeVoice"
                },
                {
                  "text": " controlling the presented state.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isPresented"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The inspector content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Apply this modifier to declare an inspector with a context-dependent",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "presentation. For example, an inspector can present as a trailing",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "column in a horizontally regular size class, but adapt to a sheet in a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "horizontally compact size class.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ShapeEditor: View {",
            "    @State var presented: Bool = false",
            "    var body: some View {",
            "        MyEditorView()",
            "            .inspector(isPresented: $presented) {",
            "                TextTraitsInspectorView()",
            "            }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Trailing column inspectors have their presentation state",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "restored by the framework.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "code": "InspectorCommands",
                  "type": "codeVoice"
                },
                {
                  "text": " for including the default inspector",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "commands and keyboard shortcuts.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "See Also",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
