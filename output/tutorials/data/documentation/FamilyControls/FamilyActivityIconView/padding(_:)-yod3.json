      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that’s padded by different amounts on each edge.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to add a different amount of padding on each edge",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of a view:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "VStack {",
            "    Text(\"Text padded by different amounts on each edge.\")",
            "        .padding(EdgeInsets(top: 10, leading: 20, bottom: 40, trailing: 0))",
            "        .border(.gray)",
            "    Text(\"Unpadded text for comparison.\")",
            "        .border(.yellow)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The order in which you apply modifiers matters. The example above",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "applies the padding before applying the border to ensure that the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "border encompasses the padded region:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To pad a view on specific edges with equal padding for all padded",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "edges, use ",
              "type": "text"
            },
            {
              "code": "View/padding(_:_:)",
              "type": "codeVoice"
            },
            {
              "text": ". To pad all edges of a view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "equally, use ",
              "type": "text"
            },
            {
              "code": "View/padding(_:)",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
