      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding that indicates whether the app presents the picker view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isPresented"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding that manages the user-selected categories, apps, and web domains.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "selection"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this view modifier to present a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FamilyControls/documentation/FamilyControls/FamilyActivityPicker",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ExampleView: View {",
            "    @State var selection = FamilyActivitySelection()",
            "    @State var isPresented = false",
            "",
            "   var body: some View {",
            "       Button(\"Present FamilyActivityPicker\") { isPresented = true }",
            "       .familyActivityPicker(isPresented: $isPresented,",
            "                             selection: $selection)",
            "       .onChange(of: selection) { newSelection in",
            "           let applications = selection.applications",
            "           let categories = selection.categories",
            "           let webDomains = selection.webDomains",
            "       }",
            "   }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
