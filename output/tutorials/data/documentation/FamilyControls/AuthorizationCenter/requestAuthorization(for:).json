    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.FamilyControls/documentation/FamilyControls/FamilyControlsMember",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that requests authorization",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
