          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional string to display as a badge.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Set the value to ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to hide the badge.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "label"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use a badge to convey optional, supplementary information about a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view. Keep the contents of the badge as short as possible. Badges",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "appear only in list rows, tab bars, and menus.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier creates a ",
              "type": "text"
            },
            {
              "code": "Text",
              "type": "codeVoice"
            },
            {
              "text": " view on your behalf, and treats the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "localized key similar to ",
              "type": "text"
            },
            {
              "code": "Text/init(_:)-9d1g4",
              "type": "codeVoice"
            },
            {
              "text": ". The following",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example shows a list with a “Default” badge on one of its rows.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NavigationView {",
            "    List(servers) { server in",
            "        Text(server.name)",
            "            .badge(server.defaultString())",
            "    }",
            "    .navigationTitle(\"Servers\")",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
