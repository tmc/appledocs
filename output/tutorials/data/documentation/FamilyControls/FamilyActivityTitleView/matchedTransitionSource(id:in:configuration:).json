      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier, often derived from the identifier of",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the data being displayed by the view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The namespace in which defines the ",
                  "type": "text"
                },
                {
                  "code": "id",
                  "type": "codeVoice"
                },
                {
                  "text": ". New",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "namespaces are created by adding an ",
                  "type": "text"
                },
                {
                  "code": "Namespace",
                  "type": "codeVoice"
                },
                {
                  "text": " variable",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to a ",
                  "type": "text"
                },
                {
                  "code": "View",
                  "type": "codeVoice"
                },
                {
                  "text": " type and reading its value in the view’s body",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "method.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "namespace"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that you can use to apply styling to the source.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "configuration"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The appearance of the source can be configured using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "configuration",
              "type": "codeVoice"
            },
            {
              "text": " trailing closure. Any modifiers applied will",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "be smoothly interpolated when a zoom transition originates from",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "this matched transition source.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "MyView()",
            "    .matchedTransitionSource(id: someID, in: someNamespace) { source in",
            "        source",
            "            .cornerRadius(8.0)",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
