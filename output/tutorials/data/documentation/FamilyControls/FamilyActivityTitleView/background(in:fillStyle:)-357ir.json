      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An instance of a type that conforms to ",
                  "type": "text"
                },
                {
                  "code": "Shape",
                  "type": "codeVoice"
                },
                {
                  "text": " that",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "SwiftUI draws behind the view using the ",
                  "type": "text"
                },
                {
                  "code": "ShapeStyle/background",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "shape style.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shape"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "FillStyle",
                  "type": "codeVoice"
                },
                {
                  "text": " to use when drawing the shape.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The default style uses the nonzero winding number rule and",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "antialiasing.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillStyle"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view with the specified shape drawn behind it.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier behaves like ",
              "type": "text"
            },
            {
              "code": "View/background(_:in:fillStyle:)",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "except that it always uses the ",
              "type": "text"
            },
            {
              "code": "ShapeStyle/background",
              "type": "codeVoice"
            },
            {
              "text": " shape style",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to fill the specified shape. For example, you can create a ",
              "type": "text"
            },
            {
              "code": "Path",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that outlines a trapezoid:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let trapezoid = Path { path in",
            "    path.move(to: .zero)",
            "    path.addLine(to: CGPoint(x: 90, y: 0))",
            "    path.addLine(to: CGPoint(x: 80, y: 50))",
            "    path.addLine(to: CGPoint(x: 10, y: 50))",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Then you can use that shape as a background for a ",
              "type": "text"
            },
            {
              "code": "Label",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ZStack {",
            "    Color.teal",
            "    Label(\"Flag\", systemImage: \"flag.fill\")",
            "        .padding()",
            "        .background(in: trapezoid)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Without the background modifier, the fill color shows",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "through the label. With the modifier, the labelâ€™s text and icon appear",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "backed by a shape filled with a color thatâ€™s appropriate for light",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or dark appearance:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To create a background with other ",
              "type": "text"
            },
            {
              "code": "View",
              "type": "codeVoice"
            },
            {
              "text": " types â€” or with a stack",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of views â€” use ",
              "type": "text"
            },
            {
              "code": "View/background(alignment:content:)",
              "type": "codeVoice"
            },
            {
              "text": " instead.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "To add a ",
              "type": "text"
            },
            {
              "code": "ShapeStyle",
              "type": "codeVoice"
            },
            {
              "text": " as a background, use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "View/background(_:ignoresSafeAreaEdges:)",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
