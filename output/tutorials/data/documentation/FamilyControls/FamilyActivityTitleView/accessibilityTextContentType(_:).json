      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The accessibility content type from the available",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "AccessibilityTextContentType",
                  "type": "codeVoice"
                },
                {
                  "text": " options.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to set the content type of this accessibility",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "element. Assistive technologies can use this property to choose",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "an appropriate way to output the text. For example, when",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "encountering a source coding context, VoiceOver could",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "choose to speak all punctuation.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The default content type ",
              "type": "text"
            },
            {
              "code": "AccessibilityTextContentType/plain",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
