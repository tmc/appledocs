          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "rendering system applies smoothing to the edges of the clipping",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "rectangle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "antialiased"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that clips this view to its bounding frame with the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "specified corner radius.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "By default, a view’s bounding frame only affects its layout, so any",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "content that extends beyond the edges of the frame remains visible. Use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "cornerRadius(_:antialiased:)",
              "type": "codeVoice"
            },
            {
              "text": " to hide any content that extends beyond",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "these edges while applying a corner radius.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following code applies a corner radius of 25 to a text view:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Text(\"Rounded Corners\")",
            "    .frame(width: 175, height: 75)",
            "    .foregroundColor(Color.white)",
            "    .background(Color.black)",
            "    .cornerRadius(25)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
