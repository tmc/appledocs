          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to change the corner radius of a presentation.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    @State private var showSettings = false",
            "",
            "    var body: some View {",
            "        Button(\"View Settings\") {",
            "            showSettings = true",
            "        }",
            "        .sheet(isPresented: $showSettings) {",
            "            SettingsView()",
            "                .presentationDetents([.medium, .large])",
            "                .presentationCornerRadius(21)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
