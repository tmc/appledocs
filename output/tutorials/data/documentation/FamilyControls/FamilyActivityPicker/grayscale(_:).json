    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The intensity of grayscale to apply from 0.0 to less",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "than 1.0. Values closer to 0.0 are more colorful, and values closer to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "1.0 are less colorful.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "amount"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that adds a grayscale effect to this view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A grayscale effect reduces the intensity of colors in this view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below shows a series of red squares with their grayscale",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "effect increasing from 0 (reddest) to 99% (fully desaturated) in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "approximate 20% increments:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Saturation: View {",
            "    var body: some View {",
            "        HStack {",
            "            ForEach(0..<6) {",
            "                Color.red.frame(width: 60, height: 60, alignment: .center)",
            "                    .grayscale(Double($0) * 0.1999)",
            "                    .overlay(Text(\"\\(Double($0) * 0.1999 * 100, specifier: \"%.4f\")%\"),",
            "                             alignment: .bottom)",
            "                    .border(Color.gray)",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
