    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A title for this view to display in the navigation bar.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "title"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style to use for displaying the navigation bar title.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "displayMode"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "navigationBarTitle(_:displayMode:)",
              "type": "codeVoice"
            },
            {
              "text": " to set the title of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "navigation bar for this view and specify a display mode for the title",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "from one of the ",
              "type": "text"
            },
            {
              "code": "NavigationBarItem/TitleDisplayMode",
              "type": "codeVoice"
            },
            {
              "text": " styles. This",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier only takes effect when this view is inside of and visible",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "within a ",
              "type": "text"
            },
            {
              "code": "NavigationView",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, text for the navigation bar title is provided",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using a ",
              "type": "text"
            },
            {
              "code": "Text",
              "type": "codeVoice"
            },
            {
              "text": " view. The navigation bar title’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "NavigationBarItem/TitleDisplayMode",
              "type": "codeVoice"
            },
            {
              "text": " is set to ",
              "type": "text"
            },
            {
              "code": ".inline",
              "type": "codeVoice"
            },
            {
              "text": " which places",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the navigation bar title in the bounds of the navigation bar.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct FlavorView: View {",
            "   let items = [\"Chocolate\", \"Vanilla\", \"Strawberry\", \"Mint Chip\",",
            "                \"Pistachio\"]",
            "   var body: some View {",
            "        NavigationView {",
            "            List(items, id: \\.self) {",
            "                Text($0)",
            "            }",
            "            .navigationBarTitle(Text(\"Today's Flavors\", displayMode: .inline)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
