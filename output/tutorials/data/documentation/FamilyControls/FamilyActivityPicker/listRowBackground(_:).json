      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "View",
                  "type": "codeVoice"
                },
                {
                  "text": " to use as the background behind the list",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "row view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "view"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A list row view with ",
              "type": "text"
            },
            {
              "code": "view",
              "type": "codeVoice"
            },
            {
              "text": " as its background view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "listRowBackground(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to place a custom background view behind a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "list row item.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, the ",
              "type": "text"
            },
            {
              "code": "Flavor",
              "type": "codeVoice"
            },
            {
              "text": " enumeration provides content for list",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "items. The SwiftUI ",
              "type": "text"
            },
            {
              "code": "ForEach",
              "type": "codeVoice"
            },
            {
              "text": " structure computes views for each element",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of the ",
              "type": "text"
            },
            {
              "code": "Flavor",
              "type": "codeVoice"
            },
            {
              "text": " enumeration and extracts the raw value of each of its",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "elements using the resulting text to create each list row item. The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "listRowBackground(_:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier then places the view you supply behind",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "each of the list row items:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    enum Flavor: String, CaseIterable, Identifiable {",
            "        var id: String { self.rawValue }",
            "        case vanilla, chocolate, strawberry",
            "    }",
            "",
            "    var body: some View {",
            "        List {",
            "            ForEach(Flavor.allCases) {",
            "                Text($0.rawValue)",
            "                    .listRowBackground(Ellipse()",
            "                                        .background(Color.clear)",
            "                                        .foregroundColor(.purple)",
            "                                        .opacity(0.3)",
            "                    )",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
