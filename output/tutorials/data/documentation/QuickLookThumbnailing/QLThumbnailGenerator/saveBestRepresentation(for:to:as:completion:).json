{
  "abstract": [
    {
      "text": "Saves a thumbnail for the request on disk at fileURL.",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "The file saved at fileURL has to be deleted when it is not used anymore.",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "This is primarily intended for file provider extensions which need to upload thumbnails and have a small memory limit.",
      "type": "text"
    }
  ],
