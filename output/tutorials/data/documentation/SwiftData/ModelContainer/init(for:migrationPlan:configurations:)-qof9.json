      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A schema that maps your app’s model classes to the associated data in the app’s persistent storage. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftData/documentation/SwiftData/Schema",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "givenSchema"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A plan that describes the evolution of your app’s schema and how the container migrates between specific versions. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftData/documentation/SwiftData/SchemaMigrationPlan",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "migrationPlan"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of configurations that describe how the container manages the persisted data for specific groups of models. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftData/documentation/SwiftData/ModelConfiguration",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "configurations"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A container must have at least one configuration. If you don’t",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "specify any, the framework creates an instance of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftData/documentation/SwiftData/ModelConfiguration",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "you by combining your app’s entitlements with the type’s default values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
