        },
        {
          "inlineContent": [
            {
              "text": "When the context nexts commits its changes, SwiftData removes the model from",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the persistent storage. If the model is new and in an unsaved state, the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "context simply discards it.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
