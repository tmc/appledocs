      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of options to apply to the attached property to customize its behavior. For possible values, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftData/documentation/SwiftData/Schema/Attribute/Option",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The previous name of the attribute, if it’s different to the one in the current schema version. The default value is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "originalName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique hash value that represents the most recent version of the attached property. The default value is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "hashModifier"
        }
      ]
    },
    {
