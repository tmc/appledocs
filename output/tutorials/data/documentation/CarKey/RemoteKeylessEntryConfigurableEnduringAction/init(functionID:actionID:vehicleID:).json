      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The vehicle-specific code that identifies the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "feature to activate. For example, the vehicle might have a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "specific code to identify the roof mechanism on a convertible vehicle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "functionID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The vehicle-specific code that defines what action",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to take on the vehicle feature. For example, a vehicle might have",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "separate actions to raise or lower the top on a convertible.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "actionID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The target vehicle for the action. Choose the vehicle",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "from one of the session’s vehicle reports. Specify the string",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "in the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.CarKey/documentation/CarKey/VehicleReport/identifier",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " property of the corresponding report.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "vehicleID"
        }
      ]
    },
