        {
          "inlineContent": [
            {
              "text": "Before attempting to begin a session, ensure the user’s device supports room scanning. This property is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " if the device contains a LiDAR Scanner; otherwise, ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
