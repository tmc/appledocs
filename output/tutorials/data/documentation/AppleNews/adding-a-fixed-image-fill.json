        {
          "anchor": "Add-a-New-Container-with-a-Fixed-Image-Fill",
          "level": 3,
          "text": "Add a New Container with a Fixed Image Fill",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNews/setting-up-the-advanced-tutorials#Download-the-Article-Bundle-Examples",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", you downloaded a bundle called ",
              "type": "text"
            },
            {
              "code": "News_Design_Tutorial_Advanced_Article_3",
              "type": "codeVoice"
            },
            {
              "text": " that contains a thumbnail image. Now, you’ll move that image into your working folder and use it as an image fill.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Move the ",
                      "type": "text"
                    },
                    {
                      "code": "static-background.jpg",
                      "type": "codeVoice"
                    },
                    {
                      "text": " file from the ",
                      "type": "text"
                    },
                    {
                      "code": "News_Design_Tutorial_Advanced_Article_3",
                      "type": "codeVoice"
                    },
                    {
                      "text": " folder to the folder that contains your working ",
                      "type": "text"
                    },
                    {
                      "code": "article.json",
                      "type": "codeVoice"
                    },
                    {
                      "text": " file.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Copy the example code ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.applenews/documentation/AppleNews/adding-a-fixed-image-fill#Fixed-Image-Fill-Copy-This-Code",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Paste the code inside your second section, near the end, before this code: ",
                      "type": "text"
                    },
                    {
                      "code": "{ \"role\": tweet,",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "orderedList"
        },
        {
          "inlineContent": [
            {
              "text": "Your code should look like the example code ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.applenews/documentation/AppleNews/adding-a-fixed-image-fill#Fixed-Image-Fill-Result",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Fixed-Image-Fill-Copy-This-Code",
          "level": 4,
          "text": "Fixed Image Fill: Copy This Code",
          "type": "heading"
        },
        {
          "code": [
            "        {",
            "          \"role\": \"container\",",
            "          \"layout\": \"fullScreenImageLayout\",",
            "          \"style\": {",
            "            \"fill\": {",
            "              \"type\": \"image\",",
            "              \"URL\": \"bundle://static-background.jpg\",",
            "              \"fillMode\": \"cover\",",
            "              \"attachment\": \"fixed\",",
            "              \"verticalAlignment\": \"center\",",
            "              \"horizontalAlignment\": \"center\"",
            "            }",
            "          },",
            "          \"components\": [",
            "            {",
            "              \"role\": \"pullquote\",",
            "              \"layout\": \"fullMarginBelowLayout\",",
            "              \"textStyle\": \"pullquoteLight\",",
            "              \"text\": \"“QUIA CONSEQUUNTUR MAGNI DOLORES EOS QUI RATIONE VOLUPTATEM SEQUI NESCIUNT.”\",",
            "              \"anchor\": {",
            "                \"targetAnchorPosition\": \"center\"",
            "              }",
            "            }",
            "          ]",
            "        },"
          ],
          "syntax": "json",
          "type": "codeListing"
        },
