{
  "abstract": [
    {
      "text": "Update your article template so that your article adapts when Dark Mode is active.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
