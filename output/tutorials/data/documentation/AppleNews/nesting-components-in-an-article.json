{
  "abstract": [
    {
      "text": "Use container components to create the component hierarchies you need for special article designs.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.applenews/documentation/AppleNews",
