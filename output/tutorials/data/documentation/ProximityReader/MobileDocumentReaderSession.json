            {
              "code": "MobileDocumentReaderSession",
              "type": "codeVoice"
            },
            {
              "text": " object to read mobile documents from a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "properly configured device. Donâ€™t create this object directly.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Instead, obtain one by calling the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.ProximityReader/documentation/ProximityReader/MobileDocumentReader/prepare(using:)",
              "isActive": true,
