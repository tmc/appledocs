          "content": [
            {
              "inlineContent": [
                {
                  "text": "The desired batch size, if no size is provided, the framework uses a  batch size of ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " that returns all the payments",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "stored  in the batch.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
