      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method if the transmission of the batch to the partner service provider fails and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the batch processing status is unknown. The framework doesn’t delete the reads; instead, it",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "dissociates them from the batch.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This method throws a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.ProximityReader/documentation/ProximityReader/PaymentCardReaderStore/StoreError",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " if there’s no batch for the framework to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "reset.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Throws",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
