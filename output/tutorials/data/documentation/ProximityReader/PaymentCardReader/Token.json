        {
          "inlineContent": [
            {
              "text": "You must create a secure token to use Tap to Pay on iPhone. Your payment",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "service provider supplies the string you use to create this token. Create",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "your token and pass it to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.ProximityReader/documentation/ProximityReader/PaymentCardReader/prepare(using:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "method to configure the current device to read cards.",
              "type": "text"
            }
          ],
