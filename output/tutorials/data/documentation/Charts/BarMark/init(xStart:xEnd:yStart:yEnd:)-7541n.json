    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The x start position. If ",
                  "type": "text"
                },
                {
                  "code": "xStart",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " then the rectangle will start at the leading edge of the plotting area.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "xStart"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The x end position. If ",
                  "type": "text"
                },
                {
                  "code": "xStart",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " then the rectangle will end at the trailing edge of the plotting area.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "xEnd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value plotted to y start.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "yStart"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value plotted to y end.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "yEnd"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 3,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to show vertical intervals for one category:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Chart(data) {",
            "   BarMark(",
            "       yStart: .value(\"Start Time\", $0.start),",
            "       yEnd: .value(\"End Time\", $0.end)",
            "   )",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "BarMarkSwift.BarMarkVertical1DIntervalBarChart.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
