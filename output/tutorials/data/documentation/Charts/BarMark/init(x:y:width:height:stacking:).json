    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 3,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to create a chart with one or more bars.  For horizontal bars, plot categories or dates with y and numbers with x. For vertical bars,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "plot categories or dates with x and numbers with y:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Chart(data) {",
            "    BarMark(",
            "        x: .value(\"Department\", $0.department),",
            "        y: .value(\"Profit\", $0.profit)",
            "    )",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "BarMarkSwift.BarMarkBarChart.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
