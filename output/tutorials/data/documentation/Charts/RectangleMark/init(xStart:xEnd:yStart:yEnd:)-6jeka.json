    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "xStart: The value plotted with x start.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "xEnd: The value plotted with x end.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "yStart: The y end position. If ",
                      "type": "text"
                    },
                    {
                      "code": "yStart",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is ",
                      "type": "text"
                    },
                    {
                      "code": "nil",
                      "type": "codeVoice"
                    },
                    {
                      "text": " then the rectangle will start at the leading edge of the plotting area.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "yEnd: The y end position. If ",
                      "type": "text"
                    },
                    {
                      "code": "yEnd",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is ",
                      "type": "text"
                    },
                    {
                      "code": "nil",
                      "type": "codeVoice"
                    },
                    {
                      "text": " then the rectangle will end at the trailing edge of the plotting area.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Discussion",
          "level": 3,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to map the x start and x end to a rectangle for each data element. Optionally, specify the yStart or yEnd position of the rectangles.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below omits the optional ",
              "type": "text"
            },
            {
              "code": "y",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "height",
              "type": "codeVoice"
            },
            {
              "text": " fields and uses a number scale starting at (0,0) and ending at (6,6). The rectangle has the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "coordinates: (2,0), (2,6), (4,6), (4,0).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Chart(data) {",
            "    RectangleMark(",
            "        xStart: .value(\"Rect xStart\", 2),",
            "        xEnd: .value(\"Rect xEnd\", 4)",
            "    )",
            "    .opacity(0.2)",
            "",
            "    PointMark(",
            "        x: .value(\"X\", $0.x),",
            "        y: .value(\"Y\", $0.y)",
            "    )",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "RectangleMarkSwift.RectangleMarkScatterWithRectangleXInterval.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
