    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 3,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to plot one property with x and another property with y:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Chart(data) {",
            "    PointMark(",
            "        x: .value(\"Wing Length\", $0.wingLength),",
            "        y: .value(\"Wing Length\", $0.wingWidth)",
            "    )",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "PointMarkSwift.PointMarkScatterChart.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For more background, see the first example used in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Charts/documentation/Charts/PointMark",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " which shows the structure that contains the ",
              "type": "text"
            },
            {
              "code": "wingLength",
              "type": "codeVoice"
            },
            {
              "text": " and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "wingHeight",
              "type": "codeVoice"
            },
            {
              "text": " properties.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
