    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 3,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to create a multi-line chart from a single data source.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Chart(sunshineData) {",
            "    LineMark(",
            "        x: .value(\"Month\", $0.date),",
            "        y: .value(\"Hours of Sunshine\", $0.hoursOfSunshine)",
            "    )",
            "    .foregroundStyle(by: .value(\"City\", $0.city))",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "LineMarkSwift.LineMarkMultiSeriesLineChart.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
