    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The data used for positioning marks.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The axis to position marks along. Set this to ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to use a default configuration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axis"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The span of the positioned marks. Use this to control",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the total amount space available to the marks.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "span"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The code below creates a grouped bar chart that positions",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "marks with the same “product” along the horizontal axis by their “type”.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Chart(cars) {",
            "    BarMark(",
            "        x: .value(\"product\", $0.product),",
            "        y: .value(\"price\", $0.price)",
            "    )",
            "    .position(by: .value(\"type\", $0.type), axis: .horizontal)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
