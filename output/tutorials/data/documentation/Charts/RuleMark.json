    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can use ",
              "type": "text"
            },
            {
              "code": "RuleMark",
              "type": "codeVoice"
            },
            {
              "text": " to plot a horizontal or vertical rule in your chart.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Show-Range-with-Rule-Marks",
          "level": 3,
          "text": "Show Range with Rule Marks",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To create a horizontal line at a ",
              "type": "text"
            },
            {
              "code": "y",
              "type": "codeVoice"
            },
            {
              "text": " position from ",
              "type": "text"
            },
            {
              "code": "xStart",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "xEnd",
              "type": "codeVoice"
            },
            {
              "text": " you use the ",
              "type": "text"
            },
            {
              "code": "init(xStart:xEnd:y:)",
              "type": "codeVoice"
            },
            {
              "text": " or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "init(xStart:xEnd:y:)",
              "type": "codeVoice"
            },
            {
              "text": ".  To create a vertical line at an ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": " position from ",
              "type": "text"
            },
            {
              "code": "yStart",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "yEnd",
              "type": "codeVoice"
            },
            {
              "text": " you use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "init(x:yStart:yEnd:)",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "init(x:yStart:yEnd:)",
              "type": "codeVoice"
            },
            {
              "text": ". The example below uses the ",
              "type": "text"
            },
            {
              "code": "Pollen",
              "type": "codeVoice"
            },
            {
              "text": " structure and the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "init(xStart:xEnd:y:)",
              "type": "codeVoice"
            },
            {
              "text": " to map horizontal lines that span from the value of the ",
              "type": "text"
            },
            {
              "code": "startDate",
              "type": "codeVoice"
            },
            {
              "text": " to the value of the ",
              "type": "text"
            },
            {
              "code": "endDate",
              "type": "codeVoice"
            },
            {
              "text": "  for x positions to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a pollen ",
              "type": "text"
            },
            {
              "code": "source",
              "type": "codeVoice"
            },
            {
              "text": " propertyâ€™s y position:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Pollen {",
            "    var source: String",
            "    var startDate: Date",
            "    var endDate: Date",
            "",
            "    init(startMonth: Int, numMonths: Int, source: String) {",
            "        self.source = source",
            "        let calendar = Calendar.autoupdatingCurrent",
            "        self.startDate = calendar.date(from: DateComponents(year: 2020, month: startMonth, day: 1))!",
            "        self.endDate = calendar.date(byAdding: .month, value: numMonths, to: startDate)!",
            "    }",
            "}",
            "",
            "var data: [Pollen] = [",
            "    Pollen(startMonth: 1, numMonths: 9, source: \"Trees\"),",
            "    Pollen(startMonth: 12, numMonths: 1, source: \"Trees\"),",
            "    Pollen(startMonth: 3, numMonths: 8, source: \"Grass\"),",
            "    Pollen(startMonth: 4, numMonths: 8, source: \"Weeds\")",
            "]",
            "",
            "var body: some View {",
            "    Chart(data) {",
            "        RuleMark(",
            "            xStart: .value(\"Start Date\", $0.startDate),",
            "            xEnd: .value(\"End Date\", $0.endDate),",
            "            y: .value(\"Pollen Source\", $0.source)",
            "        )",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "LineSegmentMarkSwift.LineSegmentMarkHorizontalLineSegmentChart.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Annotate-a-chart-with-rule-mark",
          "level": 3,
          "text": "Annotate a chart with rule mark",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can annotate a chart with horizontal or vertically spanning rules. This allows viewers to easily compare values over a range to a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "constant value.  Use the",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Charts/documentation/Charts/RuleMark/init(xStart:xEnd:y:)-444cp",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " initializer to represent a constant ",
              "type": "text"
            },
            {
              "code": "y",
              "type": "codeVoice"
            },
            {
              "text": " value or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Charts/documentation/Charts/RuleMark/init(x:yStart:yEnd:)-6zemd",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for a constant ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": " value.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "To span the plotting area of a chart with a line, omit the optional start and end parameters and plot a constant value.  The example below",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "results in a line that spans the chart horizontally at the y position of 9000:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct DepartmentProfit {",
            "    var department: String",
            "    var profit: Double",
            "}",
            "",
            "var data: [DepartmentProfit] = [",
            "    DepartmentProfit(department: \"Production\", profit: 15000),",
            "    DepartmentProfit(department: \"Marketing\", profit: 8000),",
            "    DepartmentProfit(department: \"Finance\", profit: 10000)",
            "]",
            "",
            "var body: some View {",
            "    Chart",
            "        ForEach(data) {",
            "            BarMark(",
            "                x: .value(\"Department\", $0.department),",
            "                y: .value(\"Profit\", $0.profit)",
            "            )",
            "        }",
            "        RuleMark(y: .value(\"Break Even Threshold\", 9000))",
            "            .foregroundStyle(.red)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "LineSegmentMarkSwift.LineSegmentMarkBarChartWithHorizontalLineSegmentMark.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
