    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A plottable value that will map to the angular size of the sector. Itâ€™s either a value that the angle within the full circle will be proportional with, or a value range for explicit start/end angles.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "angle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The inner radius of the sector. It is either a size in points, or a ",
                  "type": "text"
                },
                {
                  "code": ".ratio",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": ".inset",
                  "type": "codeVoice"
                },
                {
                  "text": " relative to the outer radius.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "innerRadius"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The outer radius of the sector. It is either a size in points, or a ",
                  "type": "text"
                },
                {
                  "code": ".ratio",
                  "type": "codeVoice"
                },
                {
                  "text": "or ",
                  "type": "text"
                },
                {
                  "code": ".inset",
                  "type": "codeVoice"
                },
                {
                  "text": " relative to the available plot area.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "outerRadius"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A radius for the corners of the sector.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "angularInset"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to map angular positions to a sector for each data element.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
