          "content": [
            {
              "inlineContent": [
                {
                  "text": "The y position.   If ",
                  "type": "text"
                },
                {
                  "code": "y",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
