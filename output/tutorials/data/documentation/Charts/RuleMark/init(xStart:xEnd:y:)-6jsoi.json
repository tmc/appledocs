    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 3,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to create a horizontal rule at x positions from ",
              "type": "text"
            },
            {
              "code": "xStart",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "xEnd",
              "type": "codeVoice"
            },
            {
              "text": " for a single y position:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Chart(data) {",
            "    RuleMark(",
            "        xStart: .value(\"Start Date\", $0.startDate),",
            "        xEnd: .value(\"End Date\", $0.endDate)",
            "    )",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "LineSegmentMarkSwift.LineSegmentMarkHorizontalSingleLineSegmentChart.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "See the second code example in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Charts/documentation/Charts/RuleMark",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for the setup of the structure that contains the ",
              "type": "text"
            },
            {
              "code": "startDate",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "endDate",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "source",
              "type": "codeVoice"
            },
