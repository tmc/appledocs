    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "PointPlot",
              "type": "codeVoice"
            },
            {
              "text": " when you want to visualize data in the same way as with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Charts/documentation/Charts/PointMark",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", but you want to visualize an entire data collection with a single plot.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can initialize and style the plot with simple values or key paths.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Add modifiers with ",
              "type": "text"
            },
            {
              "code": "KeyPath",
              "type": "codeVoice"
            },
            {
              "text": " before adding modifiers with simple values.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Chart {",
            "    PointPlot(",
            "        flightDelays,",
            "        x: .value(\"Flight Distance\", \\.distance),",
            "        y: .value(\"Flight Delay\", \\.delay)",
            "    )",
            "    .foregroundStyle(by: .value(\"Airline\", \\.airline))",
            "    .opacity(\\.opacity)",
            "    .symbolSize(by: .value(\"Capacity\", \\.passengerCount))",
            "    .symbol(.circle)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
