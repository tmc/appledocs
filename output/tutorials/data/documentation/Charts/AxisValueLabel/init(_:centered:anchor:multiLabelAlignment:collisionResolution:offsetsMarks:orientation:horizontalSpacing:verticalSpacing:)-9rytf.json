    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A title generated from a string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "title"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether to center the label between two axis values.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", default to true for discrete data, false to continuous data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "centered"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The anchor point on the bounding box of the text element that attaches to the ",
                  "type": "text"
                },
                {
                  "code": "position",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "anchor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "How labels along the axis are aligned with each other.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "multiLabelAlignment"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "How labels that collide with others are resolved.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "collisionResolution"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether to offset marks to accomodate for the space used by the label.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offsetsMarks"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The orientation of the label.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orientation"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The horizontal spacing of the label.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", a default spacing will be used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "horizontalSpacing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The vertical spacing of the label.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "verticalSpacing"
        }
      ]
    }
