      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The session will hold a weak ARC reference to this @link NFCReaderSessionDelegate @link/ object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "delegate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dispatch queue where NFCReaderSessionDelegate delegate callbacks will be dispatched to.  A ",
                  "type": "text"
                },
                {
                  "text": "nil",
                  "type": "text"
                },
                {
                  "text": " value will",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "cause the creation of a serial dispatch queue internally for the session.  The session object will retain the provided dispatch queue.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "queue"
        }
      ]
    },
