{
  "abstract": [
    {
      "text": "Allow users to scan NFC tags without an app using background tag reading.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
