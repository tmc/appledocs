                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Metal/MTLResource/storageMode",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " property is ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/Metal/MTLStorageMode/private",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
