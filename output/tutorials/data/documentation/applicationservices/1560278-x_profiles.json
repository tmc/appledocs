    "doc://com.apple.documentation/documentation/applicationservices/1560278-x_profiles/cmabsolutecolorimetric": {
      "abstract": [
        {
          "text": "This approach is based on a device-independent color space in which the result is an idealized print viewed on an ideal type of paper having a large dynamic range and color gamut.",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/applicationservices/1560278-x_profiles/cmabsolutecolorimetric",
      "kind": "symbol",
      "role": "symbol",
      "title": "cmAbsoluteColorimetric",
      "type": "topic",
      "url": "/documentation/applicationservices/1560278-x_profiles/cmabsolutecolorimetric"
    },
    "doc://com.apple.documentation/documentation/applicationservices/1560278-x_profiles/cmperceptual": {
      "abstract": [
        {
          "text": "All the colors of a given gamut can be scaled to fit within another gamut. This intent is best suited to realistic images, such as photographic images.",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/applicationservices/1560278-x_profiles/cmperceptual",
      "kind": "symbol",
      "role": "symbol",
      "title": "cmPerceptual",
      "type": "topic",
      "url": "/documentation/applicationservices/1560278-x_profiles/cmperceptual"
    },
    "doc://com.apple.documentation/documentation/applicationservices/1560278-x_profiles/cmrelativecolorimetric": {
      "abstract": [
        {
          "text": "The colors that fall within the gamuts of both devices are left unchanged. This intent is best suited to logo images.",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/applicationservices/1560278-x_profiles/cmrelativecolorimetric",
      "kind": "symbol",
      "role": "symbol",
      "title": "cmRelativeColorimetric",
      "type": "topic",
      "url": "/documentation/applicationservices/1560278-x_profiles/cmrelativecolorimetric"
    },
    "doc://com.apple.documentation/documentation/applicationservices/1560278-x_profiles/cmsaturation": {
      "abstract": [
        {
          "text": "The relative saturation of colors is maintained from gamut to gamut. This intent is best suited to bar graphs and pie charts in which the actual color displayed is less important than its vividness.",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/applicationservices/1560278-x_profiles/cmsaturation",
