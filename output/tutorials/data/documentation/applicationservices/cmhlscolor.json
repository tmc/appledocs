        {
          "inlineContent": [
            {
              "text": "A color value expressed in the HLS color space is composed of ",
              "type": "text"
            },
            {
              "code": "hue",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "lightness",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "saturation",
              "type": "codeVoice"
            },
            {
              "text": " component values. Each color component is expressed as a numeric value within the range of 0 to 65535 inclusive. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
