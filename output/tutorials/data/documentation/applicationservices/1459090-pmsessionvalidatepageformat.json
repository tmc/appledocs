          "content": [
            {
              "inlineContent": [
                {
                  "text": "The printing session for the specified page format object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "printSession"
        },
        {
          "content": [
            {
