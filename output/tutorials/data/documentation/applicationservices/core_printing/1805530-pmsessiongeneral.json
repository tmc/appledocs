      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The function ",
              "type": "text"
            },
            {
              "code": "PMSessionGeneral",
              "type": "codeVoice"
            },
            {
              "text": " is valid for the printing session passed to the function. In macOS, the function ",
              "type": "text"
            },
            {
              "code": "PMSessionGeneral",
              "type": "codeVoice"
            },
            {
              "text": " makes an attempt to get the requested data if the opcode is ",
              "type": "text"
            },
            {
              "code": "getPSInfoOp",
              "type": "codeVoice"
            },
            {
              "text": ". Otherwise the result code ",
              "type": "text"
            },
            {
              "code": "kPMNotImplemented",
              "type": "codeVoice"
            },
            {
              "text": " is returned.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For more information about using the function ",
              "type": "text"
            },
            {
              "code": "PMSessionGeneral",
              "type": "codeVoice"
            },
            {
              "text": ", see Supporting Printing in Your Carbon Application.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
