{
  "abstract": [
    {
      "text": "Begins a print job that, by default, draws into a QuickDraw graphics port, and suppresses the printing status dialog.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "Use ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/applicationservices/1460101-pmsessionbegincgdocumentnodialog",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " instead.",
          "type": "text"
        }
      ],
      "type": "paragraph"
    }
  ],
