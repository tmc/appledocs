        },
        {
          "text": " object and call ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/applicationservices/1459274-pmcreatepageformatwithpmpaper",
          "isActive": true,
          "type": "reference"
        },
        {
