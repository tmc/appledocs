{
  "abstract": [
    {
      "text": "Obtains the graphics context for the current page in a printing session.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "Use ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/applicationservices/1461952-pmsessiongetcggraphicscontext",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " instead.",
          "type": "text"
        }
      ],
      "type": "paragraph"
    }
  ],
  "documentVersion": 0,
