            {
              "inlineContent": [
                {
                  "text": "The speech channel whose rate you wish to set.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "chan"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The new speech rate in words per minute, expressed as an integer value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rate"
        }
      ]
    },
    {
