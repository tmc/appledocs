    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The printing session that provides a context for the print job.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "printSession"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The print settings for the print job whose destination you want to set.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "printSettings"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The destination type for the print job associated with the specified printing session and print settings. Possible values include:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "kPMDestinationPrinter",
                          "type": "codeVoice"
                        },
                        {
                          "text": " (output to a printer)",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "kPMDestinationFile",
                          "type": "codeVoice"
                        },
                        {
                          "text": " (output to a file)",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "kPMDestinationFax",
                          "type": "codeVoice"
                        },
                        {
                          "text": " (output to a fax)",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "kPMDestinationPreview",
                          "type": "codeVoice"
                        },
                        {
                          "text": " (output to print preview)",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "code": "kPMDestinationProcessPDF",
                          "type": "codeVoice"
                        },
                        {
                          "text": " (output to a PDF workflow option)",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/applicationservices/pmdestinationtype",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a complete description of destination types you can specify.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The MIME type to be generated for the specified destination type. Pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " if you want to use the default format for the specified destination type. To obtain a list of valid formats for a given destination type, use the function ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/applicationservices/1461332-pmsessioncopyoutputformatlist",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destFormat"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A reference to a Core Foundation URL that specifies a destination location. You can provide this if the destination type supports a destination location. Otherwise, pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ". For example, if the destination type is a file (",
                  "type": "text"
                },
                {
                  "code": "kPMDestinationFile",
                  "type": "codeVoice"
                },
                {
                  "text": ") you can supply a file system URL to specify where the file resides.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destLocation"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A result code. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/applicationservices/core_printing#1670007",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can use the function ",
              "type": "text"
            },
            {
              "code": "PMSessionSetDestination",
              "type": "codeVoice"
            },
            {
              "text": " when you want to send print output to a file without requiring user interaction. You must call this function between the creation and release of a printing session. See the function ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/applicationservices/1463247-pmcreatesession",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
