    "doc://com.apple.documentation/documentation/applicationservices/1552256-speech-channel_modes/modeliteral": {
      "abstract": [
        {
          "text": "When the speech channel is in text-processing mode, indicates that characters and digits are spoken literally (for example, “cat” is spoken as “C-A-T” and “12” is spoken as \"one, two\"). Use this value with ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/applicationservices/1552228-speech-channel_information_constants/socharactermode",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " and ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/applicationservices/1552228-speech-channel_information_constants/sonumbermode",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": ".",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/applicationservices/1552256-speech-channel_modes/modeliteral",
      "kind": "symbol",
      "role": "symbol",
      "title": "modeLiteral",
      "type": "topic",
      "url": "/documentation/applicationservices/1552256-speech-channel_modes/modeliteral"
    },
    "doc://com.apple.documentation/documentation/applicationservices/1552256-speech-channel_modes/modenormal": {
      "abstract": [
        {
          "text": "When the speech channel is in text-processing mode, indicates that the synthesizer should process characters as expected and assemble digits into numbers. Use this value with ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/applicationservices/1552228-speech-channel_information_constants/socharactermode",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " and ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/applicationservices/1552228-speech-channel_information_constants/sonumbermode",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": ".",
          "type": "text"
        }
      ],
      "identifier": "doc://com.apple.documentation/documentation/applicationservices/1552256-speech-channel_modes/modenormal",
      "kind": "symbol",
      "role": "symbol",
      "title": "modeNormal",
      "type": "topic",
      "url": "/documentation/applicationservices/1552256-speech-channel_modes/modenormal"
    },
    "doc://com.apple.documentation/documentation/applicationservices/1552256-speech-channel_modes/modephonemes": {
      "abstract": [
        {
          "text": "Used with ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/applicationservices/1552228-speech-channel_information_constants/soinputmode",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " to indicate that the speech channel is in phoneme-processing mode. When in phoneme-processing mode, a text buffer is interpreted to be a series of characters representing various phonemes and prosodic controls.",
          "type": "text"
        }
