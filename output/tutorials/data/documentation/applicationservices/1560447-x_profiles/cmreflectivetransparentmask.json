      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Bit 0 of ",
              "type": "text"
            },
            {
              "code": "deviceAttributes[1]",
              "type": "codeVoice"
            },
            {
              "text": " specifies whether the media is transparent or reflective. If it has the value 0, the media is reflective; if it has the value 1, the media is transparent. Use the ",
              "type": "text"
            },
            {
              "code": "cmReflectiveTransparentMask",
              "type": "codeVoice"
            },
            {
              "text": " mask to set the transparent/reflective bit in ",
              "type": "text"
            },
            {
              "code": "deviceAttributes[1]",
              "type": "codeVoice"
            },
            {
              "text": " or to clear all bits except the transparent/reflective bit.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
