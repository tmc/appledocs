          "content": [
            {
              "inlineContent": [
                {
                  "text": "The speech channel into which a dictionary is to be installed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "chan"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A handle to the dictionary data. This is often a handle to a resource of type ",
                  "type": "text"
                },
                {
                  "code": "'dict'",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dictionary"
