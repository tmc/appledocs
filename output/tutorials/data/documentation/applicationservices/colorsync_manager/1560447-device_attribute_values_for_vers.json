  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ColorSync Manager defines the structure ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/applicationservices/cm2header",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to represent the profile header for the version 2.x profile format defined by the ICC.The ",
              "type": "text"
            },
            {
              "code": "deviceAttributes",
              "type": "codeVoice"
            },
            {
              "text": " field of the ",
              "type": "text"
            },
            {
              "code": "CM2Header",
              "type": "codeVoice"
            },
            {
              "text": " structure is an array of two unsigned long values whose bits specify information about a profile. The ICC reserves the use of ",
              "type": "text"
            },
            {
              "code": "deviceAttributes[1]",
              "type": "codeVoice"
            },
            {
              "text": " and has assigned values to bits 0 and 1. All the bits of ",
              "type": "text"
            },
            {
              "code": "deviceAttributes[0]",
              "type": "codeVoice"
            },
            {
              "text": " are reserved for use by color management system (CMS) vendors.  ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
