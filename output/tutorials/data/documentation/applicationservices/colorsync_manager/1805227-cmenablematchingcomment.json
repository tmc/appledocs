              "inlineContent": [
                {
                  "text": "A flag that directs the ColorSync Manager to generate a ",
                  "type": "text"
                },
                {
                  "code": "cmEnableMatchingPicComment",
                  "type": "codeVoice"
                },
                {
                  "text": " comment if ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", or a ",
                  "type": "text"
                },
                {
                  "code": "cmDisbleMatchingPicComment",
                  "type": "codeVoice"
                },
                {
                  "text": " comment if ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ". ",
                  "type": "text"
                }
              ],
