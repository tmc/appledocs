    "doc://com.apple.documentation/documentation/applicationservices/cmsaturation": {
      "abstract": [
        {
          "text": "The relative saturation of colors is maintained from gamut to gamut. This intent is best suited to bar graphs and pie charts in which the actual color displayed is less important than its vividness.",
          "type": "text"
        }
