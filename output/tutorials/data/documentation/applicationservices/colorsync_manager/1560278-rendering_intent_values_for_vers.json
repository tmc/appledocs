    "doc://com.apple.documentation/documentation/applicationservices/cmperceptual": {
      "abstract": [
        {
          "text": "All the colors of a given gamut can be scaled to fit within another gamut. This intent is best suited to realistic images, such as photographic images.",
          "type": "text"
        }
      ],
      "fragments": [
