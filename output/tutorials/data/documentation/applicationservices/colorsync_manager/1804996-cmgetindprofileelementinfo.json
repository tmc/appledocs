  "primaryContentSections": [
    {
      "declarations": [
        {
          "languages": [
            "occ"
          ],
          "platforms": [],
          "tokens": [
            {
              "kind": "text",
              "text": "CMError CMGetIndProfileElementInfo ("
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": "   CMProfileRef prof,"
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": "   UInt32 index,"
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": "   OSType *tag,"
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": "   UInt32 *elementSize,"
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": "   Boolean *refs"
            },
            {
              "kind": "text",
              "text": "\n"
            },
            {
              "kind": "text",
              "text": ");"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "kind": "parameters",
      "languages": [
        "occ"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A profile reference of type ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/applicationservices/cmprofileref",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to the profile containing the element. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "prof"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A one-based element index within the range returned by the ",
                  "type": "text"
                },
                {
                  "code": "elementCount",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter of the ",
                  "type": "text"
                },
                {
                  "code": "CMCountProfileElements",
                  "type": "codeVoice"
                },
                {
                  "text": " function. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "index"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to an element signature. On return, the tag signature of the element corresponding to the index. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tag"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to an element size. On return, the size in bytes of the element data corresponding to the tag.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "elementSize"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a reference count flag. On return, set to ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " if more than one tag in the profile refers to element data associated with the tag corresponding to the index. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "refs"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "CMError",
              "type": "codeVoice"
            },
            {
              "text": " value. See ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/applicationservices/colorsync_manager#1670295",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The index order of elements is determined internally by the ColorSync Manager and is not publicly defined.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Before calling the ",
              "type": "text"
            },
            {
              "code": "CMGetIndProfileElementInfo",
              "type": "codeVoice"
            },
            {
              "text": " function, you should call the function ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/applicationservices/colorsync_manager/1804963-cmcountprofileelements",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", which returns the total number of elements in the profile in the ",
              "type": "text"
            },
            {
              "code": "elementCount",
              "type": "codeVoice"
            },
            {
              "text": " parameter. The number you specify for the ",
              "type": "text"
            },
            {
              "code": "index",
              "type": "codeVoice"
            },
            {
              "text": " parameter when calling ",
              "type": "text"
            },
            {
              "code": "CMGetIndProfileElementInfo",
              "type": "codeVoice"
            },
            {
              "text": " should be in the range of 1 to ",
              "type": "text"
            },
            {
              "code": "elementCount",
              "type": "codeVoice"
            },
            {
              "text": "; otherwise the function will return a result code of ",
              "type": "text"
            },
            {
              "code": "cmIndexRangeErr",
              "type": "codeVoice"
            },
            {
              "text": ". ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You might want to call this function, for example, to print out the contents of a profile. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
