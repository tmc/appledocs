      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The returned profile reference. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "prof"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The location of the profile. Commonly a profile is disk-file based. However, the profile may be a file-based profile, a handle-based profile, or a pointer-based profile. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "targetLocation"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the profile set structure. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "profileSet"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A calling-program-supplied callback function that allows your application to monitor progress or abort the operation. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "proc"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A reference constant containing data specified by the calling application program. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "refCon"
        }
      ]
