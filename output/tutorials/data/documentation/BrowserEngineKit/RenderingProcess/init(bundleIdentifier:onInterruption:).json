    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle identifier of the rendering extension to launch, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to use the default bundle identifier for this app’s rendering extension.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundleIdentifier"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A block that the operating system calls if the rendering extension process exits abnormally.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onInterruption"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Your web browser app can run one instance of each of its rendering extensions.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The first time you initialize this object, the operating system launches your rendering extension.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "If you subsequently initialize new instances of ",
              "type": "text"
            },
            {
              "code": "RenderingProcess",
              "type": "codeVoice"
            },
            {
              "text": " using the same bundle identifier, they refer to the same process.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The operating system guarantees that the process launched when this initializer returns.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
