{
  "abstract": [
    {
      "text": "Provide a safer experience in your app by detecting and alerting users to nudity",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "in images and videos before displaying them onscreen.",
      "type": "text"
    }
  ],
