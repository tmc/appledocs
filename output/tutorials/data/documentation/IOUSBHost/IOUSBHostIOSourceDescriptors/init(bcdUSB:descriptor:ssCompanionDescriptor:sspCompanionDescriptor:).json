      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The USB version that the device supports, specified as a binary-coded decimal value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bcdUSB"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A valid endpoint descriptor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "descriptor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The companion descriptor for SuperSpeed devices.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "ssCompanionDescriptor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The companion descriptor for SuperSpeedPlus devices.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sspCompanionDescriptor"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "ssCompanionDescriptor",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "sspCompanionDescriptor",
              "type": "codeVoice"
            },
            {
              "text": " descriptors may be necessary for ",
              "type": "text"
            },
            {
              "code": "bcdUSB",
              "type": "codeVoice"
            },
            {
              "text": " versions ",
              "type": "text"
            },
            {
              "code": "0x0300",
              "type": "codeVoice"
            },
            {
              "text": " and greater, depending on device operating speed and values set in the descriptors. See USB 3.2, 9.5 for more information.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
