    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of the descriptor to find.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A reference to an ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/nsuinteger",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that updates with the length of the descriptor. You can use ",
                  "type": "text"
                },
                {
                  "code": "wLength",
                  "type": "codeVoice"
                },
                {
                  "text": " when searching for variable-length configuration or BOS descriptors, or when searching for nonstandard descriptor types.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "length"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A descriptor index value that is the low byte of ",
                  "type": "text"
                },
                {
                  "code": "wValue",
                  "type": "codeVoice"
                },
                {
                  "text": " of the ",
                  "type": "text"
                },
                {
                  "code": "SET_DESCRIPTOR",
                  "type": "codeVoice"
                },
                {
                  "text": " control request (See USB 3.2, 9.4.8.). The default value is ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "index"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A descriptor language ID that is the ",
                  "type": "text"
                },
                {
                  "code": "wIndex",
                  "type": "codeVoice"
                },
                {
                  "text": " of the ",
                  "type": "text"
                },
                {
                  "code": "SET_DESCRIPTOR",
                  "type": "codeVoice"
                },
                {
                  "text": " control request (See USB 3.2, 9.4.8.). The default value is ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "languageID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nserror",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that contains an ",
                  "type": "text"
                },
                {
                  "text": "doc://com.apple.documentation/documentation/kernel/ioreturn",
                  "type": "text"
                },
                {
                  "text": " value on failure.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "error"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method searches the descriptor cache for the descriptor that matches the input arguments. If the descriptor isn’t in the cache, the method issues a ",
              "type": "text"
            },
            {
              "code": "GET_DESCRIPTOR",
              "type": "codeVoice"
            },
            {
              "text": " control request (See USB 3.2, 9.4.3.) to retrieve the descriptor from the device. If the device request is successful, the method adds the retrieved descriptor to the cache.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
