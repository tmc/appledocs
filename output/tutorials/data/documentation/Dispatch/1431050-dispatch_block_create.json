              "kind": "identifier",
              "text": "dispatch_block_create"
            },
            {
              "kind": "text",
              "text": "("
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/dispatch/dispatch_block_flags_t",
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@T@dispatch_block_flags_t",
              "text": "dispatch_block_flags_t"
            },
