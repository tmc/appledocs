      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The temporal milestone ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.dispatch/documentation/Dispatch/dispatch_time",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
