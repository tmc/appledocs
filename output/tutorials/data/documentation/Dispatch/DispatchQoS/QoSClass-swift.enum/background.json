        {
          "inlineContent": [
            {
              "text": "Background tasks have the lowest priority of all tasks. Assign this class to tasks or dispatch queues that you use to perform work while your app is running in the background.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
