        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The event handler block to submit to the source’s target queue. This function performs a ",
                  "type": "text"
                },
                {
                  "code": "Block_copy",
                  "type": "codeVoice"
                },
                {
                  "text": " on behalf of the caller, and ",
                  "type": "text"
                },
                {
                  "code": "Block_release",
                  "type": "codeVoice"
                },
                {
                  "text": " on the previous handler (if any). This parameter cannot be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
