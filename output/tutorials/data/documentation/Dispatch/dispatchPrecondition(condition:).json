    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dispatch predicate for the current context to check.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "condition"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this function to detect conditions about the current execution context that must prevent the program from proceeding even in shipping code.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "In playgrounds and ",
                      "type": "text"
                    },
                    {
                      "code": "-Onone",
                      "type": "codeVoice"
                    },
                    {
                      "text": " builds (the default for Xcode’s Debug configuration): if ",
                      "type": "text"
                    },
                    {
                      "code": "condition",
                      "type": "codeVoice"
                    },
                    {
                      "text": " evaluates to ",
                      "type": "text"
                    },
                    {
                      "code": "false",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", stop program execution in a debuggable state.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "In ",
                      "type": "text"
                    },
                    {
                      "code": "-O",
                      "type": "codeVoice"
                    },
                    {
                      "text": " builds (the default for Xcode’s Release configuration): if ",
                      "type": "text"
                    },
                    {
                      "code": "condition",
                      "type": "codeVoice"
                    },
                    {
                      "text": " evaluates to ",
                      "type": "text"
                    },
                    {
                      "code": "false",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", stop program execution.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "In ",
                      "type": "text"
                    },
                    {
                      "code": "-Ounchecked",
                      "type": "codeVoice"
                    },
                    {
                      "text": " builds, ",
                      "type": "text"
                    },
                    {
                      "code": "condition",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is not evaluated, but the optimizer may assume that it would evaluate to ",
                      "type": "text"
                    },
                    {
                      "code": "true",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". Failure to satisfy that assumption in ",
                      "type": "text"
                    },
                    {
                      "code": "-Ounchecked",
                      "type": "codeVoice"
                    },
                    {
                      "text": " builds is a serious programming error.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
