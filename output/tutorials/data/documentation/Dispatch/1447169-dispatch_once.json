          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/dispatch/dispatch_once_t",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " structure that is used to test whether the block has completed or not.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "predicate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The block object to execute once.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
