{
  "abstract": [
    {
      "text": "Access tools for debugging webpages in Safari, as well as tools for debugging web content in other apps and on other devices.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.safari.developer-tools/documentation/safari-developer-tools"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "swift",
    "url": "doc://com.apple.safari.developer-tools/documentation/Safari-Developer-Tools/develop-menu"
  },
  "kind": "article",
  "legalNotices": {
    "copyright": "Copyright &copy; 2025 Apple Inc. All rights reserved.",
