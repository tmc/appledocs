  "metadata": {
    "role": "article",
    "roleHeading": "Article",
    "title": "Safari 16.1 Release Notes"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 3,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Safari 16.1 is available for macOS Big Sur, macOS Monterey, macOS Ventura, iPadOS 16.1 and iOS 16.1.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Accessibility",
          "level": 3,
          "text": "Accessibility",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issue",
          "level": 4,
          "text": "Resolved Issue",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "display:contents",
                      "type": "codeVoice"
                    },
                    {
                      "text": " buttons failing to expose their accessible name.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Authentication",
          "level": 3,
          "text": "Authentication",
          "type": "heading"
        },
        {
          "anchor": "New-Feature",
          "level": 4,
          "text": "New Feature",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for passkeys on macOS Big Sur, macOS Monterey, macOS Ventura, and iPadOS 16.1. See ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/AuthenticationServices/supporting-passkeys",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "CSS",
          "level": 3,
          "text": "CSS",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed dynamic viewport height units (",
                      "type": "text"
                    },
                    {
                      "code": "dvh",
                      "type": "codeVoice"
                    },
                    {
                      "text": ") not matching the actual viewport height.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed scroll-snap properties set on ",
                      "type": "text"
                    },
                    {
                      "code": "<body>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to stop propagating to the viewport.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed logical viewport units to properly resolve for ",
                      "type": "text"
                    },
                    {
                      "code": "font-size",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed containing blocks with a non-visible overflow incorrectly clipping ",
                      "type": "text"
                    },
                    {
                      "code": "position: fixed",
                      "type": "codeVoice"
                    },
                    {
                      "text": " descendants.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed table user-agent styles to use ",
                      "type": "text"
                    },
                    {
                      "code": "box-sizing: border-box",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "<select>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " elements with ",
                      "type": "text"
                    },
                    {
                      "code": "contain: size",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed handling layout and paint containment.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed handling ",
                      "type": "text"
                    },
                    {
                      "code": "font-variant: normal",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "font-variant: none",
                      "type": "codeVoice"
                    },
                    {
                      "text": " shorthands to reset ",
                      "type": "text"
                    },
                    {
                      "code": "font-variant-numeric",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "font-variant-alternates",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed small caps handling to prevent synthesized small caps if any character in the font supports small caps.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Forms",
          "level": 3,
          "text": "Forms",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed the ignored CSS ",
                      "type": "text"
                    },
                    {
                      "code": "color",
                      "type": "codeVoice"
                    },
                    {
                      "text": " property on ",
                      "type": "text"
                    },
                    {
                      "code": "<select>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " elements.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "<input type=\"search\">",
                      "type": "codeVoice"
                    },
                    {
                      "text": " with ",
                      "type": "text"
                    },
                    {
                      "code": "appearance: textfield",
                      "type": "codeVoice"
                    },
                    {
                      "text": "  to hide icons.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed applying the readonly attribute to the correct ",
                      "type": "text"
                    },
                    {
                      "code": "<input>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " types and ",
                      "type": "text"
                    },
                    {
                      "code": "<textarea>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " element.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed the content width for ",
                      "type": "text"
                    },
                    {
                      "code": "<input type=\"search\">",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to not include decorations.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed input type state changes to correctly handle missing or empty string value attributes.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "form.submit()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to submit a single form to multiple frames concurrently.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed cloning a ",
                      "type": "text"
                    },
                    {
                      "code": "<textarea>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to not set the initial selection at the end of the text content.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed not firing a select event when setting a selection range results in no change to the selection.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Media",
          "level": 3,
          "text": "Media",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for non-animated AVIF on macOS Ventura, and iPadOS 16.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for AVIF animated image sequences on macOS Ventura, iPadOS 16, and iOS 16.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed some AVIF images not rendering because of their container format.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Privacy",
          "level": 3,
          "text": "Privacy",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "New Features",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for web-to-App Store advertising with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/StoreKit/SKAdNetwork",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Rendering",
          "level": 3,
          "text": "Rendering",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Stopped propagating background on ",
                      "type": "text"
                    },
                    {
                      "code": "<html>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " with any containment.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Security",
          "level": 3,
          "text": "Security",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed COOP ",
                      "type": "text"
                    },
                    {
                      "code": "same-origin",
                      "type": "codeVoice"
                    },
                    {
                      "text": " breaking forms after a back navigation.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "script-src-elem",
                      "type": "codeVoice"
                    },
                    {
                      "text": " CSP in workers.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Web-API",
          "level": 3,
          "text": "Web API",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added Web Push Notifications support on macOS Ventura.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for capturing a specific window with ",
                      "type": "text"
                    },
                    {
                      "code": "getDisplayMedia()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " on macOS Ventura.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for Scroll to Text Fragment.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed focus interactions between the shadow DOM and the ",
                      "type": "text"
                    },
                    {
                      "code": "<dialog>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " element to align with specifications.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Web-Driver",
          "level": 3,
          "text": "Web Driver",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for Wheel input source and actions.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Web-Inspector",
          "level": 3,
          "text": "Web Inspector",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for the color picker to pick a color from any pixel on the screen.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
