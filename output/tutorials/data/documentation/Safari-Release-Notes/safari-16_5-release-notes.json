    "title": "Safari 16.5 Release Notes"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Safari 16.5 is available for macOS Big Sur, macOS Monterey, macOS Ventura, iPadOS 16.5, and iOS 16.5.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Apple-Pay",
          "level": 2,
          "text": "Apple Pay",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 3,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for pre-orders and deferred payments.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "CSS",
          "level": 2,
          "text": "CSS",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 3,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for CSS Nesting.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for ",
                      "type": "text"
                    },
                    {
                      "code": ":user-valid",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": ":user-invalid",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed Scroll to Text Fragment sometimes scrolling to the top after reloading the page.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed support for ",
                      "type": "text"
                    },
                    {
                      "code": "x",
                      "type": "codeVoice"
                    },
                    {
                      "text": " resolution unit in ",
                      "type": "text"
                    },
                    {
                      "code": "calc()",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed reflecting trimmed ",
                      "type": "text"
                    },
                    {
                      "code": "block-start",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "block-end",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "inline-start",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "code": "inline-end",
                      "type": "codeVoice"
                    },
                    {
                      "text": " margins for grid or flex items in computed styles.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed the top offset of self collapsing children at the end of a block container with ",
                      "type": "text"
                    },
                    {
                      "code": "block-end",
                      "type": "codeVoice"
                    },
                    {
                      "text": " margin trim.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed triggering layout when changing ",
                      "type": "text"
                    },
                    {
                      "code": "margin-trim",
                      "type": "codeVoice"
                    },
                    {
                      "text": " value.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed increasing ",
                      "type": "text"
                    },
                    {
                      "code": "column-count",
                      "type": "codeVoice"
                    },
                    {
                      "text": " above 2 not updating the layout.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed CSS custom properties not applying to an SVG ",
                      "type": "text"
                    },
                    {
                      "code": "use",
                      "type": "codeVoice"
                    },
                    {
                      "text": " element’s shadow tree.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed new CSS property unexpectedly dropped from an empty CSS rule when tabbing through or editing a selector.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed: Made ",
                      "type": "text"
                    },
                    {
                      "code": "-webkit-image-set()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " an alias of ",
                      "type": "text"
                    },
                    {
                      "code": "image-set()",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Editing--Forms",
          "level": 2,
          "text": "Editing & Forms",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed hairline on the selection of bidi text.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed photo library picker showing videos for ",
                      "type": "text"
                    },
                    {
                      "code": "accept=\"image/*\"",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "JavaScript",
          "level": 2,
          "text": "JavaScript",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Updated digital display in ",
                      "type": "text"
                    },
                    {
                      "code": "Intl.DurationFormat",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to match spec changes.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Layout",
          "level": 2,
          "text": "Layout",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed text wrapping for bidi text when line-breaking.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Lockdown-Mode",
          "level": 2,
          "text": "Lockdown Mode",
          "type": "heading"
        },
        {
          "anchor": "New-Restrictions",
          "level": 3,
          "text": "New Restrictions",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Disabled WebCodecs API",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Media",
          "level": 2,
          "text": "Media",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed non-audible AudioContext preventing the audio session to change from play-and-record after stopping capture.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed handling video streams containing a ",
                      "type": "text"
                    },
                    {
                      "code": "CodecDelay",
                      "type": "codeVoice"
                    },
                    {
                      "text": " value that caused an audible pop at the beginning of video playback.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed video freezing in a video conference when removing AirPods Pro during the call.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Scrolling",
          "level": 2,
          "text": "Scrolling",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed snapping to the last snap position when performing layout when scroll snapping occurs with a physical mouse wheel.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed pinch-to-zoom when toggling on and off scroll snapping.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed scroll snapping jumping to the previous page when swiping to the next page.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed scroll snapping to work with a physical scroll wheel on a mouse.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Rendering",
          "level": 2,
          "text": "Rendering",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed form controls rendering.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed visual updates for ",
                      "type": "text"
                    },
                    {
                      "code": "content: counter()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " when ",
                      "type": "text"
                    },
                    {
                      "code": "position: absolute",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is set.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed an unexpected visible first frame of a ",
                      "type": "text"
                    },
                    {
                      "code": "transform",
                      "type": "codeVoice"
                    },
                    {
                      "text": " animation when ",
                      "type": "text"
                    },
                    {
                      "code": "!important",
                      "type": "codeVoice"
                    },
                    {
                      "text": " style overrides the animated value.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Web-API",
          "level": 2,
          "text": "Web API",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed filling metadata headers for preflight requests.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed OffscreenCanvas WebGL to fire the context lost event.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "getFileHandle()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to return a TypeMismatchError on unexpected entry type.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Web-Apps",
          "level": 2,
          "text": "Web Apps",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed “Untitled” label on the back to previous app button when opening a web app via a link.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Web-Assembly",
          "level": 2,
          "text": "Web Assembly",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed WASM SIMD breaking WebP decoding applications.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Web-Inspector",
          "level": 2,
          "text": "Web Inspector",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 3,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added initial support for ",
                      "type": "text"
                    },
                    {
                      "code": "color-mix",
                      "type": "codeVoice"
                    },
                    {
                      "text": " CSS values.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 3,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed element ",
                      "type": "text"
                    },
                    {
                      "code": "::backdrop",
                      "type": "codeVoice"
                    },
                    {
                      "text": " rules showing up without a backdrop.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed “Selected element” console entry filling an entire row.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed an issue causing the mini console to always opens when choosing “Inspect Element”, even if it was previously closed.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
