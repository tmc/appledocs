  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Safari 16 ships with iOS 16, and is available as an update on macOS Monterey and macOS Big Sur.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "CSS",
          "level": 3,
          "text": "CSS",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added size queries support for Container Queries.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for Container Query Units.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for Subgrid.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for animatable Grids.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for Offset Path.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for Overscroll Behavior.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for ",
                      "type": "text"
                    },
                    {
                      "code": "text-align-last",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for the ",
                      "type": "text"
                    },
                    {
                      "code": "resolution",
                      "type": "codeVoice"
                    },
                    {
                      "text": " media query.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed gradient color interpolation with alpha.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Forms",
          "level": 3,
          "text": "Forms",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for ",
                      "type": "text"
                    },
                    {
                      "code": "<form>.requestSubmit()",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added the ",
                      "type": "text"
                    },
                    {
                      "code": "showPicker()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " method for HTML input elements.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed firing the load event after a form is submitted with a ",
                      "type": "text"
                    },
                    {
                      "code": "\"_blank\"",
                      "type": "codeVoice"
                    },
                    {
                      "text": " target.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "contenteditable",
                      "type": "codeVoice"
                    },
                    {
                      "text": " anchors getting stuck with an ",
                      "type": "text"
                    },
                    {
                      "code": ":active",
                      "type": "codeVoice"
                    },
                    {
                      "text": " state.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed changing the value for ",
                      "type": "text"
                    },
                    {
                      "code": "stepUp()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "stepDown()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " with out-of-range values.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed using ",
                      "type": "text"
                    },
                    {
                      "code": "min",
                      "type": "codeVoice"
                    },
                    {
                      "text": " as the default value when ",
                      "type": "text"
                    },
                    {
                      "code": "min",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is greater than ",
                      "type": "text"
                    },
                    {
                      "code": "max",
                      "type": "codeVoice"
                    },
                    {
                      "text": " for ",
                      "type": "text"
                    },
                    {
                      "code": "<input type=\"range\">",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed making value updates visible for ",
                      "type": "text"
                    },
                    {
                      "code": "<input type=\"email\">",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed making sure ",
                      "type": "text"
                    },
                    {
                      "code": ":active",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is removed on ",
                      "type": "text"
                    },
                    {
                      "code": "keyup",
                      "type": "codeVoice"
                    },
                    {
                      "text": " event for radio inputs.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "applyStep()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " behavior to align with specifications.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed the ",
                      "type": "text"
                    },
                    {
                      "code": "select()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " method returns to align with the specifications.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed form data generated by ",
                      "type": "text"
                    },
                    {
                      "code": "<input type=\"image\">",
                      "type": "codeVoice"
                    },
                    {
                      "text": " when a ",
                      "type": "text"
                    },
                    {
                      "code": "value",
                      "type": "codeVoice"
                    },
                    {
                      "text": " attribute is present.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed the ",
                      "type": "text"
                    },
                    {
                      "code": "FormData",
                      "type": "codeVoice"
                    },
                    {
                      "text": " object to not include an entry for the submit button used to submit the form.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed invalid floating-point numbers that end with a “.” returning an empty string.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed selection range after the ",
                      "type": "text"
                    },
                    {
                      "code": "type",
                      "type": "codeVoice"
                    },
                    {
                      "text": " attribute of an ",
                      "type": "text"
                    },
                    {
                      "code": "<input>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " changes.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed selection range to be limited by the length of the current value.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed the user-agent stylesheet to include ",
                      "type": "text"
                    },
                    {
                      "code": "table { text-indent: initial }",
                      "type": "codeVoice"
                    },
                    {
                      "text": " to align with specifications.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed the user-agent stylesheet to include ",
                      "type": "text"
                    },
                    {
                      "code": "box-sizing: border-box",
                      "type": "codeVoice"
                    },
                    {
                      "text": " for ",
                      "type": "text"
                    },
                    {
                      "code": "<input type=\"color\">.",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed the ",
                      "type": "text"
                    },
                    {
                      "code": "line-height",
                      "type": "codeVoice"
                    },
                    {
                      "text": " declaration to use ",
                      "type": "text"
                    },
                    {
                      "code": "!important",
                      "type": "codeVoice"
                    },
                    {
                      "text": " for the placeholder of an input.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Removed-Features",
          "level": 4,
          "text": "Removed Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Removed most non-standard CSS ",
                      "type": "text"
                    },
                    {
                      "code": "appearance",
                      "type": "codeVoice"
                    },
                    {
                      "text": " values.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Accessibility",
          "level": 3,
          "text": "Accessibility",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added accessibility support for ",
                      "type": "text"
                    },
                    {
                      "code": "display:contents",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added Isolated Tree Mode to reduce VoiceOver hangs in WebKit by offloading accessibility work to a secondary thread.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Apple-Pay",
          "level": 3,
          "text": "Apple Pay",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for allowing scripts to be injected into ",
                      "type": "text"
                    },
                    {
                      "code": "WKWebView",
                      "type": "codeVoice"
                    },
                    {
                      "text": " that have used Apple Pay (and vice versa).",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for recurring payments, automatic reload payments, and multimerchant payments.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added a way for merchants to provide order details that are surfaced in the Wallet app after payments are authorized.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Authentication",
          "level": 3,
          "text": "Authentication",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for passkeys on iOS 16.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Media",
          "level": 3,
          "text": "Media",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for non-animated AVIF.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Safari-Web-Extensions",
          "level": 3,
          "text": "Safari Web Extensions",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for Safari Web Inspector Extensions.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added the ability to open a Safari Web Extension popover programmatically.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added the ability to sync enabled extension state and selected start page across iOS, iPadOS, and macOS.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for ",
                      "type": "text"
                    },
                    {
                      "code": "declarativeNetRequestWithHostAccess",
                      "type": "codeVoice"
                    },
                    {
                      "text": " permission.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for ",
                      "type": "text"
                    },
                    {
                      "code": "browser.runtime.getFrameID",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed incorrect counts being returned from ",
                      "type": "text"
                    },
                    {
                      "code": "getBytesInUse",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed background service worker to load bundle resources.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Extensions that request the unlimitedStorage permission no longer also need the ",
                      "type": "text"
                    },
                    {
                      "code": "storage",
                      "type": "codeVoice"
                    },
                    {
                      "text": " permission.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Updated the maximum number of static rulesets to 50 and the maximum number of enabled static rulesets to 10.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Service workers are no longer returned from ",
                      "type": "text"
                    },
                    {
                      "code": "extension.getBackgroundPage",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "extension.getViews",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Security",
          "level": 3,
          "text": "Security",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for the ",
                      "type": "text"
                    },
                    {
                      "code": "worker-src",
                      "type": "codeVoice"
                    },
                    {
                      "text": " Content Security Policy directive.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed blocking image content in object elements.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed incorrect CORP/COEP check in 304 responses.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed mixing ",
                      "type": "text"
                    },
                    {
                      "code": "strict-dynamic",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "unsafe-inline",
                      "type": "codeVoice"
                    },
                    {
                      "text": " policies.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "script-src-elem",
                      "type": "codeVoice"
                    },
                    {
                      "text": " policies in Workers.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
