              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed returning the default computed style for elements without the ",
                      "type": "text"
                    },
                    {
                      "code": "transition",
                      "type": "codeVoice"
                    },
                    {
                      "text": " or ",
                      "type": "text"
                    },
                    {
                      "code": "animation",
                      "type": "codeVoice"
                    },
                    {
                      "text": " shorthands.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
