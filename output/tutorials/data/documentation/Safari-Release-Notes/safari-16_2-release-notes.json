  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Safari 16.2 is available for macOS Big Sur, macOS Monterey, macOS Ventura, iPadOS 16.2, and iOS 16.2.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "CSS",
          "level": 3,
          "text": "CSS",
          "type": "heading"
        },
        {
          "anchor": "New-Features",
          "level": 4,
          "text": "New Features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for additional values for the  ",
                      "type": "text"
                    },
                    {
                      "code": "font-variant-alternates",
                      "type": "codeVoice"
                    },
                    {
                      "text": " property: ",
                      "type": "text"
                    },
                    {
                      "code": "annotation(value-name)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "character-variant(value-name)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "ornaments(value-name)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "styleset(value-name)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "stylistic(value-name)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "swash(value-name)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", along with the associated ",
                      "type": "text"
                    },
                    {
                      "code": "@font-feature-values",
                      "type": "codeVoice"
                    },
                    {
                      "text": " at-rule.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for ",
                      "type": "text"
                    },
                    {
                      "code": "last baseline",
                      "type": "codeVoice"
                    },
                    {
                      "text": " alignment for Flexbox and Grid.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for ",
                      "type": "text"
                    },
                    {
                      "code": "color-mix()",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Added support for Gradient Interpolation Color Spaces.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed backgrounds with a ",
                      "type": "text"
                    },
                    {
                      "code": "fixed",
                      "type": "codeVoice"
                    },
                    {
                      "text": " attachment to behave like ",
                      "type": "text"
                    },
                    {
                      "code": "scroll",
                      "type": "codeVoice"
                    },
                    {
                      "text": " inside transformed elements.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed calculating inline ",
                      "type": "text"
                    },
                    {
                      "code": "min-content",
                      "type": "codeVoice"
                    },
                    {
                      "text": " size to include the ",
                      "type": "text"
                    },
                    {
                      "code": "aspect-ratio",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and add the ",
                      "type": "text"
                    },
                    {
                      "code": "min-content",
                      "type": "codeVoice"
                    },
                    {
                      "text": " block computation.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed flexbox cases where border and padding are added twice to the computed min and max sizes.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed flex base size width calculation to not consider min and max sizes.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed focus behavior to not take into account scroll margin when checking visibility.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed layout containment to not propagate forced breaks to the parent.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed percentage based translations not working with SVG ",
                      "type": "text"
                    },
                    {
                      "code": "<text>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " elements.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "perspective: 0",
                      "type": "codeVoice"
                    },
                    {
                      "text": " behavior.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed re-snap to follow scroll snap target if necessary.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed scroll snap to choose the closest snap target if targets on each axes aren’t visible.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "text-decoration-thickness",
                      "type": "codeVoice"
                    },
                    {
                      "text": " property to not be inherited.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed transforms on SVG shapes and groups when the root element size is changed.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "text-decoration",
                      "type": "codeVoice"
                    },
                    {
                      "text": " location for superscripts and subscripts when the text string contains both regular text and either superscript or subscript.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "text-decoration",
                      "type": "codeVoice"
                    },
                    {
                      "text": " pixel alignment when the content is truncated.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed removing the intrinsic margin when specifying the width or height of an ",
                      "type": "text"
                    },
                    {
                      "code": "<input>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " element.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed rounding for ",
                      "type": "text"
                    },
                    {
                      "code": "rgba()",
                      "type": "codeVoice"
                    },
                    {
                      "text": " channels.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "font-variant",
                      "type": "codeVoice"
                    },
                    {
                      "text": " shorthand parsing to allow the ",
                      "type": "text"
                    },
                    {
                      "code": "font-variant-east-asian",
                      "type": "codeVoice"
                    },
                    {
                      "text": " property in any position.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed scroll snapping on a date input with ",
                      "type": "text"
                    },
                    {
                      "code": "scroll-margin",
                      "type": "codeVoice"
                    },
                    {
                      "text": " applied.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Rendering",
          "level": 3,
          "text": "Rendering",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed applying ",
                      "type": "text"
                    },
                    {
                      "code": "lineWidth",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "strokeStyle",
                      "type": "codeVoice"
                    },
                    {
                      "text": " when drawing on ",
                      "type": "text"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed ",
                      "type": "text"
                    },
                    {
                      "code": "contain: content",
                      "type": "codeVoice"
                    },
                    {
                      "text": " breaking scrolling when applied to the body.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed applying ",
                      "type": "text"
                    },
                    {
                      "code": "transform-origin",
                      "type": "codeVoice"
                    },
                    {
                      "text": " with a z-component.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed computing the baseline position for tables when margins are applied.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed applying transforms to table sections: ",
                      "type": "text"
                    },
                    {
                      "code": "tbody",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "thead",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "code": "tfoot",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed: transform changes recompute overflow.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Media",
          "level": 3,
          "text": "Media",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed updating MediaSessionInfo for a media element when a ",
                      "type": "text"
                    },
                    {
                      "code": "srcObject",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is used.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "Web-API",
          "level": 3,
          "text": "Web API",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed SharedWorker to honor ",
                      "type": "text"
                    },
                    {
                      "code": "Upgrade-Insecure-Request",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "anchor": "WebDriver",
          "level": 3,
          "text": "WebDriver",
          "type": "heading"
        },
        {
          "anchor": "Resolved-Issues",
          "level": 4,
          "text": "Resolved Issues",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed Element Click command failing on iOS.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Fixed: Enabled ",
                      "type": "text"
                    },
                    {
                      "code": "touch",
                      "type": "codeVoice"
                    },
                    {
                      "text": " pointer input source subtype on iOS.",
                      "type": "text"
                    }
                  ],
