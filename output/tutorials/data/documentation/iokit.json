  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The IOKit framework implements nonkernel access to IOKit objects such drivers and nubs through the device-interface mechanism.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Devices supported on macOS 11 and later require ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/driverkit",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". Use IOKit in your apps and services to discover and use devices. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "style": "important",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
