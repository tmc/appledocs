        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Returns false if ",
              "type": "text"
            },
            {
              "code": "sbuf",
              "type": "codeVoice"
            },
            {
              "text": " is ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "CMSampleBufferInvalidate",
              "type": "codeVoice"
            },
            {
              "text": " was called, true otherwise. Doesn’t perform any kind of exhaustive validation of the sample buffer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
