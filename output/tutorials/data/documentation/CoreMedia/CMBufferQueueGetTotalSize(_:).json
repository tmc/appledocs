        {
          "inlineContent": [
            {
              "text": "The total size of the ",
              "type": "text"
            },
            {
              "code": "CMBufferQueue",
              "type": "codeVoice"
            },
            {
              "text": " is the sum of all the individual buffer sizes, as reported by the ",
              "type": "text"
            },
            {
              "code": "getTotalSize",
              "type": "codeVoice"
            },
            {
              "text": " callback (provided to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.coremedia/documentation/CoreMedia/CMBufferQueueCreate(allocator:capacity:callbacks:queueOut:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ").",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This function returns 0 if there are no buffers in the queue.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
