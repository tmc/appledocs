      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The allocator to use for the new tag collection. Pass ",
                  "type": "text"
                },
                {
                  "code": "kCFAllocatorDefault",
                  "type": "codeVoice"
                },
                {
                  "text": " to use the default global allocator.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allocator"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of tagged buffer groups to combine into the new, single buffer group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "taggedBufferGroups"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The output destination for the new tagged buffer group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "groupOut"
        }
      ]
    },
    {
      "content": [
