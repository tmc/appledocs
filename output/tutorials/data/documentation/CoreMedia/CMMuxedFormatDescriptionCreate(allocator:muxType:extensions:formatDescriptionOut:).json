                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "CFDictionary",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "CFDate",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "CFBoolean",
                  "type": "codeVoice"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "code": "CFNumber",
                  "type": "codeVoice"
                },
                {
                  "text": "). Can be ",
                  "type": "text"
                },
                {
                  "code": "NULL",
