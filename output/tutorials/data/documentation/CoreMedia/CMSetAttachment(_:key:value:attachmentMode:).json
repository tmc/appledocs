    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "CMAttachmentBearer",
                  "type": "codeVoice"
                },
                {
                  "text": " object on which to add or set attachments.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "target"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "CFString",
                  "type": "codeVoice"
                },
                {
                  "text": " key identifying the desired attachment.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "key"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Core Foundation object attachment. If this parameter is ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", the function returns an error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies the attachment mode for this attachment. Any given attachment key may exist in only one mode at a time.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "attachmentMode"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can attach any Core Foundation object to a ",
              "type": "text"
            },
            {
              "code": "CMAttachmentBearer",
              "type": "codeVoice"
            },
            {
              "text": " object to store additional information. ",
              "type": "text"
            },
            {
              "code": "CMSetAttachment",
              "type": "codeVoice"
            },
            {
              "text": " stores an attachment identified by a key. If the key doesnâ€™t currently exist for the ",
              "type": "text"
            },
            {
              "code": "CMAttachmentBearer",
              "type": "codeVoice"
            },
            {
              "text": " object when you call this function, the function adds the new attachment. If the key does exist, the function replaces the existing attachment. In both cases the function increments the retain count of the attachment. The value can be any ",
              "type": "text"
            },
            {
              "code": "CFType",
              "type": "codeVoice"
            },
            {
              "text": " but a ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": " value results in an error. Given a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/CoreVideo/CVBuffer",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "CMSetAttachment",
              "type": "codeVoice"
            },
            {
              "text": " is equivalent to ",
              "type": "text"
            },
            {
              "code": "CVBufferSetAttachment",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
