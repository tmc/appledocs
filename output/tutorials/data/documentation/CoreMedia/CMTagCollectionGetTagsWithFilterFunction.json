    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tag collection to retrieve tags from.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tagCollection"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a pre-allocated C-style array of tags, containing the collection’s matching tags on function return.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tagBuffer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of tags that the ",
                  "type": "text"
                },
                {
                  "code": "tagBuffer",
                  "type": "codeVoice"
                },
                {
                  "text": " array can hold.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tagBufferCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The total number of tags copied from the collection to ",
                  "type": "text"
                },
                {
                  "code": "tagBuffer",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "numberOfTagsCopied"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The filter function to apply to the collection. When this function returns ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the tag passed as an argument is added to ",
                  "type": "text"
                },
                {
                  "code": "tagBuffer",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "filter"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer passed to the ",
                  "type": "text"
                },
                {
                  "code": "filter",
                  "type": "codeVoice"
                },
                {
                  "text": " as a parameter on each call.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "context"
        }
      ]
    },
    {
      "content": [
