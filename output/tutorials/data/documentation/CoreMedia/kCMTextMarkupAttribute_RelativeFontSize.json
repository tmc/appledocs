      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This attributeâ€™s value must be a non-negative ",
              "type": "text"
            },
            {
              "code": "CFNumber",
              "type": "codeVoice"
            },
            {
              "text": ". This is a number holding a percentage of the size of the calculated default font size. A value of ",
              "type": "text"
            },
            {
              "code": "120",
              "type": "codeVoice"
            },
            {
              "text": " indicates 20% larger than the default font size. A value of ",
              "type": "text"
            },
            {
              "code": "80",
              "type": "codeVoice"
            },
            {
              "text": " indicates 80% of the default font size. The default value of ",
              "type": "text"
            },
            {
              "code": "100",
              "type": "codeVoice"
            },
            {
              "text": " indicates no size difference.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
