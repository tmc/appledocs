      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Call this method only if the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.imagecapturecore/documentation/ImageCaptureCore/ICDevice/capabilities",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property contains ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.imagecapturecore/documentation/ImageCaptureCore/ICDeviceCapability/cameraDeviceCanAcceptPTPCommands",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". All PTP cameras have this capability.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "sendCommandDelegate",
              "type": "codeVoice"
            },
            {
              "text": " must implement a function with the signature ",
              "type": "text"
            },
            {
              "code": "- (void)didSendPTPCommand:(NSData*)command inData:(NSData*)data response:(NSData*)response error:(NSError*)error contextInfo:(void*)contextInfo",
              "type": "codeVoice"
            },
            {
              "text": ", to be called when the request is completed.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
