      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The device on which the matrix multiplication kernel will run.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "device"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A boolean value that indicates if the left input matrix should be used in its transposed form. If the value is ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/swift/true",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": ", then ",
                  "type": "text"
                },
                {
                  "code": "op(A) = A**T",
                  "type": "codeVoice"
                },
                {
                  "text": "; otherwise, ",
                  "type": "text"
                },
                {
                  "code": "op(A) = A",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transposeLeft"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A boolean value that indicates if the right input matrix should be used in its transposed form. If the value is ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/swift/true",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": ", then ",
                  "type": "text"
                },
                {
                  "code": "op(B) = B**T",
                  "type": "codeVoice"
                },
                {
                  "text": "; otherwise, ",
                  "type": "text"
                },
                {
                  "code": "op(B) = B",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transposeRight"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of rows in the result matrix (",
                  "type": "text"
                },
                {
                  "code": "M",
                  "type": "codeVoice"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "BLAS GEMM",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " description).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resultRows"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of columns in the result matrix (",
                  "type": "text"
                },
                {
                  "code": "N",
                  "type": "codeVoice"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "BLAS GEMM",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " description).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resultColumns"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of columns of the left input matrix after the appropriate transpose operation has been applied (",
                  "type": "text"
                },
                {
                  "code": "K",
                  "type": "codeVoice"
                },
                {
                  "text": " in the ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "BLAS GEMM",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " description).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "interiorColumns"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scale factor to apply to the product, specified in ",
                  "type": "text"
                },
                {
                  "code": "double",
                  "type": "codeVoice"
                },
                {
                  "text": " precision. This value will be converted to the appropriate precision in the implementation itself, subject to rounding and/or clamping as necessary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alpha"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scale factor to apply to the initial values of ",
                  "type": "text"
                },
                {
                  "code": "C",
                  "type": "codeVoice"
                },
                {
                  "text": ", specified in ",
                  "type": "text"
                },
                {
                  "code": "double",
                  "type": "codeVoice"
                },
                {
                  "text": " precision. This value will be converted to the appropriate precision in the implementation itself, subject to rounding and/or clamping as necessary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "beta"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A valid ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/metalperformanceshaders/mpsmatrixmultiplication",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " object or ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", if failure.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
