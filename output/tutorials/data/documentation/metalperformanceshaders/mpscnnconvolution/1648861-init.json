      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The device on which this kernel will run.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "device"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a valid convolution descriptor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "convolutionDescriptor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to a weights array. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Each entry is a float value. The number of entries is equal to ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/metalperformanceshaders/mpscnnconvolutiondescriptor/1648934-inputfeaturechannels",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "code": " * ",
                  "type": "codeVoice"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/metalperformanceshaders/mpscnnconvolutiondescriptor/1648852-outputfeaturechannels",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "code": " * ",
                  "type": "codeVoice"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/metalperformanceshaders/mpscnnconvolutiondescriptor/1648904-kernelheight",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "code": " * ",
                  "type": "codeVoice"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/metalperformanceshaders/mpscnnconvolutiondescriptor/1648959-kernelwidth",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The layout of the filter weight is arranged so that it can be reinterpreted as a 4D tensor (array) ",
                  "type": "text"
                },
                {
                  "code": "weight[outputChannels][kernelHeight][kernelWidth][inputChannels/groups]",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Weights are converted to half float precision (",
                  "type": "text"
                },
                {
                  "code": "fp16",
                  "type": "codeVoice"
                },
                {
                  "text": ") internally for best performance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "kernelWeights"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to bias terms to be applied to the convolution output. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Each entry is a float value. The number of entries is the number of output feature maps.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "biasTerms"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Currently unused. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "This value must be ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/metalperformanceshaders/mpscnnconvolutionflags/none",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "flags"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A valid ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/metalperformanceshaders/mpscnnconvolution",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " object or ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", if failure.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
