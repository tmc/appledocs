    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The offset is defined as the position of ",
              "type": "text"
            },
            {
              "code": "clipRect.origin",
              "type": "codeVoice"
            },
            {
              "text": " in source image coordinates. The default value is ",
              "type": "text"
            },
            {
              "code": "{0,0,0}",
              "type": "codeVoice"
            },
            {
              "text": ", indicating that the top left corners of the clip rectangle and the source image align. ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The value of ",
              "type": "text"
            },
            {
              "code": "offset.z",
              "type": "codeVoice"
            },
            {
              "text": " is the index of the starting source image in batch processing mode.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
