    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resulting cleartext message if the message is authentic.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can call this method multiple times to decrypt a series of messages.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "When using this method, the recipient of the ciphertext messages needs to decrypt",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "them in the same order that the sender encrypts them.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The system throws errors from ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.CryptoKit/documentation/CryptoKit/HPKE/Errors",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " when it encounters them.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
