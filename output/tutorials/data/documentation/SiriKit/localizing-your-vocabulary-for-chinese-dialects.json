  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For specific localizations, additional rules apply when specify values for the ",
              "type": "text"
            },
            {
              "code": "VocabularyItemPronunciation",
              "type": "codeVoice"
            },
            {
              "text": " key:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "zh_CN: China Mandarin localization:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Tones are represented using the numbers 1, 2, 3, 4, and 5. Use the number 5 for the neutral tone. Place tones at the end of the Pinyin representation of each character. For example, ",
                      "type": "text"
                    },
                    {
                      "code": "ping2 guo3",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "You can replace the special character ",
                      "type": "text"
                    },
                    {
                      "code": "ü",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in Pinyin with ",
                      "type": "text"
                    },
                    {
                      "code": "yu",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". For example, ",
                      "type": "text"
                    },
                    {
                      "code": "nü",
                      "type": "codeVoice"
                    },
                    {
                      "text": " would become ",
                      "type": "text"
                    },
                    {
                      "code": "nyu",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "zh_TW: Taiwan Mandarin localization:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Tones are represented using the numbers 1, 2, 3, 4, and 5. Use the number 5 for the neutral tone. Place tones at the end of the Pinyin representation of each character. For example, 蘇打綠 becomes ",
                      "type": "text"
                    },
                    {
                      "code": "su1 da3 lyu4",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "zh_HK: Hong Kong Cantonese localization:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Use the Jyutping 粤语拼音 (粤拼) romanization system for Cantonese.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Tones are represented using the numbers 1, 2, 3, 4, 5, and 6. Place tone numbers at the end of the Jyutping. For example, 的士 becomes ",
                      "type": "text"
                    },
                    {
                      "code": "dik1 si2",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", and 短訊 becomes ",
                      "type": "text"
                    },
                    {
                      "code": "dyun2 seon2",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
