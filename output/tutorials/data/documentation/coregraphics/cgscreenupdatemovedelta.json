      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Move operation notifications are restricted to changes that move a region by an integer number of pixels. The fields ",
              "type": "text"
            },
            {
              "code": "dX",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "dY",
              "type": "codeVoice"
            },
            {
              "text": " describe the direction of movement:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Positive values of ",
                      "type": "text"
                    },
                    {
                      "code": "dX",
                      "type": "codeVoice"
                    },
                    {
                      "text": " indicate movement to the right.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Negative values of ",
                      "type": "text"
                    },
                    {
                      "code": "dX",
                      "type": "codeVoice"
                    },
                    {
                      "text": " indicate movement to the left.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Positive values of ",
                      "type": "text"
                    },
                    {
                      "code": "dY",
                      "type": "codeVoice"
                    },
                    {
                      "text": " indicate movement downward.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Negative values of ",
                      "type": "text"
                    },
                    {
                      "code": "dY",
                      "type": "codeVoice"
                    },
                    {
                      "text": " indicate movement upward.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
