      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A rectangle defining the bounds of the new page, expressed in units of the default user space, or ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ". These bounds supersede any supplied for the media box when you created the context. If you pass ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", Core Graphics uses the rectangle you supplied for the media box when the graphics context was created. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mediaBox"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When using a graphics context that supports multiple pages, you should call this function together with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgcontext/1455027-endpage",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to delineate the page boundaries in the output. In other words, each page should be bracketed by calls to ",
              "type": "text"
            },
            {
              "code": "CGContextBeginPage",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "CGContextEndPage",
              "type": "codeVoice"
            },
            {
              "text": ". Core Graphics ignores all drawing operations performed outside a page boundary in a page-based context.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
