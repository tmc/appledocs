    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The color space in which color values are expressed. Core Graphics retains this object; upon return, you may safely release it.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "colorspace"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The center of the starting circle, in the shading's target coordinate space.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "start"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The radius of the starting circle, in the shading's target coordinate space.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "startRadius"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The center of the ending circle, in the shading's target coordinate space.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "end"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The radius of the ending circle, in the shading's target coordinate space.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "endRadius"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A CGFunction object created by the function ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgfunction/1390862-init",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". This object refers to your function for creating a radial shading. Core Graphics retains this object; upon return, you may safely release it.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "function"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that specifies whether to extend the shading beyond the starting circle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "extendStart"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that specifies whether to extend the shading beyond the ending circle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "extendEnd"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A new Core Graphics radial shading. You are responsible for releasing this object using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/1573766-cgshadingrelease",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A radial shading is a color blend that varies between two circles. To draw the shading, call the function ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgcontext/1456643-drawshading",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
