      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that specifies how far into the dash pattern the line starts, in units of the user space. For example, a value of ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " draws a line starting with the beginning of a dash pattern, and a value of ",
                  "type": "text"
                },
                {
                  "code": "3",
                  "type": "codeVoice"
                },
                {
                  "text": " means the line is drawn with the dash pattern starting at three units from its beginning. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "phase"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of values that specify the lengths, in user space coordinates, of the painted and unpainted segments  of the dash pattern.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "For example, the array ",
                  "type": "text"
                },
                {
                  "code": "[2,3]",
                  "type": "codeVoice"
                },
                {
                  "text": " sets a dash pattern that alternates between a 2-unit-long painted segment and a 3-unit-long unpainted segment. The array ",
                  "type": "text"
                },
                {
                  "code": "[1,3,4,2]",
                  "type": "codeVoice"
                },
                {
                  "text": " sets the pattern to a 1-unit painted segment, a 3-unit unpainted segment, a 4-unit painted segment, and a 2-unit unpainted segment.Pass an empty array to clear the dash pattern so that all stroke drawing in the context uses solid lines.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lengths"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An affine transform to apply to the path before dashing. Defaults to the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgaffinetransformidentity",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " transform if not specified.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transform"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A new path.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The new path is created so that filling the new path draws the same pixels as stroking the original path with the specified dash parameters.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
