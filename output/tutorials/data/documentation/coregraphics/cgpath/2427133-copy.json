      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The line width to use, in user space units. The value must be greater than ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineWidth"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The line cap style to render. (For equivalent ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgcontext",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " drawing methods, the default style is ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cglinecap/butt",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".) ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineCap"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The line join style to render. (For equivalent ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgcontext",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " drawing methods, the default style is ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cglinejoin/miter",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".) ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineJoin"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value that limits how sharp individual corners in the path can be when using the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cglinejoin/miter",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " line join style. When the ratio of a the length required for a mitered corner to the line width exceeds this value, that corner uses the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cglinejoin/bevel",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " style instead.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "miterLimit"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An affine transform to apply to the path before dashing. Defaults to the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgaffinetransformidentity",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " transform if not specified.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "transform"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A new path.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The new path is created so that filling the new path draws the same pixels as stroking the original path with the specified line style.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
