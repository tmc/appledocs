  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A PDF string object is a series of bytesâ€”unsigned integer values in the range 0 to 255.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The string elements are not integer objects, but are stored in a more compact format. For more information on the representation of strings in PDF, see the latest version of ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "PDF Reference",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": ", Adobe Systems Incorporated.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This object is not derived from CFType and therefore there are no functions for retaining and releasing it. CGPDFString objects exist as constituent parts of a CGPDFDocument object, and are managed by their container.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
