      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A given key is present in a display mode dictionary only if the property applies, and is always associated with a value of ",
              "type": "text"
            },
            {
              "code": "kCFBooleanTrue",
              "type": "codeVoice"
            },
            {
              "text": ". Keys not relevant to a particular display mode will not appear in the mode dictionary.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "1680124",
          "level": 3,
          "text": "Special Considerations",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Starting in OS X v10.6, display mode dictionaries have been made obsolete by the display mode opaque type. For more information on display modes, see",
              "type": "text"
            },
            {
              "identifier": "devLink-1729749",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
