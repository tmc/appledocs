    "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamcolorspace": {
      "abstract": [
        {
          "text": "This key specifies the color space of the output buffer. If this key is not included in the dictionary, the output buffer uses the same color space as the display. The value associated with this key must be a ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/coregraphics/cgcolorspaceref",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " for the desired color space.",
          "type": "text"
        }
      ],
      "deprecated": true,
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamcolorspace",
      "kind": "symbol",
      "role": "symbol",
      "title": "kCGDisplayStreamColorSpace",
      "type": "topic",
      "url": "/documentation/coregraphics/kcgdisplaystreamcolorspace"
    },
    "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamdestinationrect": {
      "abstract": [
        {
          "text": "This key specifies that the display stream outputs the frame data into a subset of the output ",
          "type": "text"
        },
        {
          "code": "IOSurface",
          "type": "codeVoice"
        },
        {
          "text": " object. If this key is not included in the dictionary, then the entire output surface is used. The value must be created using the ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/coregraphics/1455760-cgrectcreatedictionaryrepresenta",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " function. The rectangle is specified in pixels in the surface’s coordinate system",
          "type": "text"
        }
      ],
      "deprecated": true,
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamdestinationrect",
      "kind": "symbol",
      "role": "symbol",
      "title": "kCGDisplayStreamDestinationRect",
      "type": "topic",
      "url": "/documentation/coregraphics/kcgdisplaystreamdestinationrect"
    },
    "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamminimumframetime": {
      "abstract": [
        {
          "text": "This key specifies the desired minimum time between frame updates, allowing you to throttle the rate at which updates are received. If this key is not included in the dictionary, the default value is ",
          "type": "text"
        },
        {
          "code": "0",
          "type": "codeVoice"
        },
        {
          "text": ", meaning that updates are not throttled. The value must be specified as a ",
          "type": "text"
        },
        {
          "code": "CFNumber",
          "type": "codeVoice"
        },
        {
          "text": ".",
          "type": "text"
        }
      ],
      "deprecated": true,
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamminimumframetime",
      "kind": "symbol",
      "role": "symbol",
      "title": "kCGDisplayStreamMinimumFrameTime",
      "type": "topic",
      "url": "/documentation/coregraphics/kcgdisplaystreamminimumframetime"
    },
    "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreampreserveaspectratio": {
      "abstract": [
        {
          "text": "This key specifies whether the display stream preserves the aspect ratio of the source pixel data. If this key is not included in the dictionary, then the aspect ratio is preserved. If the aspect ratio is preserved, then the display stream adds black bars to the output data. If the aspect ratio is not preserved, then the pixel data is stretched to fit the output buffer’s dimensions. The value associated with the key must be a ",
          "type": "text"
        },
        {
          "code": "CFBoolean",
          "type": "codeVoice"
        },
        {
          "text": ".",
          "type": "text"
        }
      ],
      "deprecated": true,
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreampreserveaspectratio",
      "kind": "symbol",
      "role": "symbol",
      "title": "kCGDisplayStreamPreserveAspectRatio",
      "type": "topic",
      "url": "/documentation/coregraphics/kcgdisplaystreampreserveaspectratio"
    },
    "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamqueuedepth": {
      "abstract": [
        {
          "text": "This key specifies the number of frames to keep in the queue. If this key is not included in the dictionary, the default value is ",
          "type": "text"
        },
        {
          "code": "3",
          "type": "codeVoice"
        },
        {
          "text": " frames. Specifying more frames uses more memory, but may allow you to process frame data without stalling the display stream. The value associated with this key should be specified as a ",
          "type": "text"
        },
        {
          "code": "CFNumber",
          "type": "codeVoice"
        },
        {
          "text": ", and should not exceed ",
          "type": "text"
        },
        {
          "code": "8",
          "type": "codeVoice"
        },
        {
          "text": " frames.",
          "type": "text"
        }
      ],
      "deprecated": true,
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamqueuedepth",
      "kind": "symbol",
      "role": "symbol",
      "title": "kCGDisplayStreamQueueDepth",
      "type": "topic",
      "url": "/documentation/coregraphics/kcgdisplaystreamqueuedepth"
    },
    "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamshowcursor": {
      "abstract": [
        {
          "text": "This key specifies whether the cursor should appear in the stream. If this key is not included in the dictionary, the cursor is visible. The value must be specified as a ",
          "type": "text"
        },
        {
          "code": "CFBoolean",
          "type": "codeVoice"
        },
        {
          "text": ".",
          "type": "text"
        }
      ],
      "deprecated": true,
      "identifier": "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamshowcursor",
      "kind": "symbol",
      "role": "symbol",
      "title": "kCGDisplayStreamShowCursor",
      "type": "topic",
      "url": "/documentation/coregraphics/kcgdisplaystreamshowcursor"
    },
    "doc://com.apple.documentation/documentation/coregraphics/kcgdisplaystreamsourcerect": {
      "abstract": [
        {
          "text": "This key specifies that the display stream only samples a subset of the display’s framebuffer. If this key is not included in the dictionary, then the entire display is streamed. The value must be created using the ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/coregraphics/1455760-cgrectcreatedictionaryrepresenta",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " function. The rectangle is specified in points in the display’s logical coordinate system.",
          "type": "text"
        }
      ],
