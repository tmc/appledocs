    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Customize the layout of a preview that you define using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/SwiftUI/PreviewProvider",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "protocol by providing one of the preview layout values to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/SwiftUI/View/previewLayout(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view modifier. For example, you can tell the preview to take up only the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "amount of space that the view requires with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.DeveloperToolsSupport/documentation/DeveloperToolsSupport/PreviewLayout/sizeThatFits",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct CircleImage_Previews: PreviewProvider {",
            "    static var previews: some View {",
            "        CircleImage()",
            "            .previewLayout(.sizeThatFits)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "When you migrate away from preview providers and to preview macros,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "you specify layout using one of the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.DeveloperToolsSupport/documentation/DeveloperToolsSupport/PreviewTrait",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " layout",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "values with a macro that takes traits, like",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Preview(_:traits:_:body:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
