    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use one or more preview cameras with one of the preview macros that takes a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "cameras",
              "type": "codeVoice"
            },
            {
              "text": " input — like",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Preview(_:traits:body:cameras:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "— to create custom viewpoints for the preview. The canvas offers",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "custom cameras in its camera picker along with a set of standard cameras.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The preview uses the first custom camera that you specify as the default",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "viewpoint when the preview appears.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, you can create custom cameras from the top, leading,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and front viewpoints:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "#Preview {",
            "    CircleImage()",
            "} cameras: {",
            "    PreviewCamera(from: .top, name: \"Top\")",
            "    PreviewCamera(from: .leading, name: \"Leading\")",
            "    PreviewCamera(from: .front, name: \"Front\")",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
