      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The semantic intent of the plug-in; either one of the predefined intents,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "or a custom intent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "intent"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Any permissions needed by the command plug-in. This affects what the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "sandbox in which the plug-in is run allows. Some permissions may require",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "user approval.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "permissions"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Plug-ins that specify a ",
              "type": "text"
            },
            {
              "code": "command",
              "type": "codeVoice"
            },
            {
              "text": " capability define commands that can run",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using the SwiftPM command line interface, or in an IDE that supports",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Swift packages. You can invoke the command manually on one or more targets in a package.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "swift package <verb>"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The package can specify the ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "verb",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": " used to invoke the command.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
