    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the target.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dependencies of the target. A dependency can be another target in the package or a product from a package dependency.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dependencies"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The custom path for the target. By default, the Swift Package Manager requires a target’s sources to reside at predefined search paths;",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "for example, ",
                  "type": "text"
                },
                {
                  "code": "[PackageRoot]/Sources/[TargetName]",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Don’t escape the package root; for example, values like ",
                  "type": "text"
                },
                {
                  "code": "../Foo",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "/Foo",
                  "type": "codeVoice"
                },
                {
                  "text": " are invalid.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "path"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of paths to files or directories that the Swift Package Manager shouldn’t consider to be source or resource files.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "A path is relative to the target’s directory.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "This parameter has precedence over the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://org.swift.swiftpm.packagedescription/documentation/PackageDescription/Target/sources",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "exclude"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An explicit list of source files. If you provide a path to a directory,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the Swift Package Manager searches for valid source files recursively.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sources"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Write test targets using the XCTest testing framework.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Test targets generally declare a dependency on the targets they test.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
