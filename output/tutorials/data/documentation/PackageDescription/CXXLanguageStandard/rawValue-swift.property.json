        {
          "inlineContent": [
            {
              "text": "A new instance initialized with ",
              "type": "text"
            },
            {
              "code": "rawValue",
              "type": "codeVoice"
            },
            {
              "text": " will be equivalent to this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "instance. For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "enum PaperSize: String {",
            "    case A4, A5, Letter, Legal",
            "}",
            "",
            "let selectedSize = PaperSize.Letter",
            "print(selectedSize.rawValue)",
            "// Prints \"Letter\"",
            "",
            "print(selectedSize == PaperSize(rawValue: selectedSize.rawValue)!)",
            "// Prints \"true\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
