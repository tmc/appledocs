      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This is used when enabling the trait or when referring to it from other modifiers in the manifest.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following rules are enforced on trait names:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The first character must be a ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://unicode.org/reports/tr31/#Figure_Code_Point_Categories_for_Identifier_Parsing",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "(most letters), a digit, or ",
                      "type": "text"
                    },
                    {
                      "code": "_",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Subsequent characters must be a ",
                      "type": "text"
                    },
                    {
                      "identifier": "https://unicode.org/reports/tr31/#Figure_Code_Point_Categories_for_Identifier_Parsing",
                      "isActive": true,
                      "overridingTitle": "Unicode XID continue character",
                      "overridingTitleInlineContent": [
                        {
                          "text": "Unicode XID continue character",
                          "type": "text"
                        }
                      ],
                      "type": "reference"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "(a digit, ",
                      "type": "text"
                    },
                    {
                      "code": "_",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", or most letters), ",
                      "type": "text"
                    },
                    {
                      "code": "-",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", or ",
                      "type": "text"
                    },
                    {
                      "code": "+",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "default",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "defaults",
                      "type": "codeVoice"
                    },
                    {
                      "text": " (in any letter casing combination) are not allowed as trait names to avoid confusion with default traits.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
