          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example allows the Swift Package Manager to pick",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "versions 1.2.3, 1.2.4, 1.2.5, as well as 1.2.6.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            ".package(id: \"scope.name\", \"1.2.3\"...\"1.2.6\"),"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
