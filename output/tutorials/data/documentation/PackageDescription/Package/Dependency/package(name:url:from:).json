              "inlineContent": [
                {
                  "text": "The name of the Swift package or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to deduce the name from  the packageâ€™s Git URL.",
                  "type": "text"
                }
              ],
