    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Note that packages that use commit-based dependency requirements can’t",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "be depended upon by packages that use version-based dependency",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "requirements; you should remove commit-based dependency requirements",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "before publishing a version of your package.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example defines a version requirement for a specific",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "commit hash.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            ".revision(“e74b07278b926c9ec6f9643455ea00d1ce04a021”)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
