    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the Swift package, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to use the package’s Git URL to deduce the name.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The default localization for resources.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "defaultLocalization"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of supported platforms with a custom deployment target.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "platforms"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name to use for C modules. If present, Swift Package Manager searches for a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "<name>.pc",
                  "type": "codeVoice"
                },
                {
                  "text": " file to get the additional flags required for a system target.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pkgConfig"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The package providers for a system target.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "providers"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of products that this package makes available for clients to use.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "products"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of package dependencies.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dependencies"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of targets that are part of this package.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "targets"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list of Swift versions with which this package is compatible.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "swiftLanguageVersions"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The C language standard to use for all C targets in this package.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cLanguageStandard"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The C++ language standard to use for all C++ targets in this package.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cxxLanguageStandard"
        }
      ]
    }
