    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A package product defines an externally visible build artifact that’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "available to clients of a package. Swift Package Manager assembles the product from the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "build artifacts of one or more of the package’s targets. A package product",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can be one of three types:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Use a ",
                        "type": "text"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "library product",
                            "type": "text"
                          }
                        ],
                        "type": "emphasis"
                      },
                      {
                        "text": " to vend library targets. This makes a target’s public APIs available to clients that integrate the Swift package.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "Library",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Use an ",
                        "type": "text"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "executable product",
                            "type": "text"
                          }
                        ],
                        "type": "emphasis"
                      },
                      {
                        "text": " to vend an executable target. Use this only if you want to make the executable available to clients.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "Executable",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Use a ",
                        "type": "text"
                      },
                      {
                        "inlineContent": [
                          {
                            "text": "plugin product",
                            "type": "text"
                          }
                        ],
                        "type": "emphasis"
                      },
                      {
                        "text": " to vend plugin targets. This makes the plugin available to clients that integrate the Swift package.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "Plugin",
                    "type": "text"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows a package manifest for a library called “Paper”",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that defines multiple products:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let package = Package(",
            "    name: \"Paper\",",
            "    products: [",
            "        .executable(name: \"tool\", targets: [\"tool\"]),",
            "        .library(name: \"Paper\", targets: [\"Paper\"]),",
            "        .library(name: \"PaperStatic\", type: .static, targets: [\"Paper\"]),",
            "        .library(name: \"PaperDynamic\", type: .dynamic, targets: [\"Paper\"]),",
            "    ],",
            "    dependencies: [",
            "        .package(url: \"http://example.com.com/ExamplePackage/ExamplePackage\", from: \"1.2.3\"),",
            "        .package(url: \"http://some/other/lib\", .exact(\"1.2.3\")),",
            "    ],",
            "    targets: [",
            "        .executableTarget(",
            "            name: \"tool\",",
            "            dependencies: [",
            "                \"Paper\",",
            "                \"ExamplePackage\"",
            "            ]),",
            "        .target(",
            "            name: \"Paper\",",
            "            dependencies: [",
            "                \"Basic\",",
            "                .target(name: \"Utility\"),",
            "                .product(name: \"AnotherExamplePackage\"),",
            "            ])",
            "    ]",
            ")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
