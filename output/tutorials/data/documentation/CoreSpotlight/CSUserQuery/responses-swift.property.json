      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Getting the value of this property starts the query and begins the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "delivery of responses. Typically, you get this property as part of a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "for..in",
              "type": "codeVoice"
            },
            {
              "text": " loop to iterate over the responses, as shown in the following",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var results: [CSUserQuery.Item] = []",
            "var suggestions: [CSUserQuery.Suggestion] = []",
            "let query = CSUserQuery(userQueryString: searchText, userQueryContext: queryContext)",
            "",
            "for try await element in query.responses {",
            "    switch(element) {",
            "        case .item(let item):",
            "            self.results.append(item)",
            "            break",
            "        case .suggestion(let suggestion):",
            "            self.suggestions.append(suggestion)",
            "            break",
            "        @unknown default:",
            "            break",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
