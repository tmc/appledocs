    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The expanded presentation of a Live Activity in the Dynamic Island consists of four regions:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.WidgetKit/documentation/WidgetKit/DynamicIslandExpandedRegionPosition/center",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " places content right below the TrueDepth camera.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.WidgetKit/documentation/WidgetKit/DynamicIslandExpandedRegionPosition/leading",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " places content along the leading edge of the",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "expanded Live Activity next to the TrueDepth camera and wraps additional content below it.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.WidgetKit/documentation/WidgetKit/DynamicIslandExpandedRegionPosition/trailing",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " places content along the trailing edge of",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "the expanded Live Activity next to the TrueDepth camera and wraps additional content below it.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.WidgetKit/documentation/WidgetKit/DynamicIslandExpandedRegionPosition/bottom",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " places content below leading, trailing, and",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "center content.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
