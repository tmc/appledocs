    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.WidgetKit/documentation/WidgetKit/TimelineProvider",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " creates one or more timeline entries with",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "dates that tell WidgetKit when to display a widget. To render a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "widget, WidgetKit executes the ",
              "type": "text"
            },
            {
              "code": "content",
              "type": "codeVoice"
            },
            {
              "text": " block of the widget’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "configuration, passing the corresponding timeline entry.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "When you declare a structure conforming to ",
              "type": "text"
            },
            {
              "code": "TimelineEntry",
              "type": "codeVoice"
            },
            {
              "text": ", include any",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "additional information that the configuration’s content block requires to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "render the widget. The following code shows a timeline entry structure for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a widget that displays a game character’s health level.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct CharacterDetailEntry: TimelineEntry {",
            "    var date: Date",
            "    var healthLevel: Double",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The content block of the widget’s configuration receives the entry as a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "parameter and then passes the relevant information to the view that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "renders your widget.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct CharacterDetailWidget: Widget {",
            "    var body: some WidgetConfiguration {",
            "        StaticConfiguration(",
            "            kind: \"com.mygame.character-detail\",",
            "            provider: CharacterDetailProvider()) { entry in",
            "            CharacterDetailView(entry: entry)",
            "        }",
            "        .configurationDisplayName(\"Character Details\")",
            "        .description(\"Displays a character's health and other details\")",
            "        .supportedFamilies([.systemSmall, .systemMedium, .systemLarge])",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
