        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "bundleIds",
              "type": "codeVoice"
            },
            {
              "text": " resource represents the app’s unique identifier that you can register, modify, and delete. You need a bundle ID before you can assign capabilities with the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/bundle-id-capabilities",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " resource or create a provisioning profile with the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/profiles",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " resource.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
