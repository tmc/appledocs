      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "profiles",
              "type": "codeVoice"
            },
            {
              "text": " resource represents the provisioning profiles that allow you to install apps on your devices. You can create and delete provisioning profiles.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Provisioning profiles include signing certificates, device identifiers, and a bundle ID.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
