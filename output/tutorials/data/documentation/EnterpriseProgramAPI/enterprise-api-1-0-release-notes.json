  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This REST API provides resources that enable you to automate actions you take in developer.apple.com",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "New-features",
          "level": 3,
          "text": "New features",
          "type": "heading"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The API supports provisioning, including managing bundle IDs with the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/bundle-ids",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/bundle-id-capabilities",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resources, managing pass type IDs with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/passtypeids",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource, managing certificates with the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/certificates",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource, registering test devices with the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/devices",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource, and managing provisioning profiles with ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/profiles",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Manage your users and their access with the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/users",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resources. Invite users to join your team with the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.devpubs.enterprise-api/documentation/EnterpriseProgramAPI/user-invitations",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " resource.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
