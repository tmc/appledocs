{
  "abstract": [
    {
      "text": "Add support for universal links and a Shared with You shelf to support shared content in your app.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
