        {
          "inlineContent": [
            {
              "text": "The adopting type’s ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.healthkit/documentation/HealthKit/HKAsyncQuery/Output",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " associated type specifies the values that this method returns. For example, ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.healthkit/documentation/HealthKit/HKSampleQueryDescriptor",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " returns an array of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.healthkit/documentation/HealthKit/HKQuantitySample",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " objects.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let stepType = HKQuantityType(.stepCount)",
            "",
            "let descriptor = HKSampleQueryDescriptor(",
            "    predicates:[.quantitySample(type: stepType)],",
            "    sortDescriptors: [SortDescriptor(\\.endDate, order: .reverse)],",
            "    limit: 10)",
            "",
            "let results = try await descriptor.result(for: store)"
          ],
          "syntax": "swift",
