        {
          "inlineContent": [
            {
              "text": "Prior to iOS 9.0, anchored object queries used anchor values to track the last sample returned by a previous query. Use this method to convert those anchor values into anchor objects.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
