    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To access the attachment file as an asynchronous sequence of bytes, get a data reader from the attachment store, and then access its ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.healthkit/documentation/HealthKit/HKAttachmentDataReader/bytes",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " using a ",
              "type": "text"
            },
            {
              "code": "for-await-in",
              "type": "codeVoice"
            },
            {
              "text": " loop.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Get a data reader for the attachment.",
            "let dataReader = attachmentStore.dataReader(for: myAttachment)",
            "",
            "// Asynchronously access the attachment's bytes.",
            "var data = Data()",
            "do {",
            "    for try await byte in dataReader.bytes {",
            "        data.append(byte)",
            "    }",
            "} catch {",
            "    // Handle the error here.",
            "    fatalError(\"*** An error occurred while reading the attachment's data: \\(error.localizedDescription) ***\")",
            "}",
            "",
            "// Use the data here."
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
