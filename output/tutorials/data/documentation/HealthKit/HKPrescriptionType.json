    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.healthkit/documentation/HealthKit/HKPrescriptionType",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " class is a concrete subclass of the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.healthkit/documentation/HealthKit/HKSampleType",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " class. To create a vision prescription type instances, use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.healthkit/documentation/HealthKit/HKObjectType/visionPrescriptionType()",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " convenience method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use this data type to request permission to save vision prescriptions to the HealthKit store.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Create the prescription data type.",
            "let visionPrescriptionType = HKObjectType.visionPrescriptionType()",
            "",
            "",
            "// Request authorization to save vision prescription samples.",
            "store.requestAuthorization(toShare: [visionPrescriptionType],",
            "                           read: []) { success, error in",
            "    if let error {",
            "        // Handle errors here.",
            "        fatalError(\"*** An error occurred while requesting permission: \\(error.localizedDescription) ***\")",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Important",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Vision prescription samples require per-object authorization. Requesting authorization to read these samples using ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.healthkit/documentation/HealthKit/HKHealthStore/requestAuthorization(toShare:read:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " fails with an error. Instead, use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.healthkit/documentation/HealthKit/HKHealthStore/requestPerObjectReadAuthorization(for:predicate:completion:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to request authorization before querying for samples.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
