      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "HealthKit uses category samples to represent data that can be classified into a finite set of categories. To create a category sample, you must first create the corresponding category type, and then set its start and end dates, as shown below.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "guard let categoryType =",
            "    HKObjectType.categoryTypeForIdentifier(HKCategoryTypeIdentifierSleepAnalysis) else {",
            "        fatalError(\"*** Unable to create a sleep analysis category type ***\")",
            "}",
            " ",
            "let categorySample = HKCategorySample(type: categoryType,",
            "                                      value: HKCategoryValueSleepAnalysis.Asleep.rawValue,",
            "                                      startDate: start,",
            "                                      endDate: end)"
          ],
          "metadata": {
            "anchor": "2556406",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
