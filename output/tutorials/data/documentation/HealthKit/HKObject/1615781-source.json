          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "source",
              "type": "codeVoice"
            },
            {
              "text": " property is only available on objects you have retrieved from the HealthKit store. When you create a new object, the source is set to ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ". The system automatically sets the source property when you save the object to the HealthKit store. The source is then available the next time the object is retrieved from the store.",
              "type": "text"
            }
          ],
          "type": "paragraph"
