            {
              "code": "Yes",
              "type": "codeVoice"
            },
            {
              "text": " if the quantity is compatible; otherwise, ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/false",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
