      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of activity performed during the workout. For the complete list of activity types, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/healthkit/hkworkoutactivitytype",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "workoutActivityType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date and time when the activity started.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "startDate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date and time when the activity ended. This date must be equal to or later than the start date.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "endDate"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of workout event objects. This array specifies when the user has paused and resumed the workout activity. This method calculates the workout’s duration based on the total amount of active time between the provided start and end dates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "workoutEvents"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A quantity using energy units, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ". This property sets the workout’s ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/healthkit/hkworkout/1615491-totalenergyburned",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " property. It represents the total active energy burned during the workout.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalEnergyBurned"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A quantity using length units, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ". This property sets the workout’s ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/healthkit/hkworkout/1615756-totaldistance",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " property.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "totalDistance"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The device that generated the data for this sample.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "device"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The metadata dictionary contains extra information describing this workout. The dictionary’s keys are all ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " objects . The values may be ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsnumber",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsdate",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " objects. For a complete list of predefined metadata keys, see Metadata Keys. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Using predefined keys helps facilitate sharing data between apps; however, you are also encouraged to create your own, custom keys as needed to extend the HealthKit quantity sample’s capabilities.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "metadata"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A workout object with the specified duration, total energy burned, total distance, device, metadata, and workout events.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method calculates the workout’s duration based on the amount of time it spends in an active state. A workout starts in an active state. A pause event switches it to an inactive state, and a resume event switches it back to an active state. For more information on workout events, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/healthkit/hkworkoutevent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". ",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If the total energy burned or total distance are non-zero values, create a set of corresponding samples that add up to the calculated totals. Associate these samples with the workout by calling the health store’s ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/healthkit/hkhealthstore/1614165-add",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let distance = HKQuantity(unit: HKUnit.mileUnit(), doubleValue: 5.2)",
            "let energyBurned = HKQuantity(unit: HKUnit.kilocalorieUnit(), doubleValue: 259.0)",
            "let pause = HKWorkoutEvent(type: HKWorkoutEventType.Pause, date: pauseStart)",
            "let resume = HKWorkoutEvent(type: HKWorkoutEventType.Resume, date: pauseEnd)",
            " ",
            "let device = HKDevice(name: deviceName,",
            "                      manufacturer: manufacturerName,",
            "                      model: modelName,",
            "                      hardwareVersion: hardwareVersionNumber,",
            "                      firmwareVersion: firmwareVersionNumber,",
            "                      softwareVersion: softwareVersionNumber,",
            "                      localIdentifier: localIdentifier,",
            "                      UDIDeviceIdentifier: deviceIdentifier)",
            " ",
            "let metadata = [HKMetadataKeyIndoorWorkout:false]",
            " ",
            "let kayaking = HKWorkout(activityType: HKWorkoutActivityType.PaddleSports, startDate: start, endDate: end, workoutEvents: [pause, resume], totalEnergyBurned: energyBurned, totalDistance: distance, device: device, metadata: metadata)",
            " ",
            "healthStore.saveObject(kayaking) { (success, error) -> Void in",
            "    ",
            "    guard success else {",
            "        // Perform proper error handling here...",
            "        fatalError(\"*** An error occurred while saving this \" +",
            "            \"workout: \\(error?.localizedDescription)\")",
            "    }",
            "}"
          ],
          "metadata": {
            "anchor": "2556462",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
