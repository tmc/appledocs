    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of points required to analyze to determine that a shape follows a parabolic path. This argument value must be at least 5.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trajectoryLength"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The specific algorithm or implementation revision that the system uses to perform the request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "revision"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The duration between analysis operations. Increase this value to reduce the number of frames analyzed on slower devices. By default, Vision analyzes all frames.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "frameAnalysisSpacing"
        }
      ]
    }
