    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This type of request produces a collection of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.vision/documentation/Vision/ClassificationObservation",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " objects that describe an image. Access the possible classifications through the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.vision/documentation/Vision/ClassifyImageRequest/supportedIdentifiers",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "if let imageURL = Bundle.main.url(forResource: \"ClassificationImage\",",
            "                                      withExtension: \"jpg\") {",
            "    do {",
            "        let request = ClassifyImageRequest()",
            "        let results = try await request.perform(on: imageURL)",
            "        for classification in results {",
            "            print(\"Classified \\(classification.identifier)\")",
            "        }",
            "    } catch {",
            "        print(\"Encountered an error when performing the request: \\(error.localizedDescription)\")",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
