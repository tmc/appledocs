    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the event stream for which this handler will be invoked.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stream"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The GCD queue to which the event handler block will be submitted. This parameter may be NULL, in which case the connectionâ€™s target queue will be the default target queue of ",
                  "type": "text"
                },
                {
                  "code": "libdispatch",
                  "type": "codeVoice"
                },
                {
                  "text": ", defined as ",
                  "type": "text"
                },
                {
                  "code": "DISPATCH_TARGET_QUEUE_DEFAULT",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "targetq"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The event handler block. The event which this block receives as its first parameter will always be a dictionary which contains the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.xpc/documentation/XPC/XPC_EVENT_KEY_NAME-swift.var",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " key. The value for this key will be a string whose value is the name assigned to the XPC event specified in the ",
                  "type": "text"
                },
                {
                  "code": "launchd.plist",
                  "type": "codeVoice"
                },
                {
                  "text": ". Future keys may be added to this dictionary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "handler"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Multiple calls to this function for the same event stream will result in undefined behavior.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
