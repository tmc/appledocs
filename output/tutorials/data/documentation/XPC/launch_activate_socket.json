    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the socket entry in the service’s Sockets dictionary.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, this parameter is populated with an array of file descriptors. One socket can have many descriptors associated with it depending on the characteristics of the network interfaces on the system. The descriptors in this array are the results of calling ",
                  "type": "text"
                },
                {
                  "code": "getaddrinfo(3)",
                  "type": "codeVoice"
                },
                {
                  "text": " with the parameters described in ",
                  "type": "text"
                },
                {
                  "code": "launchd.plist",
                  "type": "codeVoice"
                },
                {
                  "text": ". The caller is responsible for calling ",
                  "type": "text"
                },
                {
                  "code": "free(3)",
                  "type": "codeVoice"
                },
                {
                  "text": " on the returned pointer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fds"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of file descriptor entries in the returned array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cnt"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "On success, ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " is returned. Otherwise, an appropriate POSIX-domain is returned.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The possible error codes are:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "ENOENT",
                      "type": "codeVoice"
                    },
                    {
                      "text": " — There was no socket of the specified name owned by the caller.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "ESRCH",
                      "type": "codeVoice"
                    },
                    {
                      "text": " — The caller isn’t a process managed by launchd.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "EALREADY",
                      "type": "codeVoice"
                    },
                    {
                      "text": " — The socket has already been activated by the caller.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
