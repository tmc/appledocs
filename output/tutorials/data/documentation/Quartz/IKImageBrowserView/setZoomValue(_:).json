      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The zoom value. This value should be greater or equal to zero and less or equal than one. A zoom value of zero corresponds to the minimum size (40x40 pixels). A zoom value of one means images fits the browser bounds. Other values are interpolated.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "aValue"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You must use ",
              "type": "text"
            },
            {
              "code": "setZoomValue",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.quartz/documentation/Quartz/IKImageBrowserView/setCellSize(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", but not both. Setting the zoom value changes the cell size, and vice versa.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
