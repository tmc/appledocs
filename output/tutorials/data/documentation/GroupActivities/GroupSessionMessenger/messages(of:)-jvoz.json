      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Call this method to receive the messages that other participants send to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "group. This method returns a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.GroupActivities/documentation/GroupActivities/GroupSessionMessenger/Messages",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " structure, which conforms to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "AsyncSequence",
              "type": "codeVoice"
            },
            {
              "text": " protocol. This sequence contains all, some, or none of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "messages sent over time. You retrieve messages by iterating over them using",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "an asynchronous ",
              "type": "text"
            },
            {
              "code": "for",
              "type": "codeVoice"
            },
            {
              "text": "-",
              "type": "text"
            },
            {
              "code": "in",
              "type": "codeVoice"
            },
            {
              "text": " loop, as shown in the following example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let sessionMessenger = GroupSessionMessenger(session: groupSession)",
            "",
            "async {",
            "    for await move in sessionMessenger.messages(of: TicTacToe.Move.self) {",
            "        self.board.addMove(move)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
