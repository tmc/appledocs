      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The address book for the logged-in user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "addressBook"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The search element that specifies the query. If ",
                  "type": "text"
                },
                {
                  "code": "search",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", this function raises an exception. Create an ABSearchElement object using the record specific functions: ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.addressbook/documentation/AddressBook/ABGroupCreateSearchElement(_:_:_:_:_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.addressbook/documentation/AddressBook/ABPersonCreateSearchElement(_:_:_:_:_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". See ",
                  "type": "text"
                },
                {
                  "code": "ABSearchElement C",
                  "type": "codeVoice"
                },
                {
                  "text": " for more functions that create compound queries.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "search"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A new array containing ABRecord objects representing all the records that match ",
              "type": "text"
            },
            {
              "code": "search",
              "type": "codeVoice"
            },
            {
              "text": ". If no records match ",
              "type": "text"
            },
            {
              "code": "search",
              "type": "codeVoice"
            },
            {
              "text": ", this function returns an empty array. You are responsible for releasing this object.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
