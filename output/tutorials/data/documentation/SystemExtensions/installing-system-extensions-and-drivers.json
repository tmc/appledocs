{
  "abstract": [
    {
      "text": "Activate system extensions and drivers to make them available to the system, and update or deactivate them as needed.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.systemextensions/documentation/SystemExtensions"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "swift",
    "url": "doc://com.apple.systemextensions/documentation/SystemExtensions/installing-system-extensions-and-drivers"
  },
  "kind": "article",
