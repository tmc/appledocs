        {
          "inlineContent": [
            {
              "text": "Use the object in this property to register your appâ€™s handler for pausing the currently playing track. In your handler, pause playback of the current item but maintain the current play position. You can disable the command if your app does not support it.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
