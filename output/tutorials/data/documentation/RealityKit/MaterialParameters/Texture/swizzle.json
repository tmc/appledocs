    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The default value performs no swizzle, sampling red from the texture’s red channel,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "green from the texture’s green channel, blue from the texture’s blue channel, and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "alpha from the texture’s alpha channel.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
