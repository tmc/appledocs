          "type": "heading"
        },
        {
          "code": [
            "if let textureResource = try? TextureResource.load(named: \"entity_cc_normalMap\") {",
            "    let ccNormalMap = CustomMaterial.Texture(textureResource)",
            "    let clearcoatNormal = CustomMaterial.ClearcoatNormal(texture: ccNormalMap)",
            "}"
