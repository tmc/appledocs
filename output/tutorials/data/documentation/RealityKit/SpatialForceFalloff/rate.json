    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "An exponent that determines how the effectâ€™s strength diminishes with distance. Use a non-negative rate.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "When the rate is ",
                      "type": "text"
                    },
                    {
                      "inlineContent": [
                        {
                          "text": "0",
                          "type": "text"
                        }
                      ],
                      "type": "strong"
                    },
                    {
                      "text": ", no falloff occurs.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "When the rate is ",
                      "type": "text"
                    },
                    {
                      "inlineContent": [
                        {
                          "text": "greater than 0 and less than 1.0",
                          "type": "text"
                        }
                      ],
                      "type": "strong"
                    },
                    {
                      "text": ", falloff occurs slower and is sublinear.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "When the rate is ",
                      "type": "text"
                    },
                    {
                      "inlineContent": [
                        {
                          "text": "1.0",
                          "type": "text"
                        }
                      ],
                      "type": "strong"
                    },
                    {
                      "text": ", falloff is linear.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "When the rate is ",
                      "type": "text"
                    },
                    {
                      "inlineContent": [
                        {
                          "text": "greater than 1",
                          "type": "text"
                        }
                      ],
                      "type": "strong"
                    },
                    {
                      "text": ", falloff occurs faster and is nonlinear.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
