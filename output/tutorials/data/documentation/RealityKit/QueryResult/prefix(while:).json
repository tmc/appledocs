            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "positive numbers at the beginning of the ",
              "type": "text"
            },
            {
              "code": "numbers",
              "type": "codeVoice"
            },
            {
              "text": " array. Every element",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of ",
              "type": "text"
            },
            {
              "code": "numbers",
              "type": "codeVoice"
            },
            {
              "text": " up to, but not including, the first negative value is",
