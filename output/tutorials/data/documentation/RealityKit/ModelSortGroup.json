    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Each model sort group relates model entities to each other",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "so that you can define what order the renderer draws them.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Pass the same model sort group instance to each ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/ModelSortGroupComponent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "whose entity you want to group, along with an order number for that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "entity within the group.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let group1 = ModelSortGroup()",
            "entityA.components.set(",
            "    ModelSortGroupComponent(group: group1, order: 0)",
            ")",
            "entityB.components.set(",
            "    ModelSortGroupComponent(group: group1, order: 1)",
            ")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In the example above, the renderer draws ",
              "type": "text"
            },
            {
              "code": "entityA",
              "type": "codeVoice"
            },
            {
              "text": " before ",
              "type": "text"
            },
            {
              "code": "entityB",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Each ",
              "type": "text"
            },
            {
              "code": "ModelSortGroup",
              "type": "codeVoice"
            },
            {
              "text": " instance represent a unique group.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let group2 = ModelSortGroup()",
            "entityC.components.set(",
            "    ModelSortGroupComponent(group: group2, order: 2)",
            ")",
            "entityD.components.set(",
            "    ModelSortGroupComponent(group: group2, order: 3)",
            ")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In this example, ",
              "type": "text"
            },
            {
              "code": "entityC",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "entityD",
              "type": "codeVoice"
            },
            {
              "text": " are in the same group as each other,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "but in a different group than ",
              "type": "text"
            },
            {
              "code": "entityA",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "entityB",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
