        {
          "inlineContent": [
            {
              "text": "Add this component to an entity, or any child of an entity, that has a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/PhysicsSimulationComponent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "All joints in the ",
              "type": "text"
            },
            {
              "code": "PhysicsJointsComponent",
              "type": "codeVoice"
            },
            {
              "text": " need to reference entities",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "under the same ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/PhysicsSimulationComponent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " tree.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Add a joint to the correct ",
              "type": "text"
            },
            {
              "code": "PhysicsJointsComponent",
              "type": "codeVoice"
            },
            {
              "text": " instance",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by calling its ",
              "type": "text"
            },
