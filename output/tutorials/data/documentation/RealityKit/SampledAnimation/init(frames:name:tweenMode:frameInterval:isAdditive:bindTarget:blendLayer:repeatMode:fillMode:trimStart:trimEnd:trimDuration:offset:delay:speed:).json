      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of values in which each element represents a discrete",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "state of the animated property at a given point in the animation’s timeline.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "frames"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A textual name for the animation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An option that determines how animation frames transition.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tweenMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The duration within the animation timeline for each",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "frame in the frames array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "frameInterval"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the animation",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "builds on the current state of the target entity or resets the state",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "before running.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isAdditive"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A textual name that identifies the particular property",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that animates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bindTarget"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The order in which the framework composites the animation",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "into the view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "blendLayer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An option that determines how the animation repeats.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "repeatMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The playback behavior outside of the normal duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The optional time, in seconds, at which the animation plays.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimStart"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The optional time, in seconds, at which the animation stops.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimEnd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional duration that overrides the calculated duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimDuration"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time, in seconds, at which the animation begins within the duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offset"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An amount of time that elapses before the animation plays.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "delay"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A factor that changes the animation’s rate of playback.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "speed"
        }
