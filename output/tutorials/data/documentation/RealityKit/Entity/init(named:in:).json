            {
              "inlineContent": [
                {
                  "text": "The base name of the file to load, omitting the file extension, or scene name if loading from a .reality file.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle containing the file. Use ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to search the appâ€™s main bundle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundle"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The root entity in the loaded file.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "RealityKit supports loading entities from USD (",
              "type": "text"
            },
            {
              "code": ".usd",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": ".usda",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": ".usdc",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": ".usdz",
              "type": "codeVoice"
            },
            {
              "text": ") and Reality files (",
              "type": "text"
            },
            {
              "code": ".reality",
              "type": "codeVoice"
            },
            {
              "text": ").",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For more information on loading entities, see",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/loading-entities-from-a-file",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use a task group to await all loads before adding them to the scene to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "display content from multiple scenes without hitches or pop in.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct SomeRealityView: View {",
            "    var body: some View {",
            "        RealityView { content in",
            "            // Add the initial RealityKit content.",
            "            let entities : [Entity] = await withTaskGroup(of: Entity?.self) { taskGroup in",
            "                // Load all the scenes concurrently.",
            "                taskGroup.addTask { return try? await Entity(named: \"SceneA\", in: realityKitContentBundle) }",
            "                taskGroup.addTask { return try? await Entity(named: \"SceneB\", in: realityKitContentBundle) }",
            "                taskGroup.addTask { return try? await Entity(named: \"SceneC\", in: realityKitContentBundle) }",
            "",
            "                var entities = [Entity]()",
            "                // Wait for all the scenes to load.",
            "                for await entity in taskGroup {",
            "                    if let entity {",
            "                        entities.append(entity)",
            "                    }",
            "                }",
            "                return entities",
            "            }",
            "            // Add all the content.",
            "            for entity in entities {",
            "                content.add(entity)",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
