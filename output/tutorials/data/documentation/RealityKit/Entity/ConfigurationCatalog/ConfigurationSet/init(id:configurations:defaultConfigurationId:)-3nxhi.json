    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ID of the configuration set that’s unique across all other configuration sets.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A dictionary of configurations you can choose from.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Each key needs to match the ID of the configuration value it maps to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "configurations"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ID of one of the configuration elements in the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "configurations",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter, which is the default configuration the entity initializer",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "applies if you don’t choose a configuration from the set.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "defaultConfigurationId"
        }
      ]
    },
    {
