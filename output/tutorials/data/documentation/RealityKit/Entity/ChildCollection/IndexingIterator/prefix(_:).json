            {
              "inlineContent": [
                {
                  "text": "The maximum number of elements to return. The",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "value of ",
                  "type": "text"
                },
                {
                  "code": "maxLength",
                  "type": "codeVoice"
                },
                {
                  "text": " must be greater than or equal to zero.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxLength"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A sequence starting at the beginning of this sequence",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with at most ",
              "type": "text"
            },
            {
              "code": "maxLength",
              "type": "codeVoice"
            },
            {
              "text": " elements.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the maximum length exceeds the number of elements in the sequence,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the result contains all the elements in the sequence.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let numbers = [1, 2, 3, 4, 5]",
            "print(numbers.prefix(2))",
            "// Prints \"[1, 2]\"",
            "print(numbers.prefix(10))",
            "// Prints \"[1, 2, 3, 4, 5]\""
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
