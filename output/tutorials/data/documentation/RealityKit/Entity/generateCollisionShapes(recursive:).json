                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " to also generate the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "collision shapes for all descendants of the entity.",
                  "type": "text"
                }
              ],
