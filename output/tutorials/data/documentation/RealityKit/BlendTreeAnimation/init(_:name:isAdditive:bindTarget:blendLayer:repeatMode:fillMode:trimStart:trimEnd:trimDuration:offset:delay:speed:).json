    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The first node in a tree of animations.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "root"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A textual name for the animation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the animation",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "builds on the current state of the target entity or resets the state",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "before running.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isAdditive"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A textual name that identifies the particular property",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that animates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bindTarget"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The order in which the framework composites the animation",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "into the view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "blendLayer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An option that determines how the animation repeats.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "repeatMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The behavior when the animated property reaches its end value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The optional time, in seconds, at which the source animation plays.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimStart"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The optional time, in seconds, at which the source animation stops.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimEnd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional duration that overrides the calculated duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimDuration"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time, in seconds, at which the animation begins within the duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offset"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An amount of time that lapses before the animation plays.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "delay"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A factor that increases or decreases the animation’s rate of playback.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "speed"
        }
      ]
    }
