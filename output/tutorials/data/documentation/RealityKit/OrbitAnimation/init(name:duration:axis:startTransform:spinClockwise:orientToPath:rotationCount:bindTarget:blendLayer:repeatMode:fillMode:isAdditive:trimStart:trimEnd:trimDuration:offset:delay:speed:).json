      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A textual name for the animation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The elapsed time for one complete rotation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "duration"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A 3D vector that points in the direction of the axis around",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "which to rotate.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axis"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The orbiting entity’s beginning position.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "startTransform"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the object",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "orbits the center point in the clockwise direction.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "spinClockwise"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the orbiting",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "object updates its orientation during the animation to orient itself",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "along the rotation path.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "orientToPath"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of times to rotate the target entity before",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "stopping.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rotationCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A textual name that identifies the particular property",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that animates.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bindTarget"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The order in which the framework composites the animation",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "into the view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "blendLayer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An option that determines how the animation repeats.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "repeatMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The playback behavior outside of the normal duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the animation",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "builds on the current state of the target entity or resets the state",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "before running.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isAdditive"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The optional time, in seconds, at which the animation plays.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimStart"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The optional time, in seconds, at which the animation stops.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimEnd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional duration that overrides the calculated duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimDuration"
        },
        {
