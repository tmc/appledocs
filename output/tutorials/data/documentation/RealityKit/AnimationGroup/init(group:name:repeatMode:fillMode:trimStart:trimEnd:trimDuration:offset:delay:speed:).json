    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A collection of animations to run.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "group"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A textual name for the group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An option that determines how the animations repeat.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "repeatMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The playback behavior outside of the normal duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time, in seconds, at which the animations play.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimStart"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time, in seconds, at which the animations stop.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimEnd"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional duration that overrides the calculated duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "trimDuration"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time, in seconds, at which the animation begins within the duration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offset"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An amount of time that lapses before the animation plays.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "delay"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A factor that increases or decreases the animation’s rate of playback.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "speed"
        }
      ]
    }
