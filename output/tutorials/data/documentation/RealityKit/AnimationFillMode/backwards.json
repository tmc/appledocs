        {
          "inlineContent": [
            {
              "text": "For example, if you wind a hand-waving animation’s duration back one",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "second by setting ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/SampledAnimation/trimStart",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "-1.0",
              "type": "codeVoice"
            },
            {
              "text": ", a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/SampledAnimation/fillMode",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " of ",
              "type": "text"
            },
            {
              "code": "backwards",
              "type": "codeVoice"
            },
            {
              "text": " determines that the hand",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "holds its initial appearance for one second before waving.",
              "type": "text"
            }
          ],
          "type": "paragraph"
