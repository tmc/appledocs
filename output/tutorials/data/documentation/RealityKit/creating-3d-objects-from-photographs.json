    "doc://com.apple.RealityKit/documentation/RealityKit/PhotogrammetrySession/process(requests:)": {
      "abstract": [
        {
          "text": "Starts processing of the provided processing ",
          "type": "text"
        },
        {
          "code": "requests",
          "type": "codeVoice"
        },
        {
          "text": ".  Messages begin to be produced to",
          "type": "text"
        },
        {
          "text": " ",
          "type": "text"
        },
        {
          "text": "the ",
          "type": "text"
        },
        {
          "code": "output",
          "type": "codeVoice"
        },
        {
          "text": " publisher.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "keyword",
          "text": "func"
        },
        {
          "kind": "text",
          "text": " "
        },
        {
          "kind": "identifier",
          "text": "process"
        },
        {
          "kind": "text",
          "text": "("
        },
        {
          "kind": "externalParam",
