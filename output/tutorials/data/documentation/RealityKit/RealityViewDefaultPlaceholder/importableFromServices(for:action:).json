          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "code": [
            "@State private var title: String",
            "var body: some View {",
            "    Color.pink",
            "        .frame(width: 400, height: 400)",
            "        .importableFromServices(for: String.self) { titles",
            "            title = titles.first ?? title",
            "            return !titles.isEmpty",
            "        }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        }
