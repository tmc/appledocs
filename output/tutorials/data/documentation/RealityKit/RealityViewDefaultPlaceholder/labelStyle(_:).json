          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to set a specific style for all labels within a view:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "VStack {",
            "    Label(\"Fire\", systemImage: \"flame.fill\")",
            "    Label(\"Lightning\", systemImage: \"bolt.fill\")",
            "}",
            ".labelStyle(MyCustomLabelStyle())"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
