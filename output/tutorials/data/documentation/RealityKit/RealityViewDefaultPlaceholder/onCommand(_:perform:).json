      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The selector to register for ",
                  "type": "text"
                },
                {
                  "code": "action",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "selector"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The action to perform. If ",
                  "type": "text"
                },
                {
                  "code": "action",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "command",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "keeps its association with this view but doesn’t trigger.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that triggers ",
              "type": "text"
            },
            {
              "code": "action",
              "type": "codeVoice"
            },
            {
              "text": " when the ",
              "type": "text"
            },
            {
              "code": "command",
              "type": "codeVoice"
            },
            {
              "text": " occurs.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This view or one of the views it contains must be in focus in order",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for the action to trigger. Other actions for the same command",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "on views ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "closer",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": " to the view in focus take priority, potentially",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "overriding this action.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
