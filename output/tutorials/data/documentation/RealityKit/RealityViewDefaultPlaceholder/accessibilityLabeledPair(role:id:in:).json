    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Determines whether this element should be used as the label",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "in the pair, or the content in the pair.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "role"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier for the label / content pair. Elements with",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "matching identifiers within the same namespace will be paired",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "together.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The namespace used to organize label and content. Label",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "and content under the same namespace with matching identifiers will",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "be paired together.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "namespace"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "accessibilityLabeledPair",
              "type": "codeVoice"
            },
            {
              "text": " with a role of ",
              "type": "text"
            },
            {
              "code": "AccessibilityLabeledPairRole.label",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to identify the label, and a role of ",
              "type": "text"
            },
            {
              "code": "AccessibilityLabeledPairRole.content",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to identify the content.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This improves the behavior of accessibility features such as VoiceOver",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "when navigating such elements, allowing users to better understand the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "relationship between them.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
