      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that returns a single",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "class instance or a value conforming to ",
                  "type": "text"
                },
                {
                  "code": "Transferable",
                  "type": "codeVoice"
                },
                {
                  "text": " that",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "represents the draggable data from this view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "payload"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "View",
                  "type": "codeVoice"
                },
                {
                  "text": " to use as the source for the dragging",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "preview, once the drag operation has begun. The preview is centered over",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the source view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "preview"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that activates this view as the source of a drag and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "drop operation, beginning with user gesture input.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Applying the ",
              "type": "text"
            },
            {
              "code": "draggable(_:preview:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier adds the appropriate gestures",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for drag and drop to this view. When a drag operation begins,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a rendering of ",
              "type": "text"
            },
            {
              "code": "preview",
              "type": "codeVoice"
            },
            {
              "text": " is generated and used as the preview image.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var title: String",
            "var body: some View {",
            "    Color.pink",
            "        .frame(width: 400, height: 400)",
            "        .draggable(title) {",
            "             Text(\"Drop me\")",
            "         }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "To customize the lift preview, shown while the system transitions",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to show your custom ",
              "type": "text"
            },
            {
              "code": "preview",
              "type": "codeVoice"
            },
            {
              "text": ", apply a ",
              "type": "text"
            },
            {
              "code": "View/contentShape(_:_:eoFill:)",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with a ",
              "type": "text"
            },
            {
              "code": "ContentShapeKinds/dragPreview",
              "type": "codeVoice"
            },
            {
              "text": " kind. For example, you can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "change the preview’s corner radius or use a nested view as the preview.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
