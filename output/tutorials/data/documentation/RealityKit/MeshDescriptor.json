    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Create or modify 3D shapes in a RealityKit scene using ",
              "type": "text"
            },
            {
              "code": "MeshDescriptor",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "which provides properties and methods to define the vertices,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "normals, texture coordinates, and other attributes of the mesh.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Apply the mesh to an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/Entity",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " by creating a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/MeshResource",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/MeshResource/generate(from:)-6l1q2",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", and create a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/ModelComponent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/ModelComponent/init(mesh:materials:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Start by creating a basic triangle with a ",
              "type": "text"
            },
            {
              "code": "MeshDescriptor",
              "type": "codeVoice"
            },
            {
              "text": " instance.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var descriptor = MeshDescriptor(name: \"triangle\")",
            "descriptor.positions = MeshBuffers.Positions([",
            "    [-1, -1, 0], [1, -1, 0], [0, 1, 0]",
            "])",
            "descriptor.primitives = .triangles([0, 1, 2])"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
