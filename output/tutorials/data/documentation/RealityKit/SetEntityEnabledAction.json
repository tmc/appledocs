    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This action toggles the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/SetEntityEnabledAction/targetEntity",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/Entity/isEnabled",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " flag.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Use the animation generated from this action within an animation group to determine when the entity should be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "enabled, and visible in the scene.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below creates an animation which disables the entity after five seconds.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "In this example, the target entity starts enabled.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Create an action entity resolution.",
            "//",
            "// The animation will be played from the parent entity.",
            "let childEntity: ActionEntityResolution = .entityNamed(\"childEntity\")",
            "",
            "// Create an action to disable the entity.",
            "let disableEntityAction = SetEntityEnabledAction(targetEntity: childEntity,",
            "                                                 isEnabled: false)",
            "",
            "// Create a small positive duration value.",
            "let duration: TimeInterval = 1 / 30.0",
            "",
            "// Create an animation to disable the target entity.",
            "let disableEntityAnimation = try AnimationResource",
            "    .makeActionAnimation(for: disableEntityAction,",
            "                         duration: duration,",
            "                         delay: 5.0)",
            "",
            "// Play the sequence animation that plays the actions.",
            "rootEntity.playAnimation(disableEntityAnimation)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "identifier": "setentityenabled-action",
          "type": "video"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Animations do not play on entities that are disabled. In order to disable, and enable the target entity,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "play this action on another entity, such as a root entity, or its parent.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This action does not animate a bound property, such as ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/BindTarget/transform",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This action performs instantaneously, consider supplying a small positive duration value when creating the animation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Tip",
          "style": "tip",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
