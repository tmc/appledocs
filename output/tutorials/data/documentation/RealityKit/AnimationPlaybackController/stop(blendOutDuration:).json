      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Time (in seconds) to fade out the animation before it stops.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "blendOutDuration"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method has no effect if the animation is complete. After you stop",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the animation, the playback controller becomes invalid. Create a new one",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with the same resource to play the animation again.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
