    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/PhysicallyBasedMaterial",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", object, you can use ",
              "type": "text"
            },
            {
              "code": "sheen",
              "type": "codeVoice"
            },
            {
              "text": " to add",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "soft specular highlights that simulate subtle reflections like the ones",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that occur with some materials, primarily fabrics. You can specify",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "sheen",
              "type": "codeVoice"
            },
            {
              "text": " using a single color, or you can provide a UV-mapped image.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "PhysicallyBasedMaterial-sheen-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example specifies ",
              "type": "text"
            },
            {
              "code": "sheen",
              "type": "codeVoice"
            },
            {
              "text": " using a single value for the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "entire material:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let sheenColor = PhysicallyBasedMaterial.Color(deviceRed: 0.8,",
            "green: 0.8, blue: 0.8, alpha: 1.0)",
            "material.sheen = .init(tint:sheenColor)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "This example shows how to specify sheen using a UV-mapped image texture:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "if let sheenResource = try? TextureResource.load(named:",
            "\"entity_sheen\") {",
            "    let sheenMap = MaterialParameters.Texture(sheenResource)",
            "    material.sheen = .init(texture: sheenMap)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
