    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An asynchronous closure that configures the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "initial content of the new ",
                  "type": "text"
                },
                {
                  "code": "RealityView",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "This closure is asynchronous to keep your app’s UI responsive while",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "you load content to populate this view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "make"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional closure that updates the ",
                  "type": "text"
                },
                {
                  "code": "RealityView",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "instance’s content as the view’s state changes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "update"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A temporary view that the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/RealityView",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " displays until",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "your closure for the ",
                  "type": "text"
                },
                {
                  "code": "make",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter completes.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "For example, you can display a loading indicator with a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/SwiftUI/ProgressView",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "instance as a placeholder.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "placeholder"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, your app can asynchronously load an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/Entity",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " from a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": ".reality",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": ".usdz",
              "type": "codeVoice"
            },
            {
              "text": " file, and display a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/SwiftUI/ProgressView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "while the system loads the file:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "RealityView { content in",
            "    if let newEntity = try? await Entity(named: \"model_file_name\") {",
            "        content.add(newEntity)",
            "    }",
            "} placeholder: {",
            "    ProgressView()",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
