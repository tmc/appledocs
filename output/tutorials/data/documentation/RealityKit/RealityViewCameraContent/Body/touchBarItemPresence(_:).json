    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A trait that describes the behavior for this Touch Bar",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "touchBarItemPresence(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to define the visibility requirements of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a particular Touch Bar view during customization by the user.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Touch Bar views may be:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": ".required",
                      "type": "codeVoice"
                    },
                    {
                      "text": ": not allowed to be removed by the user.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": ".default",
                      "type": "codeVoice"
                    },
                    {
                      "text": ": shown by default prior to user customization, but",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "removable.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": ".optional",
                      "type": "codeVoice"
                    },
                    {
                      "text": ": not visible by default, but can be added through the",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "customization palette.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "Each ",
              "type": "text"
            },
            {
              "code": "TouchBarItemPresence",
              "type": "codeVoice"
            },
            {
              "text": " must be initialized with a string that is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a globally unique identifier for this item.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, all of the Touch Bar items are visible in the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Touch Bar by default, except for the “Clubs” item. It’s set to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": ".optional",
              "type": "codeVoice"
            },
            {
              "text": " but is configurable by the user:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TextField(\"TouchBar Demo\", text: $placeholder)",
            "    .frame(maxWidth: .infinity, maxHeight: .infinity)",
            "    .focusable()",
            "    .touchBar {",
            "        Button(\"♥️\", action: selectHearts)",
            "            .touchBarItemPresence(.required(\"heartsKey\"))",
            "        Button(\"♣️\", action: selectClubs)",
            "            .touchBarItemPresence(.optional(\"clubsKey\"))",
            "        Button(\"♠️\", action: selectSpades)",
            "            .touchBarItemPresence(.required(\"spadesKey\"))",
            "        Button(\"♦️\", action: selectDiamonds)",
            "            .touchBarItemPresence(.required(\"diamondsKey\"))",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
