              "code": "content",
              "type": "codeVoice"
            },
            {
              "text": ", from ",
              "type": "text"
            },
            {
              "code": "edge",
              "type": "codeVoice"
            },
            {
              "text": ", with its safe area",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "increased by the same amount.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
