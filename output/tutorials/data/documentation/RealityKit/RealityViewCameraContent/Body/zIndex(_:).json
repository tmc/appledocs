      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A relative front-to-back ordering for this view; the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "default is ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "zIndex(_:)",
              "type": "codeVoice"
            },
            {
              "text": " when you want to control the front-to-back ordering of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "views.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In this example there are two overlapping rotated rectangles. The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "frontmost is represented by the larger index value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "VStack {",
            "    Rectangle()",
            "        .fill(Color.yellow)",
            "        .frame(width: 100, height: 100, alignment: .center)",
            "        .zIndex(1) // Top layer.",
            "",
            "    Rectangle()",
            "        .fill(Color.red)",
            "        .frame(width: 100, height: 100, alignment: .center)",
            "        .rotationEffect(.degrees(45))",
            "        // Here a zIndex of 0 is the default making",
            "        // this the bottom layer.",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
