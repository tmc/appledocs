    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The preferred visibility of the bar.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "visibility"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bars to update the visibility of or",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "ToolbarPlacement/automatic",
                  "type": "codeVoice"
                },
                {
                  "text": " if empty.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bars"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The preferred visibility flows up to the nearest container",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that renders a bar. This could be a ",
              "type": "text"
            },
            {
              "code": "NavigationView",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "TabView",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in iOS, or the root view of a ",
              "type": "text"
            },
            {
              "code": "WindowGroup",
              "type": "codeVoice"
            },
            {
              "text": " in macOS.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This examples shows a view that hides the navigation bar on iOS, or the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "window toolbar items on macOS.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NavigationView {",
            "    ContentView()",
            "        .toolbarVisibility(.hidden)",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "To hide the entire titlebar on macOS, use this modifier with",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "ToolbarPlacement/windowToolbar",
              "type": "codeVoice"
            },
            {
              "text": " placement.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NavigationView {",
            "    ContentView()",
            "        .toolbarVisibility(.hidden, for: .windowToolbar)",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can provide multiple ",
              "type": "text"
            },
            {
              "code": "ToolbarPlacement",
              "type": "codeVoice"
            },
            {
              "text": " instances to hide",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "multiple bars at once.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    NavigationView {",
            "        ContentView()",
            "            .toolbarVisibility(",
            "                .hidden, for: .navigationBar, .tabBar)",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "In macOS, if you provide ",
                  "type": "text"
                },
                {
                  "code": "ToolbarCommands",
                  "type": "codeVoice"
                },
                {
                  "text": " to the scene",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of your app, this modifier disables the toolbar visibility command",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "while the value of the modifier is not ",
                  "type": "text"
                },
                {
                  "code": "ToolbarPlacement/automatic",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "Depending on the specified bars, the requested visibility may not be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "able to be fulfilled.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
