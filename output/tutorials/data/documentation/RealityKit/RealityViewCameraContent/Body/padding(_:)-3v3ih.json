    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that’s padded by the amount you specify.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to add padding all the way around a view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "VStack {",
            "    Text(\"Text padded by 10 points on each edge.\")",
            "        .padding(10)",
            "        .border(.gray)",
            "    Text(\"Unpadded text for comparison.\")",
            "        .border(.yellow)",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The order in which you apply modifiers matters. The example above",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "applies the padding before applying the border to ensure that the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "border encompasses the padded region:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To independently control the amount of padding for each edge, use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "View/padding(_:)-6pgqq",
              "type": "codeVoice"
            },
            {
              "text": ". To pad a select set of edges by the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "same amount, use ",
              "type": "text"
            },
            {
              "code": "View/padding(_:_:)",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
