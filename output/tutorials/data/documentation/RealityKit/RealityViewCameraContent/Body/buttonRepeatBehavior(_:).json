      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A value of ",
                  "type": "text"
                },
                {
                  "code": "enabled",
                  "type": "codeVoice"
                },
                {
                  "text": " means that buttons should",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "enable repeating behavior and a value of ",
                  "type": "text"
                },
                {
                  "code": "disabled",
                  "type": "codeVoice"
                },
                {
                  "text": " means that buttons",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "should disallow repeating behavior.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "behavior"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Apply this to buttons that increment or decrement a value or perform",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "some other inherently iterative operation. Interactions such as",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "pressing-and-holding on the button, holding the buttonâ€™s keyboard",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "shortcut, or holding down the space key while the button is focused will",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "trigger this repeat behavior.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Button {",
            "    playbackSpeed.advance(by: 1)",
            "} label: {",
            "    Label(\"Speed up\", systemImage: \"hare\")",
            "}",
            ".buttonRepeatBehavior(.enabled)"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "This affects all system button styles, as well as automatically",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "affects custom ",
              "type": "text"
            },
            {
              "code": "ButtonStyle",
              "type": "codeVoice"
            },
            {
              "text": " conforming types. This does not",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "automatically apply to custom ",
              "type": "text"
            },
            {
              "code": "PrimitiveButtonStyle",
              "type": "codeVoice"
            },
            {
              "text": " conforming types,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and the ",
              "type": "text"
            },
            {
              "code": "EnvironmentValues.buttonRepeatBehavior",
              "type": "codeVoice"
            },
            {
              "text": " value should be used",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to adjust their custom gestures as appropriate.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
