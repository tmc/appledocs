      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "One of the content types available in the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/UIKit/UITextContentType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "structure that identify the semantic meaning expected for a text-entry",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "area. These include support for email addresses, location names, URLs,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "and telephone numbers, to name just a few.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "textContentType"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to set the content type for input text.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example, you can configure a ",
              "type": "text"
            },
            {
              "code": "TextField",
              "type": "codeVoice"
            },
            {
              "text": " for the entry of email",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "addresses:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TextField(\"Enter your email\", text: $emailAddress)",
            "    .textContentType(.emailAddress)"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
