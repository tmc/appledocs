    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The custom icon to use for confirmation dialogs and alerts.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Passing ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " will use the default app icon.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "icon"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "On macOS, this icon replaces the default icon of the app.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "On watchOS, this icon will be shown in any dialogs presented.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier has no effect on other platforms.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example configures a ",
              "type": "text"
            },
            {
              "code": "confirmationDialog",
              "type": "codeVoice"
            },
            {
              "text": " with a custom image.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Button(\"Delete items\") {",
            "    isShowingDialog = true",
            "}",
            ".confirmationDialog(",
            "    \"Are you sure you want to erase these items?\",",
            "        isPresented: $isShowingDialog",
            ") {",
            "    Button(\"Erase\", role: .destructive) {",
            "        // Handle item deletion.",
            "    }",
            "    Button(\"Cancel\", role: .cancel) {",
            "        isShowingDialog = false",
            "    }",
            "}",
            ".dialogIcon(Image(...))"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
