{
  "abstract": [
    {
      "text": "Ignores any bounding box information embedded in the input images and instead returns all possible geometry that can",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "be automatically estimated using the image set.  The resulting mesh will likely need post-processing.",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "Note: to recover the entire scene geometry as well as ignore the box, ",
      "type": "text"
    },
    {
      "code": "isObjectMaskingEnabled",
      "type": "codeVoice"
    },
    {
      "text": " should also be set to false.",
      "type": "text"
    }
  ],
