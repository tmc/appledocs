        {
          "inlineContent": [
            {
              "text": "The reconstructed texture map will have width",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and height dimensions both less than or equal to the provided ",
              "type": "text"
            },
            {
              "code": "maxTextureDimension",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This property is useful for fitting within the texture memory resources of the rendering system.",
              "type": "text"
            }
          ],
