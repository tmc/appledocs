            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a custom ",
              "type": "text"
            },
            {
              "code": "Sequence",
              "type": "codeVoice"
            },
            {
              "text": " it is expected that the caller has maintained the map from provided sample ID to original image",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "location.",
              "type": "text"
