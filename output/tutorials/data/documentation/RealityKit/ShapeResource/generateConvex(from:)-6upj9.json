      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A mesh with the shape of the convex polyhedron. Use meshes with a small number of vertices to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "avoid hurting performance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mesh"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The new shape, defined by the convex hull of the vertices in ",
              "type": "text"
            },
            {
              "code": "mesh",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Will throw an error if ",
                  "type": "text"
                },
                {
                  "code": "mesh",
                  "type": "codeVoice"
                },
                {
                  "text": " does not define a nonempty convex volume.  For example, will fail if",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "all the vertices in ",
                  "type": "text"
                },
                {
                  "code": "mesh",
                  "type": "codeVoice"
                },
                {
                  "text": " are coplanar.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Throws",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
