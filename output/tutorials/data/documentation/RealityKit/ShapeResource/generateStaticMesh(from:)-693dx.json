    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The mesh anchor that defines the collision shape.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "This anchor needs to have",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "meshAnchor.geometry.faces.primitive == GeometryElement.Primitive.triangle",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "meshAnchor"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The returned ",
              "type": "text"
            },
            {
              "code": "ShapeResource",
              "type": "codeVoice"
            },
            {
              "text": " can only be used under the following circumstances:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "This shape can only be used with ",
                      "type": "text"
                    },
                    {
                      "code": "PhysicsBodyMode.static",
                      "type": "codeVoice"
                    },
                    {
                      "text": " (not ",
                      "type": "text"
                    },
                    {
                      "code": ".dynamic",
                      "type": "codeVoice"
                    },
                    {
                      "text": " or ",
                      "type": "text"
                    },
                    {
                      "code": ".kinematic",
                      "type": "codeVoice"
                    },
                    {
                      "text": ")",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "This shape cannot be used with ",
                      "type": "text"
                    },
                    {
                      "code": "CollisionComponent.Mode.trigger",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "In other words, the returned shape can only be used for static world geometry.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Generating a shape resource from the AR mesh takes time,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "because the input mesh needs to be preprocessed",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "(“cooked”) by the physics engine before it can be used.  This function is marked ",
                  "type": "text"
                },
                {
                  "code": "async",
                  "type": "codeVoice"
                },
                {
                  "text": " because ideally",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "this precomputation should occur in the background.  For example, when calling from non-",
                  "type": "text"
                },
                {
                  "code": "async",
                  "type": "codeVoice"
                },
                {
                  "text": " code,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "you can use Swift’s ",
                  "type": "text"
                },
                {
                  "code": "Task",
                  "type": "codeVoice"
                },
                {
                  "text": " API to assign a priority (see the example below).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "Below is example usage of this function, assumed to be called from a non-async block:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let myAnchor: MeshAnchor = ...",
            "// It is recommended to use a low-priority task, since generating the collision mesh can take a while.",
            "let myShapeTask = Task(priority: .low) {",
            "    let shape = await ShapeResource.generateStaticMesh(from: myAnchor)",
            "",
            "    // Now we can do something with `shape`, such as create an entity with it.",
            "    // Make sure to call non-async RealityKit methods from the main actor:",
            "    await MainActor.run {",
            "        let entity = Entity()",
            "        entity.components[CollisionComponent.self] = .init(shapes: [shape])",
            "",
            "        // Note that `mode` can not be set to `.dynamic`.  Only `.static` is supported.",
            "        entity.components[PhysicsBodyComponent.self] = .init(massProperties: .default,",
            "                                                             material: nil,",
            "                                                             mode: .static)",
            "        // ...",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
