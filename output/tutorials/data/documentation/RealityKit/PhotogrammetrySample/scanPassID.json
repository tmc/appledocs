    },
    {
      "text": " can only be equated if the ",
      "type": "text"
    },
    {
      "code": "sessionID",
      "type": "codeVoice"
    },
    {
      "text": " is the same – if the ",
      "type": "text"
    },
    {
      "code": "sessionID",
      "type": "codeVoice"
    },
    {
      "text": " of two samples is",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "different but the ",
      "type": "text"
    },
    {
