      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount of space between the bottom of one",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "line and the top of the next line in points.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lineSpacing"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "lineSpacing(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to set the amount of spacing from the bottom of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "one line to the top of the next for text elements in the view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the ",
              "type": "text"
            },
            {
              "code": "Text",
              "type": "codeVoice"
            },
            {
              "text": " view in the example below, 10 points separate the bottom",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of one line to the top of the next as the text field wraps inside this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view. Applying ",
              "type": "text"
            },
            {
              "code": "lineSpacing(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to a view hierarchy applies the line",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "spacing to all text elements contained in the view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Text(\"This is a string in a TextField with 10 point spacing applied between the bottom of one line and the top of the next.\")",
            "    .frame(width: 200, height: 200, alignment: .leading)",
            "    .lineSpacing(10)"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
