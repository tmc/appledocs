      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier when you want VoiceOver to speak text as individual",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "letters, character by character. This is important for text that is not",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "meant to be spoken together, like:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "An acronym that isn’t a word, like APPL, spoken as “A-P-P-L”.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A number representing a series of digits, like 25, spoken as “two-five”",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "rather than “twenty-five”.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
