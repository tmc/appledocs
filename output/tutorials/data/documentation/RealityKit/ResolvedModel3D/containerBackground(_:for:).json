    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example uses a ",
              "type": "text"
            },
            {
              "code": "LinearGradient",
              "type": "codeVoice"
            },
            {
              "text": " as a background:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    var body: some View {",
            "        NavigationStack {",
            "            List {",
            "                NavigationLink(\"Blue\") {",
            "                    Text(\"Blue\")",
            "                    .containerBackground(.blue.gradient, for: .navigation)",
            "                }",
            "                NavigationLink(\"Red\") {",
            "                    Text(\"Red\")",
            "                    .containerBackground(.red.gradient, for: .navigation)",
            "                }",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": ".containerBackground(_:for:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier differs from the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "View/background(_:ignoresSafeAreaEdges:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier by automatically",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "filling an entire parent container. ",
              "type": "text"
            },
            {
              "code": "ContainerBackgroundPlacement",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "describes the available containers.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Parameters",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "items": [
                    {
                      "content": [
                        {
                          "inlineContent": [
                            {
                              "text": "style: The shape style to use as the container background.",
                              "type": "text"
                            }
                          ],
                          "type": "paragraph"
                        }
                      ]
                    },
                    {
                      "content": [
                        {
                          "inlineContent": [
                            {
                              "text": "container: The container that will use the background.",
                              "type": "text"
                            }
                          ],
