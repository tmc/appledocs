    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The views representing the content of the toolbar.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to add toolbar content that remains consistent",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "regardless of the content view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Unlike the toolbar modifier, which configures the toolbar of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modified view’s container, the ",
              "type": "text"
            },
            {
              "code": "contentToolbar",
              "type": "codeVoice"
            },
            {
              "text": " modifier configures the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "toolbar within the modified view’s content instead. This means that the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "contentToolbar",
              "type": "codeVoice"
            },
            {
              "text": " modifier should generally be applied directly to a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "container view, instead of to the content within a container view. For",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example, to configure the toolbar of tab view’s sidebar, apply the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "contentToolbar",
              "type": "codeVoice"
            },
            {
              "text": " modifier to the ",
              "type": "text"
            },
            {
              "code": "TabView",
              "type": "codeVoice"
            },
            {
              "text": " itself, not to any of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "tabs within the ",
              "type": "text"
            },
            {
              "code": "TabView",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below adds a button to the tab view sidebar.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    Tab(\"Home\", systemImage: \"house\") {",
            "        HomeView()",
            "    }",
            "",
            "    Tab(\"Alerts\", systemImage: \"bell\") {",
            "        AlertsView()",
            "    }",
            "",
            "    TabSection(\"Categories\") {",
            "        Tab(\"Climate\", systemImage: \"fan\") {",
            "            ClimateView()",
            "        }",
            "",
            "        Tab(\"Lights\", systemImage: \"lightbulb\") {",
            "            LightsView()",
            "        }",
            "    }",
            "}",
            ".tabViewStyle(.sidebarAdaptable)",
            ".contentToolbar(for: .tabViewSidebar) {",
            "    DisconnectDevicesButton()",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
