        },
        {
          "inlineContent": [
            {
              "text": "You can use a drag gesture from a mouse, trackpad,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or screen touches with iOS and iPadOS devices to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": ".tilt",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": ".pan",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": ".orbit",
              "type": "codeVoice"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "code": ".dolly",
              "type": "codeVoice"
            },
            {
              "text": " a virtual",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "camera.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
