    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The header appears at the top of the sidebar before",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "any tab labels and can scroll with the content. The header is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "only visible when the ",
              "type": "text"
            },
            {
              "code": "TabView",
              "type": "codeVoice"
            },
            {
              "text": " is displaying the sidebar.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example adds a welcome message to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the top of the sidebar:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    Tab(\"Home\", systemImage: \"house\") {",
            "        HomeView()",
            "    }",
            "",
            "    Tab(\"Alerts\", systemImage: \"bell\") {",
            "        AlertsView()",
            "    }",
            "",
            "    Tab(\"Browse\", systemImage: \"list.bullet\") {",
            "        MyBrowseView()",
            "    }",
            "}",
            ".tabViewStyle(.sidebarAdaptable)",
            ".tabViewSidebarHeader {",
            "    WelcomeHeaderView()",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "To have a sidebar, a",
                  "type": "text"
                },
                {
                  "code": "TabView",
                  "type": "codeVoice"
                },
                {
                  "text": " needs the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "TabViewStyle/sidebarAdaptable",
                  "type": "codeVoice"
                },
                {
                  "text": " style.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
