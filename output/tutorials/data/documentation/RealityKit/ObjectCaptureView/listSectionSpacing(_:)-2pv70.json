    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount of spacing to apply.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "spacing"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example creates a ",
              "type": "text"
            },
            {
              "code": "List",
              "type": "codeVoice"
            },
            {
              "text": " with 5 pts of spacing between",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "sections:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "List {",
            "    Section(\"Colors\") {",
            "        Text(\"Blue\")",
            "        Text(\"Red\")",
            "    }",
            "",
            "    Section(\"Shapes\") {",
            "        Text(\"Square\")",
            "        Text(\"Circle\")",
            "    }",
            "}",
            ".listSectionSpacing(5.0)"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Spacing can also be specified on an individual ",
              "type": "text"
            },
            {
              "code": "Section",
              "type": "codeVoice"
            },
            {
              "text": ", as in this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Section(\"Borders\") {",
            "    Text(\"Dashed\")",
            "    Text(\"Solid\")",
            "}",
            ".listSectionSpacing(10.0)"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If adjacent sections have different spacing applied, each section",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "applies half its spacing above and below. Sections without",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "explicit spacing apply the spacing of their adjacent sections.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "List {",
            "    Section(\"Colors\") {",
            "        Text(\"Blue\")",
            "        Text(\"Red\")",
            "    }",
            "",
            "    Section(\"Borders\") {",
            "        Text(\"Dashed\")",
            "        Text(\"Solid\")",
            "    }",
            "    .listSectionSpacing(10.0)",
            "",
            "    Section(\"Shapes\") {",
            "        Text(\"Square\")",
            "        Text(\"Circle\")",
            "    }",
            "    .listSectionSpacing(100.0)",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In the above example, the “Colors” and “Borders” section are separated",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by 10 pts of spacing, and the “Borders” and “Shapes” section are",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "separated by 55 pts of spacing.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Spacing applied on sections in the ",
              "type": "text"
            },
            {
              "code": "List",
              "type": "codeVoice"
            },
            {
              "text": " overrides spacing applied on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the ",
              "type": "text"
            },
            {
              "code": "List",
              "type": "codeVoice"
            },
            {
              "text": " as a whole.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
