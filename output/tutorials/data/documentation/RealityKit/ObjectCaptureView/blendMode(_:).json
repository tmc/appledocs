      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that applies ",
              "type": "text"
            },
            {
              "code": "blendMode",
              "type": "codeVoice"
            },
            {
              "text": " to this view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "blendMode(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to combine overlapping views and use a different",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "visual effect to produce the result. The ",
              "type": "text"
            },
            {
              "code": "BlendMode",
              "type": "codeVoice"
            },
            {
              "text": " enumeration",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "defines many possible effects.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, the two overlapping rectangles have a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "BlendMode/colorBurn",
              "type": "codeVoice"
            },
            {
              "text": " effect applied, which effectively removes the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "non-overlapping portion of the second image:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "HStack {",
            "    Color.yellow.frame(width: 50, height: 50, alignment: .center)",
            "",
            "    Color.red.frame(width: 50, height: 50, alignment: .center)",
            "        .rotationEffect(.degrees(45))",
            "        .padding(-20)",
            "        .blendMode(.colorBurn)",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
