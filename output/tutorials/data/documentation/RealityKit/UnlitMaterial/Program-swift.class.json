    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this type to control when RealityKit compiles shaders,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and to initialize ",
              "type": "text"
            },
            {
              "code": "UnlitMaterial",
              "type": "codeVoice"
            },
            {
              "text": " objects with more predicitable loading performance.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "When initializing an ",
              "type": "text"
            },
            {
              "code": "UnlitMaterial",
              "type": "codeVoice"
            },
            {
              "text": " this way, a ",
              "type": "text"
            },
            {
              "code": "Program",
              "type": "codeVoice"
            },
            {
              "text": " object is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "created first asynchronously, which is used to cache the material’s shader program so",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the ",
              "type": "text"
            },
            {
              "code": "UnlitMaterial",
              "type": "codeVoice"
            },
            {
              "text": " can be loaded immediately later.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Initialize descriptor with desired properties",
            "var descriptor = UnlitMaterial.Descriptor()",
            "descriptor.applyPostProcessToneMap = false",
            "",
            "// Create program object",
            "let program = await UnlitMaterial.Program(descriptor: descriptor)",
            "",
            "// Create material (returns immediately)",
            "let material = UnlitMaterial(program: program)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
