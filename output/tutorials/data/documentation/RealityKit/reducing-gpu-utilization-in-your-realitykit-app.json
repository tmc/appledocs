{
  "abstract": [
    {
      "text": "Prevent the GPU from limiting your app’s frame rate by reducing the complexity of",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "your render.",
      "type": "text"
    }
  ],
