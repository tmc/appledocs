          "inlineContent": [
            {
              "text": "Properties that specify a semantic of ",
              "type": "text"
            },
            {
              "code": "hdrColor",
              "type": "codeVoice"
            },
            {
              "text": " use the texture to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "store high-dynamic range RGB color data. If the source image",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "contains color space information, RealityKit modifies the individual",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "pixels to fit the color space. If the source image is grayscale,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "RealityKit converts it to an RGB image first.",
              "type": "text"
            }
          ],
