{
  "abstract": [
    {
      "text": "Specifies a timeout value in seconds when querying nextDrawable().",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "nextDrawable() will be blocked for up to the specified timeout period for a drawable to become available else throws ",
      "type": "text"
    },
    {
      "code": "NextDrawableError.timeoutReached",
      "type": "codeVoice"
    },
    {
      "text": ".",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "By default this is set to 1 second. Note that if ",
      "type": "text"
    },
    {
      "code": "allowsNextDrawableTimeout",
      "type": "codeVoice"
    },
    {
      "text": " is false, then the timeout parameter will be ignored.",
      "type": "text"
    }
  ],
