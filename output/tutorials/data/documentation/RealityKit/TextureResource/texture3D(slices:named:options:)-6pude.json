    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The source images, one per depth index.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "All images need to be square, and of equal size and format.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "slices"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique name for syncing the texture resource across the network.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The name is empty if you donâ€™t include one.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resourceName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A configuration for generating the texture.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Creates a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Metal/MTLTextureType/type3D",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "texture with ",
              "type": "text"
            },
            {
              "code": "depth == slices.count",
              "type": "codeVoice"
            },
            {
              "text": " from an array of images.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can assign the resulting texture to a material you create in Reality Composer Pro that requires a 3D texture.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Create a 3D texture from image slices.",
            "let texture3D = try await TextureResource.texture3D(",
            "    slices: [image0, image1, image2, image3],",
            "    options: TextureResource.CreateOptions(semantic: .color))",
            "",
            "// Assign the 3D texture to a compatible shader graph material parameter.",
            "var material = try await ShaderGraphMaterial(",
            "    named: \"/Root/Alien/MaterialWith3DTexture\", from: url)",
            "",
            "try material.setParameter(",
            "name: \"input3DTexture\",",
            "value: .textureResource(texture3D))"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
