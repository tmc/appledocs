    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "RealityKit extracts the cube faces from the source 2D image using a convention based on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "its aspect ratio, which needs to be one of the following:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "height == 6 * width",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", RealityKit treats the source image as a vertical strip of the cube faces, containing [+X, -X, +Y, -Y, +Z, -Z]  from top to bottom:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "identifier": "textureresource-cube-vstrip-orientation",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "6 * height == width",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", RealityKit treats the source image as a horizontal strip of the cube faces, containing [+X, -X, +Y, -Y, +Z, -Z] from left to right:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "identifier": "textureresource-cube-hstrip-orientation",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "height / 3 == width / 4",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", RealityKit treats the source image as a cross layout of the cube faces as follows:",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "code": [
            "    [+Y]",
            "[+X][-Z][-X][+Z]",
            "    [-Y]"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "textureresource-cube-cross-orientation",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use the resulting texture to create an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/EnvironmentResource",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or assign it to a material in Reality Composer Pro that requires",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a cube texture type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "if let source = CGImageSourceCreateWithURL(url as CFURL, nil),",
            "    let image = CGImageSourceCreateImageAtIndex(source, 0, nil) {",
            "",
            "    // Create a cube texture from the image.",
            "    let cube = try await TextureResource(",
            "        cubeFromImage: image, options: TextureResource.CreateOptions(semantic: .hdrColor))",
            "",
            "    // Create an environment resource from the cube texture.",
            "    let environment = try await EnvironmentResource(",
            "        cube: cube, options: EnvironmentResource.CreateOptions())",
            "",
            "    await MainActor.run {",
            "        // Assign the environment to an image-based light component.",
            "        let lightEntity = Entity()",
            "        lightEntity.components.set(",
            "            ImageBasedLightComponent(source: .single(environment)))",
            "        ...",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
