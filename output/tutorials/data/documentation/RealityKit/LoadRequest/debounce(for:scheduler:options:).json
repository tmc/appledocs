    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time the publisher should wait before publishing an element.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
