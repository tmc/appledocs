    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style to display as the background of the bar.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "style"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bars to use the style for or",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "ToolbarPlacement/automatic",
                  "type": "codeVoice"
                },
                {
                  "text": " if empty.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bars"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The preferred style flows up to the nearest container that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "renders a bar. This could be a ",
              "type": "text"
            },
            {
              "code": "NavigationView",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "TabView",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in iOS, or the root view of a ",
              "type": "text"
            },
            {
              "code": "WindowGroup",
              "type": "codeVoice"
            },
            {
              "text": " in macOS. This example",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "shows a view that renders the navigation bar with a blue background",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and dark color scheme.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NavigationView {",
            "    ContentView()",
            "        .toolbarBackground(.white)",
            "        .toolbarColorScheme(.dark)",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can provide multiple ",
              "type": "text"
            },
            {
              "code": "ToolbarPlacement",
              "type": "codeVoice"
            },
            {
              "text": " instances to customize",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "multiple bars at once.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    NavigationView {",
            "        ContentView()",
            "            .toolbarBackground(",
            "                .blue, for: .navigationBar, .tabBar)",
            "            .toolbarColorScheme(",
            "                .dark, for: .navigationBar, .tabBar)",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When used within a ",
              "type": "text"
            },
            {
              "code": "TabView",
              "type": "codeVoice"
            },
            {
              "text": ", the specified style will be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "preferred while the tab is currently active. You can use a ",
              "type": "text"
            },
            {
              "code": "Group",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to specify the same preferred background for every tab.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    Group {",
            "        MainView()",
            "        SettingsView()",
            "    }",
            "    .toolbarBackground(.blue, for: .tabBar)",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Depending on the specified bars, the requested style may not be able to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "be fullfilled.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
