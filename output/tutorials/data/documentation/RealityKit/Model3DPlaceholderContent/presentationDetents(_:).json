      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A set of supported detents for the sheet.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If you provide more that one detent, people can drag the sheet",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to resize it.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "detents"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "By default, sheets support the ",
              "type": "text"
            },
            {
              "code": "PresentationDetent/large",
              "type": "codeVoice"
            },
            {
              "text": " detent.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    @State private var showSettings = false",
            "",
            "    var body: some View {",
            "        Button(\"View Settings\") {",
            "            showSettings = true",
            "        }",
            "        .sheet(isPresented: $showSettings) {",
            "            SettingsView()",
            "                .presentationDetents([.medium, .large])",
            "        }",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        }
      ],
