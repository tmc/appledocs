    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The primary color or pattern to use when filling in",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the foreground elements. To indicate a specific value, use ",
                  "type": "text"
                },
                {
                  "code": "Color",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "or ",
                  "type": "text"
                },
                {
                  "code": "ShapeStyle/image(_:sourceRect:scale:)",
                  "type": "codeVoice"
                },
                {
                  "text": ", or one of the gradient",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "types, like",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "ShapeStyle/linearGradient(colors:startPoint:endPoint:)",
                  "type": "codeVoice"
                },
                {
                  "text": ". To set a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "style that’s relative to the containing view’s style, use one of the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "semantic styles, like ",
                  "type": "text"
                },
                {
                  "code": "ShapeStyle/primary",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "primary"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The secondary color or pattern to use when",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "filling in the foreground elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "secondary"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that uses the given foreground styles.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "SwiftUI uses these styles when rendering child views",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that don’t have an explicit rendering style, like images,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "text, shapes, and so on.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Symbol images within the view hierarchy use the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "SymbolRenderingMode/palette",
              "type": "codeVoice"
            },
            {
              "text": " rendering mode when you apply this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier, if you don’t explicitly specify another mode.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
