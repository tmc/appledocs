    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "ShapeStyle",
                  "type": "codeVoice"
                },
                {
                  "text": " that SwiftUI uses to the fill the shape",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that you specify.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "style"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An instance of a type that conforms to ",
                  "type": "text"
                },
                {
                  "code": "InsettableShape",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that SwiftUI draws behind the view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shape"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "FillStyle",
                  "type": "codeVoice"
                },
                {
                  "text": " to use when drawing the shape.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The default style uses the nonzero winding number rule and",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "antialiasing.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fillStyle"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view with the specified insettable shape drawn behind it.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to layer a type that conforms to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "InsettableShape",
              "type": "codeVoice"
            },
            {
              "text": " protocol — like a ",
              "type": "text"
            },
            {
              "code": "Rectangle",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "Circle",
              "type": "codeVoice"
            },
            {
              "text": ", or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Capsule",
              "type": "codeVoice"
            },
            {
              "text": " — behind a view. Specify the ",
              "type": "text"
            },
            {
              "code": "ShapeStyle",
              "type": "codeVoice"
            },
            {
              "text": " that’s used to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "fill the shape. For example, you can place a ",
              "type": "text"
            },
            {
              "code": "RoundedRectangle",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "behind a ",
              "type": "text"
            },
            {
              "code": "Label",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Label(\"Flag\", systemImage: \"flag.fill\")",
            "    .padding()",
            "    .background(.teal, in: RoundedRectangle(cornerRadius: 8))"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "ShapeStyle/teal",
              "type": "codeVoice"
            },
            {
              "text": " color fills the shape:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier is a  convenience method for placing a single shape behind a view. To",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "create a background with other ",
              "type": "text"
            },
            {
              "code": "View",
              "type": "codeVoice"
            },
            {
              "text": " types — or with a stack",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of views — use ",
              "type": "text"
            },
            {
              "code": "View/background(alignment:content:)",
              "type": "codeVoice"
            },
            {
              "text": " instead.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "To add a ",
              "type": "text"
            },
            {
              "code": "ShapeStyle",
              "type": "codeVoice"
            },
            {
              "text": " as a background, use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "View/background(_:ignoresSafeAreaEdges:)",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
