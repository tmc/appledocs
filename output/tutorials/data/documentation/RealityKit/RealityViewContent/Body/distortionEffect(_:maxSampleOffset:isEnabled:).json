    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The shader to apply as a distortion effect.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shader"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The maximum distance in each axis between",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the returned source pixel position and the destination pixel",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "position, for all source pixels.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxSampleOffset"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether the effect is enabled or not.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isEnabled"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A new view that renders ",
              "type": "text"
            },
            {
              "code": "self",
              "type": "codeVoice"
            },
            {
              "text": " with the shader",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "applied as a distortion effect.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For a shader function to act as a distortion effect it must",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "have a function signature matching:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "[[ stitchable ]] float2 name(float2 position, args...)"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "where ",
              "type": "text"
            },
            {
              "code": "position",
              "type": "codeVoice"
            },
            {
              "text": " is the user-space coordinates of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "destination pixel applied to the shader. ",
              "type": "text"
            },
            {
              "code": "args...",
              "type": "codeVoice"
            },
            {
              "text": " should be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "compatible with the uniform arguments bound to ",
              "type": "text"
            },
            {
              "code": "shader",
              "type": "codeVoice"
            },
            {
              "text": ". The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "function should return the user-space coordinates of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "corresponding source pixel.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Views backed by AppKit or UIKit views may not",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "render into the filtered layer. Instead, they log a warning",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "and display a placeholder image to highlight the error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
