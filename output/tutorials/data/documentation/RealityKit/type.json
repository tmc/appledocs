          "anchor": "Order-Options",
          "level": 3,
          "text": "Order Options",
          "type": "heading"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Executes in order with each action waiting for the prior action to complete before starting.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "serial",
                    "type": "codeVoice"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "Executes all actions concurrently.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "parallel",
                    "type": "codeVoice"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        },
        {
          "anchor": "Declaration",
          "level": 3,
          "text": "Declaration",
          "type": "heading"
        },
        {
          "code": [
            "uniform token type = \"serial\" (",
            "        allowedTokens = [\"serial\", \"parallel\"]",
            ")"
          ],
          "syntax": "other",
          "type": "codeListing"
