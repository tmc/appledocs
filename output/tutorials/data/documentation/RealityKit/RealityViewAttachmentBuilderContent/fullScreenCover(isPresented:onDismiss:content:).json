    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to show a modal view that covers as much of the screen",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as possible. The example below displays a custom view when the user",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "toggles the value of the ",
              "type": "text"
            },
            {
              "code": "isPresenting",
              "type": "codeVoice"
            },
            {
              "text": " binding:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct FullScreenCoverPresentedOnDismiss: View {",
            "    @State private var isPresenting = false",
            "    var body: some View {",
            "        Button(\"Present Full-Screen Cover\") {",
            "            isPresenting.toggle()",
            "        }",
            "        .fullScreenCover(isPresented: $isPresenting,",
            "                         onDismiss: didDismiss) {",
            "            VStack {",
            "                Text(\"A full-screen modal view.\")",
            "                    .font(.title)",
            "                Text(\"Tap to Dismiss\")",
            "            }",
            "            .onTapGesture {",
            "                isPresenting.toggle()",
            "            }",
            "            .foregroundColor(.white)",
            "            .frame(maxWidth: .infinity,",
            "                   maxHeight: .infinity)",
            "            .background(Color.blue)",
            "            .ignoresSafeArea(edges: .all)",
            "        }",
            "    }",
            "",
            "    func didDismiss() {",
            "        // Handle the dismissing action.",
            "    }",
            "}"
          ],
          "syntax": null,
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
