      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "position",
              "type": "codeVoice"
            },
            {
              "text": " defines the center of mass with a default value of ",
              "type": "text"
            },
            {
              "code": "(0, 0, 0)",
              "type": "codeVoice"
            },
            {
              "text": ", which means that the local origin of the model is the center of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "mass.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "orientation",
              "type": "codeVoice"
            },
            {
              "text": " defines the principal axes, such the inertia matrix is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a diagonal.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
