            },
            {
              "kind": "text",
              "text": "<"
            },
            {
              "identifier": "doc://com.externally.resolved.symbol/s:Sf",
              "kind": "typeIdentifier",
              "preciseIdentifier": "s:Sf",
              "text": "Float"
            },
            {
              "kind": "text",
              "text": ">, orientation"
            },
            {
              "kind": "text",
              "text": ": "
            },
            {
              "identifier": "doc://com.externally.resolved.symbol/c:@SA@simd_quatf",
              "kind": "typeIdentifier",
              "preciseIdentifier": "c:@SA@simd_quatf",
              "text": "simd_quatf"
            },
            {
              "kind": "text",
              "text": ") = (SIMD3<Float>(x: 0, y: 0, z: 0), simd_quatf(ix: 0, iy: 0, iz: 0, r: 1)))"
            }
          ]
        }
      ],
      "kind": "declarations"
    },
    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The mass in kilograms. If you use a mass of ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " or infinity, the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "simulation treats the object as ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/PhysicsBodyMode/kinematic",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". That is,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the object doesnâ€™t respond to forces.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mass"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The inertia in kilograms per square meter. The vector",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "contains the diagonal elements of the diagonalized inertia matrix.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inertia"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The center of mass and the orientation of the principal",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "axes, defined in the local space of the rigid body.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "position",
                  "type": "codeVoice"
                },
                {
                  "text": " defines the center of mass with a default value of ",
                  "type": "text"
                },
                {
                  "code": "(0, 0, 0)",
                  "type": "codeVoice"
                },
                {
                  "text": ", which",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "means that the local origin of the model is the center of mass.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "orientation",
                  "type": "codeVoice"
                },
                {
                  "text": " defines the principal axes, such the inertia matrix is a diagonal.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "centerOfMass"
        }
      ]
    }
