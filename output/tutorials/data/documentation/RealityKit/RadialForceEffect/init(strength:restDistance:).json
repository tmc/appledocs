      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The magnitude of the spring effectâ€”a larger strength simulates a stiffer spring.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "strength"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Objects at this distance will receive zero radial force.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Otherwise they will gravitate toward this distance along the radial direction.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Objects will be attracted to the origin directly if this property is set to zero.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "restDistance"
        }
      ]
