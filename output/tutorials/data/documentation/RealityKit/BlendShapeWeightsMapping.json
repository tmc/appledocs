    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "BlendShapeWeightsMapping",
              "type": "codeVoice"
            },
            {
              "text": " to initialize a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/BlendShapeWeightsComponent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". You can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "initialize it in two different ways:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Initialize the mapping from a user-provided ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/MeshResource",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", from which the mapping information can",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "be generated.  In this case, the number of blend shapes and each blend shape’s weights map exactly",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "to the ",
                      "type": "text"
                    },
                    {
                      "code": "MeshResource",
                      "type": "codeVoice"
                    },
                    {
                      "text": "’s structure.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "code": [
            "if let modelComponent = blendShapeEntity.components[ModelComponent.self] {",
            "    let meshResource = modelComponent.mesh",
            "    let blendShapeWeightsMapping = BlendShapeWeightsMapping(",
            "        meshResource: meshResource)",
            "    blendShapeEntity.components.set(BlendShapeWeightsComponent(",
            "        weightsMapping: blendShapeWeightsMapping))",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Initialize the mapping from a user-defined blend shape name and list of weight names.  When used to initialize",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "a ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/BlendShapeWeightsComponent",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", the weight names define which mesh targets map from the",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "owning entity’s ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/ModelComponent",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": "’s mesh resource.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "inlineContent": [
                    {
                      "text": "RealityKit expects the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/ModelComponent",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " to already be assigned to the entity on which the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/BlendShapeWeightsComponent",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "resides.  Match the weight names to the weight names found in the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/ModelComponent",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": "’s mesh resource.",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "If a weight name does not match any of the mesh weight names then it is ignored and that weight will have no effect.",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "If a weight name in the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/ModelComponent",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": "’s mesh resource is excluded from the list of weight names, then the",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "associated target mesh will not be controllable by any of the provided weights.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "inlineContent": [
                    {
                      "text": "Note also that the provided blend shape name can be used to reference the blend shape weights in the",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.RealityKit/documentation/RealityKit/BlendShapeWeightsSet",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "code": [
            "let weightNames: [String] = [",
            "    \"weight0\",",
            "    \"weight1\"",
            "]",
            "",
            "let blendShapeWeightsMapping = BlendShapeWeightsMapping(",
            "    blendShapeName: \"BlendShape0\",",
            "    weightNames: weightNames)",
            "",
            "blendShapeEntity.components.set(BlendShapeWeightsComponent(",
            "    weightsMapping: blendShapeWeightsMapping))"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
