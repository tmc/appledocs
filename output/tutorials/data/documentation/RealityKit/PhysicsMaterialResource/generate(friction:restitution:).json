          "content": [
            {
              "inlineContent": [
                {
                  "text": "The coefficient of friction, in the range ",
                  "type": "text"
                },
                {
                  "code": "[0, infinity)",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "friction"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The coefficient of restitution, in the range ",
                  "type": "text"
                },
                {
                  "code": "[0, 1]",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Use values at the high end of the range to indicate materials that",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "experience elastic collisions, meaning that objects bounce off each",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "other and kinetic energy is conserved after a collision. Use low values",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to indicate materials that lose kinetic energy when they collide.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "restitution"
        }
      ]
    },
