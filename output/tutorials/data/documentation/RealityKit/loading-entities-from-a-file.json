{
  "abstract": [
    {
      "text": "Retrieve an entity from storage on disk using a synchronous or an asynchronous load",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "operation.",
      "type": "text"
    }
  ],
