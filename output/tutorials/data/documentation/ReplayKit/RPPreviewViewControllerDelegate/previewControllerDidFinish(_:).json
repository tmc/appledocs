        {
          "inlineContent": [
            {
              "text": "When the user is finished making changes to a screen recording, your app is responsible for dismissing the view controller representing the user interface. Call ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/UIKit/UIViewController/dismiss(animated:completion:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " after getting the message to dismiss the preview view controller.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
