    "doc://com.apple.documentation/documentation/UIKit/UIApplicationDelegate/application(_:shouldAllowExtensionPointIdentifier:)": {
      "abstract": [
        {
          "text": "Asks the delegate to grant permission to use app extensions that are based on a specified extension point identifier.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "attribute",
          "text": "@"
        },
        {
