        {
          "inlineContent": [
            {
              "text": "You can get the bundle and team identifiers for an app using the ",
              "type": "text"
            },
            {
              "code": "codesign",
              "type": "codeVoice"
            },
            {
              "text": " command line utility:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
