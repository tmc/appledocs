      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A memory descriptor that contains the report from the HID device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "report"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The report type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reportType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The lower 8 bits of the report ID. The other 24 bits are options to specify the request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount of time, in milliseconds, after which to abort the command if the entire command hasnâ€™t completed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "completionTimeout"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The function to call when the request completes. If you omit this function, the method executes synchronously, blocking the current thread until the request completes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        }
      ]
    },
