    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The timestamp of the event. Use the timestamp of the report element that is the source of the event.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timeStamp"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The distance traveled along the X axis, relative to the pointer’s previous location.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dx"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The distance traveled along the Y axis, relative to the pointer’s previous location.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dy"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The button state, if any.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "buttonState"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Additional options for pointer events. Specify ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " for no options. For a list of other values, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.hiddriverkit/documentation/HIDDriverKit/IOHIDPointerEventOptions",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value indicating whether to apply the acceleration algorithm to the pointer event. Specify ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " if you don’t want to apply that logic.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "accelerate"
        }
      ]
