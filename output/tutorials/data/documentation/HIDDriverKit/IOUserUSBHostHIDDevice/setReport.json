      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A memory descriptor that describes the report to send to the HID device.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "report"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The report type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reportType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The lower 8 bits of the report ID. The other 24 bits are options to specify the request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount of time, in milliseconds, after which to abort the command if the entire command hasnâ€™t finished.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "completionTimeout"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/driverkit/osaction",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object to execute when the request completes. Specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " to execute the request synchronously, which blocks the current thread until the request completes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        }
      ]
