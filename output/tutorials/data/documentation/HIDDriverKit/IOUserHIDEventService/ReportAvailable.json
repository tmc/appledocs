      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The timestamp of the report.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timestamp"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The unique ID associated with the report.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reportID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length of the report in bytes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "reportLength"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The report type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A memory descriptor that contains the raw data for the report.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "report"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "OSAction",
                  "type": "codeVoice"
                },
                {
                  "text": " object that handles the asynchronous report callback.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Implement a custom version of this method in your event service. Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/driverkit/type",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " macro to let the system know that your method conforms to this prototype.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The system calls this method to notify your event service when a new report arrives.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
