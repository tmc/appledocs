        },
        {
          "inlineContent": [
            {
              "text": "The default behavior for keyboard events is to repeat keys if the user holds down the key for the amount of time defined in system preferences. Use this option to disable this behavior for the event.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
