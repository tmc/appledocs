    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The metadata object to search.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "metadata"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The parent tag, if any. Specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " to start the search in the top-level tags of the metadata object. If this parameter is ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", you must include a valid prefix string in the ",
                  "type": "text"
                },
                {
                  "code": "path",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "parent"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that represents the path to the tag. A path consists of the tag’s name, plus optional prefix and parent information. Separate prefix information from other path information using a colon (",
                  "type": "text"
                },
                {
                  "code": ":",
                  "type": "codeVoice"
                },
                {
                  "text": ") character. Separate parent and child tags using the period (",
                  "type": "text"
                },
                {
                  "code": ".",
                  "type": "codeVoice"
                },
                {
                  "text": ") character. For example, the string ",
                  "type": "text"
                },
                {
                  "code": "“exif:Flash.RedEyeMode”",
                  "type": "codeVoice"
                },
                {
                  "text": " represents the path to the ",
                  "type": "text"
                },
                {
                  "code": "RedEyeMode",
                  "type": "codeVoice"
                },
                {
                  "text": " field of the ",
                  "type": "text"
                },
                {
                  "code": "Flash",
                  "type": "codeVoice"
                },
                {
                  "text": " parent structure in the EXIF metadata.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "When a tag contains an ordered or unordered array, specify elements using a ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": "-based index inside square brackets. For example, use the string ",
                  "type": "text"
                },
                {
                  "code": "“dc.subject[2]”",
                  "type": "codeVoice"
                },
                {
                  "text": " to access the third element in the ",
                  "type": "text"
                },
                {
                  "code": "subject",
                  "type": "codeVoice"
                },
                {
                  "text": " array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "When the tag contains an alternate-text array, access elements using an RFC 3066 language code inside square brackets. For example, use the string ",
                  "type": "text"
                },
                {
                  "code": "“dc.description[de]”",
                  "type": "codeVoice"
                },
                {
                  "text": " to access the German description information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "Use the ? character to delimit qualifiers for tags with string values. You may not use this character for arrays and structures.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "path"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The string value for the specified tag, or ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": " if the tag wasn’t found or doesn’t contain a string value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The XMP type of the property at the specified path must be ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.imageio/documentation/ImageIO/CGImageMetadataType/string",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.imageio/documentation/ImageIO/CGImageMetadataType/alternateText",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". If the property contains alternate text, this function returns the element with the ",
              "type": "text"
            },
            {
              "code": "x-default",
              "type": "codeVoice"
            },
            {
              "text": " language qualifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
