  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "With the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.iousbhost/documentation/IOUSBHost",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " framework, you can access custom and nonâ€“class-compliant USB devices from within your apps. Use this framework to connect to cameras, audio devices, scanners, printers, keyboards, mouse devices, MIDI keyboards, and USB hubs.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This framework refers to the USB Implementers Forum (USB-IF) ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "Universal Serial Bus 3.2 Specification",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": ", Revision 1.0, September 22, 2017. You can view this specification at ",
              "type": "text"
            },
            {
              "identifier": "http://www.usb.org/",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
