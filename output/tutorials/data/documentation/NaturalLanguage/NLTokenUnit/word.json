      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this linguistic unit to tokenize text into individual words, like in the following example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let text = \"This is a sentence containing several words. ðŸ˜€\"",
            "",
            "let tokenizer = NLTokenizer(unit: .word)",
            "tokenizer.string = text",
            "",
            "let range = text.startIndex..<text.endIndex",
            "",
            "let tokenArray = tokenizer.tokens(for: range)",
            "print(\"Number of tokens: \\(tokenArray.count)\")",
            "",
            "tokenizer.enumerateTokens(in: range) { tokenRange, _ in",
            "    print(text[tokenRange])",
            "    return true",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "For more information, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.naturallanguage/documentation/NaturalLanguage/tokenizing-natural-language-text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
