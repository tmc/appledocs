    "doc://com.apple.documentation/documentation/CreateML/MLDataTable/randomSplit(by:seed:)": {
      "abstract": [
        {
          "text": "Creates two mutually exclusive, randomly divided subsets of the table.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "keyword",
          "text": "func"
        },
        {
          "kind": "text",
          "text": " "
        },
        {
          "kind": "identifier",
          "text": "randomSplit"
        },
        {
          "kind": "text",
          "text": "("
        },
        {
          "kind": "externalParam",
          "text": "by"
        },
        {
          "kind": "text",
          "text": " "
        },
        {
          "kind": "internalParam",
          "text": "proportion"
