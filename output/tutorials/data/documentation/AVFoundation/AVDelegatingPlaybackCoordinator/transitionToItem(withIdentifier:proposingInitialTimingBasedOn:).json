          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier for the new current item, which is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " if there isnâ€™t anything to play.",
                  "type": "text"
                }
              ],
