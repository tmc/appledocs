    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time to which you would like to move the playback cursor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "time"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tolerance allowed before ",
                  "type": "text"
                },
                {
                  "code": "time",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "toleranceBefore"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tolerance allowed after ",
                  "type": "text"
                },
                {
                  "code": "time",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "toleranceAfter"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The time seeked to will be within the range ",
              "type": "text"
            },
            {
              "code": "[time-beforeTolerance, time+afterTolerance]",
              "type": "codeVoice"
            },
            {
              "text": ", and may differ from the specified time for efficiency. If you pass ",
              "type": "text"
            },
            {
              "code": "kCMTimeZero",
              "type": "codeVoice"
            },
            {
              "text": " for both ",
              "type": "text"
            },
            {
              "code": "toleranceBefore",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "toleranceAfter",
              "type": "codeVoice"
            },
            {
              "text": " (to request sample accurate seeking), you may incur additional decoding delay that impacts seeking performance.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Passing ",
              "type": "text"
            },
            {
              "code": "kCMTimePositiveInfinity",
              "type": "codeVoice"
            },
            {
              "text": " for both ",
              "type": "text"
            },
            {
              "code": "toleranceBefore",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "toleranceAfter",
              "type": "codeVoice"
            },
            {
              "text": " is the same as messaging ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.avfoundation/documentation/AVFoundation/AVPlayerItem/seek(to:)-1dpto",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " directly.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
