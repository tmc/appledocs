      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The opacity to be applied at the start time of ",
                  "type": "text"
                },
                {
                  "code": "timeRange",
                  "type": "codeVoice"
                },
                {
                  "text": ". The value must be between ",
                  "type": "text"
                },
                {
                  "code": "0.0",
                  "type": "codeVoice"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "code": "1.0",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "startOpacity"
        },
