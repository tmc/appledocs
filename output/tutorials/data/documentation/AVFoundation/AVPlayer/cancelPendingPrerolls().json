          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method cancels and releases the completion handlers for any pending prerolls. The finished parameter of the completion handlers passed to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.avfoundation/documentation/AVFoundation/AVPlayer/preroll(atRate:completionHandler:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " will be set to ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
