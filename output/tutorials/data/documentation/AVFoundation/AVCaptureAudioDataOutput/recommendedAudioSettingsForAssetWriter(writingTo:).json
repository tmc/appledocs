          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies the UTI of the file type to be written. See ",
                  "type": "text"
                },
                {
                  "code": "File Format UTIs",
                  "type": "codeVoice"
                },
                {
                  "text": " for the defined UTIs.",
                  "type": "text"
                }
