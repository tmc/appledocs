        {
          "inlineContent": [
            {
              "text": "Data appropriate for writing to a file of the type specified when requesting photo capture, or ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " if the photo and attachment data cannot be flattened.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
