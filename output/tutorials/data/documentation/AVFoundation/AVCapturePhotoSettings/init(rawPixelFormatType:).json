      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The Bayer RAW pixel format type to use for capture. This value must be one of the format identifiers listed in the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.avfoundation/documentation/AVFoundation/AVCapturePhotoOutput/availableRawPhotoPixelFormatTypes-5fatm",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " array of your photo capture output.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rawPixelFormatType"
        }
      ]
    },
    {
