{
  "abstract": [
    {
      "text": "Display images for specific times within the media timeline by generating images from a video’s frames.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
