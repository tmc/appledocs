              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "You can cancel an in-progress export by calling ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/Swift/Task/cancel()",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " on the ",
