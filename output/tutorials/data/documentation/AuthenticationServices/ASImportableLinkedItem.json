      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A linked item serves as a reference to an item, and contains no data of its own.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This type is a representation of ",
              "type": "text"
            },
            {
              "code": "LinkedItem",
              "type": "codeVoice"
            },
            {
              "text": " as defined in the Credential Exchange Format (CXF) specification.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "You can supply a JSON representation of a CXF ",
              "type": "text"
            },
            {
              "code": "Item",
              "type": "codeVoice"
            },
            {
              "text": " to initialize an instance of this struct by using a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/jsondecoder",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and calling ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/jsondecoder/2895189-decode",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
