{
  "abstract": [
    {
      "text": "Use time-based codes generated on-device for a secure authentication experience.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
