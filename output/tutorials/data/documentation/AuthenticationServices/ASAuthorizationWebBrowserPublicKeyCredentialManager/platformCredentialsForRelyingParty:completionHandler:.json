            {
              "inlineContent": [
                {
                  "text": "The name of the relying party, which you receive from the web server that issues the authentication challenge.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "relyingParty"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A block that the operating system calls to provide a list of passkeys available for authenticating with the given relying party.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "completionHandler"
        }
      ]
    },
