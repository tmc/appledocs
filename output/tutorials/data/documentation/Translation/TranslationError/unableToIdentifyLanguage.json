        {
          "inlineContent": [
            {
              "text": "This error is thrown when you call",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Translation/documentation/Translation/TranslationSession/prepareTranslation()",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " without specifying the source language,",
