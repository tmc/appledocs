{
  "abstract": [
    {
      "text": "Make your iOS app launch experience faster and more responsive by customizing a",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "launch screen.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Xcode/documentation/Xcode",
        "doc://com.apple.Xcode/documentation/Xcode/asset-management"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "swift",
    "url": "doc://com.apple.Xcode/documentation/Xcode/specifying-your-apps-launch-screen"
  },
  "kind": "article",
  "legalNotices": {
