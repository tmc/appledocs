{
  "abstract": [
    {
      "text": "Discover compatible accessories and communicate with configured accessories and",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "services to perform actions.",
      "type": "text"
    }
  ],
