{
  "abstract": [
    {
      "text": "Extend your Xcode Cloud workflows with custom build scripts that perform custom tasks or install additional tools.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
