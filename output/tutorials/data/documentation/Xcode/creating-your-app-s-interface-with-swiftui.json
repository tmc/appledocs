{
  "abstract": [
    {
      "text": "Develop apps in SwiftUI with an interactive preview that keeps the code and layout",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "in sync.",
      "type": "text"
    }
  ],
