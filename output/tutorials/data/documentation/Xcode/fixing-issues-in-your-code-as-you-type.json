{
  "abstract": [
    {
      "text": "Minimize typing-related errors using code completion, and let Xcode fix common",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "mistakes for you.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Xcode/documentation/Xcode",
