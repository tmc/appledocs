{
  "abstract": [
    {
      "text": "Search your code for text, patterns, and symbols that you can refactor quickly and",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "easily.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Xcode/documentation/Xcode",
        "doc://com.apple.Xcode/documentation/Xcode/source-editor"
      ]
    ]
  },
