{
  "abstract": [
    {
      "text": "Configure Xcode Cloud to start a build when you update a branch, pull request, or Git tag, or based on a schedule.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
