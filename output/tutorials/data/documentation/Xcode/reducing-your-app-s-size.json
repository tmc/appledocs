{
  "abstract": [
    {
      "text": "Measure your app’s size, optimize its assets and settings, and adopt technologies",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "that help streamline installation over a mobile internet connection.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Xcode/documentation/Xcode",
        "doc://com.apple.Xcode/documentation/Xcode/performance-and-metrics"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "swift",
    "url": "doc://com.apple.Xcode/documentation/Xcode/reducing-your-app-s-size"
  },
