{
  "abstract": [
    {
      "text": "The operating system terminated the process,",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "often because a background task violated a requirement,",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "device resources were limited,",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "or the user force quit the app.",
      "type": "text"
    }
  ],
