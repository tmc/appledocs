  "metadata": {
    "role": "article",
    "roleHeading": "Article",
    "title": "EXC_CRASH (SIGKILL)"
  },
  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The crash report contains a ",
              "type": "text"
            },
            {
              "code": "Termination Reason",
              "type": "codeVoice"
            },
            {
              "text": " field with a code that explains the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "reason for the crash. In the following example, that code is ",
              "type": "text"
            },
            {
              "code": "0xdead10cc",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Exception Type:  EXC_CRASH (SIGKILL)",
            "Exception Codes: 0x0000000000000000, 0x0000000000000000",
            "Exception Note:  EXC_CORPSE_NOTIFY",
            "Termination Reason: Namespace RUNNINGBOARD, Code 0xdead10cc"
          ],
          "syntax": "other",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "Termination Reason",
              "type": "codeVoice"
            },
            {
              "text": " code is one of the following values:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system terminated the app due to a background task failing to complete in time.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0x2182bad2",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "562215634",
                    "type": "codeVoice"
                  },
                  {
                    "text": ")",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system terminated the app due to a background URL session failing to complete in time.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0x2182bad3",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "562215635",
                    "type": "codeVoice"
                  },
                  {
                    "text": ")",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system terminated the app due to a background fetch failing to complete in time.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0x2182bad4",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "562215636",
                    "type": "codeVoice"
                  },
                  {
                    "text": ")",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system’s watchdog terminated the app. See ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.Xcode/documentation/Xcode/addressing-watchdog-terminations",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0x8badf00d",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "2343432205",
                    "type": "codeVoice"
                  },
                  {
                    "text": ") — pronounced “ate bad food”",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system terminated the app due to a thermal event. This can be an issue with the particular device that this crash occurred on, or the environment it operated in. For tips on making your app run more efficiently, watch the ",
                        "type": "text"
                      },
                      {
                        "identifier": "https://developer.apple.com/videos/play/wwdc2011/312/",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " WWDC session. To test your app’s response to thermal events, enable a thermal device condition as described in ",
                        "type": "text"
                      },
                      {
                        "identifier": "https://help.apple.com/xcode/mac/current/#/dev308429d42",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": ".",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0xc00010ff",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "3221229823",
                    "type": "codeVoice"
                  },
                  {
                    "text": ") — pronounced “cool off”",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system terminated the app because it held on to a file lock or SQLite database lock during suspension. Request additional background execution time on the main thread with ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/UIKit/UIApplication/beginBackgroundTask(withName:expirationHandler:)",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": ". Make this request well before starting to write to the file in order to complete those operations and relinquish the lock before the app suspends. In an app extension, use ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/foundation/processinfo/1415995-beginactivity",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " to manage this work.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0xdead10cc",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "3735883980",
                    "type": "codeVoice"
                  },
                  {
                    "text": ") — pronounced “dead lock”",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system terminated the app for failing to report a ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/CallKit",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " call in response to a ",
                        "type": "text"
                      },
                      {
                        "identifier": "doc://com.apple.documentation/documentation/PushKit",
                        "isActive": true,
                        "type": "reference"
                      },
                      {
                        "text": " notification.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0xbaadca11",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "3131951633",
                    "type": "codeVoice"
                  },
                  {
                    "text": ") — pronounced “bad call”",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system terminated a VoIP application because it resumed too frequently.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0xbad22222",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "3134333474",
                    "type": "codeVoice"
                  },
                  {
                    "text": ")",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system terminated the app to delete caches, in an attempt to reclaim disk space. Many factors contribute to low disk space. You may be able to help by minimizing what you write to disk and managing the entire life cycle of your files.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0xbaddd15c",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "3135099228",
                    "type": "codeVoice"
                  },
                  {
                    "text": ") — pronounced “bad disc”",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "watchOS terminated the app because it used too much CPU time while performing a background task. Optimize the code performing the background task to be more CPU efficient, or decrease the amount of work that the app performs while running in the background to resolve this crash.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0xc51bad01",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "3306925313",
                    "type": "codeVoice"
                  },
                  {
                    "text": ")",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "watchOS terminated the app because it failed to complete a background task within the allocated time. Decrease the amount of work that the app performs while running in the background to resolve this crash.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0xc51bad02",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "3306925314",
                    "type": "codeVoice"
                  },
                  {
                    "text": ")",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "watchOS terminated the app because it failed to complete a background task within the allocated time, but the system was sufficiently busy overall that the app might not have received much CPU time to perform the background task. Although you might be able to avoid the issue by reducing the amount of work your app performs in background tasks, ",
                        "type": "text"
                      },
                      {
                        "code": "0xc51bad03",
                        "type": "codeVoice"
                      },
                      {
                        "text": " doesn’t indicate that the app did anything wrong. More likely, the app wasn’t able to complete its work because of overall system load.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0xc51bad03",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "3306925315",
                    "type": "codeVoice"
                  },
                  {
                    "text": ")",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "The operating system terminated the app due to excessive use of key system resources. You might be able to avoid the issue by reducing parallelization and ensuring that your app completes operations for preceding tasks before starting new tasks.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "code": "0xd00d2bad",
                    "type": "codeVoice"
                  },
                  {
                    "text": " (",
                    "type": "text"
                  },
                  {
                    "code": "3490524077",
                    "type": "codeVoice"
                  },
                  {
                    "text": ") — pronounced “dude, too bad”",
                    "type": "text"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        }
      ],
      "kind": "content"
    }
  ],
