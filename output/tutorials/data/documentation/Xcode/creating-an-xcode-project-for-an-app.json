{
  "abstract": [
    {
      "text": "Start developing your app by creating an Xcode project from a template.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Xcode/documentation/Xcode"
      ]
    ]
