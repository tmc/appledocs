{
  "abstract": [
    {
      "text": "Create a history of incremental code changes to your Xcode project’s source control",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "repository with Git commits.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Xcode/documentation/Xcode",
        "doc://com.apple.Xcode/documentation/Xcode/source-control-management"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "swift",
    "url": "doc://com.apple.Xcode/documentation/Xcode/tracking-code-changes-in-a-source-control-repository"
  },
  "kind": "article",
