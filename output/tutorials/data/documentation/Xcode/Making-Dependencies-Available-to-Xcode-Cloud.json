{
  "abstract": [
    {
      "text": "Review dependencies and make them available to Xcode Cloud before you configure your project to use Xcode Cloud.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
