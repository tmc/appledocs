{
  "abstract": [
    {
      "text": "Avoid unnecessary work in your Metal app by understanding the relationships between resources.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
