{
  "abstract": [
    {
      "text": "Make symbols easier to find by arranging them into groups and collections.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Xcode/documentation/Xcode",
