{
  "abstract": [
    {
      "text": "The operating system stopped the process",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "because the process exceeded a limit on resource consumption,",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "like CPU time or memory.",
      "type": "text"
    }
  ],
