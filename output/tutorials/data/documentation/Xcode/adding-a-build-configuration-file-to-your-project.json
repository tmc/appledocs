{
  "abstract": [
    {
      "text": "Specify your project’s build settings in plain-text files, and supply",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "different settings for debug and release builds.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Xcode/documentation/Xcode",
