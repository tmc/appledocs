{
  "abstract": [
    {
      "text": "Edit source files in Xcode and add Quick Help comments to improve your project’s",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "maintainability.",
      "type": "text"
    }
  ],
