{
  "abstract": [
    {
      "text": "Identify the signs of a language exception, and address the crashes caused by uncaught",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "language exceptions.",
      "type": "text"
    }
  ],
  "hierarchy": {
