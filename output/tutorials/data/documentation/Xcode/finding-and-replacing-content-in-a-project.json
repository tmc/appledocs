{
  "abstract": [
    {
      "text": "Search some or all of your project for text strings or symbol names, and perform",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "advanced searches using regular expressions.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
