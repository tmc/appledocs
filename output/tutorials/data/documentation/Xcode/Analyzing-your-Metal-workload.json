{
  "abstract": [
    {
      "text": "Investigate your app’s workload, dependencies, performance, and memory impact using the Metal debugger.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
