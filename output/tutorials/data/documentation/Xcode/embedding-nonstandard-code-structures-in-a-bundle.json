{
  "abstract": [
    {
      "text": "Use code that’s structured in a nonstandard way while avoiding code signing and distribution problems.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
        "doc://com.apple.Xcode/documentation/Xcode",
        "doc://com.apple.Xcode/documentation/Xcode/bundles-and-frameworks"
      ]
    ]
  },
  "identifier": {
    "interfaceLanguage": "swift",
    "url": "doc://com.apple.Xcode/documentation/Xcode/embedding-nonstandard-code-structures-in-a-bundle"
  },
  "kind": "article",
