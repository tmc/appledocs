{
  "abstract": [
    {
      "text": "Discover attachment issues by inspecting individual pixels and samples.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
