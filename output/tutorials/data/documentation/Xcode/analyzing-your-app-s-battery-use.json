{
  "abstract": [
    {
      "text": "Increase the available use time for your app on a single battery charge by reducing",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "your appʼs power consumption.",
      "type": "text"
    }
  ],
