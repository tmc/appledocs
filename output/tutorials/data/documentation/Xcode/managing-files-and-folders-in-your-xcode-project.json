  "abstract": [
    {
      "text": "Add new or existing files to your project, and use groups to",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "organize the files and folders in the Project navigator.",
      "type": "text"
    }
  ],
