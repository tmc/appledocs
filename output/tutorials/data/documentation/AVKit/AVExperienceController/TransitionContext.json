    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When ",
              "type": "text"
            },
            {
              "code": "AVExperienceController",
              "type": "codeVoice"
            },
            {
              "text": " transitions its ",
              "type": "text"
            },
            {
              "code": "experience",
              "type": "codeVoice"
            },
            {
              "text": " from ",
              "type": "text"
            },
            {
              "code": "fromExperience",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "toExperience",
              "type": "codeVoice"
            },
            {
              "text": ", delegate callbacks provide instances of ",
              "type": "text"
            },
            {
              "code": "TransitionContext",
              "type": "codeVoice"
            },
            {
              "text": " to allow clients to respond as the transition progresses or reverts. The normal ",
              "type": "text"
            },
            {
              "code": "Status",
              "type": "codeVoice"
            },
            {
              "text": " sequence is ",
              "type": "text"
            },
            {
              "code": ".preparing",
              "type": "codeVoice"
            },
            {
              "text": " -> ",
              "type": "text"
            },
            {
              "code": ".transitioning",
              "type": "codeVoice"
            },
            {
              "text": " -> ",
              "type": "text"
            },
            {
              "code": ".completed",
              "type": "codeVoice"
            },
            {
              "text": " Once ",
              "type": "text"
            },
            {
              "code": ".completed",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "AVExperienceController",
              "type": "codeVoice"
            },
            {
              "text": "â€™s ",
              "type": "text"
            },
            {
              "code": "experience",
              "type": "codeVoice"
            },
            {
              "text": " is now the ",
              "type": "text"
            },
            {
              "code": "toExperience",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Not all transitions are ",
              "type": "text"
            },
            {
              "code": ".completed",
              "type": "codeVoice"
            },
            {
              "text": ", instead they are ",
              "type": "text"
            },
            {
              "code": ".reversed",
              "type": "codeVoice"
            },
            {
              "text": " back to the ",
              "type": "text"
            },
            {
              "code": "fromExperience",
              "type": "codeVoice"
            },
            {
              "text": ". Reversed transitions can happen after ",
              "type": "text"
            },
            {
              "code": ".preparing",
              "type": "codeVoice"
            },
            {
              "text": " or after ",
              "type": "text"
            },
            {
              "code": ".transitioning",
              "type": "codeVoice"
            },
            {
              "text": ", but it will not happen after ",
              "type": "text"
            },
            {
              "code": ".completed",
              "type": "codeVoice"
            },
            {
              "text": " or before ",
              "type": "text"
            },
            {
              "code": ".preparing",
              "type": "codeVoice"
            },
            {
              "text": ". When a transition is reversed a reason is provided.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
