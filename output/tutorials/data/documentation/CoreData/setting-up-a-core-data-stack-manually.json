{
  "abstract": [
    {
      "text": "Create the individual components that Core Data requires manually, to support earlier versions of Apple operating systems.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
