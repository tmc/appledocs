{
  "abstract": [
    {
      "text": "Use Core Data in both a single-threaded and multithreaded app.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
