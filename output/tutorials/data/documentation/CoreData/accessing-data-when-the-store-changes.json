{
  "abstract": [
    {
      "text": "Guarantee that a context won’t see store changes until you tell it to look.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
