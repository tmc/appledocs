    "doc://com.apple.coredata/documentation/CoreData/NSManagedObjectContext/save()": {
      "abstract": [
        {
          "text": "Attempts to commit unsaved changes to registered objects to the context’s parent store.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "keyword",
          "text": "func"
