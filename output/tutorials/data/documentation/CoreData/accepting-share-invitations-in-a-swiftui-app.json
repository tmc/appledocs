{
  "abstract": [
    {
      "text": "Adapt your app to use UIKit’s application and scene delegates so it can process CloudKit share invitations.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
