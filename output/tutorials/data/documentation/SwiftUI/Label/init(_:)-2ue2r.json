    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The label style to use.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "configuration"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this initializer within the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/LabelStyle/makeBody(configuration:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "method of a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/LabelStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instance to create an instance of the label",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that’s being styled. This is useful for custom label styles that only",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "wish to modify the current style, as opposed to implementing a brand new",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "style.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following style adds a red border around the label,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "but otherwise preserves the current style:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct RedBorderedLabelStyle: LabelStyle {",
            "    func makeBody(configuration: Configuration) -> some View {",
            "        Label(configuration)",
            "            .border(Color.red)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
