      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use values of this type to control window dismissal during the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "current transaction.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, to dismiss windows showing a modal presentation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that would otherwise prohibit dismissal, use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/DismissBehavior/destructive",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "behavior:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct DismissWindowButton: View {",
            "    @Environment(\\.dismissWindow) private var dismissWindow",
            "",
            "    var body: some View {",
            "        Button(\"Close Auxiliary Window\") {",
            "            withTransaction(\\.dismissBehavior, .destructive) {",
            "                dismissWindow(id: \"auxiliary\")",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
