    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Binding",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to a value that you provide.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount to increment or decrement ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": " each time the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "user clicks or taps the stepper’s increment or decrement buttons.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Defaults to ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "step"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A format style of type ",
                  "type": "text"
                },
                {
                  "code": "F",
                  "type": "codeVoice"
                },
                {
                  "text": " to use when converting between",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the string the user edits and the underlying value of type",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "F.FormatInput",
                  "type": "codeVoice"
                },
                {
                  "text": ". If ",
                  "type": "text"
                },
                {
                  "code": "format",
                  "type": "codeVoice"
                },
                {
                  "text": " can’t perform the conversion, the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "stepper leaves ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": " unchanged. If the user stops editing the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "text in an invalid state, the stepper updates the text to the last",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "known valid value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "format"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A view describing the purpose of this stepper.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "label"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that’s called when editing begins and",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "ends. For example, on iOS, the user may touch and hold the increment",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "or decrement buttons on a stepper which causes the execution",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of the ",
                  "type": "text"
                },
                {
                  "code": "onEditingChanged",
                  "type": "codeVoice"
                },
                {
                  "text": " closure at the start and end of",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the gesture.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onEditingChanged"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to create a stepper that increments or decrements",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a bound value by a specific amount each time the user clicks or taps",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the stepper’s increment or decrement buttons, while displaying the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "current value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, a stepper increments or decrements ",
              "type": "text"
            },
            {
              "code": "value",
              "type": "codeVoice"
            },
            {
              "text": " by the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "step",
              "type": "codeVoice"
            },
            {
              "text": " value of 5 at each click or tap of the control’s increment or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "decrement button:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct StepperView: View {",
            "    @State private var value = 1",
            "    let step = 5",
            "    var body: some View {",
            "        Stepper(value: $value,",
            "                step: step,",
            "                format: .number) {",
            "            Text(\"Current value: \\(value), step: \\(step)\")",
            "        }",
            "            .padding(10)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "SwiftUI-Stepper-value-step.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
