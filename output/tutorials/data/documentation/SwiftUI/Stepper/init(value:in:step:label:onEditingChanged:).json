    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Binding",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to a value that you provide.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closed range that describes the upper and lower bounds",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "permitted by the stepper.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bounds"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount to increment or decrement the stepper when the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "user clicks or taps the stepper’s increment or decrement buttons,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "respectively.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "step"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A view describing the purpose of this stepper.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "label"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that’s called when editing begins and",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "ends. For example, on iOS, the user may touch and hold the increment",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "or decrement buttons on a stepper which causes the execution",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of the ",
                  "type": "text"
                },
                {
                  "code": "onEditingChanged",
                  "type": "codeVoice"
                },
                {
                  "text": " closure at the start and end of",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the gesture.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onEditingChanged"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to create a stepper that increments or decrements",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a binding to value by the step size you provide within the given bounds.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "By setting the bounds, you ensure that the value never goes below or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "above the lowest or highest value, respectively.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below shows a stepper that displays the effect of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "incrementing or decrementing a value with the step size of ",
              "type": "text"
            },
            {
              "code": "step",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with the bounds defined by ",
              "type": "text"
            },
            {
              "code": "range",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct StepperView: View {",
            "    @State private var value = 0",
            "    let step = 5",
            "    let range = 1...50",
            "",
            "    var body: some View {",
            "        Stepper(value: $value,",
            "                in: range,",
            "                step: step) {",
            "            Text(\"Current: \\(value) in \\(range.description) \" +",
            "                 \"stepping by \\(step)\")",
            "        }",
            "            .padding(10)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "SwiftUI-Stepper-value-step-range.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
