    "doc://com.apple.SwiftUI/documentation/SwiftUI/TextSelectionAffinity": {
      "abstract": [
        {
          "text": "A representation of the direction or association of a selection or cursor",
          "type": "text"
        },
        {
          "text": " ",
          "type": "text"
        },
        {
          "text": "relative to a text character. This concept becomes much more prominent",
          "type": "text"
        },
        {
          "text": " ",
          "type": "text"
        },
        {
          "text": "when dealing with bidirectional text (text that contains both LTR and RTL",
          "type": "text"
        },
        {
          "text": " ",
          "type": "text"
        },
        {
          "text": "scripts, like English and Arabic combined).",
          "type": "text"
        }
      ],
