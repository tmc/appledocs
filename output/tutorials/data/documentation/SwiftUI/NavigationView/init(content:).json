        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ViewBuilder",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that produces the content that",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the navigation view wraps. Any views after the first act as",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "placeholders for corresponding columns in a multicolumn display.",
                  "type": "text"
                }
              ],
