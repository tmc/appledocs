    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To recognize a window drag gesture on a view, create and configure the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "gesture, and then add it to the view using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/gesture(_:isEnabled:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier. Consider also letting the gesture ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/SwiftUI/View/allowsWindowActivationEvents(_:)",
              "isActive": true,
              "overridingTitle": "handle events that activate the containing window",
              "overridingTitleInlineContent": [
                {
                  "text": "handle events that activate the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "containing window",
                  "type": "text"
                }
              ],
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "so that dragging the containing window works even when itâ€™s inactive.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To add a window drag gesture to a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Circle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and change its color while a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "user performs the window drag gesture:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyView: View {",
            "    @GestureState var isDraggingWindow = false",
            "",
            "    var dragWindow: some Gesture {",
            "        WindowDragGesture()",
            "            .updating($isDraggingWindow) { _, state, _ in",
            "                state = true",
            "            }",
            "    }",
            "",
            "    var body: some View {",
            "        Circle()",
            "            .fill(isDraggingWindow ? Color.green : .blue)",
            "            .frame(width: 50, height: 50)",
            "            .gesture(dragWindow)",
            "            .allowsWindowActivationEvents()",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
