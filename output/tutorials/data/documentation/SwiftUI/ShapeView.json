    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this type with the drawing methods on ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Shape",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to apply multiple fills",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and/or strokes to a shape. For example, the following code applies a fill",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and stroke to a capsule shape:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Capsule()",
            "    .fill(.yellow)",
            "    .stroke(.blue, lineWidth: 8)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
