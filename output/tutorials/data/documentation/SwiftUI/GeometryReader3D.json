    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This view returns a flexible preferred size to its own container view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This container differs from ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/GeometryReader",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " in that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "it also reads available depth, and thus also returns a flexible preferred",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "depth to its parent layout. Use the 3D version only",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in situations where you need to read depth, because it affects",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "depth layout when used in a container like a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ZStack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
