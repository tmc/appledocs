          "content": [
            {
              "inlineContent": [
                {
                  "text": "The completed amount of the task to this point, in a range",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of ",
                  "type": "text"
                },
