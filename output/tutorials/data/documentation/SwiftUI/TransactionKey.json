    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can create custom transaction values by extending the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Transaction",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "structure with new properties.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "First declare a new transaction key type and specify a value for the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "required ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TransactionKey/defaultValue",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "private struct MyTransactionKey: TransactionKey {",
            "    static let defaultValue = false",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The Swift compiler automatically infers the associated ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TransactionKey/Value",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " type as the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "type you specify for the default value. Then use the key to define a new",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "transaction value property:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension Transaction {",
            "    var myCustomValue: Bool {",
            "        get { self[MyTransactionKey.self] }",
            "        set { self[MyTransactionKey.self] = newValue }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Clients of your transaction value never use the key directly.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Instead, they use the key path of your custom transaction value property.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "To set the transaction value for a change, wrap that change in a call to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "withTransaction",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "withTransaction(\\.myCustomValue, true) {",
            "    isActive.toggle()",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "To use the value from inside ",
              "type": "text"
            },
            {
              "code": "MyView",
              "type": "codeVoice"
            },
            {
              "text": " or one of its descendants, use the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/transaction(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view modifier:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "MyView()",
            "    .transaction { transaction in",
            "        if transaction.myCustomValue {",
            "            transaction.animation = .default.repeatCount(3)",
            "        }",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
