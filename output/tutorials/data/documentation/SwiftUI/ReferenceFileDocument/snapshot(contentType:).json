            {
              "text": "A snapshot of the document content that the system",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "provides to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ReferenceFileDocument/fileWrapper(snapshot:configuration:)",
              "isActive": true,
