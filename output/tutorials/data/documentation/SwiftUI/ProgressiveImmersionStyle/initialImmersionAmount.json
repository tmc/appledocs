      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The value represents how much of the spherical field of view of the user",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is covered by the portal effect of the style initially. The value can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "range from ",
              "type": "text"
            },
            {
              "code": "0.0",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "1.0",
              "type": "codeVoice"
            },
            {
              "text": ", and is capped by the minimum and maximum",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "amount of immersion of this instance.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "If this value is not set, a system default value will be used instead.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
