            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "image view, assuming that the image has an HDR (ITU-R 2100)",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "color space and the output device supports it:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
