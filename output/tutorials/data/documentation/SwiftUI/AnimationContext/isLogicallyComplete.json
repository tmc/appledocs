    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This controls when AnimationCompletionCriteria.logicallyComplete",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "completion callbacks are fired. This should be set to ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " at most",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "once in the life of an animation, changing back to ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": " later will be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "ignored. If this is never set to ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ", the behavior is equivalent to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "if this had been set to ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " just as the animation finished (by",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "returning ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ").",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
