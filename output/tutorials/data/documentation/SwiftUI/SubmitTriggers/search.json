    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In the example below, only the search field or search completions",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "placed by the searchable modifier will trigger the view model to submit",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "its current search query.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@StateObject private var viewModel = ViewModel()",
            "",
            "NavigationView {",
            "    SidebarView()",
            "    DetailView()",
            "}",
            ".searchable(",
            "    text: $viewModel.searchText,",
            "    placement: .sidebar",
            ") {",
            "    SuggestionsView()",
            "}",
            ".onSubmit(of: .search) {",
            "    viewModel.submitCurrentSearchQuery()",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
