    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Controls do not respect the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/tint(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier when applied to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "control labels, nor do controls support arbitrary tint shape styles.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Instead, define a tint color for your control by applying this modifier",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to its template:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct GarageDoorOpener: ControlWidget {",
            "    var body: some ControlWidgetConfiguration {",
            "        StaticControlConfiguration(...) {",
            "            ControlWidgetToggle(...) {",
            "                Label(",
            "                    $0 ? \"Open\" : \"Closed\",",
            "                    systemImage: $0 ? \"door.open\" : \"door.closed\"",
            "                )",
            "            }",
            "            .tint(.orange)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
