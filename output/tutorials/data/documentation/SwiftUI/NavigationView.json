    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use a ",
              "type": "text"
            },
            {
              "code": "NavigationView",
              "type": "codeVoice"
            },
            {
              "text": " to create a navigation-based app in which the user",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can traverse a collection of views. Users navigate to a destination view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by selecting a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/NavigationLink",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " that you provide. On iPadOS and macOS, the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "destination content appears in the next column. Other platforms push a new",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view onto the stack, and enable removing items from the stack with",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "platform-specific controls, like a Back button or a swipe gesture.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "NavigationView-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/NavigationView/init(content:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " initializer to create a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "navigation view that directly associates navigation links and their",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "destination views:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NavigationView {",
            "    List(model.notes) { note in",
            "        NavigationLink(note.title, destination: NoteEditor(id: note.id))",
            "    }",
            "    Text(\"Select a Note\")",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Style a navigation view by modifying it with the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/navigationViewStyle(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view modifier. Use other modifiers, like",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/navigationTitle(_:)-avgj",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", on views presented by the navigation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view to customize the navigation interface for the presented view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
