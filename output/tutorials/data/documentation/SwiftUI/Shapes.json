  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Draw shapes like circles and rectangles, as well as custom paths that define",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "shapes of your own design. Apply styles that include environment-aware colors,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "rich gradients, and material effects to the foreground, background, and outline",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of your shapes.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "shapes-hero",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If you need the efficiency or flexibility of immediate mode drawing",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "— for example, to create particle effects — use a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Canvas",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view instead.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
