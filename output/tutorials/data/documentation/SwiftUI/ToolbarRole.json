    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A toolbar role provides a description of the purpose of content that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "populates the toolbar. The purpose of the content influences how a toolbar",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "renders its content. For example, a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ToolbarRole/browser",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " will",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "automatically leading align the title of a toolbar in iPadOS.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Provide this type to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/toolbarRole(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ContentView()",
            "    .navigationTitle(\"Browser\")",
            "    .toolbarRole(.browser)",
            "    .toolbar {",
            "        ToolbarItem(placement: .primaryAction) {",
            "            AddButton()",
            "        }",
            "     }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
