        },
        {
          "inlineContent": [
            {
              "text": "A view that calls ",
              "type": "text"
            },
            {
              "code": "action",
              "type": "codeVoice"
            },
            {
              "text": " when the pointer enters,",
              "type": "text"
            },
            {
              "text": " ",
