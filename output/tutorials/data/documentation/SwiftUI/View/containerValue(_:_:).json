    "doc://com.externally.resolved.symbol/s:s15WritableKeyPathC": {
      "abstract": [
        {
          "text": "A key path that supports reading from and writing to the resulting value.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "keyword",
          "text": "class"
        },
        {
          "kind": "text",
          "text": " "
        },
        {
          "kind": "identifier",
          "text": "WritableKeyPath"
        },
        {
          "kind": "text",
          "text": "<"
