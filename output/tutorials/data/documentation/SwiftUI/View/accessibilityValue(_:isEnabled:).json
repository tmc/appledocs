    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The accessibility value to apply.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "valueDescription"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If true the accessibility value is applied; otherwise",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the accessibility value is unchanged.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isEnabled"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to describe the value represented by a view, but only",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "if that’s different than the view’s label. For example, for a slider",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that you label as “Volume” using accessibilityLabel(), you can provide",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the current volume setting, like “60%”, using accessibilityValue().",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
