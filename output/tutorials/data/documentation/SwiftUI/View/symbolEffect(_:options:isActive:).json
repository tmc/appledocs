    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A symbol effect to add to the view. Existing effects",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "added by ancestors of the view are preserved, but may be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "overridden by the new effect. Added effects will be applied",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to the ``SwiftUI/Image` views contained by the child view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "effect"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether the effect is active or inactive.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isActive"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A copy of the view with a symbol effect added.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example adds a repeating pulse effect to two",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "symbol images:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "VStack {",
            "    Image(systemName: \"bolt.slash.fill\")",
            "    Image(systemName: \"folder.fill.badge.person.crop\")",
            "}",
            ".symbolEffect(.pulse)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
