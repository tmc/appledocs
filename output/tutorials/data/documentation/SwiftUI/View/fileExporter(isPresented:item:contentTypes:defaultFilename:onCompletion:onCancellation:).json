    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding to whether the interface should be shown.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isPresented"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The item to be saved on disk.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "item"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The optional content types to use for the exported file.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If empty, SwiftUI uses the content types from the ",
                  "type": "text"
                },
                {
                  "code": "transferRepresentation",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "property provided for ",
                  "type": "text"
                },
                {
                  "code": "Transferable",
                  "type": "codeVoice"
                },
                {
                  "text": " conformance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contentTypes"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A callback that will be invoked when the operation",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "has succeeded or failed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onCompletion"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A callback that will be invoked",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "if the operation was cancelled.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onCancellation"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In order for the interface to appear ",
              "type": "text"
            },
            {
              "code": "isPresented",
              "type": "codeVoice"
            },
            {
              "text": " must be set to ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "When the operation is finished,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "isPresented",
              "type": "codeVoice"
            },
            {
              "text": " will be set to ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": " before ",
              "type": "text"
            },
            {
              "code": "onCompletion",
              "type": "codeVoice"
            },
            {
              "text": " is called. If",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the user cancels the operation, ",
              "type": "text"
            },
            {
              "code": "isPresented",
              "type": "codeVoice"
            },
            {
              "text": " will be set to ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": " and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "onCompletion",
              "type": "codeVoice"
            },
            {
              "text": " will not be called.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
