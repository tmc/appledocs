    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "tabItem(_:)",
              "type": "codeVoice"
            },
            {
              "text": " to configure a view as a tab bar item in a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". The example below adds two views as tabs in a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct View1: View {",
            "    var body: some View {",
            "        Text(\"View 1\")",
            "    }",
            "}",
            "",
            "struct View2: View {",
            "    var body: some View {",
            "        Text(\"View 2\")",
            "    }",
            "}",
            "",
            "struct TabItem: View {",
            "    var body: some View {",
            "        TabView {",
            "            View1()",
            "                .tabItem {",
            "                    Label(\"Menu\", systemImage: \"list.dash\")",
            "                }",
            "",
            "            View2()",
            "                .tabItem {",
            "                    Label(\"Order\", systemImage: \"square.and.pencil\")",
            "                }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
