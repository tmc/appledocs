      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The color to use to tint the content. Use ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "avoid overriding the inherited tint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tint"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The containing list’s style applies the tint as appropriate. For",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example, watchOS uses the tint color for its background platter",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "appearance. Sidebars on iOS and macOS apply the tint color to their",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Label",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " icons, which otherwise use the accent color by default.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This modifier is equivalent to using the version of the modifier",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that takes a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ListItemTint",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " value and specifying the ",
                  "type": "text"
                },
                {
                  "code": "tint",
                  "type": "codeVoice"
                },
                {
                  "text": " color in",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the corresponding ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ListItemTint/fixed(_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " input.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
