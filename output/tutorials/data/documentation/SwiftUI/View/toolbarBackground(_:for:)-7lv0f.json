    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The preferred visibility of the background of the bar.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "visibility"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bars to update the color scheme of or",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ToolbarPlacement/automatic",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " if empty.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bars"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The preferred visibility flows up to the nearest container that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "renders a bar. This could be a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/NavigationView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in iOS, or the root view of a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/WindowGroup",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " in macOS.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In iOS, a value of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ToolbarPlacement/automatic",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " makes the visibility",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of a tab bar or navigation bar background depend on where a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/List",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ScrollView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " settles. For example, when aligned to the bottom edge of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of a scroll view’s content, the background of a tab bar becomes",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "transparent.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Specify a value of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Visibility/visible",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to ensure that the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "background of a bar remains visible regardless of where any scroll",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view or list stops scrolling.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This example shows a view that prefers to always have the tab bar",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "visible when the middle tab is selected:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    FirstTab()",
            "    MiddleTab()",
            "        .toolbarBackground(.visible, for: .tabBar)",
            "    LastTab()",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can provide multiple placements to customize multiple bars",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "at once, as in the following example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    NavigationView {",
            "        ContentView()",
            "            .toolbarBackground(",
            "                .visible, for: .navigationBar, .tabBar)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
