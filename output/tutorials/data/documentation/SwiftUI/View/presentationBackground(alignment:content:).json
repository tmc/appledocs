    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example uses a yellow view as the sheet background:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    @State private var showSettings = false",
            "",
            "    var body: some View {",
            "        Button(\"View Settings\") {",
            "            showSettings = true",
            "        }",
            "        .sheet(isPresented: $showSettings) {",
            "            SettingsView()",
            "                .presentationBackground {",
            "                    Color.yellow",
            "                }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "presentationBackground(alignment:content:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier differs from",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/background(alignment:content:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier in several key",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "ways. A presentation background:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Automatically fills the entire presentation.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
