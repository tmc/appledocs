    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding to a Boolean value that determines whether",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to present the alert that you create in the modifier’s ",
                  "type": "text"
                },
                {
                  "code": "content",
                  "type": "codeVoice"
                },
                {
                  "text": " closure. When the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "user presses or taps OK the system sets ",
                  "type": "text"
                },
                {
                  "code": "isPresented",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "which dismisses the alert.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isPresented"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure returning the alert to present.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method when you need to show an alert to the user. The example",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "below displays an alert that is shown when the user toggles a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Boolean value that controls the presentation of the alert:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct OrderCompleteAlert: View {",
            "    @State private var isPresented = false",
            "    var body: some View {",
            "        Button(\"Show Alert\", action: {",
            "            isPresented = true",
            "        })",
            "        .alert(isPresented: $isPresented) {",
            "            Alert(title: Text(\"Order Complete\"),",
            "                  message: Text(\"Thank you for shopping with us.\"),",
            "                  dismissButton: .default(Text(\"OK\")))",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "SwiftUI-View-AlertIsPresentedContent.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
