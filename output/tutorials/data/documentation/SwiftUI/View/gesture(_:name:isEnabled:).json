    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A gesture to attach to the view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "gesture"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that identifies the gesture. In iOS, the name can be",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "used to set up failure relationships between UIKit gesture",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "recognizers and this gesture.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether the added gesture is enabled. The default value",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "is ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isEnabled"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method when you need to attach a gesture to a view. The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "example below defines a custom gesture that prints a message to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "console and attaches it to the view’s ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/VStack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". Inside the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/VStack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a red heart ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Image",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " defines its own ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TapGesture",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "handler that also prints a message to the console, and blue rectangle",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with no custom gesture handlers. Tapping or clicking the image",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "prints a message to the console from the tap gesture handler on the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "image, while tapping or clicking  the rectangle inside the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/VStack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "prints a message in the console from the enclosing vertical stack",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "gesture handler.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can also use the ",
              "type": "text"
            },
            {
              "code": "isEnabled",
              "type": "codeVoice"
            },
            {
              "text": " parameter to conditionally disable",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the gesture.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct GestureExample: View {",
            "    @State private var message = \"Message\"",
            "    var isGestureEnabled: Bool",
            "    let newGesture = TapGesture().onEnded {",
            "        print(\"Tap on VStack.\")",
            "    }",
            "",
            "    var body: some View {",
            "        VStack(spacing:25) {",
            "            Image(systemName: \"heart.fill\")",
            "                .resizable()",
            "                .frame(width: 75, height: 75)",
            "                .padding()",
            "                .foregroundColor(.red)",
            "                .onTapGesture {",
            "                    print(\"Tap on image.\")",
            "                }",
            "            Rectangle()",
            "                .fill(Color.blue)",
            "        }",
            "        .gesture(newGesture, isEnabled: isGestureEnabled)",
            "        .frame(width: 200, height: 200)",
            "        .border(Color.purple)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
