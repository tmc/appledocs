    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to change the appearance and behavior of navigation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "views. For example, by default, navigation views appear with multiple",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "columns in wider environments, like iPad in landscape orientation:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "View-navigationViewStyle-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can apply the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/NavigationViewStyle/stack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " style to force",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "single-column stack navigation in these environments:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NavigationView {",
            "    List {",
            "        NavigationLink(\"Purple\", destination: ColorDetail(color: .purple))",
            "        NavigationLink(\"Pink\", destination: ColorDetail(color: .pink))",
            "        NavigationLink(\"Orange\", destination: ColorDetail(color: .orange))",
            "    }",
            "    .navigationTitle(\"Colors\")",
            "",
            "    Text(\"Select a Color\") // A placeholder to show before selection.",
            "}",
            ".navigationViewStyle(.stack)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "View-navigationViewStyle-2",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
