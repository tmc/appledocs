    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to hide or show scroll indicators on scrollable",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "content in views like a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ScrollView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/List",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TextEditor",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This modifier applies the prefered visibility to any",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "scrollable content within a view hierarchy.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ScrollView {",
            "    VStack(alignment: .leading) {",
            "        ForEach(0..<100) {",
            "            Text(\"Row \\($0)\")",
            "        }",
            "    }",
            "}",
            ".scrollIndicators(.hidden)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ScrollIndicatorVisibility/hidden",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " value to indicate that you",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "prefer that views never show scroll indicators along a given axis.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ScrollIndicatorVisibility/visible",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " when you prefer that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "views show scroll indicators. Depending on platform conventions,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "visible scroll indicators might only appear while scrolling. Pass",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ScrollIndicatorVisibility/automatic",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to allow views to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "decide whether or not to show their indicators.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
