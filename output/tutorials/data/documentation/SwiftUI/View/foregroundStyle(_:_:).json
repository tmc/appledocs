        },
        {
          "inlineContent": [
            {
              "text": "A view that uses the given foreground styles.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "SwiftUI uses these styles when rendering child views",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that don’t have an explicit rendering style, like images,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "text, shapes, and so on.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Symbol images within the view hierarchy use the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/SymbolRenderingMode/palette",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " rendering mode when you apply this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier, if you don’t explicitly specify another mode.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
