      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The primary color or pattern to use when filling in",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the foreground elements. To indicate a specific value, use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Color",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ShapeStyle/image(_:sourceRect:scale:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", or one of the gradient",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "types, like",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ShapeStyle/linearGradient(colors:startPoint:endPoint:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". To set a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "style that’s relative to the containing view’s style, use one of the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "semantic styles, like ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ShapeStyle/primary",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "primary"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The secondary color or pattern to use when",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "filling in the foreground elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "secondary"
        }
      ]
    },
    {
      "content": [
