    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can suggest text completions during a text input operation by",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "providing data to this modifier. The interface presents the suggestion",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "views as a list of choices when someone activates the text editing",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "interface.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Associate a string with each suggestion view by adding the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/textInputCompletion(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier to the view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "Label",
              "type": "codeVoice"
            },
            {
              "text": " to get platform-standard visual representations of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "suggestion text accompanied with images, and ",
              "type": "text"
            },
            {
              "code": "Section",
              "type": "codeVoice"
            },
            {
              "text": " for labelled",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "sections of results.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
