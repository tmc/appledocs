    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The modal view that the system presents to the user.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this view modifier to present UI in your app for device administrators to add devices purchased",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "outside of the official channel to their organization â€” Apple School Manager, Apple Business Manager,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or Apple Business Essentials. The system requires sign in with a Managed Apple Account that includes",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "device enrollment privileges.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following code example shows one way to present this view to your users:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Example Usage:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "import SwiftUI",
            "import AutomatedDeviceEnrollment",
            "",
            "struct ContentView: View {",
            "    @State private var isAddingDevices: Bool = false",
            "",
            "    var body: some View {",
            "        Button(\"Add Devices to Automated Device Enrollment\") {",
            "            isAddingDevices = true",
            "        }",
            "        .automatedDeviceEnrollmentAddition(isPresented: $isAddingDevices)",
            "        .onChange(of: isAddingDevices) {",
            "            if !isAddingDevices {",
            "                // Handle dismiss action",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
