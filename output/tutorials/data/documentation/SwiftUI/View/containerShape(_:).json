          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The example below defines a view that shows its content with a rounded",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "rectangle background and the same container shape. Any",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ContainerRelativeShape",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " within the ",
              "type": "text"
            },
            {
              "code": "content",
              "type": "codeVoice"
            },
            {
              "text": " matches the rounded",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "rectangle shape from this container inset as appropriate.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct PlatterContainer<Content: View> : View {",
            "    @ViewBuilder var content: Content",
            "    var body: some View {",
            "        content",
            "            .padding()",
            "            .containerShape(shape)",
            "            .background(shape.fill(.background))",
            "    }",
            "    var shape: RoundedRectangle { RoundedRectangle(cornerRadius: 20) }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
