    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Different views may have different triggers for the provided action.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "A ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TextField",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/SecureField",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " will trigger this action when the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "user hits the hardware or software return key. This modifier may also",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "bind this action to a default action keyboard shortcut. You may set this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "action on an individual view or an entire view hierarchy.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TextField(\"Username\", text: $username)",
            "    .onSubmit {",
            "        guard viewModel.validate() else { return }",
            "        viewModel.login()",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/submitScope(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier to stop a submit",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "trigger from a control from propagating higher up in the view hierarchy",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to higher ",
              "type": "text"
            },
            {
              "code": "View.onSubmit(of:_:)",
              "type": "codeVoice"
            },
            {
              "text": " modifiers.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Form {",
            "    TextField(\"Username\", text: $viewModel.userName)",
            "    SecureField(\"Password\", text: $viewModel.password)",
            "",
            "    TextField(\"Tags\", text: $viewModel.tags)",
            "        .submitScope()",
            "}",
            ".onSubmit {",
            "    guard viewModel.validate() else { return }",
            "    viewModel.login()",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can use different submit triggers to filter the types of triggers",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that should invoke the provided submission action. For example, you",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "may provide a value of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/SubmitTriggers/search",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to only hear",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "submission triggers that originate from search fields vended by",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "searchable modifiers.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@StateObject private var viewModel = ViewModel()",
            "",
            "NavigationView {",
            "    SidebarView()",
            "    DetailView()",
            "}",
            ".searchable(",
            "    text: $viewModel.searchText,",
            "    placement: .sidebar",
            ") {",
            "    SuggestionsView()",
            "}",
            ".onSubmit(of: .search) {",
            "    viewModel.submitCurrentSearchQuery()",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
