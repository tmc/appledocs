            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "extends beyond the layout bounds of the shape.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "By default, a viewâ€™s bounding frame is used only for layout, so any",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "content that extends beyond the edges of the frame is still visible.",
              "type": "text"
            }
          ],
          "type": "paragraph"
