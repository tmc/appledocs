    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The binding to whether the Photos picker should be shown.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isPresented"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The item being shown and selected in the Photos picker.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "selection"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Types of items that can be shown. Default is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ". Setting it to ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " means all supported types can be shown.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "filter"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The encoding disambiguation policy of the selected item. Default is ",
                  "type": "text"
                },
                {
                  "code": ".automatic",
                  "type": "codeVoice"
                },
                {
                  "text": ". Setting it to ",
                  "type": "text"
                },
                {
                  "code": ".automatic",
                  "type": "codeVoice"
                },
                {
                  "text": " means the best encoding determined by the system will be used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "preferredItemEncoding"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The user explicitly grants access only to items they choose, so photo library access authorization is not needed.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
