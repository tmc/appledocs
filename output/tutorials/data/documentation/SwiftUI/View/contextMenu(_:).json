      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A context menu container for views that you",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "present as menu items in a context menu.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contextMenu"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that can show a context menu.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to attach a specified context menu to a view.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "You can make the context menu unavailable by conditionally passing ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as the value for the ",
              "type": "text"
            },
            {
              "code": "contextMenu",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below creates a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ContextMenu",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " that contains two items and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "passes them into the modifier. The Boolean value ",
              "type": "text"
            },
            {
              "code": "shouldShowMenu",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "which defaults to ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ", controls the context menu availability:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "private let menuItems = ContextMenu {",
            "    Button {",
            "        // Add this item to a list of favorites.",
            "    } label: {",
            "        Label(\"Add to Favorites\", systemImage: \"heart\")",
            "    }",
            "    Button {",
            "        // Open Maps and center it on this item.",
            "    } label: {",
            "        Label(\"Show in Maps\", systemImage: \"mappin\")",
            "    }",
            "}",
            "",
            "private struct ContextMenuMenuItems: View {",
            "    @State private var shouldShowMenu = true",
            "",
            "    var body: some View {",
            "        Text(\"Turtle Rock\")",
            "            .contextMenu(shouldShowMenu ? menuItems : nil)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "View-contextMenu-1-iOS",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
