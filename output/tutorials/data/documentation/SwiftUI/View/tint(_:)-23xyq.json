    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tint ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Color",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to apply.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tint"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to override the default accent color for this view.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Unlike an app’s accent color, which can be overridden by user",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "preference, the tint color is always respected and should be used as a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "way to provide additional meaning to the control.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This example shows Answer and Decline buttons with ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ShapeStyle/green",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ShapeStyle/red",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " tint colors, respectively.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ControlTint: View {",
            "    var body: some View {",
            "        HStack {",
            "            Button {",
            "                // Answer the call",
            "            } label: {",
            "                Label(\"Answer\", systemImage: \"phone\")",
            "            }",
            "            .tint(.green)",
            "            Button {",
            "                // Decline the call",
            "            } label: {",
            "                Label(\"Decline\", systemImage: \"phone.down\")",
            "            }",
            "            .tint(.red)",
            "        }",
            "        .buttonStyle(.borderedProminent)",
            "        .padding()",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Some controls adapt to the tint color differently based on their style,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the current platform, and the surrounding context. For example, in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "macOS, a button with the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/PrimitiveButtonStyle/bordered",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " style doesn’t",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "tint its background, but one with the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/PrimitiveButtonStyle/borderedProminent",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " style does. In macOS, neither",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of these button styles tint their label, but they do in other platforms.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
