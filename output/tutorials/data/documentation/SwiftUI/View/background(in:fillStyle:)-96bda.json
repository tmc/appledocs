    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier behaves like ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/background(_:in:fillStyle:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "except that it always uses the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ShapeStyle/background",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " shape style",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to fill the specified shape. For example, you can create a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Path",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that outlines a trapezoid:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let trapezoid = Path { path in",
            "    path.move(to: .zero)",
            "    path.addLine(to: CGPoint(x: 90, y: 0))",
            "    path.addLine(to: CGPoint(x: 80, y: 50))",
            "    path.addLine(to: CGPoint(x: 10, y: 50))",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Then you can use that shape as a background for a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Label",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ZStack {",
            "    Color.teal",
            "    Label(\"Flag\", systemImage: \"flag.fill\")",
            "        .padding()",
            "        .background(in: trapezoid)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Without the background modifier, the fill color shows",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "through the label. With the modifier, the labelâ€™s text and icon appear",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "backed by a shape filled with a color thatâ€™s appropriate for light",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or dark appearance:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "View-background-B",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To create a background with other ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " types â€” or with a stack",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of views â€” use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/background(alignment:content:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instead.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "To add a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ShapeStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " as a background, use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/background(_:ignoresSafeAreaEdges:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
