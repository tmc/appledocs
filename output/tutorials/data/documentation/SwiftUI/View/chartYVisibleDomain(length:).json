    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The length of the visible domain measured in data units.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "For categorical data, this should be the number of visible categories.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "length"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to control how much of the chart is visible",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in a scrollable chart. The example below sets the visible",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "portion of the chart to 10 units in the Y axis.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Chart(data) {",
            "    BarMark(",
            "        x: .value(\"x\", $0.x),",
            "        y: .value(\"y\", $0.y)",
            "    )",
            "}",
            ".chartScrollableAxes(.vertical)",
            ".chartYVisibleDomain(length: 10)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
