    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of data that this destination matches.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A view builder that defines a view to display",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "when the stack’s navigation state contains a value of",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "type ",
                  "type": "text"
                },
                {
                  "code": "data",
                  "type": "codeVoice"
                },
                {
                  "text": ". The closure takes one argument, which is the value",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of the data to present.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "destination"
        }
      ]
    },
    {
      "kind": "mentions",
      "mentions": [
        "doc://com.apple.SwiftUI/documentation/SwiftUI/Migrating-to-New-Navigation-Types"
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Add this view modifier to a view inside a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/NavigationStack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "describe the view that the stack displays when presenting",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a particular kind of data. Use a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/NavigationLink",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to present",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the data. For example, you can present a ",
              "type": "text"
            },
            {
              "code": "ColorDetail",
              "type": "codeVoice"
            },
            {
              "text": " view for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "each presentation of a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Color",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instance:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NavigationStack {",
            "    List {",
            "        NavigationLink(\"Mint\", value: Color.mint)",
            "        NavigationLink(\"Pink\", value: Color.pink)",
            "        NavigationLink(\"Teal\", value: Color.teal)",
            "    }",
            "    .navigationDestination(for: Color.self) { color in",
            "        ColorDetail(color: color)",
            "    }",
            "    .navigationTitle(\"Colors\")",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can add more than one navigation destination modifier to the stack",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "if it needs to present more than one kind of data.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Do not put a navigation destination modifier inside a “lazy” container,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "like ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/List",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/LazyVStack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". These containers create child views",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "only when needed to render on screen. Add the navigation destination",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier outside these containers so that the navigation stack can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "always see the destination.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
