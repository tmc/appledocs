    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding to an optional source of truth for the action",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "sheet. When ",
                  "type": "text"
                },
                {
                  "code": "item",
                  "type": "codeVoice"
                },
                {
                  "text": " is non-",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system passes",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the contents to the modifierâ€™s closure. You use this content",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to populate the fields of an action sheet that you create that the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "system displays to the user. If ",
                  "type": "text"
                },
                {
                  "code": "item",
                  "type": "codeVoice"
                },
                {
                  "text": " changes, the system",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "dismisses the currently displayed action sheet and replaces it",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "with a new one using the same process.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "item"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure returning the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ActionSheet",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " you create.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method when you need to populate the fields of an action sheet",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with content from a data source. The example below shows a custom data",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "source, ",
              "type": "text"
            },
            {
              "code": "FileDetails",
              "type": "codeVoice"
            },
            {
              "text": ", that provides data to populate the action sheet:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct FileDetails: Identifiable {",
            "    var id: String { name }",
            "    let name: String",
            "    let fileType: UTType",
            "}",
            "struct ConfirmFileImport: View {",
            "    @State private var sheetDetail: FileDetails?",
            "    var body: some View {",
            "        Button(\"Show Action Sheet\") {",
            "            sheetDetail = FileDetails(name: \"MyImageFile.png\",",
            "                                      fileType: .png)",
            "        }",
            "        .actionSheet(item: $sheetDetail) { detail in",
            "            ActionSheet(",
            "                title: Text(\"File Import\"),",
            "                message: Text(\"\"\"",
            "                         Import \\(detail.name)?",
            "                         File Type: \\(detail.fileType.description)",
            "                         \"\"\"),",
            "                buttons: [",
            "                    .destructive(Text(\"Import\"),",
            "                                 action: importFile),",
            "                    .cancel()",
            "                ])",
            "        }",
            "    }",
            "",
            "    func importFile() {",
            "        // Handle import action.",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "SwiftUI-View-ActionSheetItemContent.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
