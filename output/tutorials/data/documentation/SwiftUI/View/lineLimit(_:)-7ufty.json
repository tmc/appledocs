    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to specify a partial range of lines that a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view or a vertical ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TextField",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " can occupy. When the text of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "such views occupies more space than the provided limit, a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "truncates its content while a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TextField",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " becomes scrollable.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Form {",
            "    TextField(\"Title\", text: $model.title)",
            "    TextField(\"Notes\", text: $model.notes, axis: .vertical)",
            "        .lineLimit(...3)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
