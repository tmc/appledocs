    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The expected type of the dropped models.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "payloadType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that takes the dropped content and responds",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "appropriately. The first parameter to ",
                  "type": "text"
                },
                {
                  "code": "action",
                  "type": "codeVoice"
                },
                {
                  "text": " contains the dropped",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "items. The second",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "parameter contains the drop location in this view’s coordinate",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "space. Return ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " if the drop operation was successful;",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "otherwise, return ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that is called when a drag and drop operation",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "enters or exits the drop target area. The received value is ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " when",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the cursor is inside the area, and ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " when the cursor is outside.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isTargeted"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that provides a drop destination for a drag",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "operation of the specified type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The dropped content can be provided as binary data, file URLs, or file promises.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The drop destination is the same size and position as this view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@State private var isDropTargeted = false",
            "",
            "var body: some View {",
            "    Color.pink",
            "        .frame(width: 400, height: 400)",
            "        .dropDestination(for: String.self) { receivedTitles, location in",
            "            animateDrop(at: location)",
            "            process(titles: receivedTitles)",
            "        } isTargeted: {",
            "            isDropTargeted = $0",
            "        }",
            "}",
            "",
            "func process(titles: [String]) { ... }",
            "func animateDrop(at: CGPoint) { ... }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
