    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tip to display.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tip"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The edge of the attachmentAnchor that defines the location of the popover’s arrow. By default, the system will choose the best orientation of the popover’s arrow.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "arrowEdge"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The action to perform when the user triggers a tip’s button.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 3,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to present a tip as a popover on an existing view when the tip becomes eligible for display.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "import SwiftUI",
            "import TipKit",
            "",
            "// Define your tip's content.",
            "struct SampleTip: Tip {",
            "    var title: Text {",
            "        Text(\"Save as a Favorite\")",
            "    }",
            "",
            "    var message: Text? {",
            "        Text(\"Your favorite backyards always appear at the top of the list.\")",
            "    }",
            "",
            "    var image: Image? {",
            "        Image(systemName: \"star\")",
            "    }",
            "}",
            "",
            "struct SampleView: View {",
            "    // Create an instance of your tip.",
            "    var tip = SampleTip()",
            "",
            "    var body: some View {",
            "        VStack {",
            "            // Add `.popoverTip` to the view you want to modify.",
            "            // Tips.configure(options:) must be called before your tip will be eligible for display.",
            "            Image(systemName: \"star\")",
            "                .popoverTip(tip)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
