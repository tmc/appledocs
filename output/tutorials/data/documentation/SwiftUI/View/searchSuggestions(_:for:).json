    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "SwiftUI presents search suggestions differently depending on several",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "factors, like the platform, the position of the search field, and the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "size class. Use this modifier when you want to only display suggestions",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in certain ways under certain conditions. For example, you might choose",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to display suggestions in a menu when possible, but directly filter",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "your data source otherwise.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "enum FruitSuggestion: String, Identifiable {",
            "    case apple, banana, orange",
            "    var id: Self { self }",
            "}",
            "",
            "@State private var text = \"\"",
            "@State private var suggestions: [FruitSuggestion] = []",
            "",
            "var body: some View {",
            "    MainContent()",
            "        .searchable(text: $text) {",
            "            ForEach(suggestions) { suggestion",
            "                Text(suggestion.rawValue)",
            "                    .searchCompletion(suggestion.rawValue)",
            "            }",
            "            .searchSuggestions(.hidden, for: .content)",
            "        }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
