                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Alignment/center",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that you use to position the background view.",
                  "type": "text"
                }
              ],
