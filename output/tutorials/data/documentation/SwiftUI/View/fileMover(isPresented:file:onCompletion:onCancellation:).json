    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding to whether the dialog should be shown.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isPresented"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL of the file to be moved.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "file"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A callback that will be invoked when the operation has",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "succeeded or failed. The ",
                  "type": "text"
                },
                {
                  "code": "result",
                  "type": "codeVoice"
                },
                {
                  "text": " indicates whether",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the operation succeeded or failed. To access the received URLs, call",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "startAccessingSecurityScopedResource",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "When the access is no longer required, call ",
                  "type": "text"
                },
                {
                  "code": "stopAccessingSecurityScopedResource",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onCompletion"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A callback that will be invoked",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "if the user cancels the operation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onCancellation"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This dialog provides security-scoped URLs.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Call the ",
                  "type": "text"
                },
                {
                  "code": "startAccessingSecurityScopedResource",
                  "type": "codeVoice"
                },
                {
                  "text": " method to access or bookmark",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the URLs, and the ",
                  "type": "text"
                },
                {
                  "code": "stopAccessingSecurityScopedResource",
                  "type": "codeVoice"
                },
                {
                  "text": " method",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to release the access.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "For example, a button that allows the user to move a file might look like this:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "  struct MoveFileButton: View {",
            "      @State private var showFileMover = false",
            "      var file: URL",
            "      var onCompletion: (URL) -> Void",
            "      var onCancellation: (() -> Void)?",
            "",
            "      var body: some View {",
            "          Button {",
            "              showFileMover = true",
            "          } label: {",
            "              Label(\"Choose destination\", systemImage: \"folder.circle\")",
            "          }",
            "          .fileMover(isPresented: $showFileMover, file: file) { result in",
            "              switch result {",
            "              case .success(let url):",
            "                  guard url.startAccessingSecurityScopedResource() else {",
            "                      return",
            "                  }",
            "                  onCompletion(url)",
            "                  url.stopAccessingSecurityScopedResource()",
            "              case .failure(let error):",
            "                  print(error)",
            "                  // handle error",
            "              }",
            "          } onCancellation: {",
            "              onCancellation?()",
            "          }",
            "      }",
            "  }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
