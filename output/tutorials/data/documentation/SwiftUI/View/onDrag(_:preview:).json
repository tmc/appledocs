        },
        {
          "inlineContent": [
            {
              "text": "Applying the ",
              "type": "text"
            },
            {
              "code": "onDrag(_:preview:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier adds the appropriate gestures",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for drag and drop to this view. When a drag operation begins,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a rendering of ",
              "type": "text"
            },
            {
              "code": "preview",
              "type": "codeVoice"
            },
            {
              "text": " is generated and used as the preview image.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To customize the lift preview, shown while the system transitions",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to show your custom ",
              "type": "text"
            },
            {
              "code": "preview",
              "type": "codeVoice"
            },
            {
              "text": ", apply a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/contentShape(_:_:eoFill:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ContentShapeKinds/dragPreview",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " kind. For example, you can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "change the preview’s corner radius or use a nested view as the preview.",
              "type": "text"
            }
