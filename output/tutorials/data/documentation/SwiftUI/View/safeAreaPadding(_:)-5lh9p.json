        {
          "inlineContent": [
            {
              "text": "Use this modifier when you would like to add a fixed amount",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of space to the safe area a view sees.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ScrollView(.horizontal) {",
            "    HStack(spacing: 10.0) {",
            "        ForEach(items) { item in",
            "            ItemView(item)",
            "        }",
            "    }",
            "}",
            ".safeAreaPadding(.horizontal, 20.0)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "See the ",
              "type": "text"
            },
            {
              "code": "View/safeAreaInset(edge:alignment:spacing:content)",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier for adding to the safe area based on the size of a",
              "type": "text"
            },
