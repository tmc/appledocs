          "content": [
            {
              "inlineContent": [
                {
                  "text": "The possible data values plotted as foreground style",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "in the chart. You can define the domain with a ",
                  "type": "text"
                },
                {
                  "code": "ClosedRange",
                  "type": "codeVoice"
                },
                {
                  "text": " for number",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "or ",
                  "type": "text"
                },
                {
                  "code": "Date",
                  "type": "codeVoice"
                },
                {
                  "text": " values (e.g., ",
                  "type": "text"
                },
                {
                  "code": "0 ... 500",
                  "type": "codeVoice"
                },
                {
                  "text": "), and with an array for categorical",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "values (e.g., ",
                  "type": "text"
                },
                {
                  "code": "[\"A\", \"B\", \"C\"]",
                  "type": "codeVoice"
                },
                {
                  "text": ")",
                  "type": "text"
                }
              ],
