    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The clipping shape to use for this view. The ",
                  "type": "text"
                },
                {
                  "code": "shape",
                  "type": "codeVoice"
                },
                {
                  "text": " fills",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the view’s frame, while maintaining its aspect ratio.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shape"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The fill style to use when rasterizing ",
                  "type": "text"
                },
                {
                  "code": "shape",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "style"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that clips this view to ",
              "type": "text"
            },
            {
              "code": "shape",
              "type": "codeVoice"
            },
            {
              "text": ", using ",
              "type": "text"
            },
            {
              "code": "style",
              "type": "codeVoice"
            },
            {
              "text": " to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "define the shape’s rasterization.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "clipShape(_:style:)",
              "type": "codeVoice"
            },
            {
              "text": " to clip the view to the provided shape. By",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "applying a clipping shape to a view, you preserve the parts of the view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "covered by the shape, while eliminating other parts of the view. The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "clipping shape itself isn’t visible.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, this code applies a circular clipping shape to a ",
              "type": "text"
            },
            {
              "code": "Text",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Text(\"Clipped text in a circle\")",
            "    .frame(width: 175, height: 100)",
            "    .foregroundColor(Color.white)",
            "    .background(Color.black)",
            "    .clipShape(Circle())"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The resulting view shows only the portion of the text that lies within",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the bounds of the circle.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "SwiftUI-View-clipShape.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
