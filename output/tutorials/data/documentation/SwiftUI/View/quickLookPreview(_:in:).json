    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Binding",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to an element that’s part of the items collection. This is the URL that you currently want to preview.",
                  "type": "text"
                }
              ],
