    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The types of content that the view supports exporting",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "and importing. An empty array means the view does not currently",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "support exporting.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contentTypes"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that will be called on request of the items",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "by the shortcut or service.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onExport"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that will be called after the shortcut or",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "service completes with its output data. This should replace the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "selected subpart that was exported with ",
                  "type": "text"
                },
                {
                  "code": "onExport",
                  "type": "codeVoice"
                },
                {
                  "text": ". Return ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "indicate that there was a failure to receive the items.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onEdit"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the associated view supports selection, the exported item should",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "reflect that selected subpart.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
