    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of Identity provided by the subviews.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "idType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount required to be visible within the viewport of the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the scrollview before the ",
                  "type": "text"
                },
                {
                  "code": "action",
                  "type": "codeVoice"
                },
                {
                  "text": " is fired.  By default when the view",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "has crossed more than 50% on-screen, the action will be called.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "threshold"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The action which will be called when the views within the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "scroll view cross the provided threshold. The callback will include",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "which views are considered visible.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier along with the modifier ",
              "type": "text"
            },
            {
              "code": "View/scrollTargetLayout()",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to be informed when the views in the targetted scroll view have crossed",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the provided threshold to be considered on/off screen.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ScrollView {",
            "    LazyVStack {",
            "        ForEach(models) { model in",
            "            CardView(model: model)",
            "        }",
            "    }",
            "    .scrollTargetLayout()",
            "}",
            ".onScrollTargetVisibilityChange(for: Model.ID.self, threshold: 0.2) { onScreenCards in",
            "    // Disable video playback for cards that are offscreen...",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
