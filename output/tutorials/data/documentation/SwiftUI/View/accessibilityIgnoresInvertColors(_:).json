            {
              "inlineContent": [
                {
                  "text": "A true value ignores the system Smart Invert",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "setting. A false value follows the system setting.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "active"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to suppress Smart Invert in a view that shouldn’t",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "be inverted. Or pass an ",
              "type": "text"
            },
            {
              "code": "active",
              "type": "codeVoice"
            },
            {
              "text": " argument of ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": " to begin following",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the Smart Invert setting again when it was previously disabled.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
