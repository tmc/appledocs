              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view truncates its",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "content while a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TextField",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " becomes scrollable.",
              "type": "text"
            }
          ],
