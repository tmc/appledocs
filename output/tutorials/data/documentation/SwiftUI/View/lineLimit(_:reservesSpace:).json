        {
          "inlineContent": [
            {
              "text": "Use this modifier to specify a limit to the lines that a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or a vertical ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TextField",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " may occupy. If passed a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value of true for the ",
              "type": "text"
            },
            {
              "code": "reservesSpace",
              "type": "codeVoice"
            },
            {
              "text": " parameter, and the text of such",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "views occupies less space than the provided limit, that view expands",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to occupy the minimum number of lines. When the text occupies",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "more space than the provided limit, a ",
              "type": "text"
            },
