            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "flipsForRightToLeftLayoutDirection(_:)",
              "type": "codeVoice"
            },
            {
              "text": " when you need the system to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "horizontally mirror the contents of the view when presented in",
              "type": "text"
