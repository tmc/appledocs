    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "SwiftUI uses the actions passed to the above dialogs to determine",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "whether the dialog should block app termination by default when",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "presented. If all of the following are satisfied, the dialog will",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "not block app quit:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "There is only a single button and its role is not ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ButtonRole/destructive",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/dialogSeverity(_:)",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " is not `DialogSeverity/critical``",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "There are no ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TextField",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": "s",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier after a ",
              "type": "text"
            },
            {
              "code": "View/alert",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "View/confirmationDialog",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to specify whether the dialog should prevent app termination.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Pass ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " to explicitly request the automatic behavior/for the inert",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "version of this modifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ConfirmLogoutView: View {",
            "  @State private var isConfirming = false",
            "",
            "  var body: some View {",
            "    Button(\"Logout\") { isConfirming = true }",
            "      .confirmationDialog(",
            "        Text(\"Logout?\"),",
            "          isPresented: $isConfirming",
            "        ) {",
            "          Button(\"Yes\") {",
            "            // Handle logout action.",
            "          }",
            "        }",
            "        .dialogPreventsAppTermination(false)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
