            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "defaultWheelPickerItemHeight(_:)",
              "type": "codeVoice"
            },
            {
              "text": " when you need to change the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "default item height in a picker control. In this example, the view sets",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the default height for picker elements to 30 points.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct DefaultWheelPickerItemHeight: View {",
            "    @State private var selected = 1",
            "    var body: some View {",
            "        VStack(spacing: 20) {",
            "            Picker(selection: $selected, label: Text(\"Favorite Color\")) {",
            "                Text(\"Red\").tag(1)",
            "                Text(\"Green\").tag(2)",
            "                Text(\"Blue\").tag(3)",
            "                Text(\"Other\").tag(4)",
            "            }",
            "        }",
            "        .defaultWheelPickerItemHeight(30)",
            "    }",
            "}"
          ],
