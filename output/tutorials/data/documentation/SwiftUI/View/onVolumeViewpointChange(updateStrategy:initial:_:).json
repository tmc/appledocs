          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether the action should be run for all viewpoint",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "changes or only for supported viewpoint changes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
