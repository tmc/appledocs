          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The baseplate is a semi-transparent view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that appears on the ‘floor’ of a volume.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Usage:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "WindowGroup() {",
            "    Poker()",
            "}",
            ".windowStyle(.volumetric)",
            ".volumeBaseplateVisibility(.hidden)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Defaults to ",
              "type": "text"
            },
            {
              "code": "automatic",
              "type": "codeVoice"
            },
            {
              "text": " (visible).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
