      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view in the foreground will be tinted alongside other foreground",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "views. The color for both the foreground and background layers is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "determined by the watch face.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
