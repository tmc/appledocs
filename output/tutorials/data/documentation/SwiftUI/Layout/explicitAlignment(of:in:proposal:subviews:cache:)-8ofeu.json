    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/HorizontalAlignment",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " guide that the method calculates",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the position of.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "guide"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The region that the container view’s parent allocates to the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "container view, specified in the parent’s coordinate space.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bounds"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A proposed size for the container.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "proposal"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A collection of proxy instances that represent the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "views arranged by the container. You can use the proxies in the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "collection to get information about the subviews as you determine",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "where to place the guide.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "subviews"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Optional storage for calculated data that you can share among",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the methods of your custom layout container. See",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Layout/makeCache(subviews:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for details.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cache"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The guide’s position relative to the ",
              "type": "text"
            },
            {
              "code": "bounds",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Return ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " to indicate that the guide doesn’t have an explicit",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Implement this method to return a value for the specified alignment",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "guide of a custom layout container. The value you return affects",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the placement of the container as a whole, but it doesn’t affect how the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "container arranges subviews relative to one another.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this method to put an alignment guide in a nonstandard",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "position. For example, you can indent the container’s leading edge",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "alignment guide by 10 points:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension BasicVStack {",
            "    func explicitAlignment(",
            "        of guide: HorizontalAlignment,",
            "        in bounds: CGRect,",
            "        proposal: ProposedViewSize,",
            "        subviews: Subviews,",
            "        cache: inout ()",
            "    ) -> CGFloat? {",
            "        if guide == .leading {",
            "            return bounds.minX + 10",
            "        }",
            "        return nil",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The above example returns ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " for other guides to indicate that they",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "don’t have an explicit value. A guide without an explicit value behaves",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as it would for any other view. If you don’t implement the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "method, the protocol’s default implementation merges the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "subviews’ guides.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
