        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "Settings(content:)",
              "type": "codeVoice"
            },
            {
              "text": " to add a preferences scene when you declare",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "your app using the ",
              "type": "text"
            },
            {
