    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The scroll view displays its content within the scrollable content region.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "As the user performs platform-appropriate scroll gestures, the scroll view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "adjusts what portion of the underlying content is visible. ",
              "type": "text"
            },
            {
              "code": "ScrollView",
              "type": "codeVoice"
            },
            {
              "text": " can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "scroll horizontally, vertically, or both, but does not provide zooming",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "functionality.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the following example, a ",
              "type": "text"
            },
            {
              "code": "ScrollView",
              "type": "codeVoice"
            },
            {
              "text": " allows the user to scroll through",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/VStack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " containing 100 ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " views. The image after the listing",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "shows the scroll viewâ€™s temporarily visible scrollbar at the right; you can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "disable it with the ",
              "type": "text"
            },
            {
              "code": "showsIndicators",
              "type": "codeVoice"
            },
            {
              "text": " parameter of the ",
              "type": "text"
            },
            {
              "code": "ScrollView",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "initializer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var body: some View {",
            "    ScrollView {",
            "        VStack(alignment: .leading) {",
            "            ForEach(0..<100) {",
            "                Text(\"Row \\($0)\")",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "SwiftUI-ScrollView-rows-with-indicator.png",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Controlling-Scroll-Position",
          "level": 3,
          "text": "Controlling Scroll Position",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can influence where a scroll view is initially scrolled",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/defaultScrollAnchor(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view modifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Provide a value of `UnitPoint/center`` to have the scroll",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view start in the center of its content when a scroll view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is scrollable in both axes.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ScrollView([.horizontal, .vertical]) {",
            "    // initially centered content",
            "}",
            ".defaultScrollAnchor(.center)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Or provide an alignment of `UnitPoint/bottom`` to have the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "scroll view start at the bottom of its content when a scroll",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view is scrollable in its vertical axes.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ScrollView {",
            "    // initially bottom aligned content",
            "}",
            ".defaultScrollAnchor(.bottom)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "After the scroll view initially renders, the user may scroll",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the content of the scroll view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To perform programmatic scrolling, wrap one or more scroll views with a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ScrollViewReader",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
