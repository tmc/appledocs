    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Section order can be read by subscripting with the tab section’s id:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let order = customization[sectionID: \"com.myApp.categories\"]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Identifiers can be associated with a ",
              "type": "text"
            },
            {
              "code": "Tab",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "TabSection",
              "type": "codeVoice"
            },
            {
              "text": " using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "customizationID(_:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabSection(\"Categories\") {",
            "    Tab(\"Climate\", systemImage: \"fan\") {",
            "        ClimateView()",
            "    }",
            "    .customizationID(\"com.myApp.climate\")",
            "",
            "    Tab(\"Lights\", systemImage: \"lightbulb\") {",
            "        LightsView()",
            "    }",
            "    .customizationID(\"com.myApp.lights\")",
            "}",
            ".customizationID(\"com.myApp.categories\")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If the ID isn’t associated with a section or the section has not been customized,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a default value of ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " is returned.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
