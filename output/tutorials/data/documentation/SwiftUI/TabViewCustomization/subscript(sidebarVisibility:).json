    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Visibility can be set imperatively by subscripting with the tab’s id:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "customization[sidebarVisibility: \"com.myApp.alerts\"] = .hidden"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can change the default visibility by using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabContent/defaultVisibility(_:for:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/AdaptableTabBarPlacement/sidebar",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " placement.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Tab(\"Alerts\", systemImage: \"bell\", value: .alerts) {",
            "    AlertsView()",
            "}",
            ".customizationID(\"com.myApp.alerts\")",
            ".defaultVisibility(.hidden, for: .sidebar)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If the ID isn’t associated with a tab or the tab has not been customized,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a default value of ",
              "type": "text"
            },
            {
              "code": ".automatic",
              "type": "codeVoice"
            },
            {
              "text": " is returned.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
