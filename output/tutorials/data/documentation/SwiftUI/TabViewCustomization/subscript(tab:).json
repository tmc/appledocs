      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can imperatively set properties by subscripting with the tab ID.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The following example sets the tab’s sidebar visibility:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "customization[tab: \"com.myApp.alerts\"].sidebarVisibility = .hidden"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
