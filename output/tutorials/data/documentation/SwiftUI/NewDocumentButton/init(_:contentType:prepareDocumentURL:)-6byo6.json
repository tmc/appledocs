    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A content type of the document to create.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "contentType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that is called when a user presses the button.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "At this point, you can present a document template picker or another UI that allows",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "users to choose a theme, configuration, or a template",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to create a document from.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Return a prepared document, or throw an error if document creation failed.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Return ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " to request creation of an empty document.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "prepareDocumentURL"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A button that presents a document template picker and creates documents",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can be implemented like this:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "   struct ChooseDocumentTemplateButton: View {",
            "       @State private var showTemplatePicker = false",
            "       @State private var documentCreationContinuation:",
            "           CheckedContinuation<URL?, any Error>?",
            "",
            "       var body: some View {",
            "           NewDocumentButton(contentType: .text) {",
            "               try await withCheckedThrowingContinuation { continuation in",
            "                   documentCreationContinuation = continuation",
            "                   showTemplatePicker = true",
            "               }",
            "           }",
            "           .fullScreenCover(isPresented: $showTemplatePicker) {",
            "               TemplatePicker($documentCreationContinuation)",
            "           }",
            "       }",
            "   }",
            "",
            "   struct TemplatePicker: View {",
            "       @Binding var documentCreationContinuation:",
            "           CheckedContinuation<URL?, any Error>?",
            "",
            "       ...",
            "",
            "       func present(document: URL) {",
            "           documentCreationContinuation.resume(returning: document)",
            "           documentCreationContinuation = nil",
            "       }",
            "   }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
