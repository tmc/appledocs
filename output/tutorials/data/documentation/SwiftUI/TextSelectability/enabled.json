    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Enabling text selection allows people to perform actions on the text",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "content, such as copying and sharing. Enable text selection in views",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "where those operations are useful, such as copying unique IDs or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "error messages. This allows people to paste the data into",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "emails or documents.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example enables text selection on the second of two",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " views in a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/VStack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "VStack {",
            "    Text(\"Event Invite\")",
            "        .font(.title)",
            "    Text(invite.date.formatted(date: .long, time: .shortened))",
            "        .textSelection(.enabled)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
