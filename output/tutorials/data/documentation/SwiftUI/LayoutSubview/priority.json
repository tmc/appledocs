    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If you define a custom layout type using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Layout",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "protocol, you can read this value from subviews and use the value",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "when deciding how to assign space to subviews. For example, you",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can read all of the subview priorities into an array before",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "placing the subviews in a custom layout type called ",
              "type": "text"
            },
            {
              "code": "BasicVStack",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension BasicVStack {",
            "    func placeSubviews(",
            "        in bounds: CGRect,",
            "        proposal: ProposedViewSize,",
            "        subviews: Subviews,",
            "        cache: inout ()",
            "    ) {",
            "        let priorities = subviews.map { subview in",
            "            subview.priority",
            "        }",
            "",
            "        // Place views, based on priorities.",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Set the layout priority for a view that appears in your layout by",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "applying the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/layoutPriority(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view modifier. For example,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "you can assign two different priorities to views that you arrange",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with ",
              "type": "text"
            },
            {
              "code": "BasicVStack",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "BasicVStack {",
            "    Text(\"High priority\")",
            "        .layoutPriority(10)",
            "    Text(\"Low priority\")",
            "        .layoutPriority(1)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
