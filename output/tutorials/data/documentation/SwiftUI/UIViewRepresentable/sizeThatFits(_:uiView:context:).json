    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method may be called more than once with different proposed sizes",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "during the same layout pass. SwiftUI views choose their own size, so one",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of the values returned from this function will always be used as the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "actual size of the composite view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
