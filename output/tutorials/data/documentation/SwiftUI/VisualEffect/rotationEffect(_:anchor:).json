    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This effect rotates the content around the axis that points",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "out of the xy-plane. It has no effect on the content’s frame.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The following code rotates text by 22˚ and then draws a border around",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the modified view to show that the frame remains unchanged by the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "rotation:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Text(\"Rotation by passing an angle in degrees\")",
            "    .visualEffect { content, geometryProxy in",
            "        content",
            "            .rotationEffect(.degrees(22))",
            "    }",
            "    .border(Color.gray)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
