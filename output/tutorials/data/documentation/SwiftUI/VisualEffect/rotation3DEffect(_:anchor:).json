      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A rotation effect.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This effect causes the content to appear rotated, but",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "doesn’t change the content’s frame. The following code",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "applies a rotation of 45° about the y-axis, using",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the default anchor point at the center of the content:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Model3D(named: \"robot\")",
            "    .visualEffect { content, geometryProxy in",
            "        content",
            "            .rotation3DEffect(Rotation3D(angle: .degrees(45), axis: .y))",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "During an animation, this modifier uses spherical linear interpolation,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "which produces more natural animations, but doesn’t support rotations",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "over 360 degrees. To specify angles over 360 degrees, consider using",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "View/rotation3DEffect(_:axis:anchor:)-4enag",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
