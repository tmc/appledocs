    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date this format references.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "date"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The units of time that may be used in the format",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to express the reference. The ",
                  "type": "text"
                },
                {
                  "code": "thresholdField",
                  "type": "codeVoice"
                },
                {
                  "text": " is always assumed",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to be allowed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allowedFields"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of fields that can be shown at once",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "when   using an abolute format. For example, January 9, 2007 is",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "shown as ",
                  "type": "text"
                },
                {
                  "code": "January 2007",
                  "type": "codeVoice"
                },
                {
                  "text": " by default, but as ",
                  "type": "text"
                },
                {
                  "code": "January 9, 2007",
                  "type": "codeVoice"
                },
                {
                  "text": " if",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the ",
                  "type": "text"
                },
                {
                  "code": "maxFieldCount",
                  "type": "codeVoice"
                },
                {
                  "text": " is set to three. The style automatically",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "excludes more significant fields if their value is equal to the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "value in the reference date and they are not necessary for the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "format pattern, making room for less significant fields.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "maxFieldCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The least precise field preserving which",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "warrants increasing the field count from one, i.e. switching from",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the relative   to the absolute representation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "thresholdField"
        }
      ]
    }
