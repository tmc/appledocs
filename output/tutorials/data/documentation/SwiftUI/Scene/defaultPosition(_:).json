                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/UnitPoint",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that specifies where to place a",
                  "type": "text"
                },
                {
                  "text": " ",
