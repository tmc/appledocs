    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The resizability to use for windows created by",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "this scene.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "resizability"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A scene that uses the specified resizability strategy.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this scene modifier to apply a value of type ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/WindowResizability",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Scene",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " that you define in your ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/App",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " declaration.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The value that you specify indicates the strategy the system uses to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "place minimum and maximum size restrictions on windows that it creates",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "from that scene.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, you can create a window group that people can resize to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "between 100 and 400 points in both dimensions by applying both a frame",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with those constraints to the scene’s content, and the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/WindowResizability/contentSize",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " resizability to the scene:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@main",
            "struct MyApp: App {",
            "    var body: some Scene {",
            "        WindowGroup {",
            "            ContentView()",
            "                .frame(",
            "                    minWidth: 100, maxWidth: 400,",
            "                    minHeight: 100, maxHeight: 400)",
            "        }",
            "        .windowResizability(.contentSize)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The default value for all scenes if you don’t apply the modifier is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/WindowResizability/automatic",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". With that strategy, ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Settings",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "windows use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/WindowResizability/contentSize",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " strategy, while",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "all others use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/WindowResizability/contentMinSize",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
