    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "Commands",
                  "type": "codeVoice"
                },
                {
                  "text": " builder whose output will be used to replace",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the commands normally provided by the modified scene.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A scene that replaces any commands defined by its children",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with alternative content.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "code": "WindowGroup",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "Window",
              "type": "codeVoice"
            },
            {
              "text": ", and other scene types all have an associated",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "set of commands that they include by default. Apply this modifier to a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "scene to replace those commands with the output from the given builder.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following code adds a scene for showing the contents of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the pasteboard in a dedicated window. We replace the scene’s default",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Window > Clipboard menu command with a custom Edit > Show Clipboard",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "command that we place next to the other pasteboard commands.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@main",
            "struct Example: App {",
            "    @Environment(\\.openWindow) var openWindow",
            "",
            "    var body: some Scene {",
            "        ...",
            "",
            "        Window(\"Clipboard\", id: \"clipboard\") {",
            "            ClipboardContentView()",
            "        }",
            "        .commandsReplaced {",
            "            CommandGroup(after: .pasteboard) {",
            "                Section {",
            "                    Button(\"Show Clipboard\") {",
            "                        openWindow(id: \"clipboard\")",
            "                    }",
            "                }",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
