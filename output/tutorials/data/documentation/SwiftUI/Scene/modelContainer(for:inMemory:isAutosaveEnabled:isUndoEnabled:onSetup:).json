      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The model type defining the schema used to create the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "model container.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "modelType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether the container should store data only in memory.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "inMemory"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": " if autosave is enabled.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isAutosaveEnabled"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Use ",
                  "type": "text"
                },
                {
                  "code": "undoManager",
                  "type": "codeVoice"
                },
                {
                  "text": " in the sceneâ€™s environment to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "manage undo operations for the model container.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isUndoEnabled"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A callback that will be invoked when the creation of the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "container has has succeeded or failed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "onSetup"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In this example, ",
              "type": "text"
            },
            {
              "code": "RecipesApp",
              "type": "codeVoice"
            },
            {
              "text": " sets a shared model container to use for",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "all of its windows, configured to store instances of ",
              "type": "text"
            },
            {
              "code": "Recipe",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@Model class Recipe { ... }",
            "",
            "@main",
            "struct RecipesApp: App {",
            "    var body: some Scene {",
            "        WindowGroup {",
            "            RecipesList()",
            "        }",
            "        .modelContainer(for: Recipe.self)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The environmentâ€™s ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/EnvironmentValues/modelContext",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property will be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "assigned a new context associated with this container. All implicit",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "model context operations in this scene, such as ",
              "type": "text"
            },
            {
              "code": "Query",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "properties, will use the environmentâ€™s context.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "By default, the container stores its model data persistently on disk.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "To only store data in memory for the lifetime of the appâ€™s process, pass",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " to the ",
              "type": "text"
            },
            {
              "code": "inMemory:",
              "type": "codeVoice"
            },
            {
              "text": " parameter.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The container will only be created once. New values that are passed to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the ",
              "type": "text"
            },
            {
              "code": "modelType",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "inMemory",
              "type": "codeVoice"
            },
            {
              "text": " parameters after the view is first",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "created will be ignored.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
