      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The customization behavior of the customizable",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "toolbar content.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "behavior"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Customizable toolbar items support different kinds of customization:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A user can add an an item that is not in the toolbar.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A user can remove an item that is in the toolbar.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A user can move an item within the toolbar.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "Based on the customization behavior of the toolbar items, different",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "edits will be supported.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to the customization behavior a user can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "perform on your toolbar items. In the following example, the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "customizable toolbar item supports all of the different kinds of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "toolbar customizations and starts in the toolbar.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ContentView()",
            "    .toolbar(id: \"main\") {",
            "        ToolbarItem(id: \"new\") {",
            "            // new button here",
            "        }",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can create an item that can not be removed from the toolbar",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or moved within the toolbar  by passing a value of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ToolbarCustomizationBehavior/disabled",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to this modifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ContentView()",
            "    .toolbar(id: \"main\") {",
            "        ToolbarItem(id: \"new\") {",
            "            // new button here",
            "        }",
            "        .customizationBehavior(.disabled)",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can create an item that can not be removed from the toolbar, but",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can be moved by passing a value of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ToolbarCustomizationBehavior/reorderable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ContentView()",
            "    .toolbar(id: \"main\") {",
            "        ToolbarItem(id: \"new\") {",
            "            // new button here",
            "        }",
            "        .customizationBehavior(.reorderable)",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
