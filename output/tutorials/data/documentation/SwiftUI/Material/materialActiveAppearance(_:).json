      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Materials used as the ",
              "type": "text"
            },
            {
              "code": "window",
              "type": "codeVoice"
            },
            {
              "text": " container background on macOS will",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "automatically appear inactive when their the window appears inactive, but",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "can be made to always appear active by setting the active appearance",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "behavior to be always active:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Text(\"Hello, World!\")",
            "    .containerBackground(",
            "        Material.regular.materialActiveAppearance(.active),",
            "        for: .window)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
