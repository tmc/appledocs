    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A context menu view allows you to present a situationally specific menu",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that enables taking actions relevant to the current task.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can create a context menu by first defining a ",
              "type": "text"
            },
            {
              "code": "ContextMenu",
              "type": "codeVoice"
            },
            {
              "text": " container",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with the controls that represent the actions people can take,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and then using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/contextMenu(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view modifier to apply the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "menu to a view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below creates and applies a two item context menu container",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view. The Boolean value ",
              "type": "text"
            },
            {
              "code": "shouldShowMenu",
              "type": "codeVoice"
            },
            {
              "text": ", which defaults to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "true, controls the availability of context menu:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "private let menuItems = ContextMenu {",
            "    Button {",
            "        // Add this item to a list of favorites.",
            "    } label: {",
            "        Label(\"Add to Favorites\", systemImage: \"heart\")",
            "    }",
            "    Button {",
            "        // Open Maps and center it on this item.",
            "    } label: {",
            "        Label(\"Show in Maps\", systemImage: \"mappin\")",
            "    }",
            "}",
            "",
            "private struct ContextMenuMenuItems: View {",
            "    @State private var shouldShowMenu = true",
            "",
            "    var body: some View {",
            "        Text(\"Turtle Rock\")",
            "            .contextMenu(shouldShowMenu ? menuItems : nil)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "View-contextMenu-1-iOS",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
