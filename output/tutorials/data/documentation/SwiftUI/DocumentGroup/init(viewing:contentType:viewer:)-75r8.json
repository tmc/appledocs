    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "code": [
            " @main",
            " struct Todo: App {",
            "     var body: some Scene {",
            "         DocumentGroup(viewing: [TodoItem.self, CalendarEvent.self], contentType: .tasks) {",
            "             ContentView()",
            "         }",
            "     }",
            " }",
            "",
            " struct ContentView: View {",
            "     @Query var items: [TodoItem]",
            "     @Query var events: [CalendarEvent]",
            "",
            "     var body: some View {",
            "         Form {",
            "             Section(\"Todo\") {",
            "                 ForEach(items) { item in",
            "                     Image(systemName: item.isDone ? \"checkmark.square\" : \"square\")",
            "                 }",
            "             }",
            "             Section(\"Events\") {",
            "                 ForEach(events) { event in",
            "                     LabeledContent(event.title, value: event.date.formatted())",
            "                  }",
            "              }",
            "         }",
            "         .formStyle(.grouped)",
            "         .padding()",
            "     }",
            " }",
            "",
            " @Model",
            " final class TodoItem {",
            "     var created: Date",
            "     var text: String",
            "     var isDone = false",
            " }",
            "",
            " @Model",
            " final class CalendarEvent {",
            "     var title: String",
            "     var date: Date",
            "}",
            "",
            "",
            " extension UTType {",
            "     static var tasks = UTType(exportedAs: \"com.myApp.tasks\")",
            " }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If your app declares custom uniform type identifiers,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "include corresponding entries in the app’s ",
                  "type": "text"
                },
                {
                  "code": "Info.plist",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/UniformTypeIdentifiers/defining-file-and-data-types-for-your-app",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Also, remember to specify the supported Document types in the ",
                  "type": "text"
                },
                {
                  "code": "Info.plist",
                  "type": "codeVoice"
                },
                {
                  "text": " as well.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
