    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This value is a ratio of view points to image pixels. This relationship",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "means that values greater than ",
              "type": "text"
            },
            {
              "code": "1.0",
              "type": "codeVoice"
            },
            {
              "text": " create an image larger than the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "original content view, and less than ",
              "type": "text"
            },
            {
              "code": "1.0",
              "type": "codeVoice"
            },
            {
              "text": " creates a smaller image. The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "following example shows a 100 x 50 rectangle view and an image rendered",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "from it with a ",
              "type": "text"
            },
            {
              "code": "scale",
              "type": "codeVoice"
            },
            {
              "text": " of ",
              "type": "text"
            },
            {
              "code": "2.0",
              "type": "codeVoice"
            },
            {
              "text": ", resulting in an image size of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "200 x 100.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let rectangle = Rectangle()",
            "    .frame(width: 100, height: 50)",
            "let renderer = ImageRenderer(content: rectangle)",
            "renderer.scale = 2.0",
            "if let rendered = renderer.cgImage {",
            "    print(\"Scaled image: \\(rendered.width) x \\(rendered.height)\")",
            "}",
            "// Prints \"Scaled image: 200 x 100\""
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The default value of this property is ",
              "type": "text"
            },
            {
              "code": "1.0",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
