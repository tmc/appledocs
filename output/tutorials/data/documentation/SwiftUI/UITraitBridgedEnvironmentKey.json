    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this protocol to allow the same underlying data to be accessed using an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "environment key in SwiftUI and trait in UIKit. As the bridging is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "bidirectional, values written to the trait in UIKit can be read using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "environment key in SwiftUI, and values written to the environment key in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "SwiftUI can be read from the trait in UIKit.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Given a custom UIKit trait named ",
              "type": "text"
            },
            {
              "code": "MyTrait",
              "type": "codeVoice"
            },
            {
              "text": " with ",
              "type": "text"
            },
            {
              "code": "myTrait",
              "type": "codeVoice"
            },
            {
              "text": " properties on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "both ",
              "type": "text"
            },
            {
              "code": "UITraitCollection",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "UIMutableTraits",
              "type": "codeVoice"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyTrait: UITraitDefinition {",
            "    static let defaultValue = \"Default value\"",
            "}",
            "",
            "extension UITraitCollection {",
            "    var myTrait: String {",
            "        self[MyTrait.self]",
            "    }",
            "}",
            "",
            "extension UIMutableTraits {",
            "    var myTrait: String {",
            "        get { self[MyTrait.self] }",
            "        set { self[MyTrait.self] = newValue }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can declare an environment key to represent the same data:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyEnvironmentKey: EnvironmentKey {",
            "    static let defaultValue = \"Default value\"",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Bridge the environment key and the trait by conforming to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "UITraitBridgedEnvironmentKey",
              "type": "codeVoice"
            },
            {
              "text": " protocol, providing implementations",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/UITraitBridgedEnvironmentKey/read(from:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/UITraitBridgedEnvironmentKey/write(to:value:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to losslessly convert",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the environment value from and to the corresponding trait value:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension MyEnvironmentKey: UITraitBridgedEnvironmentKey {",
            "    static func read(",
            "        from traitCollection: UITraitCollection",
            "    ) -> String {",
            "        traitCollection.myTrait",
            "    }",
            "",
            "    static func write(",
            "        to mutableTraits: inout UIMutableTraits, value: String",
            "    ) {",
            "        mutableTraits.myTrait = value",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
