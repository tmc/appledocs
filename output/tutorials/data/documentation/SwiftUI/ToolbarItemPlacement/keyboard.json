    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "On iOS, keyboard items are above the software keyboard when present,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or at the bottom of the screen when a hardware keyboard is attached.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "On macOS, keyboard items will be placed inside the Touch Bar.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "FocusedValue",
              "type": "codeVoice"
            },
            {
              "text": "can be used to adjust the content of the keyboard bar",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "based on the currently focused view. In the example below, the keyboard",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "bar gains additional buttons only when the appropriate ",
              "type": "text"
            },
            {
              "code": "TextField",
              "type": "codeVoice"
            },
            {
              "text": " is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "focused.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "enum Field {",
            "    case suit",
            "    case rank",
            "}",
            "",
            "struct KeyboardBarDemo : View {",
            "    @FocusedValue(\\.field) var field: Field?",
            "",
            "    var body: some View {",
            "        HStack {",
            "            TextField(\"Suit\", text: $suitText)",
            "                .focusedValue(\\.field, .suit)",
            "            TextField(\"Rank\", text: $rankText)",
            "                .focusedValue(\\.field, .rank)",
            "        }",
            "        .toolbar {",
            "            ToolbarItemGroup(placement: .keyboard) {",
            "                if field == .suit {",
            "                    Button(\"♣️\", action: {})",
            "                    Button(\"♥️\", action: {})",
            "                    Button(\"♠️\", action: {})",
            "                    Button(\"♦️\", action: {})",
            "                }",
            "                DoneButton()",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
