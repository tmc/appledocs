    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "On macOS, items with an accessory bar placement are placed in a section",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "below the title bar and toolbar area of the window. Each separate",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "identifier will correspond to a separate accessory bar that is added to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "this area.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension ToolbarItemPlacement {",
            "    static let favoritesBar = accessoryBar(id: \"com.example.favorites\")",
            "}",
            "...",
            "BrowserView()",
            "    .toolbar {",
            "        ToolbarItem(placement: .favoritesBar) {",
            "            FavoritesBar()",
            "        }",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
