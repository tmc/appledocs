    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional display name for the preview. If you don’t specify a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "name, the canvas labels the preview using the line number where the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "preview appears in source.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional list of",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/DeveloperToolsSupport/PreviewTrait",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "instances that customize the appearance of the preview.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "traits"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ViewBuilder",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that produces a SwiftUI view to preview. You",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "typically specify one of your app’s custom views and optionally any",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "inputs, model data, modifiers, and enclosing views that the custom",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "view needs for normal operation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "body"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "One or more preview cameras that indicate the custom, fixed",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "viewpoints that you want to be able to view the preview from. The first",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of these replaces the front viewpoint as the default.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cameras"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This macro behaves like ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Preview(_:traits:_:body:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " except that it also",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "enables you to specify one or more",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/DeveloperToolsSupport/PreviewCamera",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "instances that define custom, fixed viewpoints from which to view the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "preview:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "#Preview {",
            "    ContentView()",
            "} cameras: {",
            "    PreviewCamera(from: .bottomLeadingBack, name: \"Corner 1\")",
            "    PreviewCamera(from: .topTrailingFront, name: \"Corner 2\")",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If you use one of the preview macros that doesn’t include a ",
              "type": "text"
            },
            {
              "code": "cameras",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "closure, the canvas displays the preview from the front by default. It also",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "provides a camera picker to choose other standard, fixed",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "viewpoints — like the top or the back. When you do specify one or more",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "preview cameras, the canvas adds a submenu to the camera picker that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "lists the viewpoints that you define, like Corner 1 and Corner 2 in the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "above example. The canvas also displays the preview from the first of these",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "custom viewpoints by default when it loads the preview.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "In addition to using fixed camera perspectives, you can",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "also interactively alter the viewpoint of a preview in the canvas using",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "controls like those that Simulator provides. For more information, see",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "doc://com.apple.documentation/documentation/visionOS/interacting-with-your-app-in-the-visionos-simulator",
                  "type": "text"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "Other preview macros provide different customization options. For example,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "if you want to preview the view as it would appear in a particular kind of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "scene, you can use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Preview(_:immersionStyle:traits:body:cameras:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Preview(_:windowStyle:traits:body:cameras:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
