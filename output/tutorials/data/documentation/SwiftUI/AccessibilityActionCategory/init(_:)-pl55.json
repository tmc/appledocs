        {
          "inlineContent": [
            {
              "text": "Extension AccessibilityActionCategory {",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "static let table = AccessibilityActionCategory(“Table Options”)",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "}",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "var body: some View {",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "TableCellView()",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": ".accessibilityActions(category: .table) {",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "ForEach(tableCellActions) { action in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Button(action.title) {",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "action()",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "}",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "}",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "}",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "}",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
