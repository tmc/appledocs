    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When focus is outside any view that is bound to this state, the wrapped",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " for optional-typed state or ",
              "type": "text"
            },
            {
              "code": "false",
              "type": "codeVoice"
            },
            {
              "text": " for Boolean state.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the following example of a simple navigation sidebar, when the user",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "presses the Filter Sidebar Contents button, focus moves to the sidebar’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "filter text field. Conversely, if the user moves focus to the sidebar’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "filter manually, then the value of ",
              "type": "text"
            },
            {
              "code": "isFiltering",
              "type": "codeVoice"
            },
            {
              "text": " automatically",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "becomes ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ", and the sidebar view updates.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Sidebar: View {",
            "    @State private var filterText = \"\"",
            "    @FocusState private var isFiltering: Bool",
            "",
            "    var body: some View {",
            "        VStack {",
            "            Button(\"Filter Sidebar Contents\") {",
            "                isFiltering = true",
            "            }",
            "",
            "            TextField(\"Filter\", text: $filterText)",
            "                .focused($isFiltering)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
