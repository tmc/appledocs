    "doc://com.apple.SwiftUI/documentation/SwiftUI/ContentMode/fill": {
      "abstract": [
        {
          "text": "An option that resizes the content so it occupies all available space,",
          "type": "text"
        },
        {
          "text": " ",
          "type": "text"
        },
        {
          "text": "both vertically and horizontally.",
          "type": "text"
