    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use a ",
              "type": "text"
            },
            {
              "code": "MultiDatePicker",
              "type": "codeVoice"
            },
            {
              "text": " when you want to provide a view that allows the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "user to select multiple dates.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example creates a basic ",
              "type": "text"
            },
            {
              "code": "MultiDatePicker",
              "type": "codeVoice"
            },
            {
              "text": ", which appears as a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "calendar view representing the selected dates:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@State private var dates: Set<DateComponents> = []",
            "",
            "var body: some View {",
            "    MultiDatePicker(\"Dates Available\", selection: $dates)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can limit the ",
              "type": "text"
            },
            {
              "code": "MultiDatePicker",
              "type": "codeVoice"
            },
            {
              "text": " to specific ranges of dates",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "allowing selections only before or after a certain date or between two",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "dates. The following example shows a multi-date picker that only permits",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "selections within the 6th and (excluding) the 16th of December 2021",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "(in the ",
              "type": "text"
            },
            {
              "code": "UTC",
              "type": "codeVoice"
            },
            {
              "text": " time zone):",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@Environment(\\.calendar) var calendar",
            "@Environment(\\.timeZone) var timeZone",
            "",
            "var bounds: Range<Date> {",
            "    let start = calendar.date(from: DateComponents(",
            "        timeZone: timeZone, year: 2022, month: 6, day: 6))!",
            "    let end = calendar.date(from: DateComponents(",
            "        timeZone: timeZone, year: 2022, month: 6, day: 16))!",
            "    return start ..< end",
            "}",
            "",
            "@State private var dates: Set<DateComponents> = []",
            "",
            "var body: some View {",
            "    MultiDatePicker(\"Dates Available\", selection: $dates, in: bounds)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can also specify an alternative locale, calendar and time zone through",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "environment values. This can be useful when using a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/PreviewProvider",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "see how your multi-date picker behaves in environments that differ from",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "your own.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows a multi-date picker with a custom locale,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "calendar and time zone:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView_Previews: PreviewProvider {",
            "    static var previews: some View {",
            "        MultiDatePicker(\"Dates Available\", selection: .constant([]))",
            "            .environment(\\.locale, Locale.init(identifier: \"zh\"))",
            "            .environment(",
            "                \\.calendar, Calendar.init(identifier: .chinese))",
            "            .environment(\\.timeZone, TimeZone(abbreviation: \"HKT\")!)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
