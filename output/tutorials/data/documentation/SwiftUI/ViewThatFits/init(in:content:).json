      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A set of axes to constrain children to. The set may",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "contain ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Axis/horizontal",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Axis/vertical",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", or both of these.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "ViewThatFits",
                  "type": "codeVoice"
                },
                {
                  "text": " chooses the first child whose size fits within the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "proposed size on these axes. If ",
                  "type": "text"
                },
                {
                  "code": "axes",
                  "type": "codeVoice"
                },
                {
                  "text": " is an empty set,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "ViewThatFits",
                  "type": "codeVoice"
                },
                {
                  "text": " uses the first child view. By default,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "ViewThatFits",
                  "type": "codeVoice"
                },
                {
                  "text": " uses both axes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axes"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A view builder that provides the child views for this",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "container, in order of preference. The builder chooses the first",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "child view that fits within the proposed width, height, or both,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "as defined by ",
                  "type": "text"
                },
                {
                  "code": "axes",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
      ]
    }
