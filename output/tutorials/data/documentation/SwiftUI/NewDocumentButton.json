    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use a new document button to give people the option to create documents in your app.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "In the following example, there are two new document buttons, both support ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " labels.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "When the user taps or clicks the first button, the system creates a new document in the directory",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "currently open in the document browser. The second button creates a new document from a template.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@State private var isTemplatePickerPresented = false",
            "@State private var documentCreationContinuation:",
            "    CheckedContinuation<TextDocument?, any Error>?",
            "",
            "var body: some Scene {",
            "    DocumentGroupLaunchScene(\"My Documents\") {",
            "        NewDocumentButton(\"Start Writing…\")",
            "        NewDocumentButton(\"Choose a Template\", for: MyDocument.self) {",
            "            try await withCheckedThrowingContinuation { continuation in",
            "                documentCreationContinuation = continuation",
            "                isTemplatePickerPresented = true",
            "            }",
            "        }",
            "        .fullScreenCover(isPresented: $isTemplatePickerPresented) {",
            "            TemplatePicker(continuation: $documentCreationContinuation)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If you don’t provide a custom label, the system provides a button with the default “Create Document” label.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
