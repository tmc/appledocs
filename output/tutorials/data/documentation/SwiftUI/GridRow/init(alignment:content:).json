    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/VerticalAlignment",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for the row. If you",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "don’t specify a value, the row uses the vertical alignment component",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Alignment",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " parameter that you specify in the grid’s",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Grid/init(alignment:horizontalSpacing:verticalSpacing:content:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "initializer, which is ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/VerticalAlignment/center",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " by default.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alignment"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The builder closure that contains the child views. Each",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "view in the closure implicitly maps to a cell in the grid.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to create a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/GridRow",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " inside of a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Grid",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Provide a content closure that defines the cells of the row, and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "optionally customize the vertical alignment of content within each cell.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The following example customizes the vertical alignment of the cells",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in the first and third rows:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Grid(alignment: .trailing) {",
            "    GridRow(alignment: .top) { // Use top vertical alignment.",
            "        Text(\"Top\")",
            "        Color.red.frame(width: 1, height: 50)",
            "        Color.blue.frame(width: 50, height: 1)",
            "    }",
            "    GridRow { // Use the default (center) alignment.",
            "        Text(\"Center\")",
            "        Color.red.frame(width: 1, height: 50)",
            "        Color.blue.frame(width: 50, height: 1)",
            "    }",
            "    GridRow(alignment: .bottom) { // Use bottom vertical alignment.",
            "        Text(\"Bottom\")",
            "        Color.red.frame(width: 1, height: 50)",
            "        Color.blue.frame(width: 50, height: 1)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The example above specifies ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Alignment/trailing",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " alignment for the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "grid, which is composed of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/VerticalAlignment/center",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " vertical",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "alignment and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/HorizontalAlignment/trailing",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " horizontal alignment.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The middle row relies on the center vertical alignment, but the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "other two rows specify custom vertical alignments:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "GridRow-init-1-iOS",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A grid row behaves like a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Group",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " if you create it",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "outside of a grid.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        },
        {
          "inlineContent": [
            {
              "text": "To override column alignment, use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/gridColumnAlignment(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". To",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "override alignment for a single cell, use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/gridCellAnchor(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
