    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A rotate gesture tracks how a rotation event sequence changes. To",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "recognize a rotate gesture on a view, create and configure the gesture,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and then add it to the view using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/gesture(_:including:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Add a rotate gesture to a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Rectangle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and apply a rotation effect:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct RotateGestureView: View {",
            "    @State private var angle = Angle(degrees: 0.0)",
            "",
            "    var rotation: some Gesture {",
            "        RotateGesture()",
            "            .onChanged { value in",
            "                angle = value.rotation",
            "            }",
            "    }",
            "",
            "    var body: some View {",
            "        Rectangle()",
            "            .frame(width: 200, height: 200, alignment: .center)",
            "            .rotationEffect(angle)",
            "            .gesture(rotation)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
