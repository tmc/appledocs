    "doc://com.apple.SwiftUI/documentation/SwiftUI/View/draggable(_:)": {
      "abstract": [
        {
          "text": "Activates this view as the source of a drag and drop operation.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "keyword",
          "text": "func"
        },
        {
          "kind": "text",
          "text": " "
        },
        {
          "kind": "identifier",
          "text": "draggable"
