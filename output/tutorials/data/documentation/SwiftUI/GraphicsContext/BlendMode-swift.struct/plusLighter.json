    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This mode implements the equation ",
              "type": "text"
            },
            {
              "code": "R = MIN(1, S + D)",
              "type": "codeVoice"
            },
            {
              "text": " where",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "R",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is the composite image.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "S",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is the source image.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "D",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is the background.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
