        {
          "inlineContent": [
            {
              "text": "If a subsequent change occurs that creates additional animations on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "properties with ",
              "type": "text"
            },
            {
              "code": "logicallyComplete",
              "type": "codeVoice"
            },
            {
              "text": " completion callbacks registered,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "then those callbacks will fire when the animations from the change that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "they were registered with logically complete, ignoring the new",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "animations.",
              "type": "text"
