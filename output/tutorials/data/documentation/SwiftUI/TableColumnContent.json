    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This type provides the body content of the column, as well as the types",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of the column’s row values and the comparator used to sort rows.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can factor column content out into separate types or properties, or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by creating a custom type conforming to ",
              "type": "text"
            },
            {
              "code": "TableColumnContent",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var body: some View {",
            "    Table(people, selection: $selectedPeople, sortOrder: $sortOrder) {",
            "        nameColumns",
            "",
            "        TableColumn(\"Location\", value: \\.location) {",
            "            LocationView($0.location)",
            "        }",
            "    }",
            "}",
            "",
            "@TableColumnBuilder<Person, KeyPathComparator<Person>>",
            "private var nameColumns: some TableColumnContent<",
            "    Person, KeyPathComparator<Person>",
            "> {",
            "    TableColumn(\"First Name\", value: \\.firstName) {",
            "        PrimaryColumnView(person: $0)",
            "    }",
            "    TableColumn(\"Last Name\", value: \\.lastName)",
            "    TableColumn(\"Nickname\", value: \\.nickname)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The above example factors three table columns into a separate computed",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "property that has an opaque type. The property’s primary associated",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "type ",
              "type": "text"
            },
            {
              "code": "TableRowValue",
              "type": "codeVoice"
            },
            {
              "text": " is a ",
              "type": "text"
            },
            {
              "code": "Person",
              "type": "codeVoice"
            },
            {
              "text": " and its associated type",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "TableColumnSortComparator",
              "type": "codeVoice"
            },
            {
              "text": " is a key comparator for the ",
              "type": "text"
            },
            {
              "code": "Person",
              "type": "codeVoice"
            },
            {
              "text": " type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A type conforming to this protocol inherits ",
              "type": "text"
            },
            {
              "code": "@preconcurrency @MainActor",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "isolation from the protocol if the conformance is included in the type’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "base declaration:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyCustomType: Transition {",
            "    // `@preconcurrency @MainActor` isolation by default",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Isolation to the main actor is the default, but it’s not required. Declare",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the conformance in an extension to opt out of main actor isolation:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension MyCustomType: Transition {",
            "    // `nonisolated` by default",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
