      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Scene/defaultLaunchBehavior(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier to apply a value",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of this type to a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Scene",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " you specify in your ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/App",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". The value you",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "specify determines how the system will present the scene in the absense of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "any previously restored scenes on launch of your application.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, you may wish to present a welcome window on launch of your",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "app when there are no previous document windows being restored:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@main",
            "struct MyApp: App {",
            "    var body: some Scene {",
            "        DocumentGroup(newDocument: MyDocument()) { configuration in",
            "            DocumentEditor(configuration.$document)",
            "        }",
            "",
            "        Window(\"Welcome to My App\", id: \"welcome\") {",
            "            WelcomeView()",
            "        }",
            "        .defaultLaunchBehavior(.presented)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
