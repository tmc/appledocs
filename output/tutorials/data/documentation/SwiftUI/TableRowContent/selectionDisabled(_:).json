            {
              "inlineContent": [
                {
                  "text": "A Boolean value that determines whether users",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "can select this row.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isDisabled"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to control the selectability of views in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "selectable containers like ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/List",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Table",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". In the example,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "below, the user canâ€™t select the first item in the table.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@Binding var rows: [Item]",
            "@Binding var selection: Set<Item.ID>",
            "",
            "var body: some View {",
            "    Table(of: Item.self, selection: $selection) {",
            "        TableColumn(\"ID\", value: \\.id.uuidString)",
            "    } rows: {",
            "        ForEach(rows) { row in",
            "            TableRow(row)",
            "                .selectionDisabled(",
            "                    row.id == rows.first?.id",
            "                )",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can also use this modifier to specify the selectability of views",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "within a ",
              "type": "text"
            },
            {
              "code": "Picker",
              "type": "codeVoice"
            },
            {
              "text": ". The following example represents a flavor picker",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that disables selection on flavors that are unavailable.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Picker(\"Flavor\", selection: $selectedFlavor) {",
            "    ForEach(Flavor.allCases) { flavor in",
            "        Text(flavor.rawValue.capitalized)",
            "            .selectionDisabled(isSoldOut(flavor))",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
