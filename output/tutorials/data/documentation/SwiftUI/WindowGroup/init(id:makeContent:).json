              "inlineContent": [
                {
                  "text": "A string that uniquely identifies the window group. Identifiers",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "must be unique among the window groups in your app.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that creates the content for each instance",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of the group.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "makeContent"
        }
      ]
    },
    {
      "content": [
