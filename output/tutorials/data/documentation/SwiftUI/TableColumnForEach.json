    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "code": "TableColumnForEach",
              "type": "codeVoice"
            },
            {
              "text": " to create columns based on a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Swift/RandomAccessCollection",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of some data type. Either the collection’s elements must conform to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Swift/Identifiable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or you",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "need to provide an ",
              "type": "text"
            },
            {
              "code": "id",
              "type": "codeVoice"
            },
            {
              "text": " parameter to the ",
              "type": "text"
            },
            {
              "code": "TableColumnForEach",
              "type": "codeVoice"
            },
            {
              "text": " initializer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows the interface for an ",
              "type": "text"
            },
            {
              "code": "AudioSampleTrack",
              "type": "codeVoice"
            },
            {
              "text": ", which h",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as a collection of ",
              "type": "text"
            },
            {
              "code": "AudioSample",
              "type": "codeVoice"
            },
            {
              "text": " across a dynamic number of ",
              "type": "text"
            },
            {
              "code": "AudioChannel",
              "type": "codeVoice"
            },
            {
              "text": "s.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "Table",
              "type": "codeVoice"
            },
            {
              "text": " is created for displaying rows for each sample. It has one",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "static column for the sample’s timestamp and uses a ",
              "type": "text"
            },
            {
              "code": "TableColumnForEach",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "instance to produce a column for each of the audio channels in the track.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct AudioChannel: Identifiable {",
            "    let name: String",
            "    let id: UUID",
            "}",
            "",
            "struct AudioSample: Identifiable {",
            "    let id: UUID",
            "    let timestamp: TimeInterval",
            "    func level(channel: AudioChannel.ID) -> Double",
            "}",
            "",
            "@Observable",
            "class AudioSampleTrack {",
            "    let channels: [AudioChannel]",
            "    var samples: some RandomAccessCollection<AudioSample> { get }",
            "}",
            "",
            "struct ContentView: View {",
            "    var track: AudioSampleTrack",
            "",
            "    var body: some View {",
            "        Table(track.samples) {",
            "            TableColumn(\"Timestamp (ms)\") { sample in",
            "                Text(sample.timestamp, format: .number.scale(1000))",
            "                    .monospacedDigit()",
            "            }",
            "            TableColumnForEach(track.channels) { channel in",
            "                TableColumn(channel.name) { sample in",
            "                    Text(sample.level(channel: channel.id),",
            "                         format: .number.precision(.fractionLength(2))",
            "                    )",
            "                    .monospacedDigit()",
            "                }",
            "                .width(ideal: 70)",
            "                .alignment(.numeric)",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
