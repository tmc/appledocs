      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Generally generated from control flow constructs like ",
              "type": "text"
            },
            {
              "code": "ForEach",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "if",
              "type": "codeVoice"
            },
            {
              "text": ", and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "AccessibilityRotorEntry",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A type conforming to this protocol inherits ",
              "type": "text"
            },
            {
              "code": "@preconcurrency @MainActor",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "isolation from the protocol if the conformance is included in the type’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "base declaration:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyCustomType: Transition {",
            "    // `@preconcurrency @MainActor` isolation by default",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Isolation to the main actor is the default, but it’s not required. Declare",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the conformance in an extension to opt out of main actor isolation:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension MyCustomType: Transition {",
            "    // `nonisolated` by default",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
