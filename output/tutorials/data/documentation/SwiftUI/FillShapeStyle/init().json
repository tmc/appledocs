      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This shape style is appropriate for items situated on top of an existing",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "background color. It incorporates transparency to allow the background",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "color to show through.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use the primary version of this style to fill thin or small shapes, such",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as the track of a slider.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Use the secondary version of this style to fill medium-size shapes, such",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as the background of a switch.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Use the tertiary version of this style to fill large shapes, such as",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "input fields, search bars, or buttons.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Use the quaternary version of this style to fill large areas that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "contain complex content, such as an expanded table cell.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
