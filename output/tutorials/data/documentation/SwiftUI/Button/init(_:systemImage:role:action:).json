      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key for the button’s localized title, that describes",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the purpose of the button’s ",
                  "type": "text"
                },
                {
                  "code": "action",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "titleKey"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the image resource to lookup.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "systemImage"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional semantic role describing the button. A value of",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " means that the button doesn’t have an assigned role.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "role"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The action to perform when the user triggers the button.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This initializer creates a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Label",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view on your behalf, and treats the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "localized key similar to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text/init(_:tableName:bundle:comment:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". See",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for more information about localizing strings.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
