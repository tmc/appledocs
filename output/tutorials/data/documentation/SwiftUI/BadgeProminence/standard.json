    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This level of prominence should be used for badges that display a value",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that suggests user action, such as a count of unread messages or new",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "invitations.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In lists on macOS, this results in a badge label on a grayscale platter;",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and in lists on iOS, this prominence of badge has no platter.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "List(mailboxes) { mailbox in",
            "    Text(mailbox.name)",
            "        .badge(mailbox.numberOfUnreadMessages)",
            "}",
            ".badgeProminence(.standard)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
