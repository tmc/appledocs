    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The stack is “lazy,” in that the stack view doesn’t create items until",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "it needs to render them onscreen.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "In the following example, a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ScrollView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " contains a ",
              "type": "text"
            },
            {
              "code": "LazyVStack",
              "type": "codeVoice"
            },
            {
              "text": " that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "consists of a vertical row of text views. The stack aligns to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "leading edge of the scroll view, and uses default spacing between the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "text views.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ScrollView {",
            "    LazyVStack(alignment: .leading) {",
            "        ForEach(1...100, id: \\.self) {",
            "            Text(\"Row \\($0)\")",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
