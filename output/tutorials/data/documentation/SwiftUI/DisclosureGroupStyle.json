    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To configure the disclosure group style for a view hierarchy, use the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/disclosureGroupStyle(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "To create a custom disclosure group style, declare a type that conforms",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to ",
              "type": "text"
            },
            {
              "code": "DisclosureGroupStyle",
              "type": "codeVoice"
            },
            {
              "text": ". Implement the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/DisclosureGroupStyle/makeBody(configuration:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method to return a view",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that composes the elements of the ",
              "type": "text"
            },
            {
              "code": "configuration",
              "type": "codeVoice"
            },
            {
              "text": " that SwiftUI provides to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "your method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyDisclosureStyle: DisclosureGroupStyle {",
            "    func makeBody(configuration: Configuration) -> some View {",
            "        VStack {",
            "            Button {",
            "                withAnimation {",
            "                    configuration.isExpanded.toggle()",
            "                }",
            "            } label: {",
            "                HStack(alignment: .firstTextBaseline) {",
            "                    configuration.label",
            "                    Spacer()",
            "                    Text(configuration.isExpanded ? \"hide\" : \"show\")",
            "                        .foregroundColor(.accentColor)",
            "                        .font(.caption.lowercaseSmallCaps())",
            "                        .animation(nil, value: configuration.isExpanded)",
            "                }",
            "                .contentShape(Rectangle())",
            "            }",
            "            .buttonStyle(.plain)",
            "            if configuration.isExpanded {",
            "                configuration.content",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "A type conforming to this protocol inherits ",
              "type": "text"
            },
            {
              "code": "@preconcurrency @MainActor",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "isolation from the protocol if the conformance is included in the type’s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "base declaration:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyCustomType: Transition {",
            "    // `@preconcurrency @MainActor` isolation by default",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Isolation to the main actor is the default, but it’s not required. Declare",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the conformance in an extension to opt out of main actor isolation:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension MyCustomType: Transition {",
            "    // `nonisolated` by default",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
