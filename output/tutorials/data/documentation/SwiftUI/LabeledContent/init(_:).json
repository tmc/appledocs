        },
        {
          "inlineContent": [
            {
              "text": "You can use this initializer within the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/LabeledContentStyle/makeBody(configuration:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method of a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/LabeledContentStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to create a labeled content instance.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "This is useful for custom styles that only modify the current style,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as opposed to implementing a brand new style.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, the following style adds a red border around the labeled",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "content, but otherwise preserves the current style:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct RedBorderLabeledContentStyle: LabeledContentStyle {",
            "    func makeBody(configuration: Configuration) -> some View {",
            "        LabeledContent(configuration)",
            "            .border(.red)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
