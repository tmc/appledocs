          "content": [
            {
              "inlineContent": [
                {
                  "text": "The clipping shape to use for the view. The ",
                  "type": "text"
                },
                {
                  "code": "shape",
                  "type": "codeVoice"
                },
                {
                  "text": " fills",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the view’s frame, while maintaining its aspect ratio.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shape"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The fill style to use when rasterizing ",
                  "type": "text"
                },
                {
                  "code": "shape",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
