    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A rename button receives its action from the environment. Use the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/renameAction(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier to set the action. The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "system disables the button if you don’t define an action.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct RowView: View {",
            "    @State private var text = \"\"",
            "    @FocusState private var isFocused: Bool",
            "",
            "    var body: some View {",
            "        TextField(text: $item.name) {",
            "            Text(\"Prompt\")",
            "        }",
            "        .focused($isFocused)",
            "        .contextMenu {",
            "            RenameButton()",
            "            // ... your own custom actions",
            "        }",
            "        .renameAction { $isFocused = true }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When someone taps the rename button in the context menu, the rename",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "action focuses the text field by setting the ",
              "type": "text"
            },
            {
              "code": "isFocused",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "property to true.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this button inside of a navigation title menu and the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "navigation title modifier automatically configures the environment",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with the appropriate rename action.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ContentView()",
            "    .navigationTitle($contentTitle) {",
            "        // ... your own custom actions",
            "        RenameButton()",
            "    }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
