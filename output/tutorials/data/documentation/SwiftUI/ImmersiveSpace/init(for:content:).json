        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An immersive space content builder that defines the content",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "for each instance of the immersive space. The closure receives a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "binding to the value that you pass to the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/EnvironmentValues/openImmersiveSpace",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " action when you call that",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "action to open an immersive space. The system automatically persists",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "and restores the value of this binding during state restoration.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
