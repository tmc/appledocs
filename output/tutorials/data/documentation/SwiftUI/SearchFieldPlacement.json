            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/searchable(text:placement:prompt:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var body: some View {",
            "    NavigationView {",
            "        PrimaryView()",
            "        SecondaryView()",
            "        Text(\"Select a primary and secondary item\")",
            "    }",
            "    .searchable(text: $text, placement: .sidebar)",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
