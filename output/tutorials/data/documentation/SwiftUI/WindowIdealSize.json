    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this type in conjunction with the ",
              "type": "text"
            },
            {
              "code": "Scene.windowIdealSize(_:)",
              "type": "codeVoice"
            },
            {
              "text": " modifier",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to override the default behavior for how windows behave when performing a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "zoom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, you can define a window group where the window has an ideal",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "width of 800 points and an ideal height of 600 points:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyApp: App {",
            "    var body: some Scene {",
            "        WindowGroup {",
            "            ContentView()",
            "                .frame(idealWidth: 800, idealHeight: 600)",
            "        }",
            "        .windowIdealSize(.fitToContent)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
