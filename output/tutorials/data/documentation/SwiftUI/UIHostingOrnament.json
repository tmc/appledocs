    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use a ",
              "type": "text"
            },
            {
              "code": "UIHostingOrnament",
              "type": "codeVoice"
            },
            {
              "text": " when you want to add ornaments to a UIKit",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view controller.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "For example, the following adds a single bottom ornament to the current",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view controller:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "self.ornaments = [",
            "    UIHostingOrnament(sceneAnchor: .bottom) {",
            "        OrnamentContent()",
            "    }",
            "]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
