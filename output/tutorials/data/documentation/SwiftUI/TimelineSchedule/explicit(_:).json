    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The sequence of dates at which a timeline view",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "updates. Use a monotonically increasing sequence of dates,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "and ensure that at least one is in the future.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dates"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Initialize a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TimelineView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with an explicit timeline schedule when",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "you want to schedule view updates at particular points in time:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let dates = [",
            "    Date(timeIntervalSinceNow: 10), // Update ten seconds from now,",
            "    Date(timeIntervalSinceNow: 12) // and a few seconds later.",
            "]",
            "",
            "struct MyView: View {",
            "    var body: some View {",
            "        TimelineView(.explicit(dates)) { context in",
            "            Text(context.date.description)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The timeline view updates its content on exactly the dates that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "you specify, until it runs out of dates, after which it stops changing.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "If the dates you provide are in the past, the timeline view updates",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "exactly once with the last entry. If you only provide dates in the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "future, the timeline view renders with the current date until the first",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "date arrives. If you provide one or more dates in the past and one or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "more in the future, the view renders the most recent past date,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "refreshing normally on all subsequent dates.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
