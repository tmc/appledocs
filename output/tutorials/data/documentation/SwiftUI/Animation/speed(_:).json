    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Setting the speed of an animation changes the duration of the animation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by a factor of ",
              "type": "text"
            },
            {
              "code": "speed",
              "type": "codeVoice"
            },
            {
              "text": ". A higher speed value causes a faster animation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "sequence due to a shorter duration. For example, a one-second animation",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with a speed of ",
              "type": "text"
            },
            {
              "code": "2.0",
              "type": "codeVoice"
            },
            {
              "text": " completes in half the time (half a second).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    @State private var adjustBy = 100.0",
            "",
            "    private var oneSecondAnimation: Animation {",
            "       .easeInOut(duration: 1.0)",
            "    }",
            "",
            "    var body: some View {",
            "        VStack(spacing: 40) {",
            "            HStack(alignment: .bottom) {",
            "                Capsule()",
            "                    .frame(width: 50, height: 175 - adjustBy)",
            "                Capsule()",
            "                    .frame(width: 50, height: 175 + adjustBy)",
            "            }",
            "            .animation(oneSecondAnimation.speed(2.0), value: adjustBy)",
            "",
            "            Button(\"Animate\") {",
            "                adjustBy *= -1",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "identifier": "animation-18-speed.mp4",
          "type": "video"
        },
        {
          "inlineContent": [
            {
              "text": "Setting ",
              "type": "text"
            },
            {
              "code": "speed",
              "type": "codeVoice"
            },
            {
              "text": " to a lower number slows the animation, extending its",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "duration. For example, a one-second animation with a speed of ",
              "type": "text"
            },
            {
              "code": "0.25",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "takes four seconds to complete.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    @State private var adjustBy = 100.0",
            "",
            "    private var oneSecondAnimation: Animation {",
            "       .easeInOut(duration: 1.0)",
            "    }",
            "",
            "    var body: some View {",
            "        VStack(spacing: 40) {",
            "            HStack(alignment: .bottom) {",
            "                Capsule()",
            "                    .frame(width: 50, height: 175 - adjustBy)",
            "                Capsule()",
            "                    .frame(width: 50, height: 175 + adjustBy)",
            "            }",
            "            .animation(oneSecondAnimation.speed(0.25), value: adjustBy)",
            "",
            "            Button(\"Animate\") {",
            "                adjustBy *= -1",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
