      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The duration after which the animation should  report",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that it is logically complete.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "duration"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "An animation that reports logical completion after the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "given duration.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Note that the indicated duration will not cause the animation to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "continue running after the base animation has fully completed.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If the animation is removed before the given duration is reached,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "logical completion will be reported immediately.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
