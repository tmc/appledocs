      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The perceptual duration, which defines the pace of the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "spring. This is approximately equal to the settling duration, but",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "for very bouncy springs, will be the duration of the period of",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "oscillation for the spring.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "duration"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "How much additional bounce should be added to the base",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "bounce of 0.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "extraBounce"
        }
      ]
    }
