    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To store a document as a reference type — like a class — create a type",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "that conforms to the ",
              "type": "text"
            },
            {
              "code": "ReferenceFileDocument",
              "type": "codeVoice"
            },
            {
              "text": " protocol and implement the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "required methods and properties. Your implementation:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Provides a list of the content types that the document can read from and",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "write to by defining ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ReferenceFileDocument/readableContentTypes",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ". If the list of content",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "types that the document can write to is different from those that it reads",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "from, you can optionally also define ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ReferenceFileDocument/writableContentTypes",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Loads documents from file in the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ReferenceFileDocument/init(configuration:)",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " initializer.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Stores documents to file by providing a snapshot of the document’s",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "content in the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ReferenceFileDocument/snapshot(contentType:)",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " method, and then serializing",
                      "type": "text"
                    },
                    {
                      "text": " ",
                      "type": "text"
                    },
                    {
                      "text": "that content in the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ReferenceFileDocument/fileWrapper(snapshot:configuration:)",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " method.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "Ensure that types that conform to this protocol are ",
              "type": "text"
            },
            {
              "code": "Sendable",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "In particular, SwiftUI calls the protocol’s methods from different isolation domains.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Don’t perform serialization and deserialization on ",
              "type": "text"
            },
            {
              "code": "MainActor",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "final class PDFDocument: ReferenceFileDocument {",
            "    struct Storage {",
            "        var contents: Data",
            "    }",
            "",
            "    static let readableContentTypes: [UTType] = [.pdf]",
            "    let storage: OSAllocatedUnfairLock<Storage>",
            "",
            "    required init(configuration: ReadConfiguration) throws {",
            "       guard let data = configuration.file.regularFileContents else {",
            "           throw CocoaError(.fileReadCorruptFile)",
            "       }",
            "        self.storage = .init(initialState: .init(contents: data))",
            "    }",
            "",
            "    func snapshot(contentType: UTType) throws -> Data {",
            "        storage.withLock { $0.contents }",
            "    }",
            "",
            "    func fileWrapper(snapshot: Data, configuration: WriteConfiguration) throws -> FileWrapper {",
            "        return FileWrapper(regularFileWithContents: snapshot)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If you store your document as a value type — like a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "structure — use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/FileDocument",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " instead.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
