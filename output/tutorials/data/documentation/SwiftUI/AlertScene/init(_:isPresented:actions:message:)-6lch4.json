      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key for the localized string that is the title of the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "alert",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "titleKey"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding to a Boolean value that determines whether to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "present the alert. When the user presses or taps one of the alert’s",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "actions, the system sets this value to ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " and dismisses.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isPresented"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A view builder returning the actions for the dialog.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "actions"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A view builder returning the message for the dialog.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "message"
        }
      ]
    }
