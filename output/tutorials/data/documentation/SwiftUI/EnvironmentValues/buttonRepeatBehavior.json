        {
          "inlineContent": [
            {
              "text": "A value of ",
              "type": "text"
            },
            {
              "code": "enabled",
              "type": "codeVoice"
            },
            {
              "text": " means that buttons will be able to repeatedly",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "trigger their action, and ",
              "type": "text"
            },
            {
              "code": "disabled",
              "type": "codeVoice"
            },
            {
              "text": " means they should not. A value of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "automatic",
              "type": "codeVoice"
            },
            {
              "text": " means that buttons will defer to default behavior.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
