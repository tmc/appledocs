    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Search suggestions render based on the platform and surrounding context",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in which you place the searchable modifier containing suggestions.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "You can render search suggestions in two ways:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "In a menu attached to the search field.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Inline with the main content of the app.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "You find the current search suggestion placement by querying the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/EnvironmentValues/searchSuggestionsPlacement",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " in your",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "search suggestions.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "enum FruitSuggestion: String, Identifiable {",
            "    case apple, banana, orange",
            "    var id: Self { self }",
            "}",
            "",
            "@State private var text: String = \"\"",
            "@State private var suggestions: [FruitSuggestion] = []",
            "",
            "var body: some View {",
            "    MainContent()",
            "        .searchable(text: $text) {",
            "            FruitSuggestions(suggestions: suggestions)",
            "        }",
            "}",
            "",
            "struct FruitSuggestions: View {",
            "    var suggestions: [FruitSuggestion]",
            "",
            "    @Environment(\\.searchSuggestionsPlacement)",
            "    private var placement",
            "",
            "    var body: some View {",
            "        if shouldRender {",
            "            ForEach(suggestions) { suggestion in",
            "                Text(suggestion.rawValue.capitalized)",
            "                    .searchCompletion(suggestion.rawValue)",
            "            }",
            "        }",
            "    }",
            "",
            "    var shouldRender: Bool {",
            "        #if os(iOS)",
            "        placement == .menu",
            "        #else",
            "        true",
            "        #endif",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In the above example, search suggestions only render in iOS",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "if the searchable modifier displays them in a menu. You might want",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to do this to render suggestions in your own list alongside",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "your own search results when they would render in a list.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
