    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Read this environment value from within a view to obtain the preferred",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "visibility for labels within the hierarchy. If you would like to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "dynamically hide the label of your custom view, make sure to include an",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "accessibility label via the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/accessibilityLabel(content:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier as illustrated below:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@Environment(\\.labelsVisibility)",
            "private var labelsVisibility",
            "",
            "var body: some View {",
            "    VStack {",
            "        QuizCardView()",
            "        if labelsVisibility != .hidden {",
            "            label",
            "        }",
            "    }",
            "    .accessibilityLabel {",
            "        label",
            "    }",
            "}",
            "",
            "private var label: some View {",
            "    Text(\"Quiz Card\")",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
