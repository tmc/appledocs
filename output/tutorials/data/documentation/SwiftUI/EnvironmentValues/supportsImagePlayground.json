      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The value of this property is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " when the current device",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "supports image generation. A device might not support this",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "feature if the device or system doesnâ€™t meet the hardware",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "requirements or the necessary environment (for example runs",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in a supported language) to generate the images.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Read this property from the environment to determine if your app can",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "use the ",
              "type": "text"
            },
            {
              "code": "imagePlaygroundSheet",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ImageGenerationPresentingView: View {",
            "    @Environment(\\.supportsImagePlayground) private var supportsImagePlayground",
            "    @State private var showsImagePlaygroundSheet = false",
            "",
            "    var body: some View {",
            "        Button(\"Open Generation Sheet\") {",
            "            showsImagePlaygroundSheet = true",
            "        }",
            "        .disabled(!supportsImagePlayground)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
