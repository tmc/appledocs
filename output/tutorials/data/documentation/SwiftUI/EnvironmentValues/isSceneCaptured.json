    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this value to determine whether the scene is actively being cloned",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to another destination (like during AirPlay) or is being mirrored or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "recorded.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Your app can respond to changes in this value to take appropriate action,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "like obscuring content.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
