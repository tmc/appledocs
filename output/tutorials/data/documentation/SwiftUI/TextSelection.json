    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A selection is either an insertion point (e.g. a cursor in the text), a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "selection over a range of text or on macOS, multiple selections.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This is frequently used to represent selection of text in a ",
              "type": "text"
            },
            {
              "code": "TextField",
              "type": "codeVoice"
            },
            {
              "text": " or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "TextEditor",
              "type": "codeVoice"
            },
            {
              "text": ". The following example shows a text editor that leverages",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "text selection to offer live suggestions based on the current selection.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct SuggestionTextEditor: View {",
            "    @State var text: String = \"\"",
            "    @State var selection: TextSelection? = nil",
            "",
            "    var body: some View {",
            "        VStack {",
            "            TextEditor(text: $text, selection: $selection)",
            "            // A helper view that offers live suggestions based on selection.",
            "            SuggestionsView(",
            "                substrings: getSubstrings(text: text, indices: selection?.indices))",
            "        }",
            "    }",
            "",
            "    private func getSubstrings(",
            "        text: String, indices: TextSelection.Indices?",
            "    ) -> [Substring] {",
            "        // Resolve substrings representing the current selection...",
            "    }",
            "}",
            "",
            "struct SuggestionsView: View { ... }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can also use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/textSelectionAffinity(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier to specify",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a selection affinity on the given hierarchy:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct SuggestionTextEditor: View {",
            "    @State var text: String = \"\"",
            "    @State var selection: TextSelection? = nil",
            "",
            "    var body: some View {",
            "        VStack {",
            "            TextEditor(text: $text, selection: $selection)",
            "            // A helper view that offers live suggestions based on selection.",
            "            SuggestionsView(",
            "                substrings: getSubstrings(text: text, indices: selection?.indices))",
            "        }",
            "        .textSelectionAffinity(.upstream)",
            "    }",
            "",
            "    private func getSubstrings(",
            "        text: String, indices: TextSelection.Indices?",
            "    ) -> [Substring] {",
            "        // Resolve substrings representing the current selection...",
            "    }",
            "}",
            "",
            "struct SuggestionsView: View { ... }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
