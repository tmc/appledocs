    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "To support multiple types of widgets, add the ",
              "type": "text"
            },
            {
              "code": "@main",
              "type": "codeVoice"
            },
            {
              "text": " attribute to a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "structure that conforms to ",
              "type": "text"
            },
            {
              "code": "WidgetBundle",
              "type": "codeVoice"
            },
            {
              "text": ". For example, a game might have",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "one widget to display summary information about the game and a second",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "widget to display detailed information about individual characters.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@main",
            "struct GameWidgets: WidgetBundle {",
            "   var body: some Widget {",
            "       GameStatusWidget()",
            "       CharacterDetailWidget()",
            "   }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
