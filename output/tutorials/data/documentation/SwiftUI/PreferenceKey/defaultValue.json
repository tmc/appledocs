        {
          "inlineContent": [
            {
              "text": "Views that have no explicit value for the key produce this default",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value. Combining child views may remove an implicit value produced by",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using the default. This means that ",
              "type": "text"
            },
            {
              "code": "reduce(value: &x, nextValue: {defaultValue})",
              "type": "codeVoice"
            },
            {
              "text": " shouldnâ€™t change the meaning of ",
              "type": "text"
            },
            {
              "code": "x",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
