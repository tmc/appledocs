      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional display name for the preview. If you don’t specify a",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "name, the canvas labels the preview using the line number where the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "preview appears in source.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/DeveloperToolsSupport/PreviewTrait",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "instance that customizes the appearance of the preview.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "traits"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Optional additional traits that further customize",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the preview.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "additionalTraits"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ViewBuilder",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that produces a SwiftUI view to preview. You",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "typically specify one of your app’s custom views and optionally any",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "inputs, model data, modifiers, and enclosing views that the custom",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "view needs for normal operation.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
