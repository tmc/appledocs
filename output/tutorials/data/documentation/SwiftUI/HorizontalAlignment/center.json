          "inlineContent": [
            {
              "text": "Use this guide to align the centers of views:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "HorizontalAlignment-center-1-iOS",
              "type": "image"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following code generates the image above using a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/VStack",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ":",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct HorizontalAlignmentCenter: View {",
            "    var body: some View {",
            "        VStack(alignment: .center, spacing: 0) {",
            "            Color.red.frame(width: 1)",
            "            Text(\"Center\").font(.title).border(.gray)",
            "            Color.red.frame(width: 1)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
