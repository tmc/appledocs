    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique identifier for this placement.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "On macOS, accessory bars are in a section below the title bar and",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "toolbar area of the window. Each separate identifier will correspond to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a separate accessory bar that is added to this area.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use a custom placement to control the appearance of the containing bar",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "for items using a custom ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ToolbarItemPlacement",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with the same",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "identifier.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "private let favoritesBarID = \"com.example.favoritesBar\"",
            "extension ToolbarItemPlacement {",
            "    static let favoritesBar = accessoryBar(id: favoritesBarID)",
            "}",
            "extension ToolbarPlacement {",
            "    static let favoritesBar = accessoryBar(id: favoritesBarID)",
            "}",
            "...",
            "BrowserView()",
            "    .toolbar {",
            "        ToolbarItem(placement: .favoritesBar) {",
            "            FavoritesBar()",
            "        }",
            "    }",
            "    .toolbar(.hidden, for: .favoritesBar)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
