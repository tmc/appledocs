    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You will rarely, if ever, need to create an ",
              "type": "text"
            },
            {
              "code": "EmptyView",
              "type": "codeVoice"
            },
            {
              "text": " directly. Instead,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "EmptyView",
              "type": "codeVoice"
            },
            {
              "text": " represents the absence of a view.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "SwiftUI uses ",
              "type": "text"
            },
            {
              "code": "EmptyView",
              "type": "codeVoice"
            },
            {
              "text": " in situations where a SwiftUI view type defines one",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or more child views with generic parameters, and allows the child views to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "be absent. When absent, the child view’s type in the generic type parameter",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is ",
              "type": "text"
            },
            {
              "code": "EmptyView",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example creates an indeterminate ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ProgressView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " without",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a label. The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ProgressView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " type declares two generic parameters,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Label",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "CurrentValueLabel",
              "type": "codeVoice"
            },
            {
              "text": ", for the types used by its subviews.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "When both subviews are absent, like they are here, the resulting type is",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "ProgressView<EmptyView, EmptyView>",
              "type": "codeVoice"
            },
            {
              "text": ", as indicated by the example’s output:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let progressView = ProgressView()",
            "print(\"\\(type(of:progressView))\")",
            "// Prints: ProgressView<EmptyView, EmptyView>"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
