    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The gradient maps its coordinate space to the unit space square",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in which its center and radii are defined, then stretches that",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "square to fill its bounding rect, possibly also stretching the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "circular gradient to have elliptical contours.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example, an elliptical gradient centered on the view, filling",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "its bounds:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "EllipticalGradient(gradient: .init(colors: [.red, .yellow]))"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When using an elliptical gradient as a shape style, you can also use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ShapeStyle/ellipticalGradient(_:center:startRadiusFraction:endRadiusFraction:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
