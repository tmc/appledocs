      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Another group of colors–standard colors, like ",
              "type": "text"
            },
            {
              "code": "Color.mint",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and semantic colors, like ",
              "type": "text"
            },
            {
              "code": "Color.accentColor",
              "type": "codeVoice"
            },
            {
              "text": "–are rendered on screen",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "differently depending on the current ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Environment",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". For transferring,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "they are resolved against the default environment and might produce",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a slightly different result at the destination if the source of drag",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or copy uses a non-default environment.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
