    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Alert scenes are not attached to any particular window, and present",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "themselves in the center of the current display. The dialog must be",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "dismissed before any further interaction with the app is permitted.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@main",
            "struct MyApp: App {",
            "    @State var showLoginAlert = true",
            "    @State var loggedIn = false",
            "",
            "    var body: some Scene {",
            "        Window(\"Welcome User Window\", id:\"WelcomeWindow\") {",
            "            ...",
            "        }",
            "        .defaultLaunchBehavior(loggedIn ? .presented : .suppressed)",
            "",
            "        AlertScene(\"Login Required\", isPresented: $showLoginAlert) {",
            "            Button(\"OK\") {",
            "                ...",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "All actions present in the ViewBuilder will dismiss the alert. Like the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "alert modifier, you can determine the role of the buttons with ",
              "type": "text"
            },
            {
              "code": ".cancel",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "or ",
              "type": "text"
            },
            {
              "code": ".destructive",
              "type": "codeVoice"
            },
            {
              "text": ". If no actions are present, we will automatically include",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "an OK button for dismissal.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
