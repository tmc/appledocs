    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer within the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/GroupBoxStyle/makeBody(configuration:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "method of a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/GroupBoxStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instance to create a styled group box,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with customizations, while preserving its existing style.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example adds a pink border around the group box,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "without overriding its current style:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct PinkBorderGroupBoxStyle: GroupBoxStyle {",
            "    func makeBody(configuration: Configuration) -> some View {",
            "        GroupBox(configuration)",
            "            .border(Color.pink)",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
