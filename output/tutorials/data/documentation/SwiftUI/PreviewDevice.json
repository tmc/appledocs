    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Create a preview device by name, like “iPhone X”, or by model number,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "like “iPad8,1”. Use the device in a call to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/previewDevice(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "modifier to set a preview device that doesn’t change when you change the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "run destination in Xcode:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct CircleImage_Previews: PreviewProvider {",
            "    static var previews: some View {",
            "        CircleImage()",
            "            .previewDevice(PreviewDevice(rawValue: \"iPad Pro (11-inch)\"))",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can get a list of supported preview device names by using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "xcrun",
              "type": "codeVoice"
            },
            {
              "text": " command in the Terminal app:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "% xcrun simctl list devicetypes"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Additionally, you can use the following values for macOS platform",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "development:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "“Mac”",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "“Mac Catalyst”",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
