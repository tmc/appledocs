    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that describes the purpose of the toggle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "title"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the image resource to lookup.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "image"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A collection of values used as the source for rendering",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the Toggle’s state.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sources"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key path of the values that determines whether the toggle",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "is on, mixed or off.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isOn"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This initializer creates a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view on your behalf. See ",
              "type": "text"
            },
            {
              "code": "Text",
              "type": "codeVoice"
            },
            {
              "text": " for more",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "information about localizing strings.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example creates a single toggle that represents",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the state of multiple alarms:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Alarm: Hashable, Identifiable {",
            "    var id = UUID()",
            "    var isOn = false",
            "    var name = \"\"",
            "}",
            "",
            "@State private var alarms = [",
            "    Alarm(isOn: true, name: \"Morning\"),",
            "    Alarm(isOn: false, name: \"Evening\")",
            "]",
            "",
            "Toggle(\"Enable all alarms\", sources: $alarms, isOn: \\.isOn)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
