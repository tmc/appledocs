    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The interval between where to run the timer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timerInterval"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If present, the date at which to pause the timer.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The default is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " which indicates to never pause.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pauseTime"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether to count up or down. The default is ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "countsDown"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether to include an hours component if there are",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "more than 60 minutes left on the timer. The default is ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "showsHours"
        }
      ]
    },
