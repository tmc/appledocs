    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Link",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to create a control that your app uses to navigate to a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "URL that you provide. The example below creates a link to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "example.com",
              "type": "codeVoice"
            },
            {
              "text": " and uses ",
              "type": "text"
            },
            {
              "code": "Visit Example Co",
              "type": "codeVoice"
            },
            {
              "text": " as the title key to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "generate a link-styled view in your app:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Link(\"Visit Example Co\",",
            "      destination: URL(string: \"https://www.example.com/\")!)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
