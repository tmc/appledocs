      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The approximate time it will take for the spring",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to come to rest.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "settlingDuration"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount of drag applied as a fraction of the amount",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "needed to produce critical damping.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dampingRatio"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The threshhold for how small all subsequent values need to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "be before the spring is considered to have settled.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "epsilon"
        }
      ]
    }
