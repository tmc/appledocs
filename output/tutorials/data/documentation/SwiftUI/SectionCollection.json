    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Sections are constructed lazily, on demand, so access only as much",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "of this collection as is necessary to create the resulting content.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can get access to a viewâ€™s ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/SectionCollection",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " by using the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "Group/init(sectionsOf:transform:)",
              "type": "codeVoice"
            },
            {
              "text": " initializer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Any content of the given view which is not explicitly specified as a section",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "is grouped with its sibling content to form implicit sections, meaning the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "minimum number of sections in a ",
              "type": "text"
            },
            {
              "code": "SectionCollection",
              "type": "codeVoice"
            },
            {
              "text": " is one.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
