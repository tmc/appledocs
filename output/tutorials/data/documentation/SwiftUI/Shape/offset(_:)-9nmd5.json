    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount, in points, by which you offset the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "shape. Negative numbers are to the left and up; positive numbers are",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "to the right and down.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "offset"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A shape offset by the specified amount.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example renders two circles. It places one circle at its",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "default position. The second circle is outlined with a stroke,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "positioned on top of the first circle and offset by 100 points to the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "left and 50 points below.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Circle()",
            ".overlay(",
            "    Circle()",
            "    .offset(CGPoint(x: -100, y: 50))",
            "    .stroke()",
            ")"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
