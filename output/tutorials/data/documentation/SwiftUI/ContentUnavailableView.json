    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "It is recommended to use ",
              "type": "text"
            },
            {
              "code": "ContentUnavailableView",
              "type": "codeVoice"
            },
            {
              "text": " in situations where a viewâ€™s",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "content cannot be displayed. That could be caused by a network error, a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "list without items, a search that returns no results etc.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You create an ",
              "type": "text"
            },
            {
              "code": "ContentUnavailableView",
              "type": "codeVoice"
            },
            {
              "text": " in its simplest form, by providing a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "label and some additional content such as a description or a call to action:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "ContentUnavailableView {",
            "    Label(\"No Mail\", systemImage: \"tray.fill\")",
            "} description: {",
            "    Text(\"New mails you receive will appear here.\")",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The system provides default ",
              "type": "text"
            },
            {
              "code": "ContentUnavailableView",
              "type": "codeVoice"
            },
            {
              "text": "s that you can use in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "specific situations. The example below illustrates the usage of the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/ContentUnavailableView/search",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct ContentView: View {",
            "    @ObservedObject private var viewModel = ContactsViewModel()",
            "",
            "    var body: some View {",
            "        NavigationStack {",
            "            List {",
            "                ForEach(viewModel.searchResults) { contact in",
            "                    NavigationLink {",
            "                        ContactsView(contact)",
            "                    } label: {",
            "                        Text(contact.name)",
            "                    }",
            "                }",
            "            }",
            "            .navigationTitle(\"Contacts\")",
            "            .searchable(text: $viewModel.searchText)",
            "            .overlay {",
            "                if searchResults.isEmpty {",
            "                    ContentUnavailableView.search",
            "                }",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
