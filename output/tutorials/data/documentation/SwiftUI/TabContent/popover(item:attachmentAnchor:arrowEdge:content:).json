    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A binding to an optional source of truth for the popover.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "When ",
                  "type": "text"
                },
                {
                  "code": "item",
                  "type": "codeVoice"
                },
                {
                  "text": " is non-",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system passes the contents to",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the modifierâ€™s closure. You use this content to populate the fields",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "of a popover that you create that the system displays to the user.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "item",
                  "type": "codeVoice"
                },
                {
                  "text": " changes, the system dismisses the currently presented",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "popover and replaces it with a new popover using the same process.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "item"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The positioning anchor that defines the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "attachment point of the popover. The default is",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Anchor/Source/bounds",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "attachmentAnchor"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The edge of the ",
                  "type": "text"
                },
                {
                  "code": "attachmentAnchor",
                  "type": "codeVoice"
                },
                {
                  "text": " that defines the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "location of the popoverâ€™s arrow in macOS. The default is ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Edge/top",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "arrowEdge"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure returning the content of the popover.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "content"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method when you need to present a popover with content",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "from a custom data source. The example below uses data in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the ",
              "type": "text"
            },
            {
              "code": "PopoverModel",
              "type": "codeVoice"
            },
            {
              "text": " structure to populate the view in the ",
              "type": "text"
            },
            {
              "code": "content",
              "type": "codeVoice"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "closure that the popover displays to the user:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct PopoverExample: View {",
            "    @State private var popover: PopoverModel?",
            "",
            "    var body: some View {",
            "        TabView {",
            "            Tab(\"Popover Anchor\", systemImage: \"arrow.down\") {",
            "                Button(\"Show Popover\") {",
            "                    popover = PopoverModel(message: \"Custom Message\")",
            "                }",
            "            }",
            "            .popover(item: $popover) { detail in",
            "                 Text(\"\\(detail.message)\")",
            "                    .padding()",
            "             }",
            "        }",
            "    }",
            "}",
            "",
            "struct PopoverModel: Identifiable {",
            "    var id: String { message }",
            "    let message: String",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
