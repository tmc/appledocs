    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The identifier to associate with a tab or section.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "id"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The identifier needs to be stable, including across app version",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "updates.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Only the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabViewStyle/sidebarAdaptable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " style supports",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "supports customization. To enable customization, attach",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabViewCustomization",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " using",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/tabViewCustomization(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "All tabs and tab sections that support customization are required to have",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a customization ID. You can mark a tab as being non-customizable",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "by specifying a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabCustomizationBehavior/disabled",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " behavior",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "in all adaptable tab bar placements",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabContent/customizationBehavior(_:for:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If you apply a customization ID to a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabSection",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ensure you specify",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "customization IDs for all of the tabs within the section, unless the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "tab has been marked as having customization turned off.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example adds customization identifiers to all tabs",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and tab sections.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@AppStorage(\"MyAppTabViewCustomization\")",
            "private var customization: TabViewCustomization",
            "",
            "TabView {",
            "    Tab(\"Home\", systemImage: \"house\") {",
            "        HomeView()",
            "    }",
            "    .customizationID(\"com.myApp.home\")",
            "",
            "    Tab(\"Alerts\", systemImage: \"bell\") {",
            "        AlertsView()",
            "    }",
            "    .customizationID(\"com.myApp.bell\")",
            "",
            "    TabSection(\"Categories\") {",
            "        Tab(\"Climate\", systemImage: \"fan\") {",
            "            ClimateView()",
            "        }",
            "        .customizationID(\"com.myApp.climate\")",
            "",
            "        Tab(\"Lights\", systemImage: \"lightbulb\") {",
            "            LightsView()",
            "        }",
            "        .customizationID(\"com.myApp.lights\")",
            "    }",
            "    .customizationID(\"com.myApp.categories\")",
            "}",
            ".tabViewStyle(.sidebarAdaptable)",
            ".tabViewCustomization($customization)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
