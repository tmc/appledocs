    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/TabView",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " with three tabs where the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "second tab is pinned to the trailing edge of the top tab bar on",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "iPad.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    Tab(\"Home\", systemImage: \"house\") {",
            "        MyHomeView()",
            "    }",
            "",
            "    Tab(\"Downloads\", systemImage: \"square.and.arrow.down.fill\") {",
            "        MyDownloadsView()",
            "    }",
            "    .tabPlacement(.pinned)",
            "",
            "    Tab(\"Browse\", systemImage: \"list.bullet\") {",
            "        MyBrowseView()",
            "    }",
            "}",
            ".tabViewStyle(.sidebarAdaptable)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
