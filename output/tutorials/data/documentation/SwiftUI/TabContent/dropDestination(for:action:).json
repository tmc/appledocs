    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Type of the models that are dropped.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "payloadType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that takes the dropped content and responds",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "appropriately. The closure takes one argument, which",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "is an array of Transferable items",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "that represents the data to insert.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A view that calls ",
              "type": "text"
            },
            {
              "code": "action",
              "type": "codeVoice"
            },
            {
              "text": " when elements are dropped onto",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the tab.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The dropped content can be provided as binary data, file URLs, or file promises.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The drop destination is the tab content.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This example adds a profile to a group when the user drops it onto",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "onto that group.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct Profile: Identifiable {",
            "    let givenName: String",
            "    let familyName: String",
            "    let image = \"person.fill\"",
            "    let id = UUID().uuidString",
            "}",
            "",
            "@State private var profiles = [",
            "    Profile(givenName: \"Juan\", familyName: \"Chavez\"),",
            "    Profile(givenName: \"Mei\", familyName: \"Chen\"),",
            "    Profile(givenName: \"Tom\", familyName: \"Clark\"),",
            "    Profile(givenName: \"Gita\", familyName: \"Kumar\")",
            "]",
            "",
            "@State private var favoriteProfiles: [Profile] = []",
            "",
            "var body: some View {",
            "    TabView {",
            "        Tab(\"All Profiles\", systemImage: \"list.bullet\") {",
            "            List(profiles) { profile in",
            "                Text(profile.givenName)",
            "                    .draggable(profile.id)",
            "            }",
            "        }",
            "",
            "        Tab(\"Favorites\", systemImage: \"star.fill\") {",
            "            ForEach(favoritedProfiles) { profile in",
            "                Tab(profile.givenName, image: profile.image) {",
            "                    Label(profile.givenName, systemImage: \"star.fill\")",
            "                }",
            "            }",
            "        }",
            "        .dropDestination(for: String.self) { receivedIds in",
            "             // Add profiles with `receivedIds` to favorites",
            "        }",
            "   }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
