      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string to display as a badge.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "label"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use a badge to convey optional, supplementary information about a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "view. Keep the contents of the badge as short as possible. The string",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "provided will appear as an indicator on the given tab.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This modifier creates a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " view on your behalf, and treats the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "localized key similar to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text/init(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". The",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "following example shows tabs that have the server name as their badge.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "TabView {",
            "    ForEach(servers) { server in",
            "        Tab(server.name, image: server.icon) {",
            "            ServerDetailView(for: server)",
            "        }",
            "        .badge(server.defaultString())",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
