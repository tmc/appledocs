    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that produces the menu’s contents. You",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "can deactivate the context menu by returning nothing from the closure.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "menuItems"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A row that can display a context menu.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this modifier to add a context menu to a tab’s sidebar representation.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Compose the menu by returning controls like ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Button",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Toggle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Picker",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " from the ",
              "type": "text"
            },
            {
              "code": "menuItems",
              "type": "codeVoice"
            },
            {
              "text": " closure. You can also use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Menu",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to define submenus, or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Section",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to group items.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example adds the ability to pin the tab or share",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the tab’s books with a friend.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "Tab(\"Currently Reading\", systemImage: \"book\") {",
            "    CurrentBooksList()",
            "}",
            ".contextMenu {",
            "    Button {",
            "        // Pin this tab.",
            "    } label: {",
            "        Label(\"Pin\", systemImage: \"pin\")",
            "    }",
            "    Button {",
            "        // Open a share sheet to share",
            "    } label: {",
            "        Label(\"Share\", systemImage: \"square.and.arrow.up\")",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
