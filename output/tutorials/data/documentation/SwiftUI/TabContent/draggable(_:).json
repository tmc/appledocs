          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that returns a single",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "instance or a value conforming to ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/CoreTransferable/Transferable",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "represents the draggable data from this tab.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
