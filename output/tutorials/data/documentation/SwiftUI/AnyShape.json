      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can use this type to dynamically switch between shape types:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct MyClippedView: View {",
            "    var isCircular: Bool",
            "",
            "    var body: some View {",
            "        OtherView().clipShape(isCircular ?",
            "            AnyShape(Circle()) : AnyShape(Capsule()))",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
