    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You can influence which modes SwiftUI displays search suggestions for by",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/searchSuggestions(_:for:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "enum FruitSuggestion: String, Identifiable {",
            "    case apple, banana, orange",
            "    var id: Self { self }",
            "}",
            "",
            "@State private var text = \"\"",
            "@State private var suggestions: [FruitSuggestion] = []",
            "",
            "var body: some View {",
            "    MainContent()",
            "        .searchable(text: $text) {",
            "            ForEach(suggestions) { suggestion in",
            "                Text(suggestion.rawValue)",
            "                    .searchCompletion(suggestion.rawValue)",
            "            }",
            "            .searchSuggestions(.hidden, for: .content)",
            "        }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In the above example, SwiftUI only displays search suggestions in",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a suggestions menu. You might want to do this when you want to",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "render search suggestions in a container, like inline with",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "your own set of search results.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You can get the current search suggestion placement by querying the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/EnvironmentValues/searchSuggestionsPlacement",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " environment value in your",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "search suggestions.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
