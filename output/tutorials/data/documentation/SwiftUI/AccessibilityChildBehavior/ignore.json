    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this behavior when you want a view represented by",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a single accessibility element. The new accessibility element",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "has no initial properties. So you will need to use other",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "accessibility modifiers, such as ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/accessibilityLabel(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to begin making it accessible.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var body: some View {",
            "    VStack {",
            "        Button(\"Previous Page\", action: goBack)",
            "        Text(\"\\(pageNumber)\")",
            "        Button(\"Next Page\", action: goForward)",
            "    }",
            "    .accessibilityElement(children: .ignore)",
            "    .accessibilityValue(\"Page \\(pageNumber) of \\(pages.count)\")",
            "    .accessibilityAdjustableAction { action in",
            "        if action == .increment {",
            "            goForward()",
            "        } else {",
            "            goBack()",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Before using the  ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/AccessibilityChildBehavior/ignore",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": "behavior, consider",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/AccessibilityChildBehavior/combine",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " behavior.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A new accessibility element is always created.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
