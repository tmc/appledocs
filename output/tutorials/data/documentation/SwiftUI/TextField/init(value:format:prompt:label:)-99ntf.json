    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The underlying value to edit.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A format style of type ",
                  "type": "text"
                },
                {
                  "code": "F",
                  "type": "codeVoice"
                },
                {
                  "text": " to use when converting between",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the string the user edits and the underlying value of type",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "code": "F.FormatInput",
                  "type": "codeVoice"
                },
                {
                  "text": ". If ",
                  "type": "text"
                },
                {
                  "code": "format",
                  "type": "codeVoice"
                },
                {
                  "text": " can’t perform the conversion, the text",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "field leaves the value unchanged. If the user stops editing",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the text in an invalid state, the text field updates the field’s",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "text to the last known valid value.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "format"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "Text",
                  "type": "codeVoice"
                },
                {
                  "text": " which provides users with guidance on what to type",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "into the text field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "prompt"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A view builder that produces a label for the text field,",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "describing its purpose.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "label"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to create a text field that binds to a bound",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value, using a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/parseableformatstyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "to convert to and from this type. Changes to the bound value update",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "the string displayed by the text field. Editing the text field",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "updates the bound value, as long as the format style can parse the",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "text. If the format style can’t parse the input, the bound value",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "remains unchanged.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/onSubmit(of:_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier to invoke an action",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "whenever the user submits this text field.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example uses a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Swift/Double",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "as the bound value, and a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/floatingpointformatstyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "instance to convert to and from a string representation. As the user types, the bound",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "value updates, which in turn updates three ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/Text",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " views that use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "different format styles. If the user enters text that doesn’t represent",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "a valid ",
              "type": "text"
            },
            {
              "code": "Double",
              "type": "codeVoice"
            },
            {
              "text": ", the bound value doesn’t update.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "@State private var myDouble: Double = 0.673",
            "var body: some View {",
            "    VStack {",
            "        TextField(",
            "            value: $myDouble,",
            "            format: .number",
            "        ) {",
            "            Text(\"Double\")",
            "        }",
            "        Text(myDouble, format: .number)",
            "        Text(myDouble, format: .number.precision(.significantDigits(5)))",
            "        Text(myDouble, format: .number.notation(.scientific))",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "TextField-init-format-1",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
