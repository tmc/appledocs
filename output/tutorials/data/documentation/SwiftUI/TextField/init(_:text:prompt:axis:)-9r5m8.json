    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The title of the text view, describing its purpose.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "title"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The text to display and edit.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "text"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "code": "Text",
                  "type": "codeVoice"
                },
                {
                  "text": " representing the prompt of the text field",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "which provides users with guidance on what to type into the text",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "field.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "prompt"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The axis in which to scroll text when it doesn’t fit",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "in the available space.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axis"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Specify a preferred axis in which the text field should scroll",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "its content when it does not fit in the available space. Depending",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "on the style of the field, this axis may not be respected.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.SwiftUI/documentation/SwiftUI/View/onSubmit(of:_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " modifier to invoke an action",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "whenever the user submits this text field.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
