    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A variable color animation draws attention to a symbol by changing the opacity of the symbol’s layers. You can choose to apply the effect to layers either cumulatively or iteratively. For cumulative animations, each layer’s opacity remains changed until the end of the animation cycle. For iterative animations, each layer’s opacity changes briefly before returning to its original state.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Variable color animations affect only symbols containing variable color layers.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Because SwiftUI is a state-driven framework, you pass a ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter when adding discrete effects, like bounce. You trigger the animation by changing the ",
                  "type": "text"
                },
                {
                  "code": "value",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter. Because AppKit and UIKit are event-driven frameworks, discrete effects animate automatically when added to an image view.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        },
        {
          "code": [
            "// Add an effect in SwiftUI.",
            "@State private var value1 = 0",
            "@State private var value2 = 0",
            "var body: some View {",
            "    HStack {",
            "        Image(systemName: \"cellularbars\")",
            "            // Iteratively activates layers.",
            "            .symbolEffect(.variableColor.iterative, value: value1)",
            "            .onTapGesture {",
            "                value1 += 1",
            "            }",
            "        Image(systemName: \"cellularbars\")",
            "            // Cumulatively activates layers reversing and repeating three times.",
            "            .symbolEffect(.variableColor.hideInactiveLayers.reversing, options: .repeat(3), value: value2)",
            "            .onTapGesture {",
            "                value2 += 1",
            "            }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "code": [
            "// Add an effect in AppKit and UIKit.",
            "// Iteratively activates layers.",
            "imageView1.addSymbolEffect(.variableColor.iterative, options: .nonRepeating)",
            "    ",
            "// Cumulatively activates layers reversing and repeating three times.",
            "imageView2.addSymbolEffect(.variableColor.hideInactiveLayers.cumulative, options: .repeat(3))"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
