      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The central manager that provides this information.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "central"
        },
        {
          "content": [
            {
