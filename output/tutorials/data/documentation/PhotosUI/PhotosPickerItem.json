    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The selection results you get from ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.photokit/documentation/PhotosUI/PhotosPicker",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " are placeholder objects. A ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.photokit/documentation/PhotosUI/PhotosPickerItem",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " conforms to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/CoreTransferable/Transferable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", and allows you to load the representation you request. To load a SwiftUI ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Image",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and track progress, use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.photokit/documentation/PhotosUI/PhotosPickerItem/loadTransferable(type:completionHandler:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "func loadTransferable(from imageSelection: PhotosPickerItem) -> Progress {",
            "    return imageSelection.loadTransferable(type: Image.self) { result in",
            "        DispatchQueue.main.async {",
            "            guard imageSelection == self.imageSelection else { return }",
            "            switch result {",
            "            case .success(let image?):",
            "                // Handle the success case with the image.",
            "            case .success(nil):",
            "                // Handle the success case with an empty value.",
            "            case .failure(let error):",
            "                // Handle the failure case with the provided error.",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "A failure can occur when the system attempts to retrieve the data. For example, if the picker tries to download data from iCloud Photos without a network connection.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/SwiftUI/Image",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " only supports ",
                  "type": "text"
                },
                {
                  "code": "PNG",
                  "type": "codeVoice"
                },
                {
                  "text": " file types through its ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/CoreTransferable/Transferable",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " conformance. For more information on creating a custom ",
                  "type": "text"
                },
                {
                  "code": "Transferable",
                  "type": "codeVoice"
                },
                {
                  "text": " model to support other image types, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.photokit/documentation/PhotoKit/bringing-photos-picker-to-your-swiftui-app",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        }
      ],
