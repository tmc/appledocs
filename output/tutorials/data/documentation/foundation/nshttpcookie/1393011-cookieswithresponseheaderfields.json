          "content": [
            {
              "inlineContent": [
                {
                  "text": "The header fields used to create the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nshttpcookie",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": " objects.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "headerFields"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL associated with the created cookies.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "URL"
        }
      ]
    },
    {
      "content": [
