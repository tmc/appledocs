      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that provides the full path to the current encoding position, and returns a customized coding key.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "custom"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The value associated with this case is a closure you use to choose the names of keys in the encoded JSON object. During encoding, the closure executes once for each key in the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/Encodable",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " value. The closure receives an array of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/CodingKey",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instances representing the sequence of keys needed to reach the value the encoder is currently encoding.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The example below shows how to encode the properties of the nested ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "B",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": " structures with custom logic that you specify in the closure value associated with the custom case.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct A: Codable {",
            "    var value: Int",
            "    var b: B",
            "    ",
            "    struct B: Codable {",
            "        var value: Int",
            "        var c: C",
            "        ",
            "        struct C: Codable {",
            "            var value: Int",
            "        }",
            "    }",
            "}",
            "",
            "let a: A = A(value: 1, b: .init(value: 2, c: .init(value: 3)))",
            "",
            "print(a.b.c.value) // Prints \"3\"",
            "",
            "let encoder = JSONEncoder()",
            "encoder.outputFormatting = [.prettyPrinted, .sortedKeys]",
            "let encodeAndPrint = { print(String(data: try encoder.encode(a), encoding: .utf8) ?? \"String encoding error.\") }",
            "",
            "/// An implementation of CodingKey that's useful for combining and transforming keys as strings.",
            "struct AnyKey: CodingKey {",
            "    var stringValue: String",
            "    var intValue: Int?",
            "    ",
            "    init?(stringValue: String) {",
            "        self.stringValue = stringValue",
            "        self.intValue = nil",
            "    }",
            "    ",
            "    init?(intValue: Int) {",
            "        self.stringValue = String(intValue)",
            "        self.intValue = intValue",
            "    }",
            "}"
          ],
          "metadata": {
            "anchor": "2950677",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "In the next example, you use the ",
              "type": "text"
            },
            {
              "code": "AnyKey",
              "type": "codeVoice"
            },
            {
              "text": " structure defined above to customize the encoding of the ",
              "type": "text"
            },
            {
              "code": "A",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "B",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": " structures.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// Customize each `value` key to contain a dot-syntax path.",
            "encoder.keyEncodingStrategy = .custom { keys in",
            "    if keys.last!.stringValue == \"value\" {",
            "        return AnyKey(stringValue: \"a.\" + keys.map { key in",
            "            key.stringValue",
            "        }.joined(separator: \".\"))!",
            "    } else {",
            "        return keys.last!",
            "    }",
            "}",
            "",
            "try encodeAndPrint()"
          ],
          "metadata": {
            "anchor": "2950688",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Here’s the JSON object that results from the custom encoding above:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            " {",
            "     \"a.value\": 1,",
            "     \"b\": {",
            "         \"a.b.value\": 2,",
            "         \"c\": {",
            "            \"a.b.c.value\": 3",
            "         }",
            "     }",
            " }"
          ],
          "metadata": {
            "anchor": "2950687",
            "title": "Listing 3"
          },
          "syntax": "javascript",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
