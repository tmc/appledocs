  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Foundation supports two approaches for data formatting:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "When working in Swift, use ",
                      "type": "text"
                    },
                    {
                      "code": "formatted",
                      "type": "codeVoice"
                    },
                    {
                      "text": " methods directly on the types you want to format, optionally using ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/formatstyle",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " and its subtypes to customize formatter output. This approach supports dates, integers, floating-point numbers, measurements, sequences, and person name components. Foundation caches identically-configured formatter instances internally, allowing you to focus on your app’s formatting needs.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "In Objective-C, create instances of ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/formatter",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " and its subtypes, and use the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/formatter/1415993-string",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " method to convert objects to formatted strings.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
