      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The list type to apply, such as cumulative (",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/listformatstyle/listtype/and",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ") or alternative (",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/listformatstyle/listtype/or",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ") elements.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The width to use when formatting, such as ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/listformatstyle/width/standard",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/listformatstyle/width/narrow",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "width"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A list format style that formats a string array as a textual list of items.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the dot-notation form of this method when the call point allows the use of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/listformatstyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", and the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/Sequence",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " element type is ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/String",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example creates an array of strings, then uses ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/Sequence/formatted(_:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and the list format style provided by this method to format the items. It modifies the list format style to use the ",
              "type": "text"
            },
            {
              "code": "en_US",
              "type": "codeVoice"
            },
            {
              "text": " locale, so the resulting string uses US English conventions for commas and conjunctions (”and”).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let items = [\"Atlantic\", \"Pacific\", \"Indian\", \"Arctic\", \"Southern\"]",
            "let formatted = items.formatted(",
            "    .list(type:.and)",
            "    .locale(Locale(identifier: \"en_US\"))) // \"Atlantic, Pacific, Indian, Arctic, and Southern\""
          ],
          "metadata": {
            "anchor": "4113436",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
