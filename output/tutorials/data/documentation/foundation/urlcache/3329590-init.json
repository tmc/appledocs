        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The memory capacity of the cache, in bytes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "memoryCapacity"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The disk capacity of the cache, in bytes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "diskCapacity"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The path to an on-disk directory, where the system stores the on-disk cache. If ",
                  "type": "text"
                },
                {
                  "code": "directory",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", the cache uses a default directory.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "directory"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A disk cache measured in the tens of megabytes is acceptable in most cases.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
