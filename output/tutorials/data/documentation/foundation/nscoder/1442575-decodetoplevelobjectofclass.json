      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The expected class of the object being decoded.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "aClass"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The archive key indicating the member to decode.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "key"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, an ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nserror",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " indicating why decoding failed, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " if no error occurred.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "error"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The decoded object, or ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " if decoding fails.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the coder responds ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nscoder/1409845-requiressecurecoding",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", then the coder calls ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nscoder/1411455-failwitherror",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " in either the following cases:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The class indicated by ",
                      "type": "text"
                    },
                    {
                      "code": "cls",
                      "type": "codeVoice"
                    },
                    {
                      "text": " does not implement ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nssecurecoding",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The unarchived class does not match ",
                      "type": "text"
                    },
                    {
                      "code": "cls",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", nor do any of its superclasses.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "If the coder does not require secure coding, it ignores the ",
              "type": "text"
            },
            {
              "code": "cls",
              "type": "codeVoice"
            },
            {
              "text": " parameter and does not check the decoded object.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
