      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key for an entry in the specified table.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "key"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A localization value to use if ",
                  "type": "text"
                },
                {
                  "code": "key",
                  "type": "codeVoice"
                },
                {
                  "text": " doesn’t exist in ",
                  "type": "text"
                },
                {
                  "code": "table",
                  "type": "codeVoice"
                },
                {
                  "text": ". Xcode’s Product > Export Localizations feature also extracts this value as the default translation in the project’s development locale.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "defaultValue"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the table containing the key-value pairs. If not provided, ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", or an empty string, this value defaults to ",
                  "type": "text"
                },
                {
                  "code": "Localizable.strings.",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "table"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The locale for the resource to use. By default, the resource uses ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/locale/2293654-current",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "locale"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/localizedstringresource/bundledescription",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that indicates where to locate the table’s strings file. By default, the resource uses the main bundle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The comment to place above the key-value pair in the strings file. This parameter provides the translator with some context about the localized string’s presentation to the user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "comment"
        }
      ]
    }
