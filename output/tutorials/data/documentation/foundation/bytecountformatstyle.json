    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example creates an ",
              "type": "text"
            },
            {
              "code": "Int",
              "type": "codeVoice"
            },
            {
              "text": " representing 1,024 bytes, and then formats it as an expression of memory storage, with the default byte count format style.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let count: Int64 = 1024",
            "let formatted = count.formatted(.byteCount(style: .memory)) // \"1 kB\""
          ],
          "metadata": {
            "anchor": "4113492",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You can also customize a byte count format style, and use this to format one or more ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/Int64",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " instances. The following example creates a format style to only use kilobyte units, and to spell out the exact byte count of the measurement.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let style = ByteCountFormatStyle(style: .memory,                                 ",
            "                                 allowedUnits: [.kb],",
            "                                 spellsOutZero: true,",
            "                                 includesActualByteCount: false,",
            "                                 locale: Locale(identifier: \"en_US\"))",
            "let counts: [Int64] = [0, 1024, 2048, 4096, 8192, 16384, 32768, 65536]",
            "let formatted = counts.map ( {style.format($0) } ) // [\"Zero kB\", \"1 kB\", \"2 kB\", \"4 kB\", \"8 kB\", \"16 kB\", \"32 kB\", \"64 kB\"]"
          ],
          "metadata": {
            "anchor": "4113494",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
