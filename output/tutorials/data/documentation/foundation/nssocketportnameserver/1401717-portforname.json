      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the desired port.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "portName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the host. ",
                  "type": "text"
                },
                {
                  "code": "hostName",
                  "type": "codeVoice"
                },
                {
                  "text": " is an Internet domain name (for example, “",
                  "type": "text"
                },
                {
                  "code": "sales.anycorp.com",
                  "type": "codeVoice"
                },
                {
                  "text": "”) or IP address (IPv4 or IPv6). If ",
                  "type": "text"
                },
                {
                  "code": "hostName",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " or empty, the local host is checked. If ",
                  "type": "text"
                },
                {
                  "code": "hostName",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "@”*”",
                  "type": "codeVoice"
                },
                {
                  "text": ", all hosts on the local network are checked.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "hostName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "portNumber",
                  "type": "codeVoice"
                },
                {
                  "text": " parameter is ignored.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "portNumber"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The port associated with ",
              "type": "text"
            },
            {
              "code": "portName",
              "type": "codeVoice"
            },
            {
              "text": " on the host ",
              "type": "text"
            },
            {
              "code": "hostName",
              "type": "codeVoice"
            },
            {
              "text": ". Returns ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " if no such port exists.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
