          "inlineContent": [
            {
              "text": "This property identifies the kind of progress for the progress object, such as ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/progresskind/1409141-file",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". It can be ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If you set a non-",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " value to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/progress/1416139-kind",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": ", the default ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/progress/1417251-localizeddescription",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " getter uses the kind of progress. The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/progress/1417251-localizeddescription",
              "isActive": false,
              "type": "reference"
            },
