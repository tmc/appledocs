            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/true",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " for all other authentication methods.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
