      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The currency code to use, such as ",
                  "type": "text"
                },
                {
                  "code": "EUR",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "JPY",
                  "type": "codeVoice"
                },
                {
                  "text": ". See ISO-4217 for a list of valid codes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "code"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A floating-point format style that uses the specified currency code.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the dot-notation form of this method when the call point allows the use of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/floatingpointformatstyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". You typically do this when calling the ",
              "type": "text"
            },
            {
              "code": "formatted",
              "type": "codeVoice"
            },
            {
              "text": " methods of types that conform to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/BinaryFloatingPoint",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example creates an array of doubles, then uses ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/BinaryFloatingPoint/formatted(_:)-83x4n",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and the currency style provided by this method to format the doubles as US dollars.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let nums: [Double] = [100.01, 1000.02, 10000.03, 100000.04, 1000000.05]",
            "let currencyNums = nums.map { $0.formatted(",
            "    .currency(code: \"USD\"))",
            "} // [\"$100.01\", \"$1,000.02\", \"$10,000.03\", \"$100,000.04\", \"$1,000,000.05\"]"
          ],
          "metadata": {
            "anchor": "4099396",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
