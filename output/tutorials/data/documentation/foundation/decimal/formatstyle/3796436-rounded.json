      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The rounding rule to apply to the format style.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "rule"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A multiple by which the formatter rounds the fractional part. The formatter produces a value that is an even multiple of this increment. If this parameter is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " (the default), the formatter doesnâ€™t apply an increment.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "increment"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A decimal format style modified to use the specified rounding rule and increment.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The following example creates a default ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/decimal/formatstyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " for the ",
              "type": "text"
            },
            {
              "code": "en_US",
              "type": "codeVoice"
            },
            {
              "text": " locale, and a modified style that rounds integers to the nearest multiple of ",
              "type": "text"
            },
            {
              "code": "100",
              "type": "codeVoice"
            },
            {
              "text": ". It then formats the value ",
              "type": "text"
            },
            {
              "code": "1999.95",
              "type": "codeVoice"
            },
            {
              "text": " using these format styles.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let defaultStyle = Decimal.FormatStyle(locale: Locale(identifier: \"en_US\"))",
            "let roundedStyle = defaultStyle.rounded(rule: .toNearestOrEven,",
            "                                        increment: 100)",
            "let num: Decimal = 1999.95",
            "let defaultNum = num.formatted(defaultStyle) // \"1,999.95\"",
            "let roundedNum = num.formatted(roundedStyle) // \"2,000\""
          ],
          "metadata": {
            "anchor": "4291913",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
