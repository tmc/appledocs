      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Upon return, contains the scanned value. Contains ",
                  "type": "text"
                },
                {
                  "code": "HUGE_VAL",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "–HUGE_VAL",
                  "type": "codeVoice"
                },
                {
                  "text": " on overflow, or ",
                  "type": "text"
                },
                {
                  "code": "0.0",
                  "type": "codeVoice"
                },
                {
                  "text": " on underflow.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "floatValue"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "identifier": "doc://com.apple.documentation/documentation/objectivec/yes",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " if the receiver finds a valid floating-point representation, otherwise ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/objectivec/no",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": ". Overflow or underflow are both considered valid floating-point representations.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Skips past excess digits in the case of overflow, so the scanner’s position is past the entire floating-point representation.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Invoke this method with ",
              "type": "text"
            },
            {
              "code": "NULL",
              "type": "codeVoice"
            },
            {
              "text": " as ",
              "type": "text"
            },
            {
              "code": "floatValue",
              "type": "codeVoice"
            },
            {
              "text": " to simply scan past a float value representation. Floating-point representations are assumed to be IEEE compliant.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
