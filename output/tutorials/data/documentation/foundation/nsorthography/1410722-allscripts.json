        {
          "inlineContent": [
            {
              "text": "The value of this property is an array of ISO 15924 script codes, such as ",
              "type": "text"
            },
            {
              "code": "\"Latn\"",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "\"Cyrl\"",
              "type": "codeVoice"
            },
            {
              "text": ", that identify the scripts.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
