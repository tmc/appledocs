      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key for a string in the specified table.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Xcode can’t export localizations for strings whose ",
                      "type": "text"
                    },
                    {
                      "code": "key",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is a string variable, ",
                      "type": "text"
                    },
                    {
                      "code": "nil",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", or an empty string.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "name": "Note",
              "style": "note",
              "type": "aside"
            }
          ],
          "name": "key"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the table containing the key-value pairs. Also, the suffix for the strings file (a file with the ",
                  "type": "text"
                },
                {
                  "code": ".strings",
                  "type": "codeVoice"
                },
                {
                  "text": " extension) to store the localized string. The default table in ",
                  "type": "text"
                },
                {
                  "code": "Localizable.strings",
                  "type": "codeVoice"
                },
                {
                  "text": " is used when ",
                  "type": "text"
                },
                {
                  "code": "tableName",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " or an empty string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tableName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle containing the table’s strings file. The main bundle is used when ",
                  "type": "text"
                },
                {
                  "code": "bundle",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The comment to place above the key-value pair in the strings file. This parameter provides the translator with some context about the localized string’s presentation to the user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "comment"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The result of sending ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsbundle/1417694-localizedstringforkey",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "bundle",
              "type": "codeVoice"
            },
            {
              "text": ", passing the specified ",
              "type": "text"
            },
            {
              "code": "key",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "tableName",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this macro to automatically generate a strings file named ",
              "type": "text"
            },
            {
              "code": "[tableName].strings",
              "type": "codeVoice"
            },
            {
              "text": " located in ",
              "type": "text"
            },
            {
              "code": "bundle",
              "type": "codeVoice"
            },
            {
              "text": " from your code when exporting localizations from Xcode or the ",
              "type": "text"
            },
            {
              "code": "genstrings",
              "type": "codeVoice"
            },
            {
              "text": " utility. You can specify Unicode characters in ",
              "type": "text"
            },
            {
              "code": "key",
              "type": "codeVoice"
            },
            {
              "text": " using ",
              "type": "text"
            },
            {
              "code": "\\\\Uxxxx",
              "type": "codeVoice"
            },
            {
              "text": "—see the ",
              "type": "text"
            },
            {
              "code": "-u",
              "type": "codeVoice"
            },
            {
              "text": " option for the ",
              "type": "text"
            },
            {
              "code": "genstrings",
              "type": "codeVoice"
            },
            {
              "text": " utility.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The initial value for ",
              "type": "text"
            },
            {
              "code": "key",
              "type": "codeVoice"
            },
            {
              "text": " in the strings file is ",
              "type": "text"
            },
            {
              "code": "key",
              "type": "codeVoice"
            },
            {
              "text": ". To avoid collisions between words or phrases with multiple meanings, use a unique ",
              "type": "text"
            },
            {
              "code": "key",
              "type": "codeVoice"
            },
            {
              "text": " for each use of the same phrase. Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nslocalizedstringwithdefaultvalue",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " macro to specify another value for ",
              "type": "text"
            },
            {
              "code": "key",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For information about inserting plural nouns and units into localized strings, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Xcode/documentation/Xcode/localizing-strings-that-contain-plurals",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "As of OS X 10.11 and iOS 9, ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsbundle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " is thread-safe. As such, you can safely call ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nslocalizedstringfromtableinbundle",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " from any execution context.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The values for ",
                  "type": "text"
                },
                {
                  "code": "key",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "tableName",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "comment",
                  "type": "codeVoice"
                },
                {
                  "text": " must be string literal values. Xcode can read these values from source code to automatically create localization tables when exporting localizations, but it doesn’t resolve string variables. If you want to use string variables, manually create a strings file and use ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsbundle/1417694-localizedstringforkey",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": " instead. For more information on strings files, see ",
                  "type": "text"
                },
                {
                  "identifier": "devLink-1695465",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "style": "important",
          "type": "aside"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Break up long string values into consecutive string literals.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "style": "tip",
          "type": "aside"
        },
        {
          "code": [
            "NSLocalizedStringFromTableInBundle(",
            "    @\"Did you know that venus flytraps have flowers\"",
            "        @\" atop very long stems?\\nThe long stem keeps\"",
            "        @\" insects a safe distance away from their\"",
            "        @\" digestive leaves below.\",",
            "    @\"Localized\",",
            "    [NSBundle mainBundle],",
            "    @\"An interesting fact about venus flytraps shown\"",
            "        @\" on the loading screen.\");"
          ],
          "metadata": {
            "anchor": "3783138",
            "title": "Listing 1"
          },
          "syntax": "occ",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
