    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method uses a date formatter configured with the specified time zone and format options. The following code examples produce the same string value:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let date = Date()",
            "var string: String",
            " ",
            "let formatter = ISO8601DateFormatter()",
            "string = formatter.string(from: date)",
            " ",
            "if let GMT = TimeZone(abbreviation: \"GMT\") {",
            "    let options: ISO8601DateFormatter.Options = [.withInternetDateTime, .withDashSeparatorInDate, .withColonSeparatorInTime, .withTimeZone]",
            "    string = ISO8601DateFormatter.string(from: date, timeZone: GMT, formatOptions: options)",
            "}"
          ],
          "metadata": {
            "anchor": "2556984",
            "title": "Listing 1"
          },
