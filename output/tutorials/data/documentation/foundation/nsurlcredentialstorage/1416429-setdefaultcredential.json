          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL credential to set as the default for the protection space. If the receiver does not contain ",
                  "type": "text"
                },
                {
                  "code": "credential",
                  "type": "codeVoice"
                },
                {
                  "text": " in the specified protection space it will be added.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "credential"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The protection space whose default credential is being set.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "protectionSpace"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The task accessing the specified protection space. Subclasses of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlcredentialstorage",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": " may use the request URL or other properties of this task to affect how the default credential is stored.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "task"
        }
      ]
