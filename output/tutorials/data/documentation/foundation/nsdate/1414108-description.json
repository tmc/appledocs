      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "code": "NSLocale",
                  "type": "codeVoice"
                },
                {
                  "text": " object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you pass ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "NSDate",
                  "type": "codeVoice"
                },
                {
                  "text": " formats the date in the same way as the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsdate/1409767-description",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " property. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "In OS X v10.4 and earlier, this parameter was an ",
                  "type": "text"
                },
                {
                  "code": "NSDictionary",
                  "type": "codeVoice"
                },
                {
                  "text": " object. If you pass in an ",
                  "type": "text"
                },
                {
                  "code": "NSDictionary",
                  "type": "codeVoice"
                },
                {
                  "text": " object in OS X v10.5, ",
                  "type": "text"
                },
                {
                  "code": "NSDate",
                  "type": "codeVoice"
                },
                {
                  "text": " uses the default user locale—the same as if you passed in ",
                  "type": "text"
                },
                {
                  "code": "[NSLocale currentLocale].",
                  "type": "codeVoice"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "locale"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A string representation of the receiver, using the given locale, or if the locale argument is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", in the international format ",
              "type": "text"
            },
            {
              "code": "YYYY-MM-DD HH:MM:SS ±HHMM",
              "type": "codeVoice"
            },
            {
              "text": ", where ",
              "type": "text"
            },
            {
              "code": "±HHMM",
              "type": "codeVoice"
            },
            {
              "text": " represents the time zone offset in hours and minutes from UTC (for example, “",
              "type": "text"
            },
            {
              "code": "2001-03-24 10:45:32 +0600",
              "type": "codeVoice"
            },
            {
              "text": "”)",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In OS X v10.4 and earlier, ",
              "type": "text"
            },
            {
              "code": "localeDictionary",
              "type": "codeVoice"
            },
            {
              "text": " is an ",
              "type": "text"
            },
            {
              "code": "NSDictionary",
              "type": "codeVoice"
            },
            {
              "text": " object containing locale data. To use the user's preferences, you can use ",
              "type": "text"
            },
            {
              "code": "[[NSUserDefaults standardUserDefaults] dictionaryRepresentation].",
              "type": "codeVoice"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
