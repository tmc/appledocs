    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Create an explicit ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/integerparsestrategy",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " to parse multiple strings according to the same parse strategy. In the following example, ",
              "type": "text"
            },
            {
              "code": "usCurrencyStrategy",
              "type": "codeVoice"
            },
            {
              "text": " is an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/integerparsestrategy",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " that uses US dollars and the ",
              "type": "text"
            },
            {
              "code": "en_US",
              "type": "codeVoice"
            },
            {
              "text": " locale’s conventions for number formatting. The example then uses this strategy to parse an array of strings, some of which represent valid US currency values.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let usCurrencyStrategy: IntegerParseStrategy =",
            "    IntegerFormatStyle<Int>.Currency(code: \"USD\",",
            "                                     locale: Locale(identifier: \"en_US\"))",
            "    .parseStrategy",
            "let currencyValues = [\"$100\", \"$1,000\", \"$10,000\", \"€100\"]",
            "let parsedValues = currencyValues.map { try? usCurrencyStrategy.parse($0) } // [Optional(100), Optional(1000), Optional(10000), nil]"
          ],
          "metadata": {
            "anchor": "4097929",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You don’t need to instantiate a parse strategy variable to parse a single string. Instead, use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/BinaryInteger",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " initializers that take a source ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/String",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and a ",
              "type": "text"
            },
            {
              "code": "format",
              "type": "codeVoice"
            },
            {
              "text": " parameter to parse the string according to the provided ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/formatstyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". The following example parses a string that represents a currency value in US dollars.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let formattedUSDollars = \"$1,234\"",
            "let parsedUSDollars = try? Int(formattedUSDollars, format: .currency(code: \"USD\")",
            "    .locale(Locale(identifier: \"en_US\"))) // 1234"
          ],
          "metadata": {
            "anchor": "4097937",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
