      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method first checks to see if the receiver is still running using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/process/1415788-isrunning",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". Then it polls the current run loop using ",
              "type": "text"
            },
            {
              "code": "NSDefaultRunLoopMode",
              "type": "codeVoice"
            },
            {
              "text": " until the task completes.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let task: NSTask = // Create and initialize a task",
            "    task.launch()",
            "task.waitUntilExit()",
            "let status = task.terminationStatus",
            " ",
            "if status == 0 {",
            "    print(\"Task succeeded.\")",
            "} else {",
            "    print(\"Task failed.\")",
            "}"
          ],
          "metadata": {
            "anchor": "2555821",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/process/1415808-waituntilexit",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " does not guarantee that the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/process/1408746-terminationhandler",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " block has been fully executed before ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/process/1415808-waituntilexit",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " returns.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
