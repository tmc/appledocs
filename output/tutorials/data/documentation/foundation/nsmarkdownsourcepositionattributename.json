        {
          "inlineContent": [
            {
              "text": "This attribute indicates the position in the Markdown source where a run of attributed text begins and ends, omitting markup characters in the source. For example, after parsing the source string ",
              "type": "text"
            },
            {
              "code": "“This is *emphasized*.”",
              "type": "codeVoice"
            },
            {
              "text": ", the text ",
              "type": "text"
            },
            {
              "code": "emphasized",
              "type": "codeVoice"
            },
            {
              "text": " has a Markdown source position that starts at column ",
              "type": "text"
            },
            {
              "code": "10",
              "type": "codeVoice"
            },
            {
              "text": ". This index is the ",
              "type": "text"
            },
            {
              "code": "“e”",
              "type": "codeVoice"
            },
            {
              "text": " character, not the ",
              "type": "text"
            },
            {
              "code": "“*”",
              "type": "codeVoice"
            },
            {
              "text": " formatting character.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "An attributed string parsed from Markdown text includes this attribute only if the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsattributedstringmarkdownparsingoptions/3929672-appliessourcepositionattributes",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " value in the directory of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsattributedstring/documentreadingoptionkey",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " options provided to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsattributedstring",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " initializer is ",
              "type": "text"
            },
            {
              "code": "YES",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
