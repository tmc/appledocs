              "inlineContent": [
                {
                  "text": "An integer identifying the position of an child in the receiver's array. If ",
                  "type": "text"
                },
                {
                  "code": "index",
                  "type": "codeVoice"
                },
                {
                  "text": " is less than zero or greater than the number of children minus one, an out-of-bounds exception is raised.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "index"
        }
      ]
    },
