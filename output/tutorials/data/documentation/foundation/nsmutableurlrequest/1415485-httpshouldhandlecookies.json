    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/true",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " if the request should use the default cookie handling for the request, ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/false",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " otherwise. The default is ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/true",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If your app sets the ",
              "type": "text"
            },
            {
              "code": "Cookie",
              "type": "codeVoice"
            },
            {
              "text": " header on an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsmutableurlrequest",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " object, then this method has no effect, and the cookie data you set in the header overrides all cookies from the cookie store.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "1681158",
          "level": 3,
          "text": "Special considerations",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "In OS X v10.2 with Safari 1.0 the value set by this method is not respected by the framework.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
