          "inlineContent": [
            {
              "text": "This property is ignored for requests used to construct ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsessionuploadtask",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/urlsessiondownloadtask",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " objects, as caching is not supported by the URL Loading System for upload or download requests.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
