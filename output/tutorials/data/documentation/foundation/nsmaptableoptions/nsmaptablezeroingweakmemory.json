{
  "abstract": [
    {
      "text": "Specifies a zeroing weak reference from the map table to its contents.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "This option has been deprecated. Use the ",
          "type": "text"
        },
        {
          "identifier": "doc://com.apple.documentation/documentation/foundation/nsmaptableoptions/nsmaptableweakmemory",
          "isActive": true,
          "type": "reference"
        },
        {
          "text": " option instead, which causes zeroing weak behavior under manual reference counting, automatic reference counting, and garbage collection.",
          "type": "text"
        }
      ],
      "type": "paragraph"
    }
  ],
