        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The object from which to generate JSON data. Must not be ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "obj"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Options for creating the JSON data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsjsonwritingoptions",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for possible values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "opt"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If an internal error occurs, upon return contains an ",
                  "type": "text"
                },
                {
                  "code": "NSError",
                  "type": "codeVoice"
                },
                {
                  "text": " object with code ",
                  "type": "text"
                },
                {
