              "inlineContent": [
                {
                  "text": "Upon return, contains the scanned value. Contains ",
                  "type": "text"
                },
                {
                  "code": "HUGE_VAL",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
