          "content": [
            {
              "inlineContent": [
                {
                  "text": "The date at which to begin the search.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "date"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Which direction in time to search. The default value is ",
                  "type": "text"
                },
                {
                  "code": ".forward",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
