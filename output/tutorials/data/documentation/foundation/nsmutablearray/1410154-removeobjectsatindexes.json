      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The indexes of the objects to remove from the array. The locations specified by ",
                  "type": "text"
                },
                {
                  "code": "indexes",
                  "type": "codeVoice"
                },
                {
                  "text": " must lie within the bounds of the array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "indexes"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method is similar to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsmutablearray/1416616-removeobjectatindex",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", but allows you to efficiently remove multiple objects with a single operation. ",
              "type": "text"
            },
            {
              "code": "indexes",
              "type": "codeVoice"
            },
            {
              "text": " specifies the locations of objects to be removed given the state of the array when the method is invoked, as illustrated in the following example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NSMutableArray *array = [NSMutableArray arrayWithObjects: @\"one\", @\"a\", @\"two\", @\"b\", @\"three\", @\"four\", nil];",
            "NSMutableIndexSet *indexes = [NSMutableIndexSet indexSetWithIndex:1];",
            "[indexes addIndex:3];",
            "[array removeObjectsAtIndexes:indexes];",
            "NSLog(@\"array: %@\", array);",
            " ",
            "// Output: array: (one, two, three, four)"
          ],
          "metadata": {
            "anchor": "2555725",
            "title": "Listing 1"
          },
          "syntax": "occ",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If ",
              "type": "text"
            },
            {
              "code": "indexes",
              "type": "codeVoice"
            },
            {
              "text": " is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": ", this method raises an exception.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
