      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A selector that specifies the comparison method to use to compare elements in the array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The ",
                  "type": "text"
                },
                {
                  "code": "comparator",
                  "type": "codeVoice"
                },
                {
                  "text": " message is sent to each object in the array and has as its single argument another object in the array. The ",
                  "type": "text"
                },
                {
                  "code": "comparator",
                  "type": "codeVoice"
                },
                {
                  "text": " method should return ",
                  "type": "text"
                },
                {
                  "code": "NSOrderedAscending",
                  "type": "codeVoice"
                },
                {
                  "text": " if the array is smaller than the argument, ",
                  "type": "text"
                },
                {
                  "code": "NSOrderedDescending",
                  "type": "codeVoice"
                },
                {
                  "text": " if the array is larger than the argument, and ",
                  "type": "text"
                },
                {
                  "code": "NSOrderedSame",
                  "type": "codeVoice"
                },
                {
                  "text": " if they are equal.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "comparator"
        }
      ]
    }
  ],
