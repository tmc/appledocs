      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The user component of a URL is an optional component that precedes the host component, and ends at either a colon (if a password is specified) or an ",
              "type": "text"
            },
            {
              "code": "@",
              "type": "codeVoice"
            },
            {
              "text": " sign (if no password is specified). For example, in the URL ",
              "type": "text"
            },
            {
              "code": "http://username:password@www.example.com/index.html",
              "type": "codeVoice"
            },
            {
              "text": ", the user component is ",
              "type": "text"
            },
            {
              "code": "username",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
