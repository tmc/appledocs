      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A compact name notation, when available in the format style’s locale, that uses prefixes or suffixes corresponding to powers of ten. The following example shows a compact name notation in the ",
              "type": "text"
            },
            {
              "code": "fr_FR",
              "type": "codeVoice"
            },
            {
              "text": " locale:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let compactNameFormatted = 1234.formatted(.number",
            "    .locale(Locale(identifier: \"fr_FR\"))",
            "    .notation(.compactName)) // \"1,2 k\""
          ],
          "metadata": {
            "anchor": "4098072",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
