      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Whether to use a cached URL handle. If ",
                  "type": "text"
                },
                {
                  "code": "shouldUseCache",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/yes",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": ", the cache is searched for a URL handle that has serviced the receiver or another identical URL. If ",
                  "type": "text"
                },
                {
                  "code": "shouldUseCache",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/objectivec/no",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": ", a newly instantiated handle is returned, even if an equivalent URL has been loaded.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shouldUseCache"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A URL handle to service the receiver.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Sophisticated clients use the URL handle directly for additional control.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "1679261",
          "level": 3,
          "text": "Special Considerations",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlsession",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsurlconnection",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " classes for loading content from remote URLs.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
