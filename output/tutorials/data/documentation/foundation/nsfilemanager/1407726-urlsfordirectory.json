          "content": [
            {
              "inlineContent": [
                {
                  "text": "The search path directory. The supported values are described in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nssearchpathdirectory",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "directory"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The file system domain to search. The value for this parameter is one or more of the constants described in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nssearchpathdomainmask",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ". ",
                  "type": "text"
