              "isActive": false,
              "type": "reference"
            },
            {
              "text": " unit width, or ",
              "type": "text"
            },
            {
              "code": "90 degrees",
              "type": "codeVoice"
            },
            {
              "text": " rather than ",
              "type": "text"
            },
            {
              "code": "90 degrees Celcius",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "90 degrees Fahrenheit",
              "type": "codeVoice"
            },
            {
              "text": " with the ",
              "type": "text"
