        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A set of strings, with each string specifying a tag assigned to resources stored in ",
                  "type": "text"
                },
                {
                  "code": "bundle",
                  "type": "codeVoice"
                },
                {
                  "text": ". The value must not be ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tags"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle used to store the loaded resources. Pass ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " for the main bundle. The bundle must be the same as the one used in the Xcode project for all the resources marked with the specified tags.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundle"
        }
      ]
    },
