    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style of byte count to express, such as memory or file system storage.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "style"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The units the format style can use to express the byte count.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "allowedUnits"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the format style should spell out zero-byte values as text, like ",
                  "type": "text"
                },
                {
                  "code": "Zero kB",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "spellsOutZero"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the format style should include the exact byte count, in addition to expressing it in terms of units. For example, ",
                  "type": "text"
                },
                {
                  "code": "1 kB (1,024 bytes)",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "includesActualByteCount"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A format style for formatting a measurement of data storage, customized with the provided behaviors.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this type method when the call point allows the use of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/bytecountformatstyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". You typically do this when calling the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/BinaryInteger/formatted(_:)-4qd73",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Swift/documentation/Swift/BinaryInteger",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " values that represent byte counts, as seen here:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let count: Int64 = 1024",
            "let formatted = count.formatted(.byteCount(style: .memory)) // \"1 kB\""
          ],
          "metadata": {
            "anchor": "4291907",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
