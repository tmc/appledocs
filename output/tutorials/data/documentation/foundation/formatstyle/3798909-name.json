    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A name format style.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use the dot-notation form of tis type method when the call point allows the use of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/personnamecomponents/formatstyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". You typically do this when calling the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/personnamecomponents/3767266-formatted",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/personnamecomponents",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows the effect of creating and using different person name format styles.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let name = PersonNameComponents(namePrefix: \"Dr.\",",
            "                                givenName: \"Thomas\",",
            "                                middleName: \"Louis\",",
            "                                familyName: \"Clark\",",
            "                                nameSuffix: \"Jr.\",",
            "                                nickname: \"Tom\")",
            "let longFormattedName = name.formatted(.name(style: .long)) // \"Dr. Thomas Louis Clark Jr.\"",
            "let mediumFormattedName = name.formatted(.name(style: .medium)) // \"Thomas Clark\"",
            "let shortFormattedName = name.formatted(.name(style: .short)) // \"Tom\"",
            "let abbreviatedFormattedName = name.formatted(.name(style: .abbreviated)) // \"TC\""
          ],
          "metadata": {
            "anchor": "4110311",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
