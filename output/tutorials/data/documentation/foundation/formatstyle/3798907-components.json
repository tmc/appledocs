      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The style to use for the fields, such as ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/date/componentsformatstyle/style/3766089-abbreviated",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/date/componentsformatstyle/style/3766095-narrow",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
