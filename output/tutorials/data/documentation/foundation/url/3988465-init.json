      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The search path for the commonly used directory, such as ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/filemanager/searchpathdirectory/desktopdirectory",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/filemanager/searchpathdirectory/downloadsdirectory",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "directory"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The file system domain to search, which the values in ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/filemanager/searchpathdomainmask",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " define. Specify only one domain for this parameter. You may not specify ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/filemanager/searchpathdomainmask/1414912-alldomainsmask",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " with this initializer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "domain"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The file URL for determining the location of the returned URL. Only the volume of this parameter is relevant.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "The initializer ignores this parameter unless the directory parameter contains the value ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/filemanager/searchpathdirectory/itemreplacementdirectory",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " and the domain parameter contains the value ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/filemanager/searchpathdomainmask/1408037-userdomainmask",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "url"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether the initializer creates the directory if it doesn’t already exist.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shouldCreate"
        }
      ]
