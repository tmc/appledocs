      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bookmark data used to construct a URL.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "data"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Options taken into account when resolving the bookmark data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "To resolve a security-scoped bookmark to support App Sandbox, include the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsurl/bookmarkresolutionoptions/1412803-withsecurityscope",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " option.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The base URL that the bookmark data is relative to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If youâ€™re resolving a security-scoped bookmark to obtain a security-scoped URL, use this parameter as follows:",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "items": [
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "To resolve an app-scoped bookmark, use a value of ",
                          "type": "text"
                        },
                        {
                          "code": "nil",
                          "type": "codeVoice"
                        },
                        {
                          "text": ".",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                },
                {
                  "content": [
                    {
                      "inlineContent": [
                        {
                          "text": "To resolve a document-scoped bookmark, use the ",
                          "type": "text"
                        },
                        {
                          "inlineContent": [
                            {
                              "text": "absolute",
                              "type": "text"
                            }
                          ],
                          "type": "emphasis"
                        },
                        {
                          "text": " path (despite this parameterâ€™s name) to the document from which you retrieved the bookmark.",
                          "type": "text"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ]
                }
              ],
              "type": "unorderedList"
            },
            {
              "inlineContent": [
                {
                  "text": "App Sandbox doesnâ€™t restrict which URL values you can pass to this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "relativeTo"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "On return, if ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the bookmark data is stale. Your app should create a new bookmark using the returned URL and use it in place of any stored copies of the existing bookmark.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bookmarkDataIsStale"
        }
      ]
    }
