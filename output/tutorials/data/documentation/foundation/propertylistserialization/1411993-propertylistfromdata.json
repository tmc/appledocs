          "name": "opt"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If the property list is valid, upon return contains the format. ",
                  "type": "text"
                },
                {
                  "code": "format",
                  "type": "codeVoice"
                },
                {
                  "text": " can be ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", in which case the property list format is not returned. For possible values, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/propertylistserialization/propertylistformat",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "format"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Upon return, if the conversion is successful, ",
                  "type": "text"
                },
                {
                  "code": "errorString",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ". If the conversion fails, upon return contains a string describing the nature of the error.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "errorString"
        }
      ]
