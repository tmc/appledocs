        },
        {
          "anchor": "4281589",
          "level": 3,
          "text": "Configure the sample code project",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Before you run the sample code project:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "In the ",
                      "type": "text"
                    },
                    {
                      "code": "Package.swift",
                      "type": "codeVoice"
                    },
                    {
                      "text": " file of an existing HTTP server project, add ",
                      "type": "text"
                    },
                    {
                      "code": ".package(path: \"/path/to/swift-nio-resumable-upload\")",
                      "type": "codeVoice"
                    },
                    {
                      "text": " as one of the dependencies.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Import ",
                      "type": "text"
                    },
                    {
                      "code": "NIOResumableUpload",
                      "type": "codeVoice"
                    },
                    {
                      "text": " in your SwiftNIO bootstrapping code.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Create an upload context: ",
                      "type": "text"
                    },
                    {
                      "code": "let uploadContext = HTTPResumableUploadContext(origin: \"https://example.com\")",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Wrap your HTTP server channel handler within ",
                      "type": "text"
                    },
                    {
                      "code": "HTTPResumableUploadHandler",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "orderedList"
        }
      ],
      "kind": "content"
    }
