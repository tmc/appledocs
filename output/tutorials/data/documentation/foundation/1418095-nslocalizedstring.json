      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key for a string in the specified table. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Xcode can’t export localizations for strings whose ",
                      "type": "text"
                    },
                    {
                      "code": "key",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is a string variable or an empty string.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "name": "Note",
              "style": "note",
              "type": "aside"
            }
          ],
          "name": "key"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the table containing the key-value pairs. Also, the suffix for the strings file (a file with th",
                  "type": "text"
                },
                {
                  "code": "e.strings",
                  "type": "codeVoice"
                },
                {
                  "text": " extension) to store the localized string. This defaults to the table in ",
                  "type": "text"
                },
                {
                  "code": "Localizable.strings",
                  "type": "codeVoice"
                },
                {
                  "text": " when ",
                  "type": "text"
                },
                {
                  "code": "tableName",
                  "type": "codeVoice"
                },
                {
                  "text": " is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " or an empty string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tableName"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The bundle containing the table’s strings file. The main bundle is used if one isn’t specified.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bundle"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localized string for the development locale. For other locales, return this value if ",
                  "type": "text"
                },
                {
                  "code": "key",
                  "type": "codeVoice"
                },
                {
                  "text": " isn’t found in the table. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The comment to place above the key-value pair in the strings file. This parameter provides the translator with some context about the localized string’s presentation to the user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "comment"
        }
      ]
    },
    {
      "content": [
        {
