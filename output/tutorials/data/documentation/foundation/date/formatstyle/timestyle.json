      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The exact format depends on the locale. Possible time styles include ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/timestyle/3802307-omitted",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/timestyle/3766392-shortened",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/timestyle/3766393-standard",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/timestyle/3766386-complete",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following code sample shows a variety of time style format results using the ",
              "type": "text"
            },
            {
              "code": "en_US",
              "type": "codeVoice"
            },
            {
              "text": " locale.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let meetingDate = Date()",
            "meetingDate.formatted(date: .numeric, time: .omitted)",
            "// 10/17/2020",
            " ",
            "meetingDate.formatted(date: .numeric, time: .shortened)",
            "// 10/17/2020, 9:54 PM",
            " ",
            "meetingDate.formatted(date: .numeric, time: .standard)",
            "// 10/17/2020, 9:54:29 PM",
            " ",
            "meetingDate.formatted(date: .numeric, time: .complete)",
            "// 10/17/2020, 9:54:29 PM CDT",
            "",
            "meetingDate.formatted()",
            "// 10/17/2020, 9:54 PM",
            ""
          ],
          "metadata": {
            "anchor": "3848764",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The default time style is ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/timestyle/3766392-shortened",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
