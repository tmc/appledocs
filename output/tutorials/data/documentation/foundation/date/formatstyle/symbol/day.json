    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/symbol/day",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " type includes static factory variables and methods that create custom ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/symbol/day",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " objects:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "header": "none",
          "metadata": {
            "anchor": "3845724",
            "title": "Table 1"
          },
          "rows": [
            [
              [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/symbol/day/3766178-defaultdigits",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The minimum number of digits that shows the numeric day of month. For example, ",
                      "type": "text"
                    },
                    {
                      "code": "1",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "18",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/symbol/day/3766181-julianmodified",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The modified Julian day. The field length specifies the minimum number of digits, zero-padded if necessary. For example, ",
                      "type": "text"
                    },
                    {
                      "code": "2451334",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/symbol/day/3766185-ordinalofdayinmonth",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The ordinal of the day in the month. For example, the second Wednesday in July would yield ",
                      "type": "text"
                    },
                    {
                      "code": "2",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/symbol/day/3766186-twodigits",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "The two-digit numeric day of month, zero-padded if necessary. For example, ",
                      "type": "text"
                    },
                    {
                      "code": "01",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "18",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ]
          ],
          "type": "table"
        },
        {
          "inlineContent": [
            {
              "text": "To customize the day format in a string representation of a ",
              "type": "text"
            },
            {
              "code": "Date",
              "type": "codeVoice"
            },
            {
              "text": ", use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/3766398-day",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". The following example shows a variety of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/symbol/day",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " formats applied to a date.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let meetingDate = Date() // Feb 9, 2021 at 3:00 PM",
            "meetingDate.formatted(Date.FormatStyle().day(.defaultDigits)) // 9",
            "meetingDate.formatted(Date.FormatStyle().day(.ordinalOfDayInMonth)) // 2 (second Tuesday of the month)",
            "meetingDate.formatted(Date.FormatStyle().day(.twoDigits)) // 09",
            "meetingDate.formatted(Date.FormatStyle().day(.julianModified(minimumLength: 12))) // 0002459255",
            "meetingDate.formatted(Date.FormatStyle().day()) // 9"
          ],
          "metadata": {
            "anchor": "3843020",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "If no format is specified as a parameter, the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/symbol/day/3766178-defaultdigits",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " static variable is the default format.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For more information about formatting dates, see the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
