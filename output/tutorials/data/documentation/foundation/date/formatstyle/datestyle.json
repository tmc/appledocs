    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The exact format depends on the locale. Possible values of date style include ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/datestyle/3802305-omitted",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/datestyle/3766159-numeric",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/datestyle/3766151-abbreviated",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/datestyle/3766157-long",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/date/formatstyle/datestyle/3766152-complete",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following code sample shows a variety of date style format results using the ",
              "type": "text"
            },
            {
              "code": "en_US",
              "type": "codeVoice"
            },
            {
              "text": " locale.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let meetingDate = Date()",
            "meetingDate.formatted(date: .omitted, time: .standard) ",
            "// 9:42:14 AM",
            "",
            "meetingDate.formatted(date: .numeric, time: .omitted) ",
            "// 10/17/2020",
            "",
            "meetingDate.formatted(date: .abbreviated, time: .omitted)",
            "// Oct 17, 2020",
            "",
            "meetingDate.formatted(date: .long, time: .omitted) ",
            "// October 17, 2020",
            "",
            "meetingDate.formatted(date: .complete, time: .omitted) ",
            "// Saturday, October 17, 2020",
            "",
            "meetingDate.formatted()",
            "// 10/17/2020, 9:42 AM"
          ],
          "metadata": {
            "anchor": "3848849",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The default date style is ",
              "type": "text"
            },
            {
              "code": "numeric",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
