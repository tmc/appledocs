      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The arguments to the ",
              "type": "text"
            },
            {
              "identifier": "devLink-1737716",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " are two objects to compare. The block returns an ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nscomparisonresult",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " value to denote the ordering of the two objects.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "You use ",
              "type": "text"
            },
            {
              "code": "NSComparator",
              "type": "codeVoice"
            },
            {
              "text": " blocks in comparison operations such as ",
              "type": "text"
            },
            {
              "code": "NSArray",
              "type": "codeVoice"
            },
            {
              "text": "’s ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsarray/1411195-sortedarrayusingcomparator",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", for example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NSArray *sortedArray = [array sortedArrayUsingComparator: ^(id obj1, id obj2) {",
            " ",
            "    if ([obj1 integerValue] > [obj2 integerValue]) {",
            "        return (NSComparisonResult)NSOrderedDescending;",
            "    }",
            " ",
            "    if ([obj1 integerValue] < [obj2 integerValue]) {",
            "        return (NSComparisonResult)NSOrderedAscending;",
            "    }",
            "    return (NSComparisonResult)NSOrderedSame;",
            "}];"
          ],
          "metadata": {
            "anchor": "2555920",
            "title": "Listing 1"
          },
          "syntax": "occ",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
