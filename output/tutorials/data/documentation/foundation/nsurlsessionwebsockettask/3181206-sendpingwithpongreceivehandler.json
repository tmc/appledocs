          "content": [
            {
              "inlineContent": [
                {
                  "text": "A block called by the task when it receives the pong from the server. The block receives an ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nserror",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " that indicates a lost connection or other error, or ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " if no error cocured.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pongReceiveHandler"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When sending multiple pings, the task always calls ",
              "type": "text"
            },
            {
              "code": "pongReceiveHandler",
              "type": "codeVoice"
            },
            {
              "text": " in the order it sent the pings.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
