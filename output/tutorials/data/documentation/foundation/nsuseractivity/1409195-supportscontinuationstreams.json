            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/false",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": ". It can dynamically be set to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/true",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " to selectively support continuation streams based on the state of the user activity.",
              "type": "text"
            }
          ],
          "type": "paragraph"
