        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An XML node object representing an attribute. If the receiver already has an attribute with the same name, ",
                  "type": "text"
                },
                {
                  "code": "anAttribute",
                  "type": "codeVoice"
                },
                {
                  "text": " replaces the old attribute.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "anAttribute"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The order of multiple attributes is preserved if the ",
              "type": "text"
            },
            {
              "code": "NSXMLPreserveAttributeOrder",
              "type": "codeVoice"
            },
            {
              "text": " option is specified when the element is created.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
