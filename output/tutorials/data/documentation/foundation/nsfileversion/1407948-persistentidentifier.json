        {
          "inlineContent": [
            {
              "text": "You can save the value of this property persistently and use it to recreate the version object later. When recreating the version object using the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsfileversion/1415443-version",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method, the version object returned is equivalent to the current object.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
