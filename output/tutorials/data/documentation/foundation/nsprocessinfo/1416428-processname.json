      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The process name is used to register application defaults and is used in error messages. It does not uniquely identify the process.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "User defaults and other aspects of the environment might depend on the process name, so be very careful if you change it. Setting the process name in this manner is not thread safe.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "style": "warning",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
