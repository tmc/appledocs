    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Extend this type with the names of user info keys you want to associate with undo actions, like this:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "extension UndoManager.UserInfoKey {",
            "    static let icon: UndoManager.UserInfoKey = \"icon\"",
            "}"
          ],
          "metadata": {
            "anchor": "4447159",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "You then use this key when you set and get undo user info values.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "self.undoManager.setActionUserInfoValue(Image(named: \"new_layer\"), forKey: .icon)",
            ""
          ],
          "metadata": {
            "anchor": "4447160",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
