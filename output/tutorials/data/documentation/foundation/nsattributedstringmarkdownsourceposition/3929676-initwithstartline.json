      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The line number where text begins in the Markdown source. Specify a 1-based number. For example, the number for the first row is 1, for the second row is 2, and so on. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "startLine"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The column number where text begins in the Markdown source. Specify a 1-based number. For example, the number for the first column is 1, for the second column is 2, and so on. Columns represent UTF-8 indices; for multi-byte characters, the column indicates the first byte. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "startColumn"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The line number where the Markdown source ends. Specify a 1-based number.  ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "endLine"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The column number where the Markdown source ends. Specify a 1-based number.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "endColumn"
        }
      ]
    }
