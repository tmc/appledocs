        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A level of indent, to make the output more readable: set ",
                  "type": "text"
                },
                {
                  "code": "level",
                  "type": "codeVoice"
                },
                {
                  "text": " to ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " to use four spaces to indent, or ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": " to indent the output with a tab character.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "level"
        }
