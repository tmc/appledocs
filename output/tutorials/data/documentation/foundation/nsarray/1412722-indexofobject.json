      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object for which to search in the array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If this value is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", throws an ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsinvalidargumentexception",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "obj"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The range within the array to search for ",
                  "type": "text"
                },
                {
                  "code": "obj",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "r",
                  "type": "codeVoice"
                },
                {
                  "text": " exceeds the bounds of the array (if the location plus length of the range is greater than the count of the array), throws an ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsrangeexception",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "r"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Options for the search. For possible values, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If you specify both ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchingfirstequal",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " and ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinglastequal",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", throws an ",
                  "type": "text"
                },
                {
                  "code": "NSInvalidArgumentException",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "opts"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A comparator block used to compare the object ",
                  "type": "text"
                },
                {
                  "code": "obj",
                  "type": "codeVoice"
                },
                {
                  "text": " with elements in the array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If this value is ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": ", throws an ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsinvalidargumentexception",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "cmp"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinginsertionindex",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " option is not specified:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the ",
                      "type": "text"
                    },
                    {
                      "code": "obj",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is found and neither ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchingfirstequal",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " nor ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinglastequal",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " is specified, returns an arbitrary matching object's index.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchingfirstequal",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " option is also specified, returns the lowest index of equal objects. ",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinglastequal",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " option is also specified, returns the highest index of equal objects. ",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the object is not found, returns ",
                      "type": "text"
                    },
                    {
                      "code": "NSNotFound",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "If the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinginsertionindex",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " option is specified, returns the index at which you should insert ",
              "type": "text"
            },
            {
              "code": "obj",
              "type": "codeVoice"
            },
            {
              "text": " in order to maintain a sorted array:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the ",
                      "type": "text"
                    },
                    {
                      "code": "obj",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is found and neither ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchingfirstequal",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " nor ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinglastequal",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " is specified, returns any equal or one larger index than any matching objectâ€™s index.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchingfirstequal",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " option is also specified, returns the lowest index of equal objects.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.documentation/documentation/foundation/nsbinarysearchingoptions/nsbinarysearchinglastequal",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " option is also specified, returns the highest index of equal objects. ",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the object is not found, returns the index of the least greater object, or the index at the end of the array if the object is larger than all other elements. ",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The elements in the array must have already been sorted using the comparator ",
              "type": "text"
            },
            {
              "code": "cmp",
              "type": "codeVoice"
            },
            {
              "text": ".  If the array is not sorted, the result is undefined.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
