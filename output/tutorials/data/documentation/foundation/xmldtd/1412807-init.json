                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nserror",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object describing any errors and warnings related to parsing and remote connection. ",
                  "type": "text"
                }
