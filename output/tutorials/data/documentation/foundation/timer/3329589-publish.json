    {
      "kind": "parameters",
      "languages": [
        "swift"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The time interval on which to publish events. For example, a value of ",
                  "type": "text"
                },
                {
                  "code": "0.5",
                  "type": "codeVoice"
                },
                {
                  "text": " publishes an event approximately every half-second.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "interval"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The allowed timing variance when emitting events. Defaults to ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", which allows any variance.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tolerance"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The run loop on which the timer runs.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "runLoop"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The run loop mode in which to run the timer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Scheduler options passed to the timer. Defaults to ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A publisher that repeatedly emits the current date on the given interval.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The return type, ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/timer/timerpublisher",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", conforms to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Combine/documentation/Combine/ConnectablePublisher",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", which means you must explicitly connect to the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/timer",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " publisher to begin publishing events. You can do this with a call to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Combine/documentation/Combine/ConnectablePublisher/connect()",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", or by using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.Combine/documentation/Combine/ConnectablePublisher/autoconnect()",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to automatically connect when a subscriber attaches, as shown here:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "cancellable = Timer.publish(every: 1, on: .main, in: .common)",
            "    .autoconnect()",
            "    .sink() {",
            "        print (\"timer fired: \\($0)\")",
            "}",
            ""
          ],
          "metadata": {
            "anchor": "3332421",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
