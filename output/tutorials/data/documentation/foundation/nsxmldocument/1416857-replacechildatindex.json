            {
              "inlineContent": [
                {
                  "text": "An integer identifying a position in the receiver's array of children. If ",
                  "type": "text"
                },
                {
                  "code": "index",
                  "type": "codeVoice"
                },
                {
                  "text": " is less than zero or greater than the number of children minus one, an out-of-bounds exception is raised.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "index"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsxmlnode",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object to replace the one at ",
                  "type": "text"
                },
                {
                  "code": "index",
                  "type": "codeVoice"
                },
                {
                  "text": "; it must represent a comment, a processing instruction, or the root element. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "node"
