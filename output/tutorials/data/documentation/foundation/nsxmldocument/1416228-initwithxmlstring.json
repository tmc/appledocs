        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string object containing XML markup text.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "string"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A bit mask for input options. You can specify multiple options by bit-OR'ing them. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsxmldocument#1659449",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a list of valid input options.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mask"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An error object that, on return, identifies any parsing errors and warnings or connection problems. ",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "error"
        }
      ]
    },
