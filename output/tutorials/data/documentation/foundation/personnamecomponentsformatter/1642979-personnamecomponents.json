            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nspersonnamecomponents",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " object parsing string using the receiverâ€™s format, or ",
              "type": "text"
