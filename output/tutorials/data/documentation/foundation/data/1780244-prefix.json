      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The resulting subsequence ",
              "type": "text"
            },
            {
              "inlineContent": [
                {
                  "text": "includes",
                  "type": "text"
                }
              ],
              "type": "emphasis"
            },
            {
              "text": " the element at the position specified by the ",
              "type": "text"
            },
            {
              "code": "through",
              "type": "codeVoice"
            },
            {
              "text": " parameter. The following example searches for the index of the number ",
              "type": "text"
            },
            {
              "code": "40",
              "type": "codeVoice"
            },
            {
              "text": " in an array of integers, and then prints the prefix of the array up to, and including, that index:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let numbers = [10, 20, 30, 40, 50, 60]",
            "if let i = numbers.firstIndex(of: 40) {",
            "    print(numbers.prefix(through: i))",
            "}",
            "// Prints \"[10, 20, 30, 40]\""
          ],
          "metadata": {
            "anchor": "2994462",
            "title": "Listing 1"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Using the ",
              "type": "text"
            },
            {
              "code": "prefix(through:)",
              "type": "codeVoice"
            },
            {
              "text": " method is equivalent to using a partial closed range as the collectionâ€™s subscript. The subscript notation is preferred over ",
              "type": "text"
            },
            {
              "code": "prefix(through:)",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "if let i = numbers.firstIndex(of: 40) {",
            "    print(numbers[...i])",
            "}",
            "// Prints \"[10, 20, 30, 40]\""
          ],
          "metadata": {
            "anchor": "2994463",
            "title": "Listing 2"
          },
          "syntax": "swift",
          "type": "codeListing"
        },
        {
