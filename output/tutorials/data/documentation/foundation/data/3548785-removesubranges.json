      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example, this code sample finds the indices of all the vowel characters in the string, and then removes those characters.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var str = \"The rain in Spain stays mainly in the plain.\"",
            "let vowels: Set<Character> = [\"a\", \"e\", \"i\", \"o\", \"u\"]",
            "let vowelIndices = str.subranges(where: { vowels.contains($0) })",
            "",
            "str.removeSubranges(vowelIndices)",
            "// str == \"Th rn n Spn stys mnly n th pln.\""
          ],
          "metadata": {
