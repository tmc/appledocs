            {
              "text": "An ",
              "type": "text"
            },
            {
              "code": "NSDateComponents",
              "type": "codeVoice"
            },
            {
              "text": " object containing ",
              "type": "text"
            },
            {
              "code": "date",
              "type": "codeVoice"
            },
            {
              "text": " decomposed into the components specified by ",
              "type": "text"
            },
            {
              "code": "unitFlags",
              "type": "codeVoice"
            },
            {
              "text": ". Returns ",
              "type": "text"
