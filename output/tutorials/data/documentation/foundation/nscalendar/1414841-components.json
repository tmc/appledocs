      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The Weekday ordinality, when requested, refers to the next larger (than Week) of the requested units. Some computations can take a relatively long time.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following example shows how to use this method to determine the current year, month, and day, using an existing calendar (",
              "type": "text"
            },
            {
              "code": "gregorian",
              "type": "codeVoice"
            },
            {
              "text": "):",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "unsigned unitFlags = NSYearCalendarUnit | NSMonthCalendarUnit |  NSDayCalendarUnit;",
            "NSDate *date = [NSDate date];",
            "NSDateComponents *comps = [gregorian components:unitFlags fromDate:date];"
          ],
          "metadata": {
            "anchor": "2555553",
            "title": "Listing 1"
          },
          "syntax": "occ",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
