          "content": [
            {
              "inlineContent": [
                {
                  "text": "Any combination of the  ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/compareoptions/1412382-caseinsensitive",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/compareoptions/1409350-widthinsensitive",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/compareoptions/1412313-diacriticinsensitive",
                  "isActive": true,
                  "type": "reference"
                },
