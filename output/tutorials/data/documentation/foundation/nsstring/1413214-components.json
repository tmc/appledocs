            {
              "text": "An ",
              "type": "text"
            },
            {
              "code": "NSArray",
              "type": "codeVoice"
            },
            {
              "text": " object containing substrings from the receiver that have been divided by ",
              "type": "text"
            },
            {
              "code": "separator",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
