      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A format string. See ",
                  "type": "text"
                },
                {
                  "identifier": "devLink-1697341",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for examples of how to use this method, and ",
                  "type": "text"
                },
                {
                  "identifier": "devLink-1729804",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for a list of format specifiers. This value must not be ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Raises an ",
                      "type": "text"
                    },
                    {
                      "code": "NSInvalidArgumentException",
                      "type": "codeVoice"
                    },
                    {
                      "text": " if ",
                      "type": "text"
                    },
                    {
                      "code": "format",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is ",
                      "type": "text"
                    },
                    {
                      "code": "nil",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "style": "important",
              "type": "aside"
            }
          ],
          "name": "format"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A list of arguments to substitute into ",
                  "type": "text"
                },
                {
                  "code": "format",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "argList"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return_value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "An ",
              "type": "text"
            },
            {
              "code": "NSString",
              "type": "codeVoice"
            },
            {
              "text": " object initialized by using ",
              "type": "text"
            },
            {
              "code": "format",
              "type": "codeVoice"
            },
            {
              "text": " as a template into which the values in ",
              "type": "text"
            },
            {
              "code": "argList",
              "type": "codeVoice"
            },
            {
              "text": " are substituted according to the current locale. The returned object may be different from the original receiver.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This method is meant to be called from within a variadic function, where the argument list will be available.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This method invokes ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/1408503-init",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " without applying any localization. This is useful, for example, when working with fixed-format representations of information that is written out and read back in at a later time.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "When working with text that’s presented to the user, use the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/1497301-localizedstringwithformat",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " method, or the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/1497317-initwithformat",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/1408503-init",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " method, passing ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nslocale/1409990-current",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " as the locale.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "style": "important",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
