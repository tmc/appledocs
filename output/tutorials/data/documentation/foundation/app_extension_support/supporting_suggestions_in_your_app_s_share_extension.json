{
  "abstract": [
    {
      "text": "Make your messaging app available for share sheet suggestions and use SiriKit intents to populate your app’s share extension.",
      "type": "text"
    }
  ],
  "documentVersion": 0,
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
