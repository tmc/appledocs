          "content": [
            {
              "inlineContent": [
                {
                  "text": "The path of the file or directory. If ",
                  "type": "text"
                },
                {
                  "code": "path",
                  "type": "codeVoice"
                },
                {
                  "text": " begins with a tilde (",
                  "type": "text"
                },
                {
                  "code": "~",
                  "type": "codeVoice"
                },
                {
                  "text": "), it must first be expanded with ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsstring/1407716-expandingtildeinpath",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": "; otherwise, this method returns ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/swift/false",
                  "isActive": false,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "App Sandbox does not restrict which path values may be passed to this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "path"
        }
