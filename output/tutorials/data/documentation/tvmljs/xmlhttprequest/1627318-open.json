      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The HTTP method for the request. ",
                  "type": "text"
                },
                {
                  "code": "connect",
                  "type": "codeVoice"
                },
                {
                  "text": ", ",
                  "type": "text"
                },
                {
                  "code": "trace",
                  "type": "codeVoice"
                },
                {
                  "text": ", and ",
                  "type": "text"
                },
                {
                  "code": "track",
                  "type": "codeVoice"
                },
                {
                  "text": " are invalid values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "method"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The URL for the request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "urlStr"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An integer value indicating if the request is asynchronous. ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": " is a synchronous request. Any positive integer is an asynchronous request. The default value is ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "asyncNum"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Not supported by Apple TV. This value must be set to ",
                  "type": "text"
                },
                {
                  "code": "null",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "user"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Not supported by Apple TV. This value must be set to ",
                  "type": "text"
                },
                {
                  "code": "null",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "password"
        }
      ]
    },
