      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The dot-separated sequence of properties from the receiver. The path can contain array indexers. For example, ",
                  "type": "text"
                },
                {
                  "code": "items[0].title",
                  "type": "codeVoice"
                },
                {
                  "text": " refers to the ",
                  "type": "text"
                },
                {
                  "code": "title",
                  "type": "codeVoice"
                },
                {
                  "text": " property stored in index location 0 in the ",
                  "type": "text"
                },
                {
                  "code": "items",
                  "type": "codeVoice"
                },
                {
                  "text": " array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "path"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object associated with the property path.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Set the property path to associate the data item objects contained in the ",
              "type": "text"
            },
            {
              "code": "value",
              "type": "codeVoice"
            },
            {
              "text": " parameter with a section element. ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/tvmljs/dataitem/2897832-setpropertypath#2902804",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " shows an example of data item objects contained in the ",
              "type": "text"
            },
            {
              "code": "newItems",
              "type": "codeVoice"
            },
            {
              "text": " object added to the section that binds the ",
              "type": "text"
            },
            {
              "code": "images",
              "type": "codeVoice"
            },
            {
              "text": " path; for example ",
              "type": "text"
            },
            {
              "code": "<section binding=\"items:{images};\" />",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let section = shelf.getElementsByTagName(\"section\").item(0)section.dataItem = new DataItem()",
            "",
            "let newItems = results.map((result) => {",
            "    let objectItem = new DataItem(result.type, result.ID);",
            "    objectItem.url = result.url;",
            "    return objectItem;",
            "});",
            "",
            "section.dataItem.setPropertyPath(\"images\", newItems)"
          ],
          "metadata": {
            "abstract": [
              {
                "text": "Setting the property path",
                "type": "text"
              }
            ],
            "anchor": "2902804",
            "title": "Listing 1"
          },
          "syntax": "javascript",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
