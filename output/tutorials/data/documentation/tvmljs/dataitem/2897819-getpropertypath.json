                {
                  "code": "items[0].title",
                  "type": "codeVoice"
                },
                {
                  "text": " refers to the ",
                  "type": "text"
                },
                {
                  "code": "title",
                  "type": "codeVoice"
                },
                {
                  "text": " property stored in index location 0 in the ",
                  "type": "text"
                },
                {
                  "code": "items",
