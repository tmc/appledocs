        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "code": "oldState",
                      "type": "codeVoice"
                    },
                    {
                      "text": "—The previous state of the player. Valid values are ",
                      "type": "text"
                    },
                    {
                      "code": "playing",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "paused",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", and ",
                      "type": "text"
                    },
                    {
                      "code": "scanning",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
