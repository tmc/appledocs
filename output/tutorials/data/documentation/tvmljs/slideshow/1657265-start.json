    {
      "kind": "parameters",
      "languages": [
        "data"
      ],
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of image request objects.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "imageRequests"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional set of options that modify the slideshow. If no options are specified, a default of ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": "is used.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An optional callback function that is called when the slideshow is dismissed. If this parameter is not specified, the page on top of the navigation stack is displayed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "exitCB"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "When a new slideshow beings using the ",
              "type": "text"
            },
            {
              "code": "start",
              "type": "codeVoice"
            },
            {
              "text": " function, if there is a currently running slideshow, that slideshow is dismissed. The ",
              "type": "text"
            },
            {
              "code": "imageRequests",
              "type": "codeVoice"
            },
            {
              "text": " parameter contains an array of image request objects that can have the following formatting:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "An array of URLs.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A dictionary with two key-value pairs, ",
                      "type": "text"
                    },
                    {
                      "code": "headers",
                      "type": "codeVoice"
                    },
                    {
                      "text": " and ",
                      "type": "text"
                    },
                    {
                      "code": "urls",
                      "type": "codeVoice"
                    },
                    {
                      "text": ". ",
                      "type": "text"
                    },
                    {
                      "code": "headers",
                      "type": "codeVoice"
                    },
                    {
                      "text": " provides a dictionary of headers that is included with all image requests. ",
                      "type": "text"
                    },
                    {
                      "code": "urls",
                      "type": "codeVoice"
                    },
                    {
                      "text": " contains an array of URLs.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "If the ",
              "type": "text"
            },
            {
              "code": "showSettings",
              "type": "codeVoice"
            },
            {
              "text": " key in the ",
              "type": "text"
            },
            {
              "code": "options",
              "type": "codeVoice"
            },
            {
              "text": " parameter is set to ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ", the user will be asked to pick a theme before the slideshow begins. ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/tvmljs/slideshow/1657265-start#1943543",
              "isActive": false,
              "type": "reference"
            },
            {
              "text": " shows an example of a simple slideshow that is created with two images.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "function mySlideshow() {",
            "   var images = ['path to image', 'path to image'];",
            "   Slideshow.start(images, { 'showSettings': '0' });",
            "}"
          ],
          "metadata": {
            "abstract": [
              {
                "text": "Creating a new slideshow example",
                "type": "text"
              }
            ],
            "anchor": "1943543",
            "title": "Listing 1"
          },
          "syntax": "javascript",
          "type": "codeListing"
        }
      ],
      "kind": "content"
