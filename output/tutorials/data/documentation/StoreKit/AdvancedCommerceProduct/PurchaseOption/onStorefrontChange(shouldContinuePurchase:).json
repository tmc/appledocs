          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that returns a Boolean value that determines whether the purchase continues when the storefront changes to ",
                  "type": "text"
                },
                {
                  "code": "Storefront",
                  "type": "codeVoice"
                },
                {
                  "text": " during the purchase process.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shouldContinuePurchase"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The default is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": " if you don’t include this option in the purchase options.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
