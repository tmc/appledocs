    {
      "kind": "mentions",
      "mentions": [
        "doc://com.apple.storekit/documentation/StoreKit/choosing-a-receipt-validation-technique",
        "doc://com.apple.storekit/documentation/StoreKit/validating-receipts-with-the-app-store",
        "doc://com.apple.storekit/documentation/StoreKit/testing-in-app-purchases-in-xcode",
