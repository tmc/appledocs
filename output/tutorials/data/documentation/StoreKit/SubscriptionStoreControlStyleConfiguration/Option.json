    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You use ",
              "type": "text"
            },
            {
              "code": "SubscriptionStoreControlStyleConfiguration.Option",
              "type": "codeVoice"
            },
            {
              "text": " very similarly to ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.storekit/documentation/StoreKit/Product",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " values, with some important differences:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The ",
                      "type": "text"
                    },
                    {
                      "code": "offer",
                      "type": "codeVoice"
                    },
                    {
                      "text": " property indicates the offer that applies to the purchase. Display the terms of the ",
                      "type": "text"
                    },
                    {
                      "code": "offer",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", and ignore offer properties on ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.storekit/documentation/StoreKit/Product/SubscriptionInfo",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", such as ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.storekit/documentation/StoreKit/Product/SubscriptionInfo/introductoryOffer",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If the ",
                      "type": "text"
                    },
                    {
                      "code": "offer",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is ",
                      "type": "text"
                    },
                    {
                      "code": "nil",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", no offer applies to the purchase.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Call the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.storekit/documentation/StoreKit/SubscriptionStoreControlStyleConfiguration/Option/subscribe()",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " method when a customer activates a control to subscribe to a subscription option, instead of methods on ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.storekit/documentation/StoreKit/Product",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ", such as ",
                      "type": "text"
                    },
                    {
                      "code": "Product/purchase(confirmIn:options:)",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Access the decorative icon using the ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.storekit/documentation/StoreKit/SubscriptionStoreControlStyleConfiguration/Option/icon",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " property.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "code": "SubscriptionStoreControlStyleConfiguration.Option",
              "type": "codeVoice"
            },
            {
              "text": " is a dynamic member lookup type, so you don’t need to use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.storekit/documentation/StoreKit/SubscriptionStoreControlStyleConfiguration/Option/subscription",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " directly to access the properties of the ",
              "type": "text"
            },
            {
              "code": "Product",
              "type": "codeVoice"
            },
            {
              "text": " value. Instead, access the properties of ",
              "type": "text"
            },
            {
              "code": "Product",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.storekit/documentation/StoreKit/Product/SubscriptionInfo",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " directly on the ",
              "type": "text"
            },
            {
              "code": "Option",
              "type": "codeVoice"
            },
            {
              "text": " value. In the example below, the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.storekit/documentation/StoreKit/Product/displayName",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " property is available on ",
              "type": "text"
            },
            {
              "code": "Option",
              "type": "codeVoice"
            },
            {
              "text": " to use as the button label:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct DisplayNameButtonsControlStyle: SubscriptionStoreControlStyle {",
            "",
            "    func makeBody(configuration: Configuration) -> some View {",
            "        ForEach(configuration.options) { option in",
            "            Button(option.displayName, action: option.subscribe)",
            "        }",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
