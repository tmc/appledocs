    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string that identifies the private key you use to generate the signature. You set up this key in App Store Connect. For more information, see ",
                  "type": "text"
                },
                {
                  "identifier": "https://developer.apple.com/help/app-store-connect/configure-in-app-purchase-settings/generate-keys-for-in-app-purchases",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "keyID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A one-time UUID value that your server generates. Generate a new nonce for each signature. The string representation of the nonce you use in the signature must be lowercase.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "nonce"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A timestamp your server generates in UNIX time format, in milliseconds. The timestamp keeps the offer active for 24 hours.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "timestamp"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The cryptographic signature your server generates to sign the promotional offer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "signature"
        }
      ]
    }
