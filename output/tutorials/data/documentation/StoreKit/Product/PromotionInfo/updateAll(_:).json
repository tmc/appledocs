    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A collection of ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.storekit/documentation/StoreKit/Product/PromotionInfo",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " objects that you list in the order they are to appear in the App Store on the user’s device. Use an empty collection to cancel previous changes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "promotions"
        }
      ]
    },
    {
      "kind": "mentions",
      "mentions": [
