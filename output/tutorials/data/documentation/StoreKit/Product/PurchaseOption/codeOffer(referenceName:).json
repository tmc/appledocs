      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The reference name of the offer code to apply to the transaction. You need to set up offer codes in your StoreKit configuration file.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "referenceName"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this purchase option when you test your app in Xcode using ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/StoreKitTest",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and call ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/StoreKitTest/SKTestSession/buyProduct(identifier:options:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Set up the offer codes to use in this call in your StoreKit configuration file. For more information, see ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/Xcode/setting-up-storekit-testing-in-xcode",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "When you apply this option, the purchase transaction simulates a customer redeeming an offer code and includes the offer code you specify.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
