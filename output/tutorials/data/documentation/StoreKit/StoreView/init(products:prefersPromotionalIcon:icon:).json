    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The products to merchandise.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "products"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates whether to use promotional images from the App Store, if they’re available. If this parameter is ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": ", the system ignores promotional images.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "prefersPromotionalIcon"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A closure that returns the image the view displays when the products finish loading from the App Store.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "icon"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If you set ",
              "type": "text"
            },
            {
              "code": "prefersPromotionalIcon",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ", the view uses promotional images for products that have a promotional image available.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The following code example shows how to create a store view using a custom icon:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "StoreView(products: [",
            "          product1,",
            "          product2,",
            "          // Add products as needed.",
            " ]) { product in",
            "    Image(systemName: \"star.fill\")",
            "        .foregroundStyle(.yellow)",
            "    }  "
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
