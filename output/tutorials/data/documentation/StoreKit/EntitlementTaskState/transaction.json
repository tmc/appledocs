        {
          "inlineContent": [
            {
              "text": "Use ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.storekit/documentation/StoreKit/EntitlementTaskState/transaction",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " as a convenience to access the transaction value in code that doesn’t depend on the reason a transaction isn’t available. The value is ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " while the transaction is loading, if it fails to load for any reason, or if the customer isn’t entitled to the product.",
              "type": "text"
            }
          ],
