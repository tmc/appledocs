    "doc://com.apple.storekit/documentation/StoreKit/ExternalPurchaseCustomLink/token(for:)": {
      "abstract": [
        {
          "text": "Requests an external purchase token of the specified type.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "keyword",
          "text": "static"
