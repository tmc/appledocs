      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.storekit/documentation/StoreKit/ExternalPurchaseCustomLink/NoticeType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " value that determines the disclosure sheet the system displays.",
