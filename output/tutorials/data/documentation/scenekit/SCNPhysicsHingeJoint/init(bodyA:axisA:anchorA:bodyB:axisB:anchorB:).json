    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The first physics body to be connected by the joint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bodyA"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The axis that the hinge pivots around, relative to the node containing the first body.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axisA"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The point at which the hinge connects, relative to the node containing the first body.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "anchorA"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The second physics body to be connected by the joint.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bodyB"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The axis that the hinge pivots around, relative to the node containing the second body.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axisB"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
