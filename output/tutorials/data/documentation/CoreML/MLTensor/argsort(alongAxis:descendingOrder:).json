      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The axis along which to sort. The default is ",
                  "type": "text"
                },
                {
                  "code": "-1",
                  "type": "codeVoice"
                },
                {
                  "text": ", which sorts the last axis.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axis"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that determines the sort order. The default is ",
                  "type": "text"
                },
                {
                  "code": "false",
                  "type": "codeVoice"
                },
                {
                  "text": " which",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "sorts from largest to least.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "descendingOrder"
        }
      ]
    },
