    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The tensors to stack. All tensors must have the same shape and scalar type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "tensors"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The axis along which to stack. Negative values wrap around but must be in the range ",
                  "type": "text"
                },
                {
                  "code": "[-rank, rank]",
                  "type": "codeVoice"
                },
                {
                  "text": ", where ",
                  "type": "text"
                },
                {
                  "code": "rank",
                  "type": "codeVoice"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "is the rank of the provided tensors.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axis"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Given that ",
              "type": "text"
            },
            {
              "code": "tensors",
              "type": "codeVoice"
            },
            {
              "text": " all have shape ",
              "type": "text"
            },
            {
              "code": "[A, B, C]",
              "type": "codeVoice"
            },
            {
              "text": ", and ",
              "type": "text"
            },
            {
              "code": "tensors.count = N",
              "type": "codeVoice"
            },
            {
              "text": ", then:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "if ",
                      "type": "text"
                    },
                    {
                      "code": "axis == 0",
                      "type": "codeVoice"
                    },
                    {
                      "text": " then the resulting tensor will have the shape ",
                      "type": "text"
                    },
                    {
                      "code": "[N, A, B, C]",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "if ",
                      "type": "text"
                    },
                    {
                      "code": "axis == 1",
                      "type": "codeVoice"
                    },
                    {
                      "text": " then the resulting tensor will have the shape ",
                      "type": "text"
                    },
                    {
                      "code": "[A, N, B, C]",
                      "type": "codeVoice"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "etc.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// 'x' is [1, 4]",
            "// 'y' is [2, 5]",
            "// 'z' is [3, 6]",
            "MLTensor(stacking: [x, y, z]) // is [[1, 4], [2, 5], [3, 6]]",
            "MLTensor(stacking: [x, y, z], alongAxis: 1) // is [[1, 2, 3], [4, 5, 6]]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
