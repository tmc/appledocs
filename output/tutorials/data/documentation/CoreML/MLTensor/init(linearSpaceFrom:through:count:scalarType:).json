      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The starting value to use for the sequence. If the sequence contains any values, the first one is ",
                  "type": "text"
                },
                {
                  "code": "start",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "start"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An end value to limit the sequence. ",
                  "type": "text"
                },
                {
                  "code": "end",
                  "type": "codeVoice"
                },
                {
                  "text": " is the last element of the resulting sequence.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "end"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of values in the resulting sequence. ",
                  "type": "text"
                },
                {
                  "code": "count",
                  "type": "codeVoice"
                },
                {
                  "text": " must be greater than ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "count"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scalar type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "scalarType"
        }
      ]
