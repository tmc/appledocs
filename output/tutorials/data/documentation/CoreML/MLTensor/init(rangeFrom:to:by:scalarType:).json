      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The starting value to use for the sequence. If the sequence contains any values, the first one is ",
                  "type": "text"
                },
                {
                  "code": "start",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "start"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An end value to limit the sequence. ",
                  "type": "text"
                },
                {
                  "code": "end",
                  "type": "codeVoice"
                },
                {
                  "text": " is never an element of the resulting sequence.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "end"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The amount to step by with each iteration. ",
                  "type": "text"
                },
                {
                  "code": "stride",
                  "type": "codeVoice"
                },
                {
                  "text": " must be positive.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "stride"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scalar type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "scalarType"
        }
      ]
    }
