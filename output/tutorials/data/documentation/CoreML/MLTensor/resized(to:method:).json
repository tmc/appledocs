      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The new size for the spatial dimensions of the tensor. The size must be positive.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "size"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The resize method. The default value is ",
                  "type": "text"
                },
                {
                  "code": ".nearest",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "method"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let image = MLTensor(shape: [1, 1, 2, 2], scalars: [",
            "    1, 0,",
            "    0, 1",
            "], scalarType: Float.self)",
            "let resizedImage = image.resized(to: (4, 4), method: .nearest)",
            "// [[[[1, 1, 0, 0],",
            "//    [1, 1, 0, 0],",
            "//    [0, 0, 1, 1],",
            "//    [0, 0, 1, 1]]]]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The tensor must be either a 4-dimensional float tensor of shape ",
              "type": "text"
            },
            {
              "code": "[batch, channels, height, width]",
              "type": "codeVoice"
            },
            {
              "text": " or 3-dimensional float",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "tensor of shape ",
              "type": "text"
            },
            {
              "code": "[channel, height, width]",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
