    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of diagonals in the lower triangle to keep. If ",
                  "type": "text"
                },
                {
                  "code": "-1",
                  "type": "codeVoice"
                },
                {
                  "text": ", keep the entire lower triangle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "lowerBandCount"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of diagonals in the upper triangle to keep. If ",
                  "type": "text"
                },
                {
                  "code": "-1",
                  "type": "codeVoice"
                },
                {
                  "text": ", keep the entire upper triangle.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "upperBandCount"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The band part of the tensor.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The framework copies a diagonal band of values from the tensor to the result tensor of the same size.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "A coordinate ",
              "type": "text"
            },
            {
              "code": "[..., i, j]",
              "type": "codeVoice"
            },
            {
              "text": " is considered in band if",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "(lowerBandCount < 0 || (i-j) <= lowerBandCount) && (upperBandCount < 0 || (j-i) <= upperBandCount)"
          ],
          "syntax": "md",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "Values outside of the band are set to ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For example:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let t = Tensor([",
            "    [ 5,  1,  2, 3],",
            "    [-1,  5,  1, 2],",
            "    [-2, -1,  5, 1],",
            "    [-3, -2, -1, 5]",
            "], scalarType: Float.self)",
            "",
            "t.bandPart(lowerBandCount: 0, upperBandCount: 0)",
            "// [[ 5,  0,  0, 0]",
            "//  [ 0,  5,  0, 0]",
            "//  [ 0,  0,  5, 0]",
            "//  [ 0,  0,  0, 5]]",
            "",
            "t.bandPart(lowerBandCount: 0, upperBandCount: -1)",
            "// [[ 5,  1,  2, 3]",
            "//  [ 0,  5,  1, 2]",
            "//  [ 0,  0,  5, 1]",
            "//  [ 0,  0,  0, 5]]",
            "",
            "t.bandPart(lowerBandCount: -1, upperBandCount: 0)",
            "// [[ 5,  0,  0, 0]",
            "//  [-1,  5,  0, 0]",
            "//  [-2, -1,  5, 0]",
            "//  [-3, -2, -1, 5]]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
