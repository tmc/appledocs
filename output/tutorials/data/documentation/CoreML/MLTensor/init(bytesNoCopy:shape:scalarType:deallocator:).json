    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A pointer to the C-contiguous memory address for the tensor. Expecting the data to be zero-offset, alignment to match",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the alignment of the scalar type, and byte count to be equal or greater than the given shape’s contiguous size.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bytes"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The shape of the tensor.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shape"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The scalar type.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "scalarType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Specifies the mechanism to free the indicated buffer.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "deallocator"
        }
      ]
    }
