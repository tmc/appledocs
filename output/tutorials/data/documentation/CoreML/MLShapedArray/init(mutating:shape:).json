    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The backing pixel buffer. It must be backed by ",
                  "type": "text"
                },
                {
                  "code": "IOSurface",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "pixelBuffer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The shape of the MLShapedArray. The last dimension of ",
                  "type": "text"
                },
                {
                  "code": "shape",
                  "type": "codeVoice"
                },
                {
                  "text": " must match the pixel buffer’s width. The product of the rest of the dimensions must match the height.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "shape"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this initializer to create an ",
              "type": "text"
            },
            {
              "code": "IOSurface",
              "type": "codeVoice"
            },
            {
              "text": " backed ",
              "type": "text"
            },
            {
              "code": "MLShapedArray",
              "type": "codeVoice"
            },
            {
              "text": ", which can reduce the inference latency by avoiding the buffer copy.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The pixel buffer’s pixel format type must be ",
              "type": "text"
            },
            {
              "code": "OneComponent16Half",
              "type": "codeVoice"
            },
            {
              "text": ". As such, the scalar type must be ",
              "type": "text"
            },
            {
              "code": "Float16",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var pixelBuffer: CVPixelBuffer?",
            "let pixelBufferAttributes = [",
            "    kCVPixelBufferIOSurfacePropertiesKey : [:]",
            "]",
            "// Pixel buffer's width is the last dimension of `shape`, which is 4.",
            "// The height is the product of the rest of the dimensions, which is",
            "// 2 * 3 = 6.",
            "CVPixelBufferCreate(kCFAllocatorDefault,",
            "                    4, 6,",
            "                    kCVPixelFormatType_OneComponent16Half,",
            "                    pixelBufferAttributes as CFDictionary,",
            "                    &pixelBuffer)",
            "",
            "let shapedArray = MLShapedArray<Float16>(mutating: pixelBuffer!,",
            "                                         shape: [2, 3, 4])"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When there is one and only one owner of the shaped array, mutating operations modifies the underlying pixel buffer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var shapedArray = MLShapedArray<Float16>(mutating: pixelBuffer, shape: [1])",
            "shapedArray[scalarAt: 0] = 42",
            "// The pixel buffer now has 42 in its frame buffer."
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "It follows the value semantics. The mutation doesn’t affect the copy.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "var array1 = MLShapedArray<Float16>(mutating: pixelBuffer, shape: [1])",
            "array1[scalarAt: 0] = 0 // pixelBuffer is mutated.",
            "let array2 = array1",
            "array1[scalarAt: 0] = 42 // Copy-on-Write",
            "",
            "assert(array1[scalarAt: 0] == 42)",
            "assert(array2[scalarAt: 0] == 0)"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "It is undefined behavior to mutate the pixel buffer directly without using ",
              "type": "text"
            },
            {
              "code": ".withMutablePixelBufferIfAvailable",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
