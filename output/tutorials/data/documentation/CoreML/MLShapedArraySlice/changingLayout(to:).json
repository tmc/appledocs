    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The desired buffer layout.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bufferLayout"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The returned shaped array slice will have ",
              "type": "text"
            },
            {
              "code": ".strides",
              "type": "codeVoice"
            },
            {
              "text": " property according to the requested",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "layout.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The function may return a heap-memory backed shaped array even if ",
              "type": "text"
            },
            {
              "code": "self",
              "type": "codeVoice"
            },
            {
              "text": " is backed by a",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "pixel buffer.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let source = MLShapedArray<Int32>(scalars: 0..., shape: [4, 4])",
            "let slice = source[1...2, 1...2] // slice.shape == [2, 2]",
            "",
            "// Returns a new MLShapedArraySlice with the specified strides.",
            "_ = slice.changingLayout(to: .custom(strides: [3, 1]))",
            "",
            "// Returns a new MLShapedArraySlice with the first-major contiguous layout.",
            "_ = source.changingLayout(to: .firstMajorContiguous) // strides = [2, 1]",
            "",
            "// Returns a new MLShapedArraySlice with the last-major contiguous layout.",
            "_ = source.changingLayout(to: .lastMajorContiguous) // strides = [1, 2]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "withUnsafeShapedBufferPointer",
              "type": "codeVoice"
            },
            {
              "text": " function provides read-only access to the underlying",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "buffer of the layout.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "code": "withUnsafeMutableShapedBufferPointer(body:)",
              "type": "codeVoice"
            },
            {
              "text": " function may provide a buffer of",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "different layout due to copy-on-write. Use",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "code": "withUnsafeMutableShapedBufferPointer(bufferLayout:body:)",
              "type": "codeVoice"
            },
            {
              "text": " if you need a specific buffer",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "layout.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "It raises a precondition error if the custom strides and the shape have different ranks.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
