        },
        {
          "inlineContent": [
            {
              "text": "The shape of the new ",
              "type": "text"
            },
            {
              "code": "MLShapedArraySlice",
              "type": "codeVoice"
            },
            {
              "text": " gets a new dimension of 1 at the specified axis index.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let original = MLShapedArraySlice<Int32>(scalars: 0..., shape: [2, 3])",
            "let expanded = original.expanded(alongAxis: 0)",
            "expanded.shape // [1, 2, 3]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
