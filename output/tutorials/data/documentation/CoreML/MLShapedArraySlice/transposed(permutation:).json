    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this method to convert, for example, the image data layout from ",
              "type": "text"
            },
            {
              "code": "[C, H, W]",
              "type": "codeVoice"
            },
            {
              "text": " to ",
              "type": "text"
            },
            {
              "code": "[C, W, H]",
              "type": "codeVoice"
            },
            {
              "text": ", where ",
              "type": "text"
            },
            {
              "code": "C",
              "type": "codeVoice"
            },
            {
              "text": " is channel, ",
              "type": "text"
            },
            {
              "code": "W",
              "type": "codeVoice"
            },
            {
              "text": " is width, and ",
              "type": "text"
            },
            {
              "code": "H",
              "type": "codeVoice"
            },
            {
              "text": " is height.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "// The source tensor has 3 channels, 128 x 64 image in [C, H, W] layout.",
            "let imageCHW = MLShapedArray<Int32>(scalars: pixelValues,",
            "                                    shape: [3, 64, 128])",
            "// Slice for the first two channels.",
            "let imageSliceCHW = imageCHW[0..<2]",
            "",
            "// Transpose.",
            "let imageSliceCWH = imageSliceCHW.transposed(permutation: [0, 2, 1])",
            "imageSliceCHW.shape // [2, 64, 128]",
            "imageSliceCWH.shape // [2, 128, 64]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The shape (",
              "type": "text"
            },
            {
              "code": "shape_out",
              "type": "codeVoice"
            },
            {
              "text": ") is transposed from the input shape (",
              "type": "text"
            },
            {
              "code": "shape_in",
              "type": "codeVoice"
            },
            {
              "text": ") as follows.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "shape_out[i]",
            "  == permutation.map { shape_in[$0] }"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The scalar value of the output shaped array (",
              "type": "text"
            },
            {
              "code": "array_out",
              "type": "codeVoice"
            },
            {
              "text": ") is related to the input shaped array (",
              "type": "text"
            },
            {
              "code": "array_in",
              "type": "codeVoice"
            },
            {
              "text": ") as follows.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "array_out(scalarAt: permutation.map { indices[$0] })",
            "  == array_in[scalarAt: indices]]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
