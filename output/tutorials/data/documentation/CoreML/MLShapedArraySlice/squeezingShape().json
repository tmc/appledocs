    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The new shape removes 1s in the original shape.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let original = MLShapedArraySlice<Int32>(scalars: 0..., shape: [1, 2, 1, 2])",
            "let squeezed = original.squeezed()",
            "squeezed.shape // [2, 2]"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "When all the dimensions of the original shape is one, the resultant",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "shaped array is a scalar.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let original = MLShapedArray<Int32>(scalars: 42, shape: [1, 1])",
            "let squeezed = original.squeezed()",
            "squeezed.scalar // 42"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
