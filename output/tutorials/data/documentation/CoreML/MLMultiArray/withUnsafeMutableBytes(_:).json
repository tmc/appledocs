    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The buffer contains a collection of ",
              "type": "text"
            },
            {
              "code": "int32",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "float16",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "float32",
              "type": "codeVoice"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "code": "float64",
              "type": "codeVoice"
            },
            {
              "text": " values, depending on the multiarray’s data type. It may not store these scalar values contiguously; use ",
              "type": "text"
            },
            {
              "code": "strides",
              "type": "codeVoice"
            },
            {
              "text": " to get the buffer layout.",
              "type": "text"
