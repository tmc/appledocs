    },
    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.coreml/documentation/CoreML/MLMultiArray",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " array.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "multiArrays"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A zero-based axis number the instances in ",
                  "type": "text"
                },
                {
                  "code": "multiArray",
                  "type": "codeVoice"
                },
                {
                  "text": " merge along.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axis"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.coreml/documentation/CoreML/MLMultiArrayDataType",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " instance that represents the underlying type of all the instances in ",
                  "type": "text"
                },
                {
                  "code": "multiArrays",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dataType"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "All multiarray instances in ",
              "type": "text"
            },
            {
              "code": "multiArrays",
              "type": "codeVoice"
            },
            {
              "text": " must have:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The same data type",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The same number of dimensions",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The same size for each corresponding dimension, except for the concatenation axis",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "For example, this code concatenates two multiarrays along their first dimension:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "let multiarray1 = try MLMultiArray(shape: [1, 5, 7], dataType: .int32)",
            "let multiarray2 = try MLMultiArray(shape: [2, 5, 7], dataType: .int32)",
            "",
            "// Merge the two multiarrays along the first dimension.",
            "let multiArray3 = MLMultiArray(concatenating: [multiarray1, multiarray2],",
            "                               axis: 0,",
            "                               dataType: .int32)",
            "",
            "assert(multiArray3.shape == [3, 5, 7])"
          ],
          "syntax": "swift",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
