{
  "abstract": [
    {
      "text": "A string that encodes the hardware and software release versions",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "Use this string to determine which applet bundle that you have submitted on ABR",
      "type": "text"
    },
    {
      "text": " ",
      "type": "text"
    },
    {
      "text": "to install on this device.",
      "type": "text"
    }
