      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Example value: “JCOP X.Y”",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "- X: Integer corresponding to chip type",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "Y: Integer corresponding to release iteration",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "For more details, refer to the JCOP official version on NFC & SE Platform Mobile Secure Element Specification",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "gated behind NDA on Apple Business Register Portal.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
