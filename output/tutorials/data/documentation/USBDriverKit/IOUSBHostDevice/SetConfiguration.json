          "content": [
            {
              "inlineContent": [
                {
                  "text": "The configuration to select. You can get this value from the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.usbdriverkit/documentation/USBDriverKit/IOUSBConfigurationDescriptor/bConfigurationValue",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " field of the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.usbdriverkit/documentation/USBDriverKit/IOUSBConfigurationDescriptor",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " structure.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bConfigurationValue"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value indicating whether you want the system to perform matching on the interfaces of the new configuration. Specify ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/swift/false",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " to skip the matching process.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "matchInterfaces"
        }
      ]
    },
