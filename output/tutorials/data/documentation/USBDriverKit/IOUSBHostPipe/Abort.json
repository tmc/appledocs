    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Options for how to abort the requests. For a list of possible values, see ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.usbdriverkit/documentation/USBDriverKit/IOUSBAbortOptions",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "options"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The error value to report for each request. Specify ",
                  "type": "text"
                },
                {
                  "code": "kIOReturnAborted",
                  "type": "codeVoice"
                },
                {
                  "text": " for this parameter.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "withError"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The service that initiated the requests. Specify a non ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " value for this parameter only for pipes associated with a control endpoint; specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " for other endpoint types. For a control endpoint, you can also specify ",
                  "type": "text"
                },
                {
                  "code": "NULL",
                  "type": "codeVoice"
                },
                {
                  "text": " to abort all requests.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "forClient"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
