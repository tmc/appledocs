      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean indicating whether to send a ",
                  "type": "text"
                },
                {
                  "code": "CLEAR_FEATURE",
                  "type": "codeVoice"
                },
                {
                  "text": " request to the device with the value ",
                  "type": "text"
                },
                {
                  "code": "ENDPOINT_HALT",
                  "type": "codeVoice"
                },
                {
                  "text": ". To keep the device’s data toggle synchronized with the host’s data toggle, it’s recommended that you specify ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/swift/true",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ", but you may safely specify ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/swift/false",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for control endpoints. For information about this request, see section 9.4.1 of the USB 2.0 specification.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "withRequest"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
