    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The characteristics of the device request. See the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/iokit/usbmakebmrequesttype",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " macro for information about how to construct this request.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bmRequestType"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The request you want to make.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "bRequest"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Request-specific data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "wValue"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Request-specific data.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "wIndex"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The number of bytes to transfer, if there’s a data stage.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "wLength"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The memory descriptor to use for the request’s data phase, if any.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "dataBuffer"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/driverkit/osaction",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object that contains the method to call when the request finishes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "completion"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The timeout duration in milliseconds. If you specify ",
                  "type": "text"
                },
                {
                  "code": "0",
                  "type": "codeVoice"
                },
                {
                  "text": ", the request never times out.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "completionTimeoutMs"
        }
      ]
    },
    {
      "content": [
        {
