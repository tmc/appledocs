{
  "abstract": [
    {
      "text": "An atom that specifies an entry in a tween sequence.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "Tween media is deprecated in the QuickTime file format. The information that follows documents existing content containing tween media and should not be used for new development.",
          "type": "text"
        }
      ],
      "type": "paragraph"
    }
  ],
