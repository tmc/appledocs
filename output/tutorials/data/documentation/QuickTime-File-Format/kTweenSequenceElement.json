  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Its parent is the tween QT atom container (which you specify with the constant ",
              "type": "text"
            },
            {
              "code": "kParentAtomIsContainer",
              "type": "codeVoice"
            },
            {
              "text": ").",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The ID of a ",
              "type": "text"
            },
            {
              "code": "kTweenSequenceElement",
              "type": "codeVoice"
            },
            {
              "text": " atom must be unique among the ",
              "type": "text"
            },
            {
              "code": "kTweenSequenceElement",
              "type": "codeVoice"
            },
            {
              "text": " atoms in the same QT atom container. The index of a ",
              "type": "text"
            },
            {
              "code": "kTweenSequenceElement",
              "type": "codeVoice"
            },
            {
              "text": " atom specifies its order in the sequence; the first entry in the sequence has the index ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ", the second ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": ", and so on.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is a leaf atom. The data type of its data is ",
              "type": "text"
            },
            {
              "code": "TweenSequenceEntryRecord",
              "type": "codeVoice"
            },
            {
              "text": ", a data structure that contains the following fields:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A value of type Fixed that specifies the point in the duration of the tween media sample at which the sequence entry ends. This is expressed as a percentage; for example, if the value is ",
                        "type": "text"
                      },
                      {
                        "code": "75.0",
                        "type": "codeVoice"
                      },
                      {
                        "text": ", the sequence entry ends after three-quarters of the total duration of the tween media sample have elapsed. The sequence entry begins after the end of the previous sequence entry or, for the first entry in the sequence, at the beginning of the tween media sample.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "endPercent",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A value of type ",
                        "type": "text"
                      },
                      {
                        "code": "QTAtomID",
                        "type": "codeVoice"
                      },
                      {
                        "text": " that specifies the ",
                        "type": "text"
                      },
                      {
                        "code": "kTweenEntry",
                        "type": "codeVoice"
                      },
                      {
                        "text": " atom containing the tween for the sequence element. The ",
                        "type": "text"
                      },
                      {
                        "code": "kTweenEntry",
                        "type": "codeVoice"
                      },
                      {
                        "text": " atom and the ",
                        "type": "text"
                      },
                      {
                        "code": "kTweenSequenceElement",
                        "type": "codeVoice"
                      },
                      {
                        "text": " atom must both be a child atoms of the same tween QT atom container.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "tweenAtomID",
                    "type": "text"
                  }
                ]
              }
            },
            {
              "definition": {
                "content": [
                  {
                    "inlineContent": [
                      {
                        "text": "A value of type ",
                        "type": "text"
                      },
                      {
                        "code": "QTAtomID",
                        "type": "codeVoice"
                      },
                      {
                        "text": " that specifies the ",
                        "type": "text"
                      },
                      {
                        "code": "kTweenData",
                        "type": "codeVoice"
                      },
                      {
                        "text": " atom containing the data for the tween. This atom must be a child atom of the atom specified by the ",
                        "type": "text"
                      },
                      {
                        "code": "tweenAtomID",
                        "type": "codeVoice"
                      },
                      {
                        "text": " field.",
                        "type": "text"
                      }
                    ],
                    "type": "paragraph"
                  }
                ]
              },
              "term": {
                "inlineContent": [
                  {
                    "text": "dataAtomID",
                    "type": "text"
                  }
                ]
              }
            }
          ],
          "type": "termList"
        }
      ],
      "kind": "content"
    }
  ],
