  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The VR world header atom is a leaf atom. Its atom type is ",
              "type": "text"
            },
            {
              "code": "kQTVRWorldHeaderAtomType",
              "type": "codeVoice"
            },
            {
              "text": " (",
              "type": "text"
            },
            {
              "code": "'vrsc'",
              "type": "codeVoice"
            },
            {
              "text": "). It contains the name of the scene and the default node ID to be used when the file is first opened as well as fields reserved for future use.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The structure of a VR world header atom is defined by the ",
              "type": "text"
            },
            {
              "code": "QTVRWorldHeaderAtom",
              "type": "codeVoice"
            },
            {
              "text": " data type.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "typedef struct VRWorldHeaderAtom {",
            "    UInt16                              majorVersion;",
            "    UInt16                              minorVersion;",
            "    QTAtomID                            nameAtomID;",
            "    UInt32                              defaultNodeID;",
            "    UInt32                              vrWorldFlags;",
            "    UInt32                              reserved1;",
            "    UInt32                              reserved2;",
            "} VRWorldHeaderAtom, *QTVRWorldHeaderAtomPtr;",
            "QT",
            "QT"
          ],
          "syntax": "c",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
