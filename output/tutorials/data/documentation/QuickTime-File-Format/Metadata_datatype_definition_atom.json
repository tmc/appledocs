  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A metadata datatype definition atom can be used to specify the data type of the metadata key atom’s value. The type of the metadata datatype definition atom is ",
              "type": "text"
            },
            {
              "code": "‘dtyp’",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The layout of a metadata datatype definition atom is as follows:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "header": "row",
          "rows": [
            [
              [
                {
                  "inlineContent": [
                    {
                      "text": "Data field",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "Bytes",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.qtff/documentation/quicktime-file-format/Metadata_datatype_definition_atom/Size",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "4",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.qtff/documentation/quicktime-file-format/Metadata_datatype_definition_atom/Type",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": " = ",
                      "type": "text"
                    },
                    {
                      "code": "'dtyp'",
                      "type": "codeVoice"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "4",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.qtff/documentation/quicktime-file-format/Metadata_datatype_definition_atom/Datatype_namespace",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "4",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ],
            [
              [
                {
                  "inlineContent": [
                    {
                      "identifier": "doc://com.apple.qtff/documentation/quicktime-file-format/Metadata_datatype_definition_atom/Datatype_array",
                      "isActive": true,
                      "type": "reference"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              [
                {
                  "inlineContent": [
                    {
                      "text": "variable",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            ]
          ],
          "type": "table"
        },
        {
          "inlineContent": [
            {
              "text": "The combination of ",
              "type": "text"
            },
            {
              "code": "datatype namespace",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "datatype array",
              "type": "codeVoice"
            },
            {
              "text": " indicate the data type (or structure) of a metadata item value. The ",
              "type": "text"
            },
            {
              "code": "datatype namespace",
              "type": "codeVoice"
            },
            {
              "text": " type indicates the interpretation of the ",
              "type": "text"
            },
            {
              "code": "datatype array",
              "type": "codeVoice"
            },
            {
              "text": " value. This specification defines two ",
              "type": "text"
            },
            {
              "code": "datatype namespace",
              "type": "codeVoice"
            },
            {
              "text": " types:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "datatype namespace",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is ",
                      "type": "text"
                    },
                    {
                      "code": "0",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "datatype array",
                      "type": "codeVoice"
                    },
                    {
                      "text": " contains a big-endian 32-bit unsigned integer corresponding to a well-known type specified in ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.qtff/documentation/QuickTime-File-Format/Well-known_types",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ". For example, a well-known type of ",
                      "type": "text"
                    },
                    {
                      "code": "1",
                      "type": "codeVoice"
                    },
                    {
                      "text": " indicates UTF-8 text and ",
                      "type": "text"
                    },
                    {
                      "code": "23",
                      "type": "codeVoice"
                    },
                    {
                      "text": " indicates a big-endian 32-bit floating-point number.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "If ",
                      "type": "text"
                    },
                    {
                      "code": "datatype namespace",
                      "type": "codeVoice"
                    },
                    {
                      "text": " is ",
                      "type": "text"
                    },
                    {
                      "code": "1",
                      "type": "codeVoice"
                    },
                    {
                      "text": ", ",
                      "type": "text"
                    },
                    {
                      "code": "datatype array",
                      "type": "codeVoice"
                    },
                    {
                      "text": " contains a reverse-address style UTF-8 string indicating an extended data type. This data type namespace type can be used if the data type does not have a corresponding well-known data type. ",
                      "type": "text"
                    },
                    {
                      "code": "Datatype array",
                      "type": "codeVoice"
                    },
                    {
                      "text": " consists of the bytes of a case-sensitive UTF-8 string without a null (",
                      "type": "text"
                    },
                    {
                      "code": "‘\\0’",
                      "type": "codeVoice"
                    },
                    {
                      "text": ") terminator. For example, a hypothetical ",
                      "type": "text"
                    },
                    {
                      "code": "datatype array",
                      "type": "codeVoice"
                    },
                    {
                      "text": " “com.company.my-custom-datatype” could register a custom data type belonging to the owner of the DNS registration “mycompany.com”.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "datatype namespace",
              "type": "codeVoice"
            },
            {
              "text": " other than ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": " may occur in a timed metadata track, perhaps written according to a later version of this specification. Metadata item values with unrecognized data types should be ignored. Even so, some processing is still possible on the metadata item with unrecognized data type, such as copying it between tracks.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "New datatype namespaces must be registered with Apple.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Many uses for proprietary or custom metadata data types can be satisfied by using the extended data type namespace type code ",
                  "type": "text"
                },
                {
                  "code": "1",
                  "type": "codeVoice"
                },
                {
                  "text": ". This allows the new data type to be specified without registration. The reason to add a custom ",
                  "type": "text"
                },
                {
                  "code": "datatype namespace",
                  "type": "codeVoice"
                },
                {
                  "text": " type is to allow an existing numbering or naming scheme from a foreign metadata standard to be used with metadata items.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
