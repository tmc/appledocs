  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "For a tween atom in a tween track of a QuickTime movie, specifies a time offset from the start of the tween media sample to the start of the tween atom. The time units are the units used for the tween track.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Its parent atom is a ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom can contain only one ",
              "type": "text"
            },
            {
              "code": "kTweenStartOffset",
              "type": "codeVoice"
            },
            {
              "text": " atom. The ID of this atom is always ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ". The index of this atom is always ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is a leaf atom. The data type of its data is ",
              "type": "text"
            },
            {
              "code": "TimeValue",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is optional. If it is not included, the tween operation begins at the start of the tween media sample.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
