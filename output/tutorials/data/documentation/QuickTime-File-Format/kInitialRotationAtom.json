  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Specifies an initial angle of rotation for a path tween atom of type ",
              "type": "text"
            },
            {
              "code": "kTweenTypePathToMatrixRotation",
              "type": "codeVoice"
            },
            {
              "text": ", ",
              "type": "text"
            },
            {
              "code": "kTweenTypePathToMatrixTranslation",
              "type": "codeVoice"
            },
            {
              "text": ", or ",
              "type": "text"
            },
            {
              "code": "kTweenTypePathToMatrixTranslationAndRotation",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Its parent atom is a ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom can contain only one ",
              "type": "text"
            },
            {
              "code": "kInitialRotationAtom",
              "type": "codeVoice"
            },
            {
              "text": " atom. The ID of this atom is always ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ". The index of this atom is always ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is a leaf atom. Its data type is ",
              "type": "text"
            },
            {
              "code": "Fixed",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is optional. If it is not included, no initial rotation of the tween atom is performed.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
