  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Specifies an interpolation tween atom to use for a specified ",
              "type": "text"
            },
            {
              "code": "kTweenData",
              "type": "codeVoice"
            },
            {
              "text": " atom. There can be any number of ",
              "type": "text"
            },
            {
              "code": "kTweenInterpolationID",
              "type": "codeVoice"
            },
            {
              "text": " atoms for a tween atom, one for each ",
              "type": "text"
            },
            {
              "code": "kTweenData",
              "type": "codeVoice"
            },
            {
              "text": " atom to be interpolated.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Its parent atom is a ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The index of a ",
              "type": "text"
            },
            {
              "code": "kTweenInterpolationID",
              "type": "codeVoice"
            },
            {
              "text": " atom specifies when it was added to the ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom; the first added has the index ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ", the second ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": ", and so on. The ID of a ",
              "type": "text"
            },
            {
              "code": "kTweenInterpolationID",
              "type": "codeVoice"
            },
            {
              "text": " atom must match the atom ID of the ",
              "type": "text"
            },
            {
              "code": "kTweenData",
              "type": "codeVoice"
            },
            {
              "text": " atom to be interpolated, and be unique among the ",
              "type": "text"
            },
            {
              "code": "kTweenInterpolationID",
              "type": "codeVoice"
            },
            {
              "text": " atoms contained in the same ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is a leaf atom. The data type of its data is ",
              "type": "text"
            },
            {
              "code": "QTAtomID",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is required for an interpolation tween atom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
