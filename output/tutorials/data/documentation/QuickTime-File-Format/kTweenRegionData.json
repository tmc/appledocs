  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Used only by a ",
              "type": "text"
            },
            {
              "code": "kTweenTypeQDRegion",
              "type": "codeVoice"
            },
            {
              "text": " atom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Its parent atom is a ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom can contain only one ",
              "type": "text"
            },
            {
              "code": "kTweenRegionData",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "kTweenPictureData",
              "type": "codeVoice"
            },
            {
              "text": " atom. The ID of this atom is always ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ". The index of this atom is always ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is a leaf atom. The data type of its data is ",
              "type": "text"
            },
            {
              "code": "Region",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Either a ",
              "type": "text"
            },
            {
              "code": "kTweenPictureData",
              "type": "codeVoice"
            },
            {
              "text": " or ",
              "type": "text"
            },
            {
              "code": "kTweenRegionData",
              "type": "codeVoice"
            },
            {
              "text": " atom is required for a ",
              "type": "text"
            },
            {
              "code": "kTweenTypeQDRegion",
              "type": "codeVoice"
            },
            {
              "text": " tween.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
