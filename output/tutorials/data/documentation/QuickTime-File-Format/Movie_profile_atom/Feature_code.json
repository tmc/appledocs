  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The third field is the feature code, or name, a 32-bit unsigned integer that is usually best interpreted as four ASCII characters. Example: the maximum video bit rate feature has a feature code or name of ",
              "type": "text"
            },
            {
              "code": "'mvbr'",
              "type": "codeVoice"
            },
            {
              "text": ". It is permissible to use a feature code value of zero (",
              "type": "text"
            },
            {
              "code": "0x00000000",
              "type": "codeVoice"
            },
            {
              "text": ", not four ASCII zero characters) as a placeholder in one or more name-value pairs. The reader should ignore feature codes of value zero.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
