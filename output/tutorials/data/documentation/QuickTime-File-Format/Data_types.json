            "    Float32                             bestPan;",
            "    Float32                             bestTilt;",
            "    Float32                             bestFOV;",
            "    FloatPoint                          bestViewCenter;",
            "    Rect                                hotSpotRect;",
            "    UInt32                              flags;",
