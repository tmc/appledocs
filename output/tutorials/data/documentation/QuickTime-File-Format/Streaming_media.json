  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "QuickTime movies store streaming data in a streaming media track. This media has a media type of ",
              "type": "text"
            },
            {
              "code": "'strm'",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "anchor": "Streaming-Media-Sample-Description",
          "level": 2,
          "text": "Streaming Media Sample Description",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The streaming media sample description contains information that defines how to interpret streaming media data. This sample description is based on the standard sample description header, as described in ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.qtff/documentation/quicktime-file-format/Sample_table_atom",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The streaming media sample description is documented in the QuickTime header file ",
              "type": "text"
            },
            {
              "code": "QTSMovie.h",
              "type": "codeVoice"
            },
            {
              "text": ", as follows:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "struct QTSSampleDescription {",
            "    long                            descSize;",
            "    long                            dataFormat;",
            "    long                            resvd1;     /* set to 0*/",
            "    short                           resvd2;     /* set to 0*/",
            "    short                           dataRefIndex;",
            "    UInt32                          version;",
            "    UInt32                          resvd3;     /* set to 0*/",
            "    SInt32                          flags;",
            "                                                /* qt atoms follow:*/",
            "                                    /* long size, long type, some  data*/",
            "                                                /* repeat as necessary*/",
            "};",
            "typedef struct QTSSampleDescription     QTSSampleDescription;"
          ],
          "syntax": "c",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "text": "The sample format depends on the ",
              "type": "text"
            },
            {
              "code": "dataFormat",
              "type": "codeVoice"
            },
            {
              "text": " field of the ",
              "type": "text"
            },
            {
              "code": "QTSSampleDescription",
              "type": "codeVoice"
            },
            {
              "text": ". The ",
              "type": "text"
            },
            {
              "code": "dataFormat",
              "type": "codeVoice"
            },
            {
              "text": " field can be any value you specify. The currently defined values are ",
              "type": "text"
            },
            {
              "code": "'rtsp'",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "'sdp '",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If ",
              "type": "text"
            },
            {
              "code": "'rtsp'",
              "type": "codeVoice"
            },
            {
              "text": ", the sample can be just an rtsp URL. It can also be any value that you can put in a ",
              "type": "text"
            },
            {
              "code": ".rtsp",
              "type": "codeVoice"
            },
            {
              "text": " file, as defined at",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "http://streaming.apple.com/qtstreaming/documentation/userdocs/rtsptags.htm",
              "isActive": true,
              "type": "reference"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If ",
              "type": "text"
            },
            {
              "code": "'sdp '",
              "type": "codeVoice"
            },
            {
              "text": ", then the sample is an SDP file. This would be used to receive a multicast broadcast.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
