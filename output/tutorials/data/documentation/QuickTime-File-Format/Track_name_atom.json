  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A movie atomâ€™s user data atom may contain a track name atom (",
              "type": "text"
            },
            {
              "code": "'tnam'",
              "type": "codeVoice"
            },
            {
              "text": ").",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A track can have multiple ",
              "type": "text"
            },
            {
              "code": "'tnam'",
              "type": "codeVoice"
            },
            {
              "text": " atoms with different language codes. Normally it is sufficient for each track to have a single ",
              "type": "text"
            },
            {
              "code": "'tnam'",
              "type": "codeVoice"
            },
            {
              "text": " atom in the same language as the track content. Alternate tracks might also have ",
              "type": "text"
            },
            {
              "code": "'tnam'",
              "type": "codeVoice"
            },
            {
              "text": " atoms; their presence implies only that the name is a good user-readable label for the track.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
