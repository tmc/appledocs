  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A node header atom is a leaf atom that describes the type and ID of a node, as well as other information about the node. Its atom type is ",
              "type": "text"
            },
            {
              "code": "kQTVRNodeHeaderAtomType",
              "type": "codeVoice"
            },
            {
              "text": " (",
              "type": "text"
            },
            {
              "code": "'ndhd'",
              "type": "codeVoice"
            },
            {
              "text": ").",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The structure of a node header atom is defined by the QTVRNodeHeaderAtom data type:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "typedef struct VRNodeHeaderAtom {",
            "    UInt16                              majorVersion;",
            "    UInt16                              minorVersion;",
            "    OSType                              nodeType;",
            "    QTAtomID                            nodeID;",
            "    QTAtomID                            nameAtomID;",
            "    QTAtomID                            commentAtomID;",
            "    UInt32                              reserved1;",
            "    UInt32                              reserved2;",
            "} VRNodeHeaderAtom, *VRNodeHeaderAtomPtr;"
          ],
          "syntax": "c",
          "type": "codeListing"
        }
      ],
      "kind": "content"
    }
  ],
