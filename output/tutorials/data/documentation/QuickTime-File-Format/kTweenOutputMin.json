  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "Overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If both ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMin",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMax",
              "type": "codeVoice"
            },
            {
              "text": " atoms are included for an interpolation tween atom, output values for the tween atom are scaled to be within the minimum and maximum values. For example, if an interpolation tween atom has values between ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "4",
              "type": "codeVoice"
            },
            {
              "text": ", and it has ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMin",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMax",
              "type": "codeVoice"
            },
            {
              "text": " atoms with values ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": ", respectively, a value of ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " (the minimum value before scaling) is scaled to ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": " (the minimum specified by the ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMin",
              "type": "codeVoice"
            },
            {
              "text": " atom), a value of ",
              "type": "text"
            },
            {
              "code": "4",
              "type": "codeVoice"
            },
            {
              "text": " (the maximum value before scaling) is scaled to ",
              "type": "text"
            },
            {
              "code": "2",
              "type": "codeVoice"
            },
            {
              "text": " (the maximum specified by the ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMax",
              "type": "codeVoice"
            },
            {
              "text": " atom), and a value of ",
              "type": "text"
            },
            {
              "code": "3",
              "type": "codeVoice"
            },
            {
              "text": " (three-quarters of the way between the maximum and minimum values before scaling) is scaled to ",
              "type": "text"
            },
            {
              "code": "1.75",
              "type": "codeVoice"
            },
            {
              "text": " (three-quarters of the way between the values of the ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMin",
              "type": "codeVoice"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMax",
              "type": "codeVoice"
            },
            {
              "text": " atoms).",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "If a ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMin",
              "type": "codeVoice"
            },
            {
              "text": " atom is included but a ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMax",
              "type": "codeVoice"
            },
            {
              "text": " atom is not, QuickTime does not scale interpolation tween values.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Its parent atom is a ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "A ",
              "type": "text"
            },
            {
              "code": "kTweenEntry",
              "type": "codeVoice"
            },
            {
              "text": " atom can contain only one ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMin",
              "type": "codeVoice"
            },
            {
              "text": " atom. The ID of this atom is always ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ". The index of this atom is always ",
              "type": "text"
            },
            {
              "code": "1",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is a leaf atom. The data type of its data is ",
              "type": "text"
            },
            {
              "code": "Fixed",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This atom is optional. If it is not included but a ",
              "type": "text"
            },
            {
              "code": "kTweenOutputMax",
              "type": "codeVoice"
            },
            {
              "text": " atom is, the tween component uses ",
              "type": "text"
            },
            {
              "code": "0",
              "type": "codeVoice"
            },
            {
              "text": " as the minimum value for scaling values of an interpolation tween atom.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
  ],
