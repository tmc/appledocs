      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A unique string that identifies your application. Your ",
                  "type": "text"
                },
                {
                  "code": "sessionID",
                  "type": "codeVoice"
                },
                {
                  "text": " should be the short name of an approved Bonjour service type. If ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", the session uses the application’s bundle identifier to create a ",
                  "type": "text"
                },
                {
                  "code": "sessionID",
                  "type": "codeVoice"
                },
                {
                  "text": " string.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "sessionID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A string identifying the user to display to other peers. If ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", the session uses the device name.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "name"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The mode the session should run in. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.gamekit/documentation/GameKit/GKSessionMode",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " for possible values.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "mode"
        }
      ]
    },
    {
      "content": [
