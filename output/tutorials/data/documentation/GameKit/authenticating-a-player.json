    "doc://com.apple.gamekit/documentation/GameKit/GKLocalPlayer/isMultiplayerGamingRestricted": {
      "abstract": [
        {
          "text": "A Boolean value that indicates whether the player can join multiplayer games.",
          "type": "text"
        }
      ],
      "fragments": [
        {
          "kind": "keyword",
          "text": "var"
        },
        {
          "kind": "text",
          "text": " "
        },
        {
          "kind": "identifier",
          "text": "isMultiplayerGamingRestricted"
        },
