{
  "abstract": [
    {
      "text": "Save game data during play or after a game in the player’s iCloud account that’s accessible from any device.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
