{
  "abstract": [
    {
      "text": "Set criteria for assigning players to teams in your game using matchmaking rules.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
