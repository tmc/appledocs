{
  "abstract": [
    {
      "text": "Add a rule that lets players invite players and join matches using a shared party code.",
      "type": "text"
    }
  ],
  "hierarchy": {
    "paths": [
      [
        "doc://com.apple.documentation/documentation/technologies",
