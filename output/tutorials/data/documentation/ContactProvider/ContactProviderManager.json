      "kind": "declarations"
    },
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Use this class to initially enable your contact provider app extension, making it visible to the system and other apps. To use the default domain, create a manager and call ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.ContactProvider/documentation/ContactProvider/ContactProviderManager/enable()",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ". This is an ",
              "type": "text"
            },
            {
              "code": "async",
              "type": "codeVoice"
            },
            {
              "text": " call, because it may prompt the person using your app to approve turning on the contact provider.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "import ContactProvider",
            "",
            "func enableExtensionExample() async {",
            "    do {",
            "        // The app creates a contact provider manager with a default domain.",
            "        let manager = try ContactProviderManager()",
            "        ",
            "        // May prompt the person to enable the default domain.",
            "        try await manager.enable()",
            "    } catch {",
            "        // Handle the error.",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "anchor": "Signaling-the-app-extension",
          "level": 3,
          "text": "Signaling the app extension",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "You also use ",
              "type": "text"
            },
            {
              "code": "ContactProviderManager",
              "type": "codeVoice"
            },
            {
              "text": " when you need to invoke the app extension on demand. For example, when your app knows new contacts are available from your server, call ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.ContactProvider/documentation/ContactProvider/ContactProviderManager/signalEnumerator(for:)",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " so the extension can fetch and provide the changed contacts.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "import ContactProvider",
            "",
            "func refreshForNewContacts() async {",
            "    do {",
            "        let manager = try ContactProviderManager()",
            "        try await manager.signalEnumerator()",
            "    } catch {",
            "        // Handle the error.",
            "    }",
            "}"
          ],
          "syntax": "swift",
          "type": "codeListing"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "You can only use ",
                  "type": "text"
                },
                {
                  "code": "ContactProviderManager",
                  "type": "codeVoice"
                },
                {
                  "text": " in an app, not in an app extension.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Important",
          "style": "important",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
