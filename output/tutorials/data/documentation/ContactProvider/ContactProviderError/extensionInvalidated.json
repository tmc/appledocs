    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "A loaded and enumerating extension can become invalidated for many reasons, including:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The app invalidates the extension by calling ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.ContactProvider/documentation/ContactProvider/ContactProviderManager/invalidate()",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The app disables the domain by calling ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.ContactProvider/documentation/ContactProvider/ContactProviderManager/disable()",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The app resets the domain by calling ",
                      "type": "text"
                    },
                    {
                      "identifier": "doc://com.apple.ContactProvider/documentation/ContactProvider/ContactProviderManager/reset()",
                      "isActive": true,
                      "type": "reference"
                    },
                    {
                      "text": ".",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "The person using the app disables the domain in the Settings app.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        }
      ],
      "kind": "content"
    }
  ],
