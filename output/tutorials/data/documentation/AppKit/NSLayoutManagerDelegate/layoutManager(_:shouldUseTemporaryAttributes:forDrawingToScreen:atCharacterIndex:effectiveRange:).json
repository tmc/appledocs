              "isActive": true,
              "type": "reference"
            },
            {
              "text": " and ",
              "type": "text"
            },
            {
              "code": "nil",
              "type": "codeVoice"
            },
            {
              "text": " otherwise, without changing ",
              "type": "text"
            },
            {
              "code": "effectiveCharRange",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
