    {
      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appkit/documentation/AppKit/NSAlert",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " class places the accessory view between the informative text or suppression checkbox (if present) and the response buttons. Before you change the location of the accessory view, first call the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appkit/documentation/AppKit/NSAlert/layout()",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " method.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "identifier": "doc://com.apple.appkit/documentation/AppKit/NSAlert/alertStyle",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " shows an example of adding an accessory view to an alert. ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appkit/documentation/AppKit/NSAlert/buttons",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " shows the alert generated.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "Listing 1. Adding an accessory view to an alert",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "code": [
            "NSTextView *accessory = [[NSTextView alloc] initWithFrame:NSMakeRect(0,0,200,15)];",
            "NSFont *font = [NSFont systemFontOfSize:[NSFont systemFontSize]];",
            "NSDictionary *textAttributes = [NSDictionary dictionaryWithObject:font forKey:NSFontAttributeName];",
            "[accessory insertText:[[NSAttributedString alloc] initWithString:@\"Text in accessory view.\"",
            "                                                      attributes:textAttributes]];",
            "[accessory setEditable:NO];",
            "[accessory setDrawsBackground:NO];",
            " ",
            "NSAlert *alert = [[NSAlert alloc] init];",
            "alert.messageText = @\"Message text.\";",
            "[alert setInformativeText:@\"Informative text.\"];",
            "alert.accessoryView = accessory;",
            "[alert runModal];",
            "[alert release];"
          ],
          "syntax": "objc",
          "type": "codeListing"
        },
        {
          "inlineContent": [
            {
              "identifier": "media-1965585",
              "type": "image"
            }
          ],
          "type": "paragraph"
        }
      ],
