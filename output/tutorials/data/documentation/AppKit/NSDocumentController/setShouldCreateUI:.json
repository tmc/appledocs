{
  "abstract": [
    {
      "text": "Sets whether the window controllers of a document should be created when the document is created.",
      "type": "text"
    }
  ],
  "deprecationSummary": [
    {
      "inlineContent": [
        {
          "text": "Use the display parameter of ",
