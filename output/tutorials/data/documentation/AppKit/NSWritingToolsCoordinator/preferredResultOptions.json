      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "Writing Tools can create plain text or rich text, and it can format text",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "using lists or tables as needed. If your view doesnâ€™t support specific",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "types of content, specify the types you do support in this property.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The default value of this property is ",
              "type": "text"
            },
            {
              "code": "NSWritingToolsResult/default",
              "type": "codeVoice"
            },
            {
              "text": ",",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "which lets the system determine the type of content to generate.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
