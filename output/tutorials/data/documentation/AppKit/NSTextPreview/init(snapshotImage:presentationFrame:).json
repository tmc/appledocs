          "content": [
            {
              "inlineContent": [
                {
                  "text": "An image that contains the requested text from your view.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Create the image using a transparent background and the current rendering",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "attributes for your text.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "snapshotImage"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A rectangle in your frame’s coordinate space. The",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "system uses this rectangle to place your image precisely over your view’s",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "actual text. Set its size to the size of your snapshot image, and set its",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "origin to the point that allows the system to place your image directly over the text.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "presentationFrame"
        }
      ]
    }
