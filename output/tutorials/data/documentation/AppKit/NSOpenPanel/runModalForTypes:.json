      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of file extensions and/or HFS file types. Specifies the files the panel allows the user to select. ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " makes all files selectable by the user. An array of types passed in here will override one set using ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appkit/documentation/AppKit/NSSavePanel/allowedFileTypes",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "fileTypes"
        }
      ]
    },
    {
      "content": [
