      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If any view on the screen has this enabled, the screen which the OpenGL surface is on may have its ",
              "type": "text"
            },
            {
              "code": "maximumExtendedDynamicRangeColorComponentValue",
              "type": "codeVoice"
            },
            {
              "text": " value increased. When composited by the Window Server, color values rendered by this OpenGL surface will be clamped to the screenâ€™s ",
              "type": "text"
            },
            {
              "code": "maximumExtendedDynamicRangeColorComponentValue",
              "type": "codeVoice"
            },
            {
              "text": " value rather than ",
              "type": "text"
            },
            {
              "code": "1.0",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
