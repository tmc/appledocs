        {
          "inlineContent": [
            {
              "text": "This method calls the ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appkit/documentation/AppKit/NSViewController/present(_:animator:)",
              "isActive": true,
              "type": "reference"
