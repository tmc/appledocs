      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/true",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", then when an incremental search begins, the ",
              "type": "text"
            },
            {
              "code": "findBarContainer",
              "type": "codeVoice"
            },
            {
              "text": " instanceâ€™s parent ",
              "type": "text"
            },
            {
              "code": "contentView",
              "type": "codeVoice"
            },
            {
              "text": " will be dimmed, except for the locations of the incremental matches. If ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/false",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ", then the incremental matches will not be highlighted automatically, but you can use incrementalMatchRanges to highlight the matches yourself.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "The default value is ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/true",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
