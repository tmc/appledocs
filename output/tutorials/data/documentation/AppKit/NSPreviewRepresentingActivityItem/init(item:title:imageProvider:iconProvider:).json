          "content": [
            {
              "inlineContent": [
                {
                  "text": "The item to share from the Mac share sheet. The item must conform to the ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appkit/documentation/AppKit/NSPasteboardWriting",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " protocol, or be an ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsitemprovider",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.appkit/documentation/AppKit/NSDocument",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " object.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "item"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The localized name of the item.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "title"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object that provides the image to display as a preview for the item. For example, when you share a URL for a webpage, you might specify the hero image for that page or a rendering of the webpage itself.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "imageProvider"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An object that a thumbnail-size image of the source of the item. Typically, you specify your appâ€™s icon but you can provide a different icon if the content has a different source.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "iconProvider"
        }
      ]
    },
