    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The text view sending the message. This is the first text view in a series shared by a layout manager, not necessarily the text view displaying the selected text.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "textView"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The array of ranges of characters to be replaced. This array must be a non-nil, non-empty array of objects responding to the NSValue ",
                  "type": "text"
                },
                {
                  "code": "rangeValue",
                  "type": "codeVoice"
                },
                {
                  "text": " method, and in addition its elements must be sorted, non-overlapping, non-contiguous, and (except for the case of a single range) have non-zero-length.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "affectedRanges"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The array of strings that will replace the characters in ",
                  "type": "text"
                },
                {
                  "code": "affectedRanges",
                  "type": "codeVoice"
                },
                {
                  "text": ", one string for each range; ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " if only text attributes are being changed.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "replacementStrings"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "return-value",
          "level": 2,
          "text": "Return Value",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/true",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to allow the replacement, or ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.documentation/documentation/swift/false",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " to reject the change.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
