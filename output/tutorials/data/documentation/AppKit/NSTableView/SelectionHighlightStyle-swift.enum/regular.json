        {
          "inlineContent": [
            {
              "text": "The regular highlight style of NSTableView. In OS X v10.7 a light blue (returned by sending ",
              "type": "text"
            },
            {
              "code": "NSColor",
              "type": "codeVoice"
            },
            {
              "text": " a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appkit/documentation/AppKit/NSColor/alternateSelectedControlColor",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " message) or light gray color (returned by sending NSColor a ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.appkit/documentation/AppKit/NSColor/secondarySelectedControlColor",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " message).",
              "type": "text"
            }
          ],
          "type": "paragraph"
