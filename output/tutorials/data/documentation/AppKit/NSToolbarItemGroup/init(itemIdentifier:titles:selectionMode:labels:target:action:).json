            }
          ],
          "name": "itemIdentifier"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An array of titles to present as subitems in the grouped toolbar item.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "titles"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "A Boolean value that indicates how the grouped toolbar item presents selections.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "selectionMode"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "Labels that correspond to the specified titles.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "labels"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The target that the toolbar calls upon selection.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            },
            {
              "inlineContent": [
                {
                  "text": "If target is ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": ", the toolbar attempts to invoke the specified action on the first responder and, failing that, passes the action up the responder chain.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "target"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The selector that the toolbar invokes on the target.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "action"
        }
      ]
    }
