    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "An ",
                  "type": "text"
                },
                {
                  "code": "NSColorSpace",
                  "type": "codeVoice"
                },
                {
                  "text": " object representing a color space. An exception is raised if the color model of the provided color space is not RGB.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "space"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The hue (color) component, expressed as a floating-point value in the range 0–1.0.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "hue"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The color saturation component, expressed as a floating-point value in the range 0–1.0.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "saturation"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The brightness component, expressed as a floating-point value in the range 0–1.0.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "brightness"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The alpha (opacity), expressed as a floating-point value in the range 0 (transparent) to 1.0 (opaque).",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "alpha"
        }
      ]
    },
