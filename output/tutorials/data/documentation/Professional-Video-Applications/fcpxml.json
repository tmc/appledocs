  "primaryContentSections": [
    {
      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "The root element in an FCPXML document isÂ ",
              "type": "text"
            },
            {
              "code": "fcpxml",
              "type": "codeVoice"
            },
            {
              "text": ", which can contain the following elements:",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "A ",
                      "type": "text"
                    },
                    {
                      "code": "resources",
                      "type": "codeVoice"
                    },
                    {
                      "text": " element, that contains descriptions of media assets and other resources.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "An optional ",
                      "type": "text"
                    },
                    {
                      "code": "import-options",
                      "type": "codeVoice"
                    },
                    {
                      "text": " element, that controls how Final Cut Pro imports the FCPXML document.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "One of the following optional elements that describe how to organize and use media assets:  a ",
                      "type": "text"
                    },
                    {
                      "code": "library",
                      "type": "codeVoice"
                    },
                    {
                      "text": " element that contains a list of ",
                      "type": "text"
                    },
                    {
                      "code": "event",
                      "type": "codeVoice"
                    },
                    {
                      "text": " elements; a series of ",
                      "type": "text"
                    },
                    {
                      "code": "event",
                      "type": "codeVoice"
                    },
                    {
                      "text": " elements that contain story elements and ",
                      "type": "text"
                    },
                    {
                      "code": "project",
                      "type": "codeVoice"
                    },
                    {
                      "text": " elements; or a combination of story elements and ",
                      "type": "text"
                    },
                    {
                      "code": "<project>",
                      "type": "codeVoice"
                    },
                    {
                      "text": " elements.",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ],
          "type": "unorderedList"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "Starting in FCPXML 1.9, the elements that describe how to organize and use media assets are optional. The only required element in the ",
                  "type": "text"
                },
                {
                  "code": "fcpxml",
                  "type": "codeVoice"
                },
                {
                  "text": " root element is the ",
                  "type": "text"
                },
                {
                  "code": "resources",
                  "type": "codeVoice"
                },
                {
                  "text": " element.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
  ],
