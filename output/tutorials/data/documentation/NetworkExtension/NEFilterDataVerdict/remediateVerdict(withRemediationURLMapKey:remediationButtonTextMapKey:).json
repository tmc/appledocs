    {
      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key in the Filter Control Provider’s ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.networkextension/documentation/NetworkExtension/NEFilterControlProvider/remediationMap",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " dictionary corresponding to the URL of the remediation link to give to the user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "remediationURLMapKey"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The key in the Filter Control Provider’s ",
                  "type": "text"
                },
                {
                  "code": "remediationMap",
                  "type": "codeVoice"
                },
                {
                  "text": " dictionary that corresponds to the text of the remediation link text to give to the user.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
