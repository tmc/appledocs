      "content": [
        {
          "anchor": "Discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This key specifies the display name for the application, if an alternate name is desired. If this property is not specified, the application’s name is used.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "This name appears in Settings -> Wi-Fi underneath the Wi-Fi network name if the helper indicated that it was able to handle the network.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
