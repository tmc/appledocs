      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The SSID of the WEP or WPA/WPA2 personal Wi-Fi Network. See ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.networkextension/documentation/NetworkExtension/NEHotspotConfiguration/ssid",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "SSID"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The network’s passphrase credential: for WPA or WPA2 personal networks, 8-63 characters; for static 64-bit WEP, 10 hexadecimal digits; for static 128-bit WEP, 26 hexadecimal digits.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "passphrase"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "If ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ", the network is WEP Wi-Fi; otherwise it is a WPA or WPA2 personal Wi-Fi network.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "isWEP"
        }
      ]
    }
