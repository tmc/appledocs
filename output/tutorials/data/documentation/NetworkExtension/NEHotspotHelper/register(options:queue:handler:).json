          "content": [
            {
              "inlineContent": [
                {
                  "text": "If not nil, a ",
                  "type": "text"
                },
                {
                  "identifier": "doc://com.apple.documentation/documentation/foundation/nsdictionary",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " containing ",
                  "type": "text"
                },
                {
                  "code": "kNEHotspotHelperOption*",
