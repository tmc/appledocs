                },
                {
                  "text": " keys (currently just ",
                  "type": "text"
                },
                {
                  "code": "kNEHotspotHelperOptionDisplayName",
                  "type": "codeVoice"
                },
                {
                  "text": ").",
                  "type": "text"
                }
