      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The remote endpoint to send UDP datagrams to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "remoteEndpoint"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The local endpoint to bind the UDP session to. If nil, the UDP session will be bound to an ephemeral port on the primary physical interface.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "localEndpoint"
        }
      ]
    },
    {
      "content": [
