          "inlineContent": [
            {
              "text": "An ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FinanceKit/documentation/FinanceKit/AuthorizationStatus",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " value that indicates the current state of authorization.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "If there are no accounts are available to display, the framework presents a “No Accounts”",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "screen and returns a status of ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FinanceKit/documentation/FinanceKit/AuthorizationStatus/authorized",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " or",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "identifier": "doc://com.apple.FinanceKit/documentation/FinanceKit/AuthorizationStatus/denied",
              "isActive": true,
              "type": "reference"
            },
            {
              "text": " depending on the state of a person’s consent.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "inlineContent": [
            {
              "text": "It’s safe to call this method multiple times; the framework prompts a person only if necessary.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        }
      ],
      "kind": "content"
    }
